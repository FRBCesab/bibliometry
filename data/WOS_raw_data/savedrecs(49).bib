@article{ ISI:000220396600005,
Author = {Reshetin, VP and Kazazyan, VI},
Title = {{Public-health impact of outdoor air pollution in Russia}},
Journal = {{ENVIRONMENTAL MODELING \& ASSESSMENT}},
