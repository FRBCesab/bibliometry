@inproceedings{ ISI:000189426400057,
Author = {Veenker, H and Ulber, B},
Editor = {{Simon, JC and Dedryver, CA and Rispe, C and Hulle, M}},
Title = {{Phenology and population structure in the autumn flight of Rhopalosiphum
   padi in Northern Germany}},
Booktitle = {{APHIDS IN A NEW MILLENNIUM}},
Series = {{SCIENCE UPDATE}},
Year = {{2004}},
Pages = {{375-381}},
Note = {{6th International Symposium on Aphids, Rennes, FRANCE, SEP, 2001}},
Abstract = {{The flight activity of the bird-cherry oat aphid, Rhopalosiphum padi
   (L.) was monitored at Gottingen (Northern-Germany) in autumn 1993-1997
   by using two types of Rothamsted suction traps (12.2m and 1.7m above
   ground). To determine the phenology and relative abundance of holocyclic
   and anholocyclic morphs, virginoparae and gynoparae were identified by
   the morph of their offspring. The size of the autumn flight varied
   largely between years. High flight activities of females were observed
   from mid September to mid October. Males were mainly caught throughout
   October. The peak migration of females occurred 7-31 days earlier than
   that of males. The sex ratio (gynoparae: males) varied between 1:
   0.1-0.3 in 1993, 1996 and 1997 and between 1: 0.8-1.2 in 1994 and 1995.
   In all years, gynoparae predominated in the female population (>82\%).
   In 1993-1995, a higher percentage of virginoparae was caught in both
   traps (5.4-17.9\%) than in 1996-1997, when virginoparae were only
   trapped at 1.7m (0.2-2.4\%). In the year 1995, the flight of
   virginoparae was exceptionally extended throughout October, resulting in
   high densities in newly sown winter barley. The number of alatae in the
   crop was not correlated significantly with the abundance of
   virginoparae.}},
