@article{ ISI:000220897300006,
Author = {Koukaras, K and Nikolaidis, G},
Title = {{Dinophysis blooms in Greek coastal waters (Thermaikos Gulf, NW Aegean
   Sea)}},
Journal = {{JOURNAL OF PLANKTON RESEARCH}},
