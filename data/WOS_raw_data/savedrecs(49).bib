@article{ ISI:000222936600008,
Author = {Branco, M and Jactel, H and Silva, EB and Binazzi, A and Mendel, Z},
Title = {{Effect of trap design, trap size and pheromone dose on male capture of
   two pine bast scales species (Hemiptera : Matsucoccidae): implications
   for monitoring and mass-trapping}},
Journal = {{AGRICULTURAL AND FOREST ENTOMOLOGY}},
