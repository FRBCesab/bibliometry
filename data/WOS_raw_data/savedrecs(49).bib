@article{ ISI:000221403900008,
Author = {Newell, RIE},
Title = {{Ecosystem influences of natural and cultivated populations of
   suspension-feeding bivalve molluscs: A review}},
Journal = {{JOURNAL OF SHELLFISH RESEARCH}},
