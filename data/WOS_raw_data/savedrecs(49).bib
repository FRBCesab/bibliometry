@article{ ISI:000222409000006,
Author = {Sandland, GJ and Minchella, DJ},
Title = {{Context-dependent life-history variation in a pond snail (Lymnaea
   elodes) exposed to desiccation and a sterilizing parasite}},
Journal = {{ECOSCIENCE}},
Year = {{2004}},
Volume = {{11}},
Number = {{2}},
Pages = {{181-186}},
Abstract = {{In this study we used a combination of field monitoring and laboratory
   experimentation to investigate the effects of desiccation and
   Echinostoma revolution infection on life-history traits of the pond
   snail, Lymnaea elodes. Field-collected snails demonstrated a sharp
   decline in survivorship within 8 weeks of pond drying. Results from a
   corresponding laboratory experiment, where snails were exposed to
   parasites prior to aestivation, demonstrated that desiccation played a
   more important role in snail survival than E. revolutum infection.
   However, infection influenced host growth patterns as
   aestivated-infected snails were significantly larger at the end of the
   experiment relative to aestivated controls. In a second laboratory
   experiment, exposure after a period of aestivation resulted in higher
   levels of infection compared to snails that were continuously hydrated.
   Moreover, exposed-uninfected snails demonstrated higher mortality than
   either control or infected snails regardless of aestivation treatment.
   Results from this work 1) demonstrate the importance of incorporating
   multiple variables into life-history studies and 2) provide insight into
   the factors underlying host-parasite dynamics in the field.}},
Publisher = {{TAYLOR \& FRANCIS INC}},
Address = {{530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA}},
Type = {{Article}},
