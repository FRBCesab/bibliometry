@article{ ISI:000231161700002,
Author = {Alder, P and Silverman, J},
Title = {{A comparison of monitoring methods used to detect changes in Argentine
   ant (Hymenoptera : Formicidae) populations}},
Journal = {{JOURNAL OF AGRICULTURAL AND URBAN ENTOMOLOGY}},
Year = {{2004}},
Volume = {{21}},
Number = {{3}},
Pages = {{142-149}},
Month = {{JUL}},
Abstract = {{Various sampling procedures have been used to assess changes in
   Argentine ant, Linepithema humile (Mayr), populations as the result of
   insecticide treatment, yet the utility of these procedures has never
   been compared. We compared sampling variation over time and effort
   required to assess populations for four commonly used monitoring
   methods: trailing activity, ant counts at baits, sucrose consumption,
   and pitfall trap collections. We found that pitfall trapping provided
   the greatest daily variation and was the most time-consuming procedure.
   Although sampling variation for the other methods was similar, worker
   counts at baits required the least time to assess and, consequently, we
   recommend it as a monitoring tool for estimating changes in Argentine
   ant populations.}},
