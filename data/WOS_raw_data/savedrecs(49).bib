@article{ ISI:000222303600004,
Author = {Civelek, HS and Yoldas, Z and Ulusoy, MR},
Title = {{Seasonal population trends of Liriomyza huidobrensis (Blanchard, 1926)
   (Diptera : Agromyzidae) on cucumber (Cucumis sativius L.) in Western
   Turkey}},
