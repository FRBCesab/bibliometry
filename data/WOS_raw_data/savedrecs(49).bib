@inproceedings{ ISI:000230655100033,
Author = {Johnson, AC and Musslewhite, J and De Santo, TL and Edwards, RT},
Editor = {{Peterson, CE and Maquire, DA}},
Title = {{Observed changes in ground saturation, aquatic invertebrate densities,
   and breeding bird populations following various harvesting intensities}},
Booktitle = {{Balancing Ecosystem Values: Innovative Experiments for Sustainable
   Forestry}},
Series = {{USDA FOREST SERVICE GENERAL TECHNICAL REPORT PACIFIC NORTHWEST}},
