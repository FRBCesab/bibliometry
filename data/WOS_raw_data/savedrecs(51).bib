@article{ ISI:000184989300016,
Author = {Kjellman, J and Lappalainen, J and Urho, L and Hudd, R},
Title = {{Early determination of perch and pikeperch recruitment in the northern
   Baltic Sea}},
Journal = {{HYDROBIOLOGIA}},
