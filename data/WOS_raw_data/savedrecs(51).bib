@inproceedings{ ISI:000239950800083,
Author = {Ntougias, S. and Ehaliotis, C. and Zervakis, G. I. and Katsaris, P. and
   Papadopoulou, K.},
Editor = {{Lekkas, TD}},
Title = {{Monitoring the composting process of different agricultural wastes and
   evaluation of the effects of the final products on plants}},
Booktitle = {{Proceedings of the 8th International Conference on Environmental Science
   and Technology, Vol A, Oral Presentations}},
