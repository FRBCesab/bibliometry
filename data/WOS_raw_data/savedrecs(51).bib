Booktitle = {{INTEGRATED TECHNOLOGIES FOR ENVIRONMENTAL MONITORING AND INFORMATION
   PRODUCTION}},
Series = {{NATO Science Series IV Earth and Environmental Sciences}},
Year = {{2003}},
