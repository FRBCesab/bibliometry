@article{ ISI:000185455100006,
Author = {Agrawal, M and Singh, B and Rajput, M and Marshall, F and Bell, JNB},
Title = {{Effect of air pollution on peri-urban agriculture: a case study}},
Journal = {{ENVIRONMENTAL POLLUTION}},
Year = {{2003}},
Volume = {{126}},
Number = {{3}},
Pages = {{323-329}},
Note = {{2nd International Conference on Plants and Environmental Pollution, NATL
   BOT RES INST, LUCKNOW, INDIA, FEB 04-09, 2002}},
Organization = {{Int Soc Environm Botanists}},
Abstract = {{Peri-urban agriculture is vital for the urban populations of many
   developing countries. Increases in both industrialization and
   urbanization, and associated air pollution threaten urban food
   production and its quality. Six hour mean concentrations were monitored
   for SO2, NO2 and O-3 and plant responses were measured in terms of
   physiological characteristics, pigment, biomass and yield. Parameter
   reductions in mung bean (Vigna radiata), palak (Beta vulgaris), wheat
   (Triticum aestivum) and mustard (Brassica compestris) grown within the
   urban fringes of Varanasi, India correlated directly with the gaseous
   pollutants levels. The magnitude of response involved all three gaseous
   pollutants at peri-m-ban sites; O-3 had more influence at a rural site.
   The study concluded that air pollution in Varanasi could negatively
   influence crop yield. (C) 2003 Elsevier Ltd. All rights reserved.}},
