@inproceedings{ ISI:000228129800023,
Author = {Godbois, IA and Conner, LM and Warren, RJ},
Editor = {{Eversole, AG}},
Title = {{Habitat use of bobcats at two spatial scales in southwestern Georgia}},
Booktitle = {{PROCEEDINGS OF THE FIFTY-SEVENTH ANNUAL CONFERENCE OF THE SOUTHEASTERN
   ASSOCIATION OF FISH AND WILDLIFE AGENCIES}},
