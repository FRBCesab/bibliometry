@article{ ISI:000181810200001,
Author = {Hurtado, S and Ramstedt, M},
Title = {{Comparing Chilean and Swedish rust populations: possible impact of
   long-distance transport of Melampsora rust}},
Journal = {{FOREST PATHOLOGY}},
Year = {{2003}},
Volume = {{33}},
Number = {{2}},
Pages = {{69-80}},
Month = {{APR}},
Abstract = {{As willow (Salix ) cultivation grows globally, worldwide monitoring of
   pathotypes for Melampsora rust, the most serious disease in willow,
   becomes increasingly important. We compared the pathotype composition of
   the Melampsora rust population in Chile with that in Sweden and assessed
   inocula exchange for the rust between the continents. For pathotyping
   rust isolates, a willow differential was used (a standardized set of
   willow test clones) to obtain clone-virulence patterns. These patterns
   were used to identify virulence components involved in determining which
   willow clones a given individual pathogen isolate (genotype) can
   successfully attack. Each virulence component detected earlier in
   Sweden's rust population was present in Chile's rust population. Using
   isoenzyme analysis to classify willow species in Chile, a low genetic
   variation of Salix viminalis (an introduced willow species) was found,
   compared with the endemic species Salix humboldtiana. Comparison of rust
   populations for the two countries supports the present hypothesis that
   intercontinental inocula exchange can be a significant determinant of
   local pathotype structure, and consequently can be important for
   willow-resistance breeding.}},
