Title = {{Monitoring multi-functional landscapes at a national scale - guidelines
   drawn up from the Countryside Survey of Great Britain}},
Booktitle = {{MULTIFUNCTIONAL LANDSCAPES, VOL II: MONITORING, DIVERSITY AND MANAGEMENT}},
Series = {{ADVANCES IN ECOLOGICAL SCIENCES}},
