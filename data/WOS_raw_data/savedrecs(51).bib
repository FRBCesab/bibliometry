@inproceedings{ ISI:000186103600036,
Author = {Walker, GP and Wallace, AR and Bush, R and Macdonald, FH and Suckling,
   DM},
Editor = {{Zydenbos, SM}},
Title = {{Evaluation of pheromone trapping for prediction of diamondback moth
   infestations in vegetable brassicas}},
Booktitle = {{NEW ZEALAND PLANT PROTECTION, VOL 56}},
