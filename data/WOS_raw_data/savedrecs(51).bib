@article{ ISI:000179560300012,
Author = {Steyer, GD and Sasser, CE and Visser, JM and Swenson, EM and Nyman, JA
   and Raynie, RC},
Title = {{A proposed coast-wide reference monitoring system for evaluating wetland
   restoration trajectories in Louisiana}},
