@article{ ISI:000180774100005,
Author = {Paixao, SM and Santos, P and Baeta-Hall, L and Tenreiro, R and Anselmo,
   AM},
Title = {{Alternative inocula as activated sludge surrogate culture for a toxicity
   test}},
Journal = {{ENVIRONMENTAL TOXICOLOGY}},
