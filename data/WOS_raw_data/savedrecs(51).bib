@inproceedings{ ISI:000232369200131,
Author = {Conyers, ST and Llewellin, BE and Cook, DA and Bell, CH},
Editor = {{Credland, PF and Armitage, DM and Bell, CH and Cogan, PM and Highley, E}},
Title = {{The use of a propane burner to control an artificially induced
   ``hotspot{''}}},
Booktitle = {{ADVANCES IN STORED PRODUCT PROTECTION}},
