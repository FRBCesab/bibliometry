@article{ ISI:000181357700003,
Author = {Greenwood, MT and Wood, PJ},
Title = {{Effects of seasonal variation in salinity on a population of Enochrus
   bicolor Fabricius 1792 (Coleoptera : Hydrophilidae) and implications for
   other beetles of conservation interest}},
