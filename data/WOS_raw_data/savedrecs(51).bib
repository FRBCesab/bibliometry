@article{ ISI:000181429900026,
Author = {Schmidt, BC and Roland, J and Wakarchuk, D},
Title = {{Evaluation of synthetic pheromones for monitoring forest tent
   caterpillar (Lepidoptera : Lasiocampidae) populations}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
