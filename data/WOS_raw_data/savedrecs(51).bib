@inproceedings{ ISI:000185059300030,
Author = {Cooke, DW and Leach, SD},
Editor = {{Limburg, KE and Waldman, JR}},
Title = {{Beneficial effects of increased river flow and upstream fish passage on
   anadromous alosine stocks}},
Booktitle = {{BIODIVERSITY, STATUS, AND CONSERVATION OF THE WORLD'S SHADS}},
Series = {{AMERICAN FISHERIES SOCIETY SYMPOSIUM}},
Year = {{2003}},
Volume = {{35}},
Pages = {{331-338}},
Note = {{International Conference on Status and Conservation of Shads World Wide,
   BALTIMORE, MD, MAY 20-23, 2001}},
Organization = {{Natl Fish \& Wildlife Fdn; Norcross Wildlife Fdn; US Fisha Wildlife
   Serv; Hudson River Estuary Management Program, Reg 5; New York State
   Dept Environm Conservat; Constellat Energy Grp; Penn Fish \& Boat
   Commiss; S Carolina Dept Natural Resources; Elect Power Res Inst; US
   Natl Marine Fisheries Serv}},
Abstract = {{In 1941, approximately 80\% of Santee River, South Carolina, flows were
   diverted to the adjacent Cooper River. Average annual flows were reduced
   from 525 m(3)/s to 63 m(3)/s, ranging from a low continuous flow of 14.6
   m(3)/s to periodic flood events reaching 4,387 m(3)/s. In 1985, a
   portion of the water was re-diverted to the middle reach of Santee River
   via a new canal and hydroelectricity facility, and average flows
   increased to 295 m(3)/s. A fish passage facility was also installed,
   allowing alosines access to upstream habitat. Results of mark-recapture
   population estimates, upstream passage monitoring, and commercial
   fishery catch data were used to examine the effects of the 1985 flow
   increase and implementation of fish passage on stocks of blueback
   herring Alosa aestivalis and American shad A. sapidissima. Blueback
   herring population estimates indicate that stock size quickly expanded
   following the increased flow in 1985. The timing of the expansion
   suggests that a large proportion of the population was fish that shifted
   migration routes from the Cooper River. Commercial fishery data show
   that blueback herring migration patterns within the Santee River were
   also altered by the increased flows. The majority appear to have
   abandoned the old river and begun migrating up the new canal. Passage
   numbers and commercial catch data indicate the blueback herring
   population has increased significantly and now supports an approximately
   115-metric-ton-per-year commercial fishery. American shad landings
   fluctuated prior to 1985 depending on annual flood stage. Average annual
   landings from 1977 through 1985 were 6.5 metric tons. Passage counts and
   commercial fishery landings indicate that the American shad stock has
   been increasing since 1985 and, currently supports a fishery landing
   more than 100 metric tons per year.}},
