@inproceedings{ ISI:000269997100051,
Author = {Montagna, P. A. and Hill, E. M. and Moulton, B.},
Editor = {{Brebbia, CA and Tiezzi, E}},
Title = {{Role of science-based and adaptive management in allocating
   environmental flows to the Nueces Estuary, Texas, USA}},
Booktitle = {{ECOSYSTEMS AND SUSTAINABLE DEVELOPMENT VII}},
Series = {{WIT Transactions on Ecology and the Environment}},
