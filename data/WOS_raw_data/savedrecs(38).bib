@article{ ISI:000207911200011,
Author = {Kaneko, Daijiro and Kumakura, Toshiro and Yang, Peng},
Title = {{Data assimilation for crop yield and CO2 fixation monitoring in Asia by
   a photosynthetic sterility model using satellites and meteorological
   data}},
