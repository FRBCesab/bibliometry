Title = {{Can the Ecosystem Approach Improve Management of Tropical Estuarine
   Fisheries for Diadromous Species?}},
Booktitle = {{CHALLENGES FOR DIADROMOUS FISHES IN A DYNAMIC GLOBAL ENVIRONMENT}},
Series = {{American Fisheries Society Symposium}},
Year = {{2009}},
Volume = {{69}},
