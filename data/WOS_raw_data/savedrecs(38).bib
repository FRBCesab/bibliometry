@article{ ISI:000263715900004,
Author = {Kopjar, Nevenka and Kasuba, Vilena and Rozgaj, Ruzica and Zeljezic,
   Davor and Milic, Mirta and Ramic, Snjezana and Pavlica, Vesna and
   Milkovic-Kraus, Sanja},
Title = {{The genotoxic risk in health care workers occupationally exposed to
   cytotoxic drugsA comprehensive evaluation by the SCE assay}},
Journal = {{JOURNAL OF ENVIRONMENTAL SCIENCE AND HEALTH PART A-TOXIC/HAZARDOUS
   SUBSTANCES \& ENVIRONMENTAL ENGINEERING}},
Year = {{2009}},
Volume = {{44}},
Number = {{5}},
Pages = {{462-479}},
Abstract = {{Present study aimed at an integral assessment of sister chromatid
   exchange (SCE) frequencies in the health care workers occupationally
   exposed to cytostatics. The results of 500 individual analyses were
   evaluated. Drug handling practice was investigated in parallel and the
   results showed that cytostatics are mostly prepared outside hospital
   pharmacy (98\%) and mainly handled by nurses (96\%). Mean frequency of
   SCE was 5.63 2.28, while HFC represented 9.65\% of the cells analysed.
   Both values were higher compared to previously established control
   values for Croatian population. The duration of exposure, profession,
   age, gender, smoking habit, medical exposures, and simultaneous exposure
   to other occupational mutagens significantly contributed to SCE and HFC
   values. The usefulness both biomarkers in the assessment of cytogenetic
   damage is confirmed. Since current practice in Croatian hospitals does
   not include regular monitoring of workplaces, to ensure maximal
   occupational safety, a surveillance on exposed health care workers,
   including periodic biomonitoring, is recommended.}},
