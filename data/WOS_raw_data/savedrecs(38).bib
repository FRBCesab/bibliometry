@article{ ISI:000262573200013,
Author = {Viveiros, Fatima and Ferreira, Teresa and Silva, Catarina and Gaspar,
   Joao L.},
Title = {{Meteorological factors controlling soil gases and indoor CO2
   concentration: A permanent risk in degassing areas}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
