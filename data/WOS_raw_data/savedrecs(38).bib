@article{ ISI:000263396600003,
Author = {Godlewska, M. and Colon, M. and Doroszczyk, L. and Dlugoszewski, B. and
   Verges, C. and Guillard, J.},
Title = {{Hydroacoustic measurements at two frequencies: 70 and 120 kHz -
   consequences for fish stock estimation}},
Journal = {{FISHERIES RESEARCH}},
Year = {{2009}},
