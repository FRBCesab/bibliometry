@article{ ISI:000263719400019,
Author = {Rout, Tracy M. and Hauser, Cindy E. and Possingham, Hugh P.},
Title = {{Optimal adaptive management for the translocation of a threatened
   species}},
Journal = {{ECOLOGICAL APPLICATIONS}},
