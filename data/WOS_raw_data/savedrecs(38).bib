@inproceedings{ ISI:000277990500012,
Author = {Brumo, Abel F. and Grandmontagne, Leo and Namitz, Steven N. and Markle,
   Douglas F.},
Editor = {{Brown, LR and Chase, SD and Mesa, MG and Beamish, RJ and Moyle, PB}},
Title = {{Approaches for Monitoring Pacific Lamprey Spawning Populations in a
   Coastal Oregon Stream}},
Booktitle = {{BIOLOGY, MANAGEMENT, AND CONSERVATION OF LAMPREYS IN NORTH AMERICA}},
Series = {{American Fisheries Society Symposium}},
