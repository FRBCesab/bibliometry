@article{ ISI:000263159900013,
Author = {Marshall, Harold G. and Lane, Michael F. and Nesius, Kneeland K. and
   Burchardt, Lubomira},
Title = {{Assessment and significance of phytoplankton species composition within
   Chesapeake Bay and Virginia tributaries through a long-term monitoring
   program}},
