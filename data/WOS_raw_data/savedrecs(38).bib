@inproceedings{ ISI:000305214400087,
Author = {Asfaw, Z.},
Editor = {{Jaenicke, H and Ganry, J and HoeschleZeledon, I and Kahane, R}},
Title = {{The Future of Wild Food Plants in Southern Ethiopia: Ecosystem
   Conservation Coupled with Enhancement of the Roles of Key Social Groups}},
