@inproceedings{ ISI:000305512800021,
Author = {Ulrichs, C. and Mucha-Pelzer, T. and Buettner, C. and Mewis, I. and
   Scobel, E. and Bauer, E.},
Editor = {{Fischer, G and Magnitskiy, S and Nicola, S}},
Title = {{New Approaches in the Control of Insect Pests in Tomato}},
