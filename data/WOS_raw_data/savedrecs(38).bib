@article{ ISI:000263203700017,
Author = {Ishihara, Akinori and Rahman, Farhana B. and Leelawatwattana, Ladda and
   Prapunpoj, Porntip and Yamauchi, Kiyoshi},
Title = {{IN VITRO THYROID HORMONE-DISRUPTING ACTIVITY IN EFFLUENTS AND SURFACE
   WATERS IN THAILAND}},
