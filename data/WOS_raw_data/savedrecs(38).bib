@inproceedings{ ISI:000305709600018,
Author = {Ploetz, R. C.},
Editor = {{Jones, D and VanDenBergh, I}},
Title = {{Pathogen Diversity: Contributions to Applied and Basic Banana Research}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON RECENT ADVANCES IN BANANA CROP PROTECTION FOR
   SUSTAINABLE PRODUCTION AND IMPROVED LIVELIHOODS}},
Series = {{Acta Horticulturae}},
