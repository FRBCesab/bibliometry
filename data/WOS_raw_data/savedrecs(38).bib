@article{ ISI:000267128000004,
Author = {Cotter, John and Mesnil, Benoit and Witthames, Peter and
   Parker-Humphreys, Matt},
Title = {{Notes on nine biological indicators estimable from trawl surveys with an
   illustrative assessment for North Sea cod}},
Journal = {{AQUATIC LIVING RESOURCES}},
