@article{ ISI:000263090500002,
Author = {Barac, Tanja and Weyens, Nele and Oeyen, Licy and Taghavi, Safiyh and
   van der Lelie, Daniel and Dubin, Dirk and Spliet, Marco and
   Vangronsveld, Jaco},
Title = {{FIELD NOTE: HYDRAULIC CONTAINMENT OF A BTEX PLUME USING POPLAR TREES}},
