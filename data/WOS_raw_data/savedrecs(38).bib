@article{ ISI:000262430700016,
Author = {Gauthier, Lewis T. and Potter, Dave and Hebert, Craig E. and Letcher,
   Robert J.},
Title = {{Temporal Trends and Spatial Distribution of Non-polybrominated Diphenyl
   Ether Flame Retardants in the Eggs of Colonial Populations of Great
   Lakes Herring Gulls}},
