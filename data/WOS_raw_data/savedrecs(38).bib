@article{ ISI:000265563000003,
Author = {Bowen, Kenneth D. and Beever, Erik A. and Gafvert, Ulf B.},
Title = {{Improving the Design of Amphibian Surveys Using Soil Data: a Case Study
   in Two Wilderness Areas}},
Journal = {{NATURAL AREAS JOURNAL}},
