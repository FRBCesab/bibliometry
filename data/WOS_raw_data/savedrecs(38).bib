@article{ ISI:000263152900001,
Author = {Grisoli, Pietro and Rodolfi, Marinella and Villani, Simona and Grignani,
   Elena and Cottica, Danilo and Berri, Angelo and Picco, Anna Maria and
   Dacarro, Cesare},
Title = {{Assessment of airborne microorganism contamination in an industrial area
   characterized by an open composting facility and a wastewater treatment
   plant}},
