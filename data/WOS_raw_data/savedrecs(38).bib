Title = {{Unutilized Potential of True Service Tree (Sorbus domestica L.)}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON UNDERUTILIZED PLANTS FOR FOOD SECURITY,
   NUTRITION, INCOME AND SUSTAINABLE DEVELOPMENT}},
Series = {{Acta Horticulturae}},
