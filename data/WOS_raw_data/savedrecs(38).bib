@inproceedings{ ISI:000274244900023,
Author = {Taub, Frieda B.},
Editor = {{Jones, J}},
Title = {{Contributions of cellulose to a microbial-algal-Daphnia food chain in
   Closed Ecological Systems}},
Booktitle = {{INTERNATIONAL ASSOCIATION OF THEORETICAL AND APPLIED LIMNOLOGY, VOL 30,
   PT 8, PROCEEDINGS: NOW: INTERNATIONAL SOCIETY OF LIMNOLOGY}},
