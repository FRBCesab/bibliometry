@article{ ISI:000266009500013,
Author = {Ramani, K. V. and Mavalankar, Dileep V. and Govil, Dipti},
Title = {{Study of Blood-transfusion Services in Maharashtra and Gujarat States,
   India}},
Journal = {{JOURNAL OF HEALTH POPULATION AND NUTRITION}},
