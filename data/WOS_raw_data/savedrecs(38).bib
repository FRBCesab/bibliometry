@article{ ISI:000262825200010,
Author = {Wegge, P. and Storaas, T.},
Title = {{Sampling tiger ungulate prey by the distance method: lessons learned in
   Bardia National Park, Nepal}},
Journal = {{ANIMAL CONSERVATION}},
