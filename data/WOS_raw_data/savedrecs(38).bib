@article{ ISI:000261918900010,
Author = {Zugmeyer, Claire A. and Koprowski, John L.},
Title = {{Severely insect-damaged forest: A temporary trap for red squirrels?}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
