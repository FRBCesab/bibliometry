@inproceedings{ ISI:000265786100017,
Author = {Holliday, Dale Vance},
Editor = {{Beamish, RJ and Rothschild, BJ}},
Title = {{Technology for Evaluating Marine Ecosystems in the Early Twenty-First
   Century}},
Booktitle = {{FUTURE OF FISHERIES SCIENCE IN NORTH AMERICA}},
