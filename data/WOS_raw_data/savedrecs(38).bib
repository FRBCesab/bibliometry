@inproceedings{ ISI:000266407200022,
Author = {Correll, J. C. and Boza, E. J. and Seyran, E. and Cartwright, R. D. and
   Jia, Yulin and Lee, F. N.},
Editor = {{Wang, GL and Valent, B}},
Title = {{Examination of the Rice Blast Pathogen Population Diversity in Arkansas,
   USA - Stable or Unstable?}},
