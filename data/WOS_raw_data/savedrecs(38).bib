@article{ ISI:000267167600007,
Author = {Lewis, Theodore W. and Makarewicz, Joseph C.},
Title = {{Winter application of manure on an agricultural watershed and its impact
   on downstream nutrient fluxes}},
Journal = {{JOURNAL OF GREAT LAKES RESEARCH}},
