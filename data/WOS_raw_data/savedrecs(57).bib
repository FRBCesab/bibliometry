@article{ ISI:000083356500006,
Author = {Wells, PG},
Title = {{Biomonitoring the health of coastal marine ecosystems - The roles and
   challenges of microscale toxicity tests}},
Journal = {{MARINE POLLUTION BULLETIN}},
