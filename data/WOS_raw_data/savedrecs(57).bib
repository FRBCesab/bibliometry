@article{ ISI:000080993900006,
Author = {Palenzuela, O and Trobridge, G and Bartholomew, JL},
Title = {{Development of a polymerase chain reaction diagnostic assay for
   Ceratomyxa shasta, a myxosporean parasite of salmonid fish}},
Journal = {{DISEASES OF AQUATIC ORGANISMS}},
