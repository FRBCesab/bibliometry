@article{ ISI:000082903500034,
Author = {Caron, F and Tremblay, S},
Title = {{Structure and management of an exploited population of Atlantic sturgeon
   (Acipenser oxyrinchus) in the St. Lawrence Estuary, Quebec, Canada}},
Journal = {{JOURNAL OF APPLIED ICHTHYOLOGY}},
Year = {{1999}},
Volume = {{15}},
Number = {{4-5}},
