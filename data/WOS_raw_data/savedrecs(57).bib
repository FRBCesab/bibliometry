@article{ ISI:000083121500014,
Author = {Chong, S and Garelick, H and Revitt, DM and Shutes, RBE and Worrall, P
   and Brewer, D},
Title = {{The microbiology associated with glycol removal in constructed wetlands}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{1999}},
Volume = {{40}},
Number = {{3}},
Pages = {{99-107}},
Note = {{6th International Conference on Wetlands Systems for Water Pollution
   Control, SAO PEDRO, BRAZIL, SEP 27-OCT 02, 1998}},
Organization = {{IAWQ; Univ Estadual paulista}},
Abstract = {{During the winter months, the application of large quantities of glycol
   based anti- and de-icers to aircraft and runways poses a serious threat
   to receiving waters because of their toxicity and BOD effect. The
   orthodox approach has been to store runoff waters in aerated lagoons
   before discharge to receiving waters or to sewage treatment works. In
   the development of a pollution control strategy for Heathrow Airport
   Ltd. (HAL), a combination of alternative methods for treatment are being
   considered, and amongst these is the use of constructed wetlands on a
   pilot scale. Results from this study indicate that most constructed
   wetland plants and substrate micro-organism populations throughout the
   beds are nor adversely affected by airport runoff or exposure to
   shock-loads of glycols at initial total concentrations of 1l80mg/1 and
   632mg/1 in the subsurface and surface flow reedbeds respectively. Apart
   from Typha spp, the aquatic macrophytes have adapted well to the
   exposure to airport runoff. Glycol removal efficiencies have improved as
   the beds have matured, and average removal efficiencies of 78\% for the
   sub-surface system and 54\% for the surface system have been recorded.
   Complimentary monitoring of substrate micro-organism populations prior
   to and after glycol dosings have shown that aerobic microbial groups of
   bacteria, fungi and actinomyctes, are present in higher numbers
   (10(5)-10(7) CFU/g substrate dry weight) than their anaerobic
   counterparts (10(3)-10(5) CFU/g substrate dry weight). In the
   laboratory, studies have shown fungi and bacteria to be most tolerant of
   glycol, with several strains able to utilise these compounds. (C) 1999
   IAWQ Published by Elsevier Science Ltd. All rights reserved.}},
