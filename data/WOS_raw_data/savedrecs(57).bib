@inproceedings{ ISI:000082416000006,
Author = {Biehle, AA and Hall, WB and Gregory, GE},
Editor = {{Alleman, BC and Leeson, A}},
Title = {{Monitored natural attenuation of a methylene chloride plume}},
Booktitle = {{NATURAL ATTENUATION OF CHLORINATED SOLVENTS, PETROLEUM HYDROCARBONS, AND
   OTHER ORGANIC COMPOUNDS}},
Year = {{1999}},
Pages = {{35-40}},
Note = {{5th International In Situ and On-Site Bioremediation Symposium, SAN
   DIEGO, CA, APR 19-22, 1999}},
Organization = {{Battelle Mem Inst; Gas Res Inst; IT Grp Inc; Celtic Technol; Parsons
   Engn Sci Inc; U S Microb Inc; U S Naval Fac Engn Command; Waste
   Management Inc}},
Abstract = {{Methylene chloride is present in shallow groundwater beneath an
   industrial facility. Groundwater sampling results from the study area
   indicate a limited, well-defined plume smaller than that predicted by
   groundwater modeling. To explain the difference between the model
   prediction and sampling results, laboratory microcosm studies were
   performed to assess microbial activity, and natural attenuation
   parameters were added to the sampling program. Laboratory microcosm
   data, using soil and groundwater from within and upgradient of the
   contamination, showed that the microbial population within the plume
   could utilize methylene chloride as a source of carbon and energy and
   also demonstrated that these organisms were absent outside of the area
   of contamination. Groundwater sampling results provide evidence of
   significant anaerobic microbial activity and dechlorination based on
   elevated levels of carbon dioxide and methane within the plume and
   elevated levels of chloride downgradient of the plume. These field data
   indicate an anaerobic, microbial dechlorination process is occurring
   within the plume. From these data we conclude that natural attenuation,
   due to a localized microbial population, has inhibited movement of the
   methylene chloride plume.}},
