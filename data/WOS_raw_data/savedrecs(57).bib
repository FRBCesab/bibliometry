@inproceedings{ ISI:000082615800015,
Author = {Crouse, DT},
Editor = {{Musick, JA}},
Title = {{The consequences of delayed maturity in a human-dominated world}},
Booktitle = {{LIFE IN THE SLOW LANE: ECOLOGY AND CONSERVATION OF LONG-LIVED MARINE
   ANIMALS}},
