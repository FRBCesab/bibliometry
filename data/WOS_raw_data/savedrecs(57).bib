@article{ ISI:000083157600016,
Author = {Empson, RA and Miskelly, CM},
Title = {{The risks, costs and benefits of using brodifacoum to eradicate rats
   from Kapiti Island, New Zealand}},
Journal = {{NEW ZEALAND JOURNAL OF ECOLOGY}},
Year = {{1999}},
