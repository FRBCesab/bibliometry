@inproceedings{ ISI:000081665400021,
Author = {Kus, BE},
Editor = {{Morrison, ML and Hall, LS and Robinson, SK and Rothstein, SI and Hahn, DC and Rich, TD}},
Title = {{Impacts of Brown-headed Cowbird parasitism on productivity of the
   endangered Least Bell's Vireo}},
Booktitle = {{RESEARCH AND MANAGEMENT OF THE BROWN-HEADED COWBIRD IN WESTERN
