@article{ ISI:000081959400031,
Author = {Chumak, VV and Likhtarev, IA and Pavlenko, JV},
Title = {{Monitoring of individual doses of populations residing in the
   territories contaminated after the Chernobyl accident}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{1999}},
Volume = {{85}},
Number = {{1-4}},
Pages = {{137-139}},
Note = {{12th International Conference on Solid State Dosimetry, BURGOS, SPAIN,
   JUL 05-10, 1998}},
Organization = {{Bicron Technol; Bubble Technol Ind; Caja Burgos; Comis Interminist Cienc
   \& Tecnolog; Consejo Seguridad Nucl; Direcc Gen Investigacion Cienc \&
   Tecnol; Empressa Nacl Residuos SA; Excmo Ayuntamiento Burgos; Excma
   Diputac Province Burgos; Landauer Inc; Nucl Technol Publishing; Siemens
   Environm Syst Ltd; Stillwater Sci LLC; Studsvik Instrument AB}},
Abstract = {{To provide instrumental validation of radioecological dosimetric models
   used for estimation of external doses to the Chernobyl population, about
   1000 direct dose measurements were conducted in 1996-1997 in 54
   settlements in contaminated territories of Ukraine. The areas covered by
   the measurements have Cs-137 contamination density ranging from 55 to
   491 kBq.m(-2) Individual dose measurements were conducted using standard
   LiF dosemeters, type Harshaw 8814 (TLD-100), and automated TLD system
   Harshaw 8800. Relatively low contamination and, thus, an unfavourable
   `Chernobyl/natural background' dose ratio, called for sophisticated
   analysis of experimental results. Linear regression of dose relative to
   Cs-137 contamination density which was conducted in two different ways
   provided consistent results. Annual background dose, as derived from the
   results of individual dose measurements, is about 1.1 mSv per annum; the
   Chernobyl related component is expressed by the rate of 1.24-1.3 mu Sv
   per kBq.m(-2) per annum. These results are in reasonable agreement with
   somewhat conservative modelling parameters which are assumed to be 1.91
   mu Sv per kBq.m(-2) per annum for the case of the rural population.}},
