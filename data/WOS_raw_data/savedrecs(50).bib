@article{ ISI:000223288400001,
Author = {McAllister, CD and Lentz, GL and Van Tol, NB},
Title = {{Impact of early-season insecticides on Roundup Ready (R) cotton for
   control of thrips (Thysanoptera : Thripidae)}},
Journal = {{JOURNAL OF AGRICULTURAL AND URBAN ENTOMOLOGY}},
Year = {{2003}},
Volume = {{20}},
Number = {{4}},
Pages = {{173-185}},
Month = {{OCT}},
Abstract = {{The efficacy of several early-season insecticides on genetically
   modified, herbicide-tolerant Roundup Ready(R) cotton was evaluated
   during 1999 and 2000 by monitoring densities of larval and adult thrips
   (Thysanoptera: Thripidae) populations and selected plant growth
   characteristics. Thrips densities and plant growth characteristics were
   not affected by glyphosate or conventional herbicides. In 1999,
   conventional herbicides caused a delay in crop maturity by producing
   higher nodes of first position fruiting square. Densities of larval and
   adult thrips were lower in aldicarb-treated plots compared with other
   treatments, and insecticide treatments protected plants from thrips
   feeding. Thrips species composition was also determined for each year of
   the study. Frankliniella fusca (Hinds) was the most abundant species
   detected both years. Insecticide-treated plants had more leaf area than
   untreated plants. During both years, phorate-treated plants had
   significantly fewer (Pless than or equal to0.05) flowers than other
   insecticide-treated or untreated plants. Lint yields in 1999 were not
   affected by insecticide treatment. However, in 2000, six of the
   treatments did not differ in the first harvest yield produced and only
   aldicarb-treated plots had significantly higher first harvest yields
   than either phorate-treated or untreated plots. Only plots treated with
   aldicarb produced significantly higher total lint yields than untreated
   plots.}},
