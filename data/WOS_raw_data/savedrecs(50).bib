@article{ ISI:000183816200001,
Author = {Lenczewski, M and Jardine, P and McKay, L and Layton, A},
Title = {{Natural attenuation of trichloroethylene in fractured shale bedrock}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2003}},
