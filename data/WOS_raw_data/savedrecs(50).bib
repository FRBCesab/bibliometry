@article{ ISI:000186216800005,
Author = {Theuerkauf, J and Jedrzejewski, W and Schmidt, K and Gula, R},
Title = {{Spatiotemporal segregation of wolves from humans in the Bialowieza
   Forest (Poland)}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
