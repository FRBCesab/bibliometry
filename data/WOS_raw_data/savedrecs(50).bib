@article{ ISI:000185100800007,
Author = {Diamond, AW and Devlin, CM},
Title = {{Seabirds as indicators of changes in marine ecosystems: Ecological
   monitoring on Machias Seal Island}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
