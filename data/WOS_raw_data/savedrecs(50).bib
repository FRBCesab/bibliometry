@article{ ISI:000187673100018,
Author = {Lee, H and Yoon, E and Hwang, MS and Lee, GY and Hong, MK and Yang, JS
   and Yang, KH and Shin, HS},
Title = {{Health risk assessment of lead in the Republic of Korea}},
Journal = {{HUMAN AND ECOLOGICAL RISK ASSESSMENT}},
