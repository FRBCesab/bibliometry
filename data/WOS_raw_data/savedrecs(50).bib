@inproceedings{ ISI:000228130200023,
Author = {Reisenbichler, RR},
Editor = {{Nickum, MJ and Mazik, PM and Nickum, JG and Mackinlay, DD}},
Title = {{Uncertainty and research needs for supplementing wild populations of
   anadromous Pacific salmon}},
Booktitle = {{PROPAGATED FISH IN RESOURCE MANAGEMENT}},
