@article{ ISI:000226203000008,
Author = {Bell, JC and Powell, M and Devey, ME and Moran, GF},
Title = {{DNA profiling, pedigree lineage analysis and monitoring in the
   Australian breeding program of radiata pine}},
Journal = {{SILVAE GENETICA}},
Year = {{2004}},
Volume = {{53}},
Number = {{3}},
Pages = {{130-134}},
Abstract = {{Ten microsatellite loci were used to DNA profile 215 first and second
   generation breeding clones in the Australian radiata pine breeding
   program. The clones were essentially unique from one another and these
   DNA profiles were used to examine the sexual and asexual phases in the
   breeding program. Only 2.6\% of ramets from 25 breeding clones were
   incorrect whereas 8.4\% of progeny from 10 families were genetically
   incorrect. The lineages of second generation clones were checked for DNA
   profiles that were available for putative parents. Some parents appear
   to be incorrect on the basis of the DNA profiles and further sampling
   will be required to confirm these results across the breeding program.
   Microsatellite markers can now enable most questions to be resolved in
   breeding programs and also will allow novel breeding strategies to be
   used in the future.}},
