@inproceedings{ ISI:000189487500025,
Author = {Lange, D},
Editor = {{Craker, LE and Simon, JE and Jatisatienr, A and Lewinsohn, E}},
Title = {{Medicinal and aromatic plants: Trade, production, and management of
   botanical resources}},
Booktitle = {{FUTURE FOR MEDICINAL AND AROMATIC PLANTS}},
Series = {{ACTA HORTICULTURAE}},
