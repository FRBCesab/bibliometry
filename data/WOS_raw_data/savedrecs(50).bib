@article{ ISI:000185897300011,
Author = {Lichko, LE and Calhoun, AJK},
Title = {{An evaluation of vernal pool creation projects in New England: Project
   documentation from 1991-2000}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
