@article{ ISI:000184575400011,
Author = {Price, CA and Weltzin, JF},
Title = {{Managing non-native plant populations through intensive community
   restoration in Cades Cove, Great Smoky Mountains National Park, USA}},
Journal = {{RESTORATION ECOLOGY}},
