Title = {{Gene flow from sugar beet in Ukraine: Current status and future research}},
Booktitle = {{GENOMICS FOR BIOSAFETY IN PLANT BIOTECHNOLOGY}},
Series = {{NATO SCIENCE SERIES, SUB-SERIES I: LIFE AND BEHAVIOURAL SCIENCES}},
