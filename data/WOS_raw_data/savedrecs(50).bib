@article{ ISI:000187881500005,
Author = {Minganti, V and Cornara, L and Piana, M and Corallo, A and Mariotti, MG},
Title = {{Arsenic biomonitoring using a hyperaccumulator fern (Pteris vittata)}},
Journal = {{JOURNAL OF ENVIRONMENTAL MONITORING}},
Year = {{2004}},
Volume = {{6}},
Number = {{1}},
Pages = {{23-25}},
Abstract = {{Samples of Pteris vittata L. ( brake fern or ladder brake) collected in
   Genova and in areas outside urban centres, have been analysed for
   arsenic content in order to assess if hyperaccumulating plants are
   suitable for monitoring purposes. Hyperaccumulation ability of the
   Ligurian fern populations was evaluated by analysing specimens grown
   with hydroponic media added with As( V). Arsenic concentrations in the
   range 2 - 310 mug g(-1) dry weight have been measured in samples
   collected in different sites along the Ligurian coast. Arsenic
   concentrations in fern fronds correlate with the estimated arsenic
   emission in the area, verifying the applicability of P. vittata as an
   arsenic biomonitor.}},
