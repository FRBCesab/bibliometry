@article{ ISI:000188250900008,
Author = {Al-Kazwini, AT and Hasan, MA},
Title = {{Radon concentration in Jordanian drinking water and hot springs}},
Journal = {{JOURNAL OF RADIOLOGICAL PROTECTION}},
