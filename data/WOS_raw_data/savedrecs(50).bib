@inproceedings{ ISI:000235161800027,
Author = {Breeden, JB and Hernandez, F and Silvy, NJ and Bingham, RL and
   Waggerman, GL},
Editor = {{Eversole, AG}},
Title = {{An evaluation of sampling methods for white-winged dove surveys in urban
   areas}},
Booktitle = {{PROCEEDINGS OF THE FIFTY-EIGHTH ANNUAL CONFERENCE OF THE SOUTHEASTERN
   ASSOCIATION OF FISH AND WILDLIFE AGENCIES}},
Year = {{2004}},
