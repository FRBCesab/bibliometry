@article{ ISI:000291754800019,
Author = {Hovick, Torre J. and Miller, James R. and Koford, Rolf R. and Engle,
   David M. and Debinski, Diane M.},
Title = {{POSTFLEDGING SURVIVAL OF GRASSHOPPER SPARROWS IN GRASSLANDS MANAGED WITH
   FIRE AND GRAZING}},
