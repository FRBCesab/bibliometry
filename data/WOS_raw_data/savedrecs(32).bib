@article{ ISI:000291813800008,
Author = {Chen, Yongjiu and Childs, Michael R. and Keeler-Foster, Connie},
Title = {{Evaluation of Woundfin Augmentation Efforts in the Virgin River by
   Estimation of Admixture Proportions}},
