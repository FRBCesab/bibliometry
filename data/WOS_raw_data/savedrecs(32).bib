@article{ ISI:000286945700008,
Author = {Yang, Chang Yeol and Cho, Myoung Rae and Choi, Dong Ro},
Title = {{Identification and field evaluation of the sex pheromone components of a
   Korean population of Glossosphecia romanovi}},
Journal = {{ENTOMOLOGIA EXPERIMENTALIS ET APPLICATA}},
