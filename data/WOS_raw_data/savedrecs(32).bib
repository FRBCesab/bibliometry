@article{ ISI:000291228700009,
Author = {Queenborough, Simon A. and Burnet, Kirsty M. and Sutherland, William J.
   and Watkinson, Andrew R. and Freckleton, Robert P.},
Title = {{From meso- to macroscale population dynamics: a new density-structured
   approach}},
