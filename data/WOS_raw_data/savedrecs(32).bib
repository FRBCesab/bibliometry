@article{ ISI:000289288700019,
Author = {Conway, Courtney J. and Gibbs, James P.},
Title = {{Summary of Intrinsic and Extrinsic Factors Affecting Detection
   Probability of Marsh Birds}},
Journal = {{WETLANDS}},
