@article{ ISI:000286833400013,
Author = {Csergo, Anna-Maria and Molnar, Edit and Begona Garcia, Maria},
Title = {{Dynamics of isolated Saponaria bellidifolia Sm. populations at northern
   range periphery}},
Journal = {{POPULATION ECOLOGY}},
