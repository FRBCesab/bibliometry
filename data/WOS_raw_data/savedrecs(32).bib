@article{ ISI:000291079300022,
Author = {Yazar, Mine and Bellander, Tom and Merritt, Anne-Sophie},
Title = {{Personal exposure to carcinogenic and toxic air pollutants in Stockholm,
   Sweden: A comparison over time}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
