@article{ ISI:000285035000021,
Author = {Robledano, F. and Esteve, M. A. and Martinez-Fernandez, J. and Farinos,
   P.},
Title = {{Determinants of wintering waterbird changes in a Mediterranean coastal
   lagoon affected by eutrophication}},
