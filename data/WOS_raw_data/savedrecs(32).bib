@article{ ISI:000291007800007,
Author = {Manning, Jeffrey A. and Kaler, Robb S. A.},
Title = {{Effects of Survey Methods on Burrowing Owl Behaviors}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
