@article{ ISI:000286204800019,
Author = {Li, Xiaogang and Liu, Biao and Cui, Jinjie and Liu, Doudou and Ding,
   Shuai and Gilna, Ben and Luo, Junyu and Fang, Zhixiang and Cao, Wei and
   Han, Zhengmin},
Title = {{No evidence of persistent effects of continuously planted transgenic
   insect-resistant cotton on soil microorganisms}},
Journal = {{PLANT AND SOIL}},
Year = {{2011}},
Volume = {{339}},
Number = {{1-2}},
Pages = {{247-257}},
Month = {{FEB}},
Abstract = {{Transgenic insect-resistant cotton expressing the Cry1Ac and/or CpTI
   protein has been released into the environment for commercial
   cultivation for over a decade in China. In this consecutive 3-year field
   study (2007-2009), we characterized the microbial populations in two
   different transgenic cotton fields and one non-transgenic cotton field
   to assess the environmental impact of long-term cultivation of
   transgenic insect-resistant plants in credible field conditions. We
   obtained samples at four different growth stages of cotton (seedling,
   budding, boll forming and boll opening). The results showed significant
   seasonal variation in the number of bacteria, fungi, azotobacter,
   denitrifying bacteria and ammonia-oxidizing bacteria and in diversity
   indices of microorganisms, but no significant difference in the number
   of each microbial population or diversity indices attributable to
   long-term cultivation of transgenic cotton.}},
