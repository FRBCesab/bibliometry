@article{ ISI:000286368700014,
Author = {Bradley, C. A. and Pedersen, D. K.},
Title = {{Baseline Sensitivity of Cercospora zeae-maydis to Quinone Outside
   Inhibitor Fungicides}},
Journal = {{PLANT DISEASE}},
Year = {{2011}},
Volume = {{95}},
Number = {{2}},
Pages = {{189-194}},
Month = {{FEB}},
Abstract = {{Cercospora zeae-maydis, the causal agent of gray leaf spot on corn (Zea
   mays), can cause severe yield loss in the United States. Quinone outside
   inhibitor (QoI) fungicides are effective tools that can be used to
   manage gray leaf spot, and their use has increased in corn production in
   the United States. In total, 61 C. zeae-maydis isolates collected from
   fields in which QoI fungicides had never been applied were tested in
   vitro using azoxystrobin-, pyraclostrobin-, or trifloxystrobin-amended
   medium to determine the effective fungicide concentration at which 50\%
   of the conidial germination was inhibited (EC50). The effect of
   salicylhydroxamic acid (SHAM) also was evaluated for seven isolates to
   determine whether C. zeae-maydis is capable of using alternative
   respiration in azoxystrobin-amended medium. All seven C. zeae-maydis
   isolates tested had significantly greater (P < 0.02) EC50 values when
   SHAM was not included in medium amended with azoxystrobin, indicating
   that C. zeae-maydis has the potential to utilize alternative respiration
   to overcome QoI fungicide inhibition in vitro. Baseline EC50 values of
   azoxystrobin ranged from 0.003 to 0.031 mu g/ml, with mean and median
   values of 0.018 and 0.019 mu g/ml, respectively. Baseline EC50 values of
   pyraclostrobin ranged from 0.0003 to 0.0025 mu g/ml, with mean and
   median values of 0.0010 and 0.0010 mu g/ml, respectively. Baseline EC50
   values of trifloxystrobin ranged from 0.0004 to 0.0034 mu g/ml, with
   mean and median values of 0.0023 and 0.0024 mu g/ml, respectively. These
   baseline sensitivity values will be used in a fungicide resistance
   monitoring program to determine whether shifts in sensitivity to QoI
   fungicides are occurring in C. zeae-maydis populations.}},
