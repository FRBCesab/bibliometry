@article{ ISI:000290825200031,
Author = {Paul, John F. and Munns, Jr., Wayne R.},
Title = {{PROBABILITY SURVEYS, CONDITIONAL PROBABILITY, AND ECOLOGICAL RISK
   ASSESSMENT}},
