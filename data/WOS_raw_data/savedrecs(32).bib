@article{ ISI:000290039300014,
Author = {Bowman, Sharen and Hubert, Sophie and Higgins, Brent and Stone, Cynthia
   and Kimball, Jennifer and Borza, Tudor and Bussey, Jillian Tarrant and
   Simpson, Gary and Kozera, Catherine and Curtis, Bruce A. and Hall,
   Jennifer R. and Hori, Tiago S. and Feng, Charles Y. and Rise, Marlies
   and Booman, Marije and Gamperl, A. Kurt and Trippel, Edward and Symonds,
   Jane and Johnson, Stewart C. and Rise, Matthew L.},
Title = {{An Integrated Approach to Gene Discovery and Marker Development in
   Atlantic Cod (Gadus morhua)}},
Journal = {{MARINE BIOTECHNOLOGY}},
