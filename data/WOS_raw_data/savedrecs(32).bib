@article{ ISI:000291189600009,
Author = {Fukui, Yoshinari and Ohashi, Fumiko and Sakuragi, Sonoko and Moriguchi,
   Jiro and Ikeda, Masayuki},
Title = {{Comparative Evaluation of GFAAS and ICP-MS for Analyses of Cadmium in
   Blood}},
Journal = {{INDUSTRIAL HEALTH}},
Year = {{2011}},
Volume = {{49}},
Number = {{3, SI}},
Pages = {{338-343}},
Month = {{MAY}},
Abstract = {{Cadmium in blood (Cd-B) is an important indicator, next to Cd in urine,
   in biological monitoring of exposure to Cd. The present study was
   initiated to examine compatibility in results of analysis for Cd-B
   between graphite furnace atomic absorption spectrophotometry (GFAAS) and
   inductively-coupled plasma mass-spectrometry (ICP-MS). For this purpose,
   1,159 blood samples were collected from adult women (with no
   occupational exposure to Cd) in eight prefectures nation-widely in
   Japan. The samples were analyzed by the two methods; geometric mean (the
   maximum) concentrations were 1.22 (6.90) mu g/l by ICP-MS, and 1.47
   (7.40) mu g/l by GFAAS. Statistical analyses showed that there was a
   close correlation between the results by the two methods. The regression
   line (with ICP-MS and GFAAS results as an independent variable and a
   dependent variable, respectively) had a slope close to one and an
   intercept next to zero to suggest that ICP-MS gave values compatible
   with that of GFAAS. Further analysis with the ratio of Cd-B by GFAAS
   over that by ICP-MS revealed that the two results were close to each
   other, and that the agreement was even closer when Cd-B was >2 mu g/l.
   Thus, the two methods can be employed inter-convertibly when Cd-B is
   relatively high, e.g. >2 mu g/l. Care may need to be practiced, however,
   for possible `between methods' difference when Cd-B is low, e.g., <= 2
   mu g/l.}},
