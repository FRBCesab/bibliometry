@article{ ISI:000291947700014,
Author = {Zhilyaev, G. G.},
Title = {{Population and subpopulation dynamics of Soldanella hungarica Simonk.
   (Primulaceae) in spruce forests of the Carpathians}},
Journal = {{CONTEMPORARY PROBLEMS OF ECOLOGY}},
Year = {{2011}},
Volume = {{4}},
Number = {{3}},
Pages = {{334-338}},
Month = {{JUN}},
Abstract = {{Regularities in the dynamics of natural populations of Soldanella
   hungarica Simonk. in spruce forests of the Ukrainian Carpathians are
   generalized from the results of long-term monitoring (1974-2009). It is
   proved that the size and vitality structure are the major factors
   determining of the asynchronous functioning principle and the large
   range of dynamics of elements in subpopulation loci. The rate and scale
   of the local dynamics are smoothed out at the population level. The
   populations are characterized by reversibility of population number and
   a low range of dynamics with long-term cycles of natural turnover of
   generations. It is suggested that the use of the term ``small
   population{''} to referring to subpopulation loci is inappropriate.}},
