@article{ ISI:000288907700014,
Author = {Fujisaki, Ikuko and Mazzotti, Frank J. and Dorazio, Robert M. and Rice,
   Kenneth G. and Cherkiss, Michael and Jeffery, Brian},
Title = {{Estimating Trends in Alligator Populations from Nightlight Survey Data}},
Journal = {{WETLANDS}},
