@article{ ISI:A1997XE26600014,
Author = {VanderMeer, J},
Title = {{Sampling design of monitoring programmes for marine benthos: A
   comparison between the use of fixed versus randomly selected stations}},
Journal = {{JOURNAL OF SEA RESEARCH}},
Year = {{1997}},
Volume = {{37}},
Number = {{1-2}},
Pages = {{167-179}},
Month = {{MAY}},
Abstract = {{Three possible sampling designs for monitoring programmes were compared
   for both bias and variance of the associated The statistical power of
   the accompanying univariate analysis of variance models was also
   analysed. The analysis was based on species abundance data from
   monitoring programmes for soft-bottom marine benthos in The Netherlands.
   The first design, with random selection of stations in each year,
   revealed larger variance and smaller power than the second design, in
   which stations are sampled randomly in the first year and revisited in
   succeeding years. The third design, with fixed non-randomly selected
   stations, yielded the largest power, bur gives biased estimates of the
   year-to-year changes. These results suggest the following rule-of-thumb
   for monitoring programmes focusing on the abundance of marine benthic
   species: revisit many randomly selected stations, and make little effort
   per station.}},
