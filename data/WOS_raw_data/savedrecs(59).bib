@article{ ISI:A1997XT48300013,
Author = {Busby, WH and Brecheisen, WR},
Title = {{Chorusing phenology and habitat associations of the crawfish frog, Rana
   areolata (Anura: Ranidae), in Kansas}},
Journal = {{SOUTHWESTERN NATURALIST}},
Year = {{1997}},
Volume = {{42}},
Number = {{2}},
Pages = {{210-217}},
Month = {{JUN}},
Abstract = {{The chorusing phenology of the crawfish frog, Rana areolata, was
   monitored at a site in east-central Kansas from 1989 to 1993. Calling
   was initiated at temporary pools each spring following rainfall events
   with temperatures of ca. greater than or equal to 13.0 degrees C. Each
   year, peak chorusing spanned only 3 to 4 nights while some calling
   occurred periodically during a 22 to 55-d period in March to early May.
   Aural surveys of calling males conducted during peak chorusing periods
   in a 9-county area revealed that R areolata remains locally common in
   areas of suitable habitat. The largest populations, as judged by calling
   activity occurred in areas with large tracts of native tallgrass
   prairie, gentle terrain, and deep, clay soils. Few calling males were
   heard in areas with shallow soils or intensive agricultural development.}},
