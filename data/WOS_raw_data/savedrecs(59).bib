@article{ ISI:000088381300003,
Author = {Szollosi-Nagy, A and Najlis, P and Bjorklund, G},
Title = {{Assessing the world's freshwater resources}},
Journal = {{NATURE \& RESOURCES}},
Year = {{1998}},
Volume = {{34}},
Number = {{1}},
Pages = {{8-18}},
Month = {{JAN-MAR}},
Abstract = {{This century has seen a rapid growth in water demand due to increasing
   use of water in irrigated food production and industry and increased per
   capita use for domestic purposes. between 1900 and 1995, water
   withdrawals increased by a factor of over six, more than double the rate
   of population growth, increased demands are causing water stress in many
   areas of the world, to the extent that nearly 10\% of the world's
   population live in countries using so much of their water resources that
   they can be considered to be highly water stressed. Under the current
   patterns of water use, an estimated two thirds of the total world
   population may well face water stress by the year 2025, A comprehensive
   review of the world's freshwater resources has underlined the need to
   move water issues higher up the environmental management and political
   agenda, and to upgrade capabilities for monitoring and assessing the
   world's water resources.}},
