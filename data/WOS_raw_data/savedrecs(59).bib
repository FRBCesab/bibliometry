@article{ ISI:A1997XU74100009,
Author = {McNulty, SA and Porter, WF and Mathews, NE and Hill, JA},
Title = {{Localized management for reducing white-tailed deer populations}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
