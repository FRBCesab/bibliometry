@article{ ISI:A1997XA02700011,
Author = {Inchausty, VH and Foutz, M and Heckmann, RA and Ruas, C and Ruas, P},
Title = {{Diplostomiasis in native and introduced fishes from Yellowstone Lake,
   Wyoming}},
Journal = {{GREAT BASIN NATURALIST}},
Year = {{1997}},
Volume = {{57}},
Number = {{2}},
Pages = {{178-183}},
Month = {{APR}},
Abstract = {{Totals of 101 native Yellowstone cutthroat (Oncorhynchus clarki
   bouvieri), 27 introduced lake trout (Salvelinus namaycush), and 40
   introduced longnose sucker (Catostomus catostomus) from Yellowstone
   Lake, Wyoming, USA, were examined for eve flukes. Metacercariae of the
   trematode fluke Diplostomum were in vitreous humor and/or lens of 94\%
   of Yellowstone cutthroat trout, 92\% of lake trout, and 78\% of longnose
   sucker. Longnose sucker had 7\% prevalence of infection in both lens and
   vitreous humor of metacercariae, while Yellowstone cutthroat trout had
   3\% and lake trout 8\%. Diplostomum spathaceum was in lens tissue of 5\%
   of infected Yellowstone cutthroat trout and 93\% of longnose sucker;
   Diplostomum baeri was in vitreous humor of 92\% each of infected
   Yellowstone cutthroat trout and lake trout. Morphological
   characteristics indicate that a single species infected the lens of
   Yellowstone cutthroat trout and longnose sucker, while another species
   infected lake trout. Impacts of the parasite interchange between native
   and introduced ashes of Yellowstone Lake, Wyoming, are unknown but
   should be monitored each year.}},
