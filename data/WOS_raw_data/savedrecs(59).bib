@article{ ISI:000072007500008,
Author = {Kutenkov, AP},
Title = {{On the role of weather in population dynamics of the common frog (Rana
   temporaria) in the north}},
Journal = {{RUSSIAN JOURNAL OF ECOLOGY}},
Year = {{1998}},
Volume = {{29}},
Number = {{1}},
Pages = {{43-48}},
Month = {{JAN-FEB}},
Abstract = {{The long-term dynamics of relative abundance of generations, with
   parallel weather monitoring throughout the year, was traced. The yearly
   success of metamorphosis is independent of the amount of spawn produced
   and is only conditioned by the water balance of reproductive ponds. The
   abundance of young of the year in autumn weakly depends on reproductive
   success, being associated mainly with the duration of fattening and the
   weather during this period. The abundance of yearlings, vaguely
   depending on their number in the year of metamorphosis, is determined by
   both duration and conditions of fattening before the first hibernation
   and by wintering conditions. The group of mature individuals in autumn
   is represented mainly by 2- and 3-year-old individuals, whose proportion
   is nonvariable and indeterminate. The size of this group depends on the
   conditions (temperature of the environment) in which the animals spent
   their first winter.}},
