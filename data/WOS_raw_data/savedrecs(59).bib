Author = {Turcani, M},
Editor = {{Mcmanus, ML and Liebhold, AM}},
Title = {{Monitoring gypsy moth (Lymantria dispar L.) populations (Lep.,
   Lymantriidae) in Slovakia using pheromone traps}},
