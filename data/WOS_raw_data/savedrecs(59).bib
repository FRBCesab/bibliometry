@article{ ISI:A1997XL30100009,
Author = {Chevalier, G and Dumont, C and Langlois, C and Penn, A},
Title = {{Mercury in northern Quebec: Role of the Mercury Agreement and status of
   research and monitoring}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{1997}},
Volume = {{97}},
Number = {{1-2}},
Pages = {{75-84}},
Month = {{JUN}},
