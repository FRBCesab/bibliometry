@inproceedings{ ISI:000084832400019,
Author = {Williams, GA and McMahon, BR},
Editor = {{Morton, B}},
Title = {{Haemolymph pH and oxygen levels in a naturally stressed tropical limpet,
   Cellana grata}},
Booktitle = {{MARINE BIOLOGY OF THE SOUTH CHINA SEA III}},
Year = {{1998}},
Pages = {{239-245}},
Note = {{3rd International Conference on the Marine Biology of the South China
   Sea, HONG KONG, HONG KONG, OCT 28-NOV 01, 1996}},
Organization = {{Marine Biol Assoc Hong Kong}},
Abstract = {{Many high shore marine species are limited in both their distribution
   and behaviour by environmental factors such as high temperature and
   desiccation. High shore limpets show a variety of responses to counter
   physical extremes and are remarkably tolerant of high ionic haemolymph
   concentrations and water loss. Despite this, populations of limpets can
   suffer mass mortalities on hot, calm, days during low water periods in
   Hong Kong. The exact cause of death is uncertain but ionic stress due to
   water loss has been suggested as a likely contributor. In this work we
   tested the hypothesis that either a decrease in haemolymph oxygenation
   and/or an increase in haemolymph acidosis might contribute to mortality
   in a tropical high shore limpet, Cellana grata. Populations of C. grata
   were transplanted from an east facing shore into high stress (horizontal
   rock surfaces; temperatures > 43 degrees C) and reduced stress (vertical
   rock surfaces; temperatures < 35 degrees C) habitats and their
   haemolymph pH and oxygen levels monitored over one daytime emersion
   period. Severely stressed animals showed no decrease in haemolymph
   oxygen levels as compared to less stressed animals but marked acidosis
   (pH 6.8) was observed with progressive exposure and all individuals died
   after 7 hours exposure. Individuals on vertical rock surfaces did not
   suffer any mortality and maintained haemolymph pH and oxygen values at
   similar levels as unstressed animals (individuals actively foraging
   whilst awash by waves at the next high tide). The marked acidosis
   effect, either in isolation or together with osmotic and ion imbalance,
   may contribute to the mass mortalities of Cellana grata associated with
   desiccating conditions in Hong Kong summers.}},
