@article{ ISI:A1997XH07200028,
Author = {Bruce, LJ and McCracken, DI and Foster, GN and Aitken, MN},
Title = {{The effects of cadmium and zinc-rich sewage sludge on epigeic Collembola
   populations}},
Journal = {{PEDOBIOLOGIA}},
Year = {{1997}},
Volume = {{41}},
Number = {{1-3}},
Pages = {{167-172}},
Month = {{JUN}},
Note = {{IX International Colloquium on Apterygota, DUBLIN, IRELAND, 1996}},
Abstract = {{Sewage sludge contaminated with cadmium or zinc was applied to
   experimental plots on grassland. The effects on Collembola populations
   were monitored using pitfall traps and suction samples. Multivariate
   analyses indicated that the majority of between-plot variation in
   Collembola assemblages was a consequence of grass cutting. In addition,
   differences were also evident between treatments, with the greatest
   contrast occurring between uncontaminated sludge and cadmium-rich
   sludge. Cadmium-rich sludge had detrimental effects on both Lepidocyrtus
   cyaneus and Isotoma viridis, and I. viridis also declined on the
   zinc-rich plots.}},
