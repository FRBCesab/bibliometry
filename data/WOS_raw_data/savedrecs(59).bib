@inproceedings{ ISI:000074947200030,
Author = {Novotny, J and Turcani, M and Zubrik, M},
Editor = {{Mcmanus, ML and Liebhold, AM}},
Title = {{The system of gypsy moth population regulation in the Slovak Republic}},
Booktitle = {{POPULATION DYNAMICS, IMPACTS, AND INTEGRATED MANAGEMENT OF FOREST
   DEFOLIATING INSECTS - PROCEEDINGS}},
