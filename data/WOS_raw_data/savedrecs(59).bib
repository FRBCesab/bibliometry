@inproceedings{ ISI:000175882700018,
Author = {Stone, C and Clarke, AR},
Editor = {{Zalucki, MP and Drew, R and White, GG}},
Title = {{A phytocentric approach towards future pest management in Australian
   Eucalypt plantations - an alternative to classical IPM}},
Booktitle = {{PEST MANAGEMENT - FUTURE CHALLENGES, VOLS 1 AND 2, PROCEEDINGS}},
Year = {{1998}},
Pages = {{144-152}},
Note = {{6th Australisian Applied Entomological Research Conference, UNIV
