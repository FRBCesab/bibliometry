@article{ ISI:A1997YL05600007,
Author = {Swanberg, TR},
Title = {{Movements of bull trout (Salvelinus confluentus) in the Clark Fork River
   system after transport upstream of Milltown Dam}},
Journal = {{NORTHWEST SCIENCE}},
Year = {{1997}},
Volume = {{71}},
Number = {{4}},
Pages = {{313-317}},
Month = {{NOV}},
Abstract = {{Populations of bull trout in the Clark Fork River drainage of western
   Montana are small and fragmented by hydroelectric dams. The movements of
   four adult bull trout in this river were monitored by radiotelemetry
   after transport upstream of Milltown Dam. Two fish migrated 40 and 130
   km to tributary streams of Rock Creek; one of these likely spawned. Two
   other bull trout moved downstream of the dam and appeared to later
   attempt to re-ascend. Movements of these bull trout suggest that
   Milltown Dam blacks migrations and that transporting fish upstream of
   the dam can enhance populations. Given the small sizes of bull trout
   populations in the Clark Fork River drainage, the benefits derived from
   transporting adult fish over dams is potentially great.}},
