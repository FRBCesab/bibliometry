@article{ ISI:A1997XR58800010,
Author = {Kubisiak, JF and Paisley, RN and Wright, RG},
Title = {{Estimating the accuracy of counting Eastern Wild Turkeys, Meleagris
   gallopavo silvestris, using helicopters in Wisconsin}},
Journal = {{CANADIAN FIELD-NATURALIST}},
Year = {{1997}},
Volume = {{111}},
Number = {{3}},
Pages = {{417-421}},
Month = {{JUL-SEP}},
Abstract = {{Forty-three flocks of Eastern Wild Turkeys (Meleagris gallopavo
   silvestris), 26 of which contained greater than or equal to 1 bird with
   a functional radio, were used to compare the number of flocks and number
   of birds counted in winter by aerial observers with known numbers on the
   ground. Research was conducted in an experimental turkey hunting zone
   (THZ) where Turkeys were intensively monitored as part of a population
   dynamics study. Helicopter-based counts (H) of total flocks and total
   birds avenged 86\% and 80\%, respectively, of ground-based (G) counts in
   the THZ where H/G comparisons were made in 1989, 1991, and 1993. Turkey
   densities adjusted by the 0.80 H/G ratio based on the total number of
   birds counted by helicopter-and ground-based observers in the
   experimental THZ, varied from 0.9 to 11.0/km(2) during 12 helicopter
   counts in 5 THZs during 1989-1994. Turkey densities were correlated (P =
   0.025) with the spring harvest density derived from the number of
   gobblers killed/km(2) of forest from mandatory registration. Helicopter
   surveys of Wild Turkeys are most applicable in areas dominated by
   deciduous woodlands and agricultural crop fields where snow depths
   greater than or equal to 15 cm occur.}},
