@inproceedings{ ISI:000080892300006,
Author = {Case, RL and Buckland, L},
Editor = {{Miller, SD and Reynolds, HV}},
Title = {{Reproductive characteristics of grizzly bears in the Kugluktuk area,
   Northwest Territories, Canada}},
Booktitle = {{URSUS, VOL 10 - 1998}},
