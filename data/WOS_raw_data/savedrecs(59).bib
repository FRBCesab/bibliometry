@article{ ISI:A1997YH73300008,
Author = {Riley, DG and Ciomperlik, MA},
Title = {{Regional population dynamics of whitefly (Homoptera: Aleyrodidae) and
   associated parasitoids (Hymenoptera: Aphelinidae)}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{1997}},
Volume = {{26}},
Number = {{5}},
Pages = {{1049-1055}},
Month = {{OCT}},
Abstract = {{Regional monitoring of whiteflies, Bemisia sp., in the lower Rio Grande
   Valley of Texas was undertaken between 1991 and 1995 to determine
   seasonal abundance and spatial population dynamics of this pest. Samples
   of whiteflies were taken using yellow sticky traps along roadways and
   samples from plants at selected locations. The abundance of parasitoids
   of whiteflies was quantified from each plant sample by species. The
   number of whitefly adults on yellow sticky traps consistently increased
   dramatically during late spring and summer and declined in the fall.
   Dispersion of whiteflies on sticky traps generally changed from highly
   clumped in the spring to less clumped in the summer and early fall of
   each year. The population of the native parasitoid complex of the
   whitefly correlated with increases of whitefly populations. Six whitefly
   parasitoid species were collected routinely and identified during this
   survey. In order of decreasing abundance they were Eretmocerus sp.,
   Encarsia pergandiella, Encarsia meritoria, Encarsia sp. nr. strenua,
   Encarsia luteola, and Encarsia formosa. Population levels were higher
   for whitefly and lower for whitefly parasitoids in 1995 than observed in
   1994.}},
