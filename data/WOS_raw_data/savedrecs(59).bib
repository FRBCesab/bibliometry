@inproceedings{ ISI:000079137400036,
Author = {Oliver, RL and Whittington, J},
Editor = {{Imberger, J}},
Title = {{Using measurements of variable chlorophyll-a fluorescence to investigate
   the influence of water movement on the photochemistry of phytoplankton}},
Booktitle = {{PHYSICAL PROCESSES IN LAKES IN OCEANS}},
