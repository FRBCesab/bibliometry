@article{ ISI:A1997XW29800016,
Author = {Noel, O and LeBail, PY},
Title = {{Does cyclicity of growth rate in rainbow trout exist?}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
Year = {{1997}},
Volume = {{51}},
Number = {{3}},
Pages = {{634-642}},
Month = {{SEP}},
Abstract = {{Ninety rainbow trout Oncorhynchus mykiss were reared in three tanks
   (n=30), and their growth rate was monitored for 25 weeks. During the
   experiment, fish were fed nd libitum, and kept under simulated natural
   photoperiod and constant temperature (14 +/- 1 degrees C). Throughout
   the experiment, water quality (pH, O-2, N-NH3, N-NH4, NO2) was
   considered optimum. Two growth rate periods were observed on the
   population: first an acclimation period (weeks 1-5), followed by a
   period of general decrease in growth rate (weeks 5-25). This trend was
   modelled with a third-order polynomial function. The autocorrelation
   analysis of the fluctuations revealed a cyclicity of the growth rate
   with a 4-5-week period. This cyclicity was independent of the tanks and
   of the growth rate trend type (long- v. short-term acclimation period).
   However, at the individual level, trends as well as short-term
   fluctuations were heterogeneous and no clear cyclicity could be
   detected. The biological relevance of the population growth rate
   cyclicity and the apparent absence of similar individual fluctuations
   are discussed. (C) 1997 The Fisheries Society of the British Isles.}},
