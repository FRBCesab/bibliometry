@article{ ISI:000075404000025,
Author = {Schmitz-Feuerhake, I and Dannheim, B and Heimers, A and Oberheitmann, B
   and Schroder, H and Ziggel, H},
Title = {{Leukemia in the proximity of a German boiling-water nuclear reactor:
   Evidence of population exposure by chromosome studies and environmental
   radioactivity}},
