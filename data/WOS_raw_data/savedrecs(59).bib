@article{ ISI:000071342200004,
Author = {Woodworth, BL and Farm, BP and Mufungo, C and Borner, M and Kuwai, JO},
Title = {{A photographic census of flamingos in the Rift Valley lakes of Tanzania}},
Journal = {{AFRICAN JOURNAL OF ECOLOGY}},
Year = {{1997}},
Volume = {{35}},
Number = {{4}},
Pages = {{326-334}},
Month = {{DEC}},
Abstract = {{An aerial photographic census of flamingos (Phoenicopterus minor and P.
   ruber) was conducted on 20 July 1994 on nine lakes in Tanzania: Lakes
   Natron, Manyara, Magadi (Ngorongoro), Burungi, Embagai, Eyasi, Lagarja,
   Masek, and Ambussel. This census. conducted concurrently with a survey
   in Kenya, documented the numbers and distribution of flamingos in the
   Rift Valley Lakes of East Africa. The total population size of flamingos
   in the Tanzanian lakes surveyed was approximate to 907,000. The largest
   concentrations in Tanzania were found in Lake Natron and Embagai Crater
   Lake. Although flamingos are known to breed in Lake Natron, no evidence
   of breeding was observed during the survey. The total estimate for the
   southern Kenya and northern Tanzania Rift Valley was approximate to 28
   million. Trends in flamingo populations, survey methodology, and future
   monitoring needs are discussed.}},
