@article{ ISI:000084831700024,
Author = {O'Halloran, K and Booth, LH and Hodge, S and Thomsen, S and Wratten, SD},
Title = {{Biomarker responses of the earthworm Aporrectodea caliginosa to
   organophosphates: Hierarchical tests}},
Journal = {{PEDOBIOLOGIA}},
Year = {{1999}},
Volume = {{43}},
Number = {{6}},
Pages = {{646-651}},
Month = {{DEC}},
Abstract = {{Aporrectodea caliginosa is the most abundant earthworm inhabiting arable
   and pasture land in New Zealand. The sublethal effects of the
   organophosphate pesticides diazinon and chlorpyrifos on this earthworm
   were measured via the activity of an enzymatic biomarker, cholinesterase
   (ChE). ChE responses were monitored in a series of test systems: from
   laboratory assays, to field mesocosms and natural populations of
   earthworms. In the laboratory, both pesticides elicited an effect on ChE
   in juvenile A. caliginosa at srrbIethaI concentrations, with ChE
   activity being inhibited by greater than or equal to 75\% after I day
   and greater than or equal to 90\% after 7 and 14 days. In the mesocosm
   studies, laboratory-reared juvenile earthworms were caged in lengths of
   plastic pipe, sunk into the ground in an established pasture. The
   pasture was then sprayed with pesticides using conventional farm
   machinery. After 2 and 7 days exposure in the mesocosms, earthworm ChE
   activity was unaffected by the test compounds. In natural populations,
   no differences in biomarker levels were observed in juvenile earthworms
   sampled from pasture sprayed with the organophosphates compared to those
   from control pasture. A survey of earthworms from local farms found no
   relationship between ChE activity and from type. Therefore, although ChE
   was highly responsive to sublethal organophosphate exposures in a
   controlled laboratory situation, the response became less obvious in
   field situations using standard application rates of these two
   pesticides. These results suggest that diazinon and chlorpyrifos applied
   at recommended rates cause no acute effects in A. caliginosa or induce
   physiological stresses as indicated by ChE activity.}},
