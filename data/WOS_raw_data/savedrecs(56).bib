@inproceedings{ ISI:000168019200016,
Author = {Shepherd, CR},
Editor = {{VanDijk, PP and Stuart, BL and Rhodin, GJ}},
Title = {{Export of live freshwater turtles and tortoises from North Sumatra and
   Riau, Indonesia: A case study}},
Booktitle = {{ASIAN TURTLE TRADE: PROCEEDINGS OF A WORKSHOP ON CONSERVATION AN D TRADE
   OF FRESHWATER TURTLES AND TORTOISES IN ASIA}},
Series = {{CHELONIAN RESEARCH MONOGRAPHS}},
