@article{ ISI:000089722900040,
Author = {Balonov, MI and Bruk, GY and Zvonova, IA and Pitkevich, VA and
   Bratilova, AA and Jesko, TV and Shutov, VN},
Title = {{Methodology of internal dose reconstruction for a Russian population
   after the Chernobyl accident}},
