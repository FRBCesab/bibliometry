@inproceedings{ ISI:000085514200012,
Author = {Kurz, RC and Tomasko, DA and Burdick, D and Ries, TF and Patterson, K
   and Finck, R},
Editor = {{Bortone, SA}},
Title = {{Recent trends in seagrass distributions in southwest Florida coastal
   waters}},
