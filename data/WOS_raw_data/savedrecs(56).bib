@article{ ISI:000083182900037,
Author = {Prus-Glowacki, W and Wojnicka-Poltorak, A and Oleksyn, J and Reich, PB},
Title = {{Industrial pollutants tend to increase genetic diversity: Evidence from
   field-grown European Scots pine populations}},
