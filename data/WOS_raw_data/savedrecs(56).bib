@article{ ISI:000085778900018,
Author = {Gorsky, G and Flood, PR and Youngbluth, M and Picheral, M and Grisoni,
   JM},
Title = {{Zooplankton distribution in four western Norwegian fjords}},
Journal = {{ESTUARINE COASTAL AND SHELF SCIENCE}},
