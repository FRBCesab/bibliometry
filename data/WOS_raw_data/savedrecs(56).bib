@inproceedings{ ISI:000166665900004,
Author = {Bengtsson, K},
Editor = {{Ehrlen, J and Svensson, BM}},
Title = {{Long-term demographic variation in range-margin populations of
   Gypsophila fastigiata}},
Booktitle = {{PLANT INTERACTIONS, DISPERSAL AND COMMUNITY STRUCTURE}},
Series = {{SPECIAL FEATURES IN VEGETATION SCIENCE}},
