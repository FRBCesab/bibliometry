@article{ ISI:000083467700008,
Author = {Guinez, R and Castilla, JC},
Title = {{A tridimensional self-thinning model for multilayered intertidal mussels}},
Journal = {{AMERICAN NATURALIST}},
Year = {{1999}},
Volume = {{154}},
Number = {{3}},
Pages = {{341-357}},
Month = {{SEP}},
Abstract = {{Intertidal mussels usually form complex multilayered matrices with
   density-dependent effects on survival and growth, and self-thinning
   scaling between biomass (B) and density (N) is expected. This article
   develops a tridimensional model of space-driven self-thinning that in
   addition to B-N explicitly includes the degree of packing of the
   mussels, measured as the number of layers (L). The structure of our
   model (B-N-L) encompasses previous bidimensional models (B-N) of
   self-thinning as special cases and enables comparisons between mono- and
   multilayered populations. We contrast the predictions of the bi- and
   tridimensional models using data obtained from Perumytilus purpuratus
   mussel beds on the rocky shores of central Chile monitored during a
   28-mo period. The tridimensional model suggests that density dependence
   is much more frequent than hitherto indicated by bidimensional models.
   We propose that our space-driven tridimensional model may be applied nor
   only to mussels but also to other species where spatial overlapping
   configurations occur.}},
