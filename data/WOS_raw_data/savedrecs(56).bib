@article{ ISI:000165681700009,
Author = {Ionescu, A and Candau, Y and Mayer, E and Colda, I},
Title = {{Analytical determination and classification of pollutant concentration
   fields using air pollution monitoring network data - Methodology and
   application in the Paris area, during episodes with peak nitrogen
   dioxide levels}},
