@article{ ISI:000166519000004,
Author = {Maisonneuve, C and Mc Nicoll, R and Desrosiers, A},
Title = {{Comparative productivity of American Black Ducks and Mallards nesting in
   agricultural landscapes of southern Quebec}},
Journal = {{WATERBIRDS}},
Year = {{2000}},
Volume = {{23}},
Number = {{3}},
Pages = {{378-387}},
Abstract = {{We monitored radio-marked female American Black Ducks (Anas rubripes)
   and Mallards (A. platyrhynchos) in agricultural landscapes of southern
   Quebec in 1994-1996 to characterize nesting habitat, and to compare
   nesting success and female survival rates. In early spring, when nesting
   cover is almost non-existent in agricultural fields, both species avoid
   fields for nesting. These habitats are rather used later in cases of
   renesting. Both species selected recent wood cuts and edge-transition
   habitats (abandoned farmland, hedgerows and riparian areas) as nesting
   habitat. Median nest initiation date, clutch size, and nest success
   rates did not differ between the two species. Especially high Black Duck
   nesting success (100\%) in peatlands underlines the importance of these
   habitats. When peatland nests are excluded to provide a better
   indication of what prevails in typical agricultural landscapes, nesting
   effort of female Mallards is almost twice that of Black; Ducks. Survival
   nhs of females were similar for both species, both during laying and
   post-laying periods.}},
