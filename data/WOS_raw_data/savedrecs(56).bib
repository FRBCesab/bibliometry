@article{ ISI:000082717100011,
Author = {Meneses, F and Romieu, I and Ramirez, M and Colome, S and Fung, K and
   Ashley, D and Hernandez-Avila, M},
Title = {{A survey of personal exposures to benzene in Mexico City}},
Journal = {{ARCHIVES OF ENVIRONMENTAL HEALTH}},
