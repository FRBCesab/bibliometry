@article{ ISI:000083601000002,
Author = {Lyytikainen-Saarenmaa, P and Anderbrant, O and Lofqvist, J and
   Hedenstrom, E and Hogberg, HE},
Title = {{Monitoring European pine sawfly population densities with pheromone
   traps in young pine plantations}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
Year = {{1999}},
Volume = {{124}},
Number = {{2-3}},
Pages = {{113-121}},
Month = {{DEC 6}},
Abstract = {{Field trials utilizing pheromone traps were evaluated to develop a
   population monitoring and predicting system for the European pine sawfly
   (Neodiprion sertifer). Larval density was censused in a transect across
   each study site. Three traps per site were bailed with 100 mu g of
   (2S,3S,7S)-3,7-dimethyl-2-pentadecyl (diprionyl) acetate. During a two-
   year period traps loaded with 1, 10 and 100 mu g of diprionyl acetate
   were compared. Correlations and coefficients of determination between
   numbers of captured males and larvae of the same or successive
   generations were seldom high. Between-generation relationships were
   significant, when population density was increasing. Having years with
   increasing densities in a row, the present monitoring method might
   provide warning of an outbreak. (C) 1999 Elsevier Science B.V. All
   rights reserved.}},
