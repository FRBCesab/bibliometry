@article{ ISI:000083856800011,
Author = {Bruelheide, H and Flintrop, T},
Title = {{Evaluating the transplantation of a meadow in the Harz Mountains,
   Germany}},
Journal = {{BIOLOGICAL CONSERVATION}},
