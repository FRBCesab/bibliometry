@article{ ISI:000088061000014,
Author = {DeMark, JJ and Thomas, JD},
Title = {{Seasonal activity, wood consumption rates, and response to above-ground
   delivery of hexaflumuron-treated bait to Reticulitermes flavipes
   (Isoptera : Rhinotermitidae) in Pennsylvania and Wisconsin}},
Journal = {{SOCIOBIOLOGY}},
Year = {{2000}},
Volume = {{36}},
Number = {{1}},
Pages = {{181-200}},
Abstract = {{Foraging patterns of the eastern subterranean termite, Reticulitermes
   flavipes (Kollar) (Isoptera: Rhinotermitidae), were studied inside and
   around two infested structures (using an Acoustic Emission Detector
   (AED) and monitoring stations containing wood) in Pennsylvania and
   Wisconsin for greater than one year. Inside both structures, it was
   found that the termites were active throughout the year, including the
   winter months. After the foraging patterns were examined, the structures
   were then baited with the chitin synthesis inhibitor (insect growth
   regulator) hexaflumuron. Bait was delivered inside the structures only
   through above-ground bait stations (Recruit(TM) AG prototypes; Dow
   AgroSciences LLC, Indianapolis, IN). The structure in Wisconsin was
   baited in June 1996 and the termites were eliminated by October 1996
   utilizing 18 different bait placement locations. The structure in
   Pennsylvania was first baited in October 1996 and the termites were
   eliminated as of August 1997 using five different bait placement
   locations. Both sites were confirmed to have no termite activity the
   following spring (May 1997 for Wisconsin site and May 1998 for
   Pennsylvania site) upon examination of bait placement, monitoring, and
   AED locations.}},
