@inproceedings{ ISI:000174706000014,
Author = {Knox, SC and Chambers, C and Germaine, SS},
Editor = {{Vance, RK and Edminster, CB and Covington, WW and Blake, JA}},
Title = {{Habitat associations of the sagebrush lizard (Sceloporus graciosus):
   Potential responses of an ectotherm to ponderosa pine forest restoration
   treatments}},
