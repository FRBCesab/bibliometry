@article{ ISI:000089076500005,
Author = {Marshall, AR and Blankenship, HL and Connor, WP},
Title = {{Genetic characterization of naturally spawned Snake River fall-run
   chinook salmon}},
Journal = {{TRANSACTIONS OF THE AMERICAN FISHERIES SOCIETY}},
Year = {{2000}},
Volume = {{129}},
Number = {{3}},
Pages = {{680-698}},
Month = {{MAY}},
Abstract = {{We sampled juvenile Snake River chinook salmon Oncorhynchus tshawytscha
   to genetically characterize the endangered Snake River fall-run
   population. Juveniles from fall and spring-summer lineages coexisted in
   our sampling areas but were differentiated by large allozyme allele
   frequency differences. We sorted juveniles by multilocus genotypes into
   putative fall and spring lineage subsamples and determined lineage
   composition using maximum likelihood estimation methods. Paired
   sMEP-1{*} and PGK-2{*} genotypes-encoding malic enzyme (NADP(+)) and
   phosphoglycerate kinase, respectively-were very effective for sorting
   juveniles by lineage, and subsamples estimated to be 100\% fall lineage
   were obtained in four annual samples. We examined genetic relationships
   of these fall lineage juveniles with adjacent populations from the
   Columbia River and from Lyons Ferry Hatchery, which was established to
   perpetuate the Snake River fall-run population. Our samples of naturally
   produced Snake River fall lineage juveniles were most closely aligned
   with Lyons Ferry Hatchery samples. Although fall-run strays of Columbia
   River hatchery origin found on spawning grounds threaten the genetic
   integrity of the Snake River population, juvenile samples (a) showed
   distinctive patterns of allelic diversity, (b) were differentiated from
   Columbia River populations, and (c) substantiate earlier conclusions
   that this population is an important genetic resource. This first
   characterization of naturally produced Snake River fall chinook salmon
   provides a baseline for monitoring and recovery planning.}},
