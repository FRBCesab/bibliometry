@article{ ISI:000368443200006,
Author = {Mangano, M. C. and Kaiser, M. J. and Porporato, E. M. D. and Lambert, G.
   I. and Spano, N.},
Title = {{Trawling disturbance effects on the trophic ecology of two co-generic
   Astropectinid species}},
Journal = {{MEDITERRANEAN MARINE SCIENCE}},
Year = {{2015}},
Volume = {{16}},
Number = {{3}},
Pages = {{538-549}},
Abstract = {{Physical disturbance by trawling can have both negative and positive
   effects on populations of scavenging benthic organisms. In the present
   study the impact of fishing activity on feeding behaviour of the two
   Astropectinids, Astropecten bispinosus and A. irregularis, was assessed
   based on stomach contents analysis. The study was carried out along
   trawled seabed highlighting the positive response of the two facultative
   scavengers to carrion generated by trawl disturbance. Furthermore, there
   was greater food specialization in areas that were more heavily
   exploited by trawling. This specialisation could be linked to the
   availability of certain prey that results from the passage of fishing
   gears across the seabed. Interestingly, differences between the two
   species analysed have been highlighted in term of population dynamic,
   feeding rate, diet composition and diet diversity, testifying their
   capacity to coexist in the same fishing grounds.}},
