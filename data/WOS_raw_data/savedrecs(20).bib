@article{ ISI:000380152100017,
Author = {Al Naggar, Yahya and Tan, Yang and Rutherford, Colton and Connor, Wayne
   and Griebel, Philip and Giesy, John P. and Robertson, Albert J.},
Title = {{Effects of treatments with Apivar((R)) and Thymovar((R)) on V-destructor
   populations, virus infections and indoor winter survival of Canadian
   honey bee (Apis mellifera L.) colonies}},
Journal = {{JOURNAL OF APICULTURAL RESEARCH}},
Year = {{2015}},
Volume = {{54}},
Number = {{5}},
Pages = {{548-554}},
Abstract = {{Efficacies of two miticides, Apivar((R)) and Thymovar((R)), were
   evaluated as a fall treatment against V. destructor. The effect of
   treatment with miticides was further evaluated by monitoring both viral
   load and rate of indoor overwintering survival of colonies of European
   honey bees (Apis mellifera L.) in the vicinity of Saskatoon,
   Saskatchewan, Canada. Forty-five colonies were randomly assigned to
   three treatment groups with 15 hives per group: Group 1; 2 strips of
   Thymovar((R)) (thymol); Group 2; 2 strips of Apivar((R)) (Amitraz); and
   Group 3; no treatment (control). Significant decreases in the rates of
   colony infestation (Mites per hundred bees, MPHB) by V. destructor were
   observed (p<0.05) between colonies of bees treated with Apivar((R)) in
   October 2013 when compared to control colonies. Efficacy of Apivar((R))
   and Thymovar((R)) against V. destructor after treatment for 22days were
   76.5 and 26.7\%, respectively. After 22days, concentrations of the two
   miticides in bees were 15.4ng amitraz/g wet mass (wm) and 64,800ng
   thymol/g wm. There were no significant differences (p>0.05) in the
   percentage of colonies infected by deformed wing virus (DWV) and Israeli
   acute paralysis virus (IAPV) either before or after treatment with
   Apivar((R)) or Thymovar((R)) in October 2013 and 7months post treatment
   in April 2014. Only the Apivar((R)) treatment group showed IAPV
   infections in April 2014. The group treated with Apivar((R)) exhibited a
   better overwintering rate of survival (93\%), than hives treated with
   Thymovar((R)) (67\%). These results suggest volatile miticides like
   Thymovar((R)) should be avoided in geographical areas with colder fall
   temperatures.}},
