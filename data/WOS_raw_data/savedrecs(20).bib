@article{ ISI:000348639700008,
Author = {Seto, Takayuki and Matsuda, Naoko and Okahisa, Yuji and Kaji, Koichi},
Title = {{Effects of Population Density and Snow Depth on the Winter Diet
   Composition of Sika Deer}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
