@article{ ISI:000364844800011,
Author = {Li, Bai and Cao, Jie and Chang, Jui-Han and Wilson, Carl and Chen, Yong},
Title = {{Evaluation of Effectiveness of Fixed-Station Sampling for Monitoring
   American Lobster Settlement}},
