@article{ ISI:000364389200003,
Author = {Scalici, M. and Traversetti, L. and Malafoglia, V. and Colamartino, M.
   and Persichini, T. and Colasanti, M.},
Title = {{Concepts and tools for exploiting sessile bio-filters as early warning
   elements: introductory applications for marine ecosystem preservation}},
Journal = {{ISJ-INVERTEBRATE SURVIVAL JOURNAL}},
Year = {{2015}},
Volume = {{12}},
Pages = {{278-286}},
Abstract = {{Current evidence suggests that integrating diverse warning systems at
   different biological levels may not only increase the probability of
   detecting threats but also mitigate their impact. Here, we propose the
   use of both molecular and morphological descriptors at different
   biological levels in sessile bivalves (a suitable biological model in
   monitoring programs) to collect information on the ecosystem health of
   coastal marine habitats. In this context, studies may be implemented on
   biomarkers to exploit some population features, with the aim to propose
   an actual monitoring program that predictively would provide possible
   scenarios on the species fitness and ecosystem changes. Thus, the use of
   quality biotic elements may provide an objective environmental
   monitoring method and facilitate the development of sanitary, economic,
   and social strategies related to sustainable exploitation.}},
