@article{ ISI:000371938000023,
Author = {El-Heneidy, A. H. and Khidr, A. A. and Taman, A. A.},
Title = {{Side-effects of Insecticides on Non-target Organisms: 1-In Egyptian
   Cotton Fields}},
Journal = {{EGYPTIAN JOURNAL OF BIOLOGICAL PEST CONTROL}},
Year = {{2015}},
Volume = {{25}},
Number = {{3}},
Pages = {{685-690}},
Note = {{4th International Conference for Applied Biological Control (ESBCP),
   Cairo, EGYPT, OCT 19-22, 2015}},
Abstract = {{Side effects of some groups of recommended insecticides in cotton fields
   were evaluated on non-target pests and predators through experimental
   field trials at two Egyptian Governorates for two growing seasons, 2013
   and 2014. Direct counts of target and non-target organisms,
   pre-treatment and at 3 day intervals post treatments for each
   insecticide/location were practiced. Reduction rate in insects'
   population was estimated. As well, a discriminating concentration
   technique was used for rapid monitoring of insecticidal resistance in
   field-samples of the pink bollworm (PBW), Pectinophora gossypiella
   (Saund.) and the cotton leaf worm (CLW) Spodoptera littoralis (Boisd.)
   larvae, collected from sprayed cotton fields. Rates of reduction in P.
   gossypiella population (target pest) ranged between 63.2-72.5 and
   61.9-74.8\% in 2013 and 2014, respectively. Respective rates in
   non-target pest species; Aphis gossypii (Glov.), Bemisia tabaci (Germ.)
   and Tetranychus urticae Koch were 35.6-83.3 and 50.3-83.0\%, while they
   attained 79.2-96.5 and 65.2-91.8\% in the populations of the predatory
   species; coccinellids, Chlysoperla carnea (Steph.) and spiders.
   Resistance \% in the PBW and CLW collected from sprayed fields was
   estimated by 19.2-54.6\%.}},
Publisher = {{SPRINGEROPEN}},
Address = {{CAMPUS, 4 CRINAN ST, LONDON, N1 9XW, ENGLAND}},
