@article{ ISI:000365192000002,
Author = {Colloff, Matthew J. and Caley, Peter and Saintilan, Neil and Pollino,
   Carmel A. and Crossman, Neville D.},
Title = {{Long-term ecological trends of flow-dependent ecosystems in a major
   regulated river basin}},
