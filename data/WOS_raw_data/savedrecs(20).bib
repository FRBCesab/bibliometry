@article{ ISI:000344370200009,
Author = {Dolezal, B. A. and Boland, D. M. and Carney, J. and Abrazado, M. and
   Smith, D. L. and Cooper, C. B.},
Title = {{Validation of Heart Rate Derived from a Physiological Status
   Monitor-Embedded Compression Shirt Against Criterion ECG}},
Journal = {{JOURNAL OF OCCUPATIONAL AND ENVIRONMENTAL HYGIENE}},
