@article{ ISI:000349154400003,
Author = {Williams, Jessica L. and Pierce, Simon J. and Fuentes, Mariana M. P. B.
   and Hamann, Mark},
Title = {{Effectiveness of recreational divers for monitoring sea turtle
   populations}},
