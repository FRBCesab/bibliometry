@article{ ISI:000350025600003,
Author = {Neville, Chrys M. and Beamish, Richard J. and Chittenden, Cedar M.},
Title = {{Poor Survival of Acoustically-Tagged Juvenile Chinook Salmon in the
   Strait of Georgia, British Columbia, Canada}},
