@article{ ISI:000363091800022,
Author = {Grecea, C. and Brebu, F. M. and Bala, A. C. and Grecea, O. A.},
Title = {{ENVIRONMENT PROTECTING BY PERMANENT MONITORING OF SALT MINES}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2015}},
Volume = {{16}},
Number = {{3}},
Pages = {{988-997}},
Abstract = {{Entire mining activity produces, because of its specificity, multiple
   and various negative environmental effects exemplified by: changes in
   the relief, degradation of land, noise and vibration in the environment,
   etc., all of these being also a risk factor for the build areas and
   population. In Romania, exploitation of the salt deposits produced
   particularly complex phenomena materialised by changes into the surface
   morphology caused especially by landslides appeared in the area. In this
   context, the paper propose, implementation of new measurement
   technologies by using 3D laser scanning in monitoring these phenomena.
   The study case refers to the horizon of +210 m situated in the mining
   basin Ocnele Mari of Ramnicu Valcea County. Based on this study we
   consider that the implementation of laser scanning technology can be a
   permanent control system for real time monitoring of the behaviour of
   any modification that may lead to major damage on the environment with
   respect to multipurpose sustainable development.}},
