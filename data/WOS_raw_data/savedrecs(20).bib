@article{ ISI:000345721600015,
Author = {Kienast, Felix and Frick, Jacqueline and van Strien, Maarten J. and
   Hunziker, Marcel},
Title = {{The Swiss Landscape Monitoring Program - A comprehensive indicator set
   to measure landscape change}},
Journal = {{ECOLOGICAL MODELLING}},
