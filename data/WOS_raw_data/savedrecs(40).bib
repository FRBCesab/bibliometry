@article{ ISI:000254360200017,
Author = {Laikre, Linda and Larsson, Lena C. and Palme, Anna and Charlier, Johan
   and Josefsson, Melanie and Ryman, Nils},
Title = {{Potentials for monitoring gene level biodiversity: using Sweden as an
   example}},
