@article{ ISI:000257498700003,
Author = {Yanosk, Jeff D. and Paciorek, Christopher J. and Schwartz, Joel and
   Laden, Francine and Puett, Robin and Suh, Helen H.},
Title = {{Spatio-temporal modeling of chronic PM10 exposure for the nurses' health
   study}},
