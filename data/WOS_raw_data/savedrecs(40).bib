@article{ ISI:000255819300009,
Author = {Slater, G. F. and Cowie, B. R. and Harper, N. and Droppo, I. G.},
Title = {{Variation in PAH inputs and microbial community in surface sediments of
   Hamilton Harbour: Implications to remediation and monitoring}},
