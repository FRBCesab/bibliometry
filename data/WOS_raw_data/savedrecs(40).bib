@article{ ISI:000258930800026,
Author = {Pauli, Jonathan N. and Hamilton, Matthew B. and Crain, Edward B. and
   Buskirk, Steven W.},
Title = {{A single-sampling hair trap for mesocarnivores}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{2008}},
Volume = {{72}},
Number = {{7}},
Pages = {{1650-1652}},
Month = {{SEP}},
Abstract = {{Although techniques to analyze and quantify, DNA-based data have
   progressed, methods to noninvasively collect samples lag behind. Samples
   are generally collected from devices that permit coincident sampling of
   multiple individuals. Because of cross-contamination, substantive
   genotyping errors can arise. We developed a cost-effective
   (US\$4.60/trap) single-capture hair trap for American martens (Martes
   americana). In the field, traps effectively targeted martens; >75\% of
   all hair samples were identified as marten. Eighty percent of marten
   hair (n = 180) contained sufficient quality for DNA-based analyses. This
   effective and affordable trap can be used by, managers to monitor
