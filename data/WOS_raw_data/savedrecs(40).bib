Author = {Horreo, Jose L. and Machado-Schiaffino, Gonzalo and Griffiths, Andrew
   and Bright, Dylan and Stevens, Jamie and Garcia-Vazquez, Eva},
Title = {{Identification of differential broodstock contribution affecting genetic
   variability in hatchery stocks of Atlantic salmon (Salmo salar)}},
