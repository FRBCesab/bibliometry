@article{ ISI:000352653700019,
Author = {Bertrand, Benedicte and Alburaki, Mohamed and Legout, Helene and Moulin,
   Sibyle and Mougel, Florence and Garnery, Lionel},
Title = {{MtDNA COI-COII marker and drone congregation area: An efficient method
   to establish and monitor honeybee (Apis mellifera L.) conservation
   centres}},
