@article{ ISI:000351849700021,
Author = {Walker, Emily and Rivoirard, Jacques and Gaspar, Philippe and Bez,
   Nicolas},
Title = {{From forager tracks to prey distributions: an application to tuna vessel
   monitoring systems (VMS)}},
Journal = {{ECOLOGICAL APPLICATIONS}},
Year = {{2015}},
Volume = {{25}},
