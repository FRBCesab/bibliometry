Title = {{Anadromous char as an alternate food choice to marine animals: A
   synthesis of Hg concentrations, population features and other
   influencing factors}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2015}},
Volume = {{509}},
