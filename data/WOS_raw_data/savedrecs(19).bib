@article{ ISI:000352637700003,
Author = {Yrjola, Rauno A. and Santaharju, Jarkko L. M.},
Title = {{The impact of road construction on a community of farmland birds}},
Journal = {{ANNALES ZOOLOGICI FENNICI}},
Year = {{2015}},
Volume = {{52}},
Number = {{1-2}},
Pages = {{33-44}},
Month = {{APR}},
Abstract = {{Birds are commonly used as tools in environmental monitoring. Bird
   population changes were studied during construction of the new Vuosaari
   harbour in 2001-2011. The aim of the monitoring programme was to
   determine the impacts of road construction on bird populations in the
   Osterangen agricultural area (Helsinki, Finland). Our results indicate
   that the number of species and territories increased during and after
   construction, with more territories located closer to the road than
   before the road was constructed. Thus, road construction resulted in a
   momentary positive impact on bird populations. New grass and bush areas
   and even large rocky outcrops clearly benefited some species such as the
   threatened northern wheatear. The bird populations declined slightly
   several years later, probably as a result of habitat succession.}},
