@article{ ISI:000352087500064,
Author = {Aristizabal, Luis F. and Jimenez, Mauricio and Bustillo, Alex E. and
   Trujillo, Hector I. and Arthurs, Steven P.},
Title = {{Monitoring coffee berry borer, Hypothenemus hampei (Coleoptera:
   Curculionidae), populations with alcohol-baited funnel traps in coffee
   farms in Colombia}},
Journal = {{FLORIDA ENTOMOLOGIST}},
Year = {{2015}},
Volume = {{98}},
Number = {{1}},
Pages = {{381-383}},
Month = {{MAR}},
Abstract = {{We monitored dispersing coffee berry borer (CBB), Hypothenemus hampei
   (Ferrari) (Coleoptera: Curculionidae: Scolytinae), with
   easily-manufactured, alcohol-baited funnel traps and quantified
   associated berry infestation on 8 coffee farms in 2 regions in Caldas,
   Colombia. Flight activity and damage to berries was highest in the 2-3
   months after the main harvest (Jan through Mar) and was greater in large
   farms, where insecticides were used, compared with small farms where
   efficient and frequent cultural control was implemented. The use of
   traps and attractants will help farmers track CBB populations and
   localize efforts to control this pest.}},
