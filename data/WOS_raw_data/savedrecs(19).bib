@article{ ISI:000352095300008,
Author = {Ferretti, Francesco and Storer, Kate and Coats, Julia and Massei,
   Giovanna},
Title = {{Temporal and Spatial Patterns of Defecation in Wild Boar}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
Year = {{2015}},
Volume = {{39}},
Number = {{1}},
Pages = {{65-69}},
Month = {{MAR}},
Abstract = {{Worldwide, wild boar (Sus scrofa) and feral pigs are involved in
   environmental damage and disease transmission. These impacts are often
   associated with relatively high local densities of pigs, so the
   monitoring of population trends is important. Dung counts can be used to
   estimate population trends, but knowledge of daily defecation rates
   (DDRs) is needed to estimate absolute numbers. To address this issue, we
   calculated the DDRs of 18 captive, adult wild boar in July 2005 and
   November 2007. The mean DDR was 3.8-4.3 dung/boar/day, depending on the
   trial. We discuss the results by comparing the DDR of wild boar to that
   of other ungulates and omnivores, and we consider the implications of
   these results for estimating feral pig and wild boar density through
   dung counts. (C) 2014 The Wildlife Society.}},
