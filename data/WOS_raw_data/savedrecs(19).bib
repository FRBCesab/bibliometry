Title = {{Fluctuations in bush and rock hyrax (Hyracoidea: Procaviidae) abundances
   over a 13-year period in the Matopos, Zimbabwe}},
Journal = {{AFRICAN JOURNAL OF WILDLIFE RESEARCH}},
