@article{ ISI:000351453600002,
Author = {Putthividhya, Aksara and Pipitsombat, Ratchapon},
Title = {{GIS-based and laboratory-scaled leaching assessment of agricultural
   contamination in heavily cultivated area of Thailand}},
Journal = {{ENVIRONMENTAL EARTH SCIENCES}},
