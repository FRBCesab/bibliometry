@article{ ISI:000354619600002,
Author = {Capdevila, P. and Linares, C. and Aspillaga, E. and Navarro, L. and
   Kersting, D. K. and Hereu, B.},
Title = {{Recruitment patterns in the Mediterranean deep-water alga Cystoseira
   zosteroides}},
Journal = {{MARINE BIOLOGY}},
