@article{ ISI:000357268500013,
Author = {Park, Heejin and Lee, Kyoungho and Moon, Chan-Seok and Woo, Kyungsook
   and Kang, Tack-Shin and Chung, Eun-Kyung and Son, Bu-Soon},
Title = {{Simultaneous Exposure to Heavy Metals among Residents in the Industrial
   Complex: Korean National Cohort Study}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH}},
Year = {{2015}},
Volume = {{12}},
Number = {{6}},
Pages = {{5905-5917}},
Month = {{JUN}},
Abstract = {{A survey was conducted to evaluate the multi-exposure level and
   correlation among toxic metal biomarkers (Cd, Pb, and Hg). A total of
   592 individuals who participated in the survey were residents near an
   industrial complex in Gwangyang and Yeosu (exposed group) and of Hadong
   and Namhae (control group) in southern Korea from May 2007 to November
   2010. The Gwangyang and Yeosu area exposed groups had slightly higher
   blood Pb (2.21 and 1.90 mu g/dL), urinary Cd observed values (2.20 and
   1.46 mu g/L), urinary Cd with a urinary creatinine correction (1.43 and
   1.25 mu g/g Cr), and urinary Hg observed values (2.26 and 0.98 mu g/L)
   in women participants than those in the Hadong and Namhae area (control
   group). Blood Pb (3.18 and 2.55 mu g/dL), urinary Hg observed values
   (1.14 and 0.92 mu g/L), and urinary Hg with a urinary creatinine
   correction (1.06 and 0.96 mu g/L) for male participants were also
   slightly higher than those in the Hadong and Namhae area (control
   group). The correlation among urinary Cd, Hg and Pb concentrations in
   the blood was significant. We suggest that the exposed group of
   residents were simultaneously exposed to Pb, Cd, and Hg from
   contaminated ambient air originating from the iron manufacturing
   industrial complex.}},
