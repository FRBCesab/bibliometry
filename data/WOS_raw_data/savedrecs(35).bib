@article{ ISI:000279305900005,
Author = {Li, Zonglai and Yu, Jianwei and Yang, Min and Zhang, Jing and Burch,
   Michael D. and Han, Wei},
Title = {{Cyanobacterial population and harmful metabolites dynamics during a
   bloom in Yanghe Reservoir, North China}},
Journal = {{HARMFUL ALGAE}},
