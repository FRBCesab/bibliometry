@inproceedings{ ISI:000306447900023,
Author = {Aschan-Leygonie, C. and Baudet-Michel, S.},
Editor = {{Brebbia, CA and Longhurst, JWS}},
Title = {{Building comparable synthetic health-related indicators of air quality
   in cities}},
Booktitle = {{AIR POLLUTION XVIII}},
