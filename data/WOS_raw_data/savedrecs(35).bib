@article{ ISI:000280131200003,
Author = {Altesor, Paula and Horas, Vanusa R. and Arcia, Maria P. and Rossini,
   Carmen and Zarbin, Paulo H. G. and Gonzalez, Andres},
Title = {{Reproductive Behaviour of Crocidosema (=Epinotia) aporema (Walsingham)
   (Lepidoptera: Tortricidae): Temporal Pattern of Female Calling and
   Mating}},
Journal = {{NEOTROPICAL ENTOMOLOGY}},
Year = {{2010}},
Volume = {{39}},
Number = {{3}},
Pages = {{324-329}},
Month = {{MAY-JUN}},
Abstract = {{Crocidosema aporema (Walsingham) is a major pest of legumes in the
   southern cone of Latin America. The mating behaviour of two allopatric
   populations (Uruguay and Brazil) of C. aporema kept in captivity was
   studied by observing the posture of calling females, the temporal
   pattern of pheromone emission and mating, and the response of males to
   calling females in olfactometer tests. Female calling and mating was
   observed during the scotophase, from the first to the seventh night
   after adult emergence. Male response was evaluated at night using a
   single calling female in a Y-shaped olfactometer. Females adopted a
   characteristic calling posture, extruding the pheromone gland from the
   tip of the abdomen. Most females started calling during the second
   scotophase, and all females called from the third, between the fifth and
   seventh hours after the onset of the scotophase. Most of the couples
   mated once throughout the experiment, between the third and sixth night
   and during the middle of the dark phase. Males preferentially chose the
   female arm in olfactometer tests, considering both the first arm chosen
   and the number of visits during the observation period. Our results
   describe for the first time the temporal pattern associated to the
   reproductive behaviour of C. aporema. We also provide evidence that this
   tortricid is monoandrous, and that pheromones are used in intersexual
   communication for mate finding. Our data will be used to optimize the
   collection of female sex pheromones for chemical characterization in
   order to develop a monitoring tool for this pest.}},
