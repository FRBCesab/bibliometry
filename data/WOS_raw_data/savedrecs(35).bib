@article{ ISI:000277490100009,
Author = {Marins, Flavia O. and Novaes, Roberto L. M. and Rocha, Rosana M. and
   Junqueira, Andrea O. R.},
Title = {{Non indigenous ascidians in port and natural environments in a tropical
   Brazilian bay}},
