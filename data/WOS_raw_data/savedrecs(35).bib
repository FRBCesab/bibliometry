@article{ ISI:000275455700003,
Author = {Luikart, Gordon and Ryman, Nils and Tallmon, David A. and Schwartz,
   Michael K. and Allendorf, Fred W.},
Title = {{Estimation of census and effective population sizes: the increasing
   usefulness of DNA-based approaches}},
