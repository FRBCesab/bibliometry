@article{ ISI:000278602300016,
Author = {Sas, Istvan and Antal, Cornel and Covaciu-Marcov, Severus D.},
Title = {{Tropics patch in the Holarctic: A new case of wintertime breeding of a
   Pelophylax ridibundus population in North-Western Romania}},
Journal = {{NORTH-WESTERN JOURNAL OF ZOOLOGY}},
Year = {{2010}},
Volume = {{6}},
Number = {{1}},
Pages = {{128-133}},
Month = {{JUN}},
Abstract = {{Throughout the 2006-2008 periods we conducted a monitoring study of the
   known thermal habitats with non-hibernating populations of P. ridibundus
   from north-western Romania. For the first time, on the 4(th) of January
   2007, in the thermal water habitat at Calan Spa (Hunedoara County), we
   noticed the presence of small-sized tadpoles of Peophylax ridibundus.
   Hundreds of tadpoles of P. ridibundus were identified both in the winter
   of 2006/2007 and 2007/2008. In both cold seasons of the study, beside
   the tadpoles, were observed calling males and pairs in amplexus, which
   demonstrates unequivocally the wintertime breeding of this population.}},
