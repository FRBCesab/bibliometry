@article{ ISI:000273979000008,
Author = {Bouskill, Nicholas J. and Barker-Finkel, Jill and Galloway, Tamara S.
   and Handy, Richard D. and Ford, Timothy E.},
Title = {{Temporal bacterial diversity associated with metal-contaminated river
   sediments}},
