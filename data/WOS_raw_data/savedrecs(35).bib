@article{ ISI:000277233300011,
Author = {bin Othman, Ahmad Syukri and Goh, Gideon H. S. and Todd, Peter A.},
Title = {{THE DISTRIBUTION AND STATUS OF GIANT CLAMS (FAMILY TRIDACNIDAE) - A
   SHORT REVIEW}},
Journal = {{RAFFLES BULLETIN OF ZOOLOGY}},
