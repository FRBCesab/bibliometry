@article{ ISI:000275782400011,
Author = {Brittain, John E. and Gjerseth, Jan Egil},
Title = {{Long-term trends and variation in Cs-137 activity concentrations in
   brown trout (Salmo trutta) from Ovre Heimdalsvatn, a Norwegian subalpine
   lake}},
Journal = {{HYDROBIOLOGIA}},
Year = {{2010}},
Volume = {{642}},
Number = {{1}},
Pages = {{107-113}},
Month = {{MAR}},
Abstract = {{The Norwegian mountain lake, vre Heimdalsvatn, received major fallout
   from the Chernobyl accident in the spring of 1986. Activity
   concentrations of Cs-137 have been monitored annually in the brown trout
   (Salmo trutta) population of the lake over a 22-year period since 1986.
   The activity concentrations of Cs-137 in brown trout have declined over
   the period, although in recent years there has been little or no
   reduction in activity concentrations. Throughout the period, there has
   been considerable individual variation in Cs-137 activity
   concentrations. Within a single year, fish weight was the most
   significant factor affecting activity concentrations of Cs-137 in
   individual fish, although sex and age center dot weight were significant
   explanatory variables. The ``size effect{''} was not significant during
   the initial period after fallout and in some recent years. During the
   first years after fallout, the ecological half-life of Cs-137 in brown
   trout was 3-4 years, but during the last decade this has increased
   significantly and has approached the physical half-life of 30 years for
   Cs-137, suggesting a dynamic equilibrium between catchments inputs,
   possible remobilisation from lake sediments and lake outputs of Cs-137.}},
