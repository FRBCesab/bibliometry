@article{ ISI:000244822500013,
Author = {Rowat, David},
Title = {{Occurrence of whale shark (Rhincodon typus) in the Indian Ocean: A case
   for regional conservation}},
Journal = {{FISHERIES RESEARCH}},
Year = {{2007}},
Volume = {{84}},
Number = {{1, SI}},
Pages = {{96-101}},
Month = {{MAR}},
Note = {{1st International Whale Shark Conference, Perth, AUSTRALIA, MAY 09-12,
   2005}},
Abstract = {{From the first whale shark (Rhincodon typus) described in 1828 from the
   Indian Ocean, the region continues to be one of the most important areas
   for whale shark sightings. However, the species has been the subject of
   several targeted fisheries and thus sustained massive, rapid declines in
   population numbers.
   The known range of occurrence and targeted fisheries for whale sharks in
   the Indian Ocean are discussed, along with stated national conservation
   measures in the range states. The results of a preliminary survey of 16
   regional cooperative partners from 11 of the Indian Ocean range states
   are presented for whale shark occurrence, monitoring, perceived threats
   and realized conservation measures. These data are already proving
   valuable by facilitating cooperation between organisations regionally.
   The current international conservation framework is briefly described
   and suggestions made as to possible linked regional conservation
   initiatives, such as under the auspices of the Convention on Migratory
   Species. (c) 2006 Elsevier B.V. All rights reserved.}},
