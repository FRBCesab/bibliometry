@article{ ISI:000246917100008,
Author = {Calixto, Alejandro A. and Harris, Marvin K. and Barr, Charles},
Title = {{Resurgence and persistence of Dorymyrmex flavus after reduction of
   Solenopsis invicta buren with a broadcast bait}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{2007}},
Volume = {{36}},
Number = {{3}},
Pages = {{549-554}},
Month = {{JUN}},
Abstract = {{The effects of bait treatment(s) on population dynamics of Solenopsis
   invicta and Dorymyrmex flavus were studied, and various factors
   underlying the resurgence and persistence of D. flavus to reinvasion by
   S. invicta were studied in more detail. Pitfall traps, bait vials,
   transect sampling, and direct inspections were used to monitor densities
   of these two species, and inspections of D. flavus midden contents,
   video monitoring of D. flavus colonies, and studies of the fate of
   marked S. invicta were used to further clarify interactions of these two
   species, D. flavus abundance increased after the reduction of S. invicta
   with baits. D. flavus was also observed to sustain higher densities for
   an extended period (2 yr) after cessation of bait treatment and to
   exhibit antagonistic behaviors toward S. invicta, showing an ability to
   resist reinvasion of the treated area by S. invicta. Given these
   findings, D. flavus may retard domination of the ant assemblage by S.
   invicta. Additional studies are justified regarding how to enhance the
   role of this species in affected ecosystems.}},
