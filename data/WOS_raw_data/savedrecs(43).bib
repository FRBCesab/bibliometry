@inproceedings{ ISI:000249090200023,
Author = {Wziaqtek, Bogdan and Martyniak, Andrzej and Hliwa, Piotr and Kozlowski,
   Jacek and Krzywosz, Tadeusz and Osewski, Michal and Sobocki, Marek and
   Szymanska, Urszula and Gomutka, Piotr},
Editor = {{Jankun, M and Brzuzan, P and Hliwa, P and Luczynski, M}},
Title = {{Great Cormorant predation on coregonid fishes at seven sites in Poland}},
Booktitle = {{BIOLOGY AND MANAGEMENT OF COREGONID FISHES - 2005}},
Series = {{Advances in Limnology}},
Year = {{2007}},
Volume = {{60}},
Pages = {{285+}},
Note = {{9th International Symposium on Biology and Management of Coregonid
   Fishes, Olsztyn, POLAND, JUL 21-AUG 27, 2005}},
Abstract = {{The population of Great Cormorant (Phalacrocorax carbo (L.)) has rapidly
   increased in Poland over the last 20 years. We evaluated the diet of
   this species at seven roost sites and breeding colonies. Food
   composition was analysed based on pellets and fish regurgitated by
   birds. Coregonid fishes were recorded in cormorant diets in several
   colonies; we found whitefish (Coregonus lavaretus) in two colonies and
   vendace (C. albula) in four colonies. The highest share of coregonids
   was found in the diet of birds from Wigierski National Park (roost
   site), where vendace constituted 8.2\% of the total mass of consumed
   fish, representing on average 4.0 tons per year. The mean total
   consumption rate of the colony was 62.5 tons of fish per year. In the
   remaining colonies and roost sites, whitefish and vendace contributed
   less than 1 \%. The impact of cormorants on vendace in Lake Wigry was
   irregular in time. The highest weight share was recorded in October
   (16.5\%) and May (7.6\%). In other months, its share did not exceed 5\%.
   The length of consumed vendace individuals was highest in May and June
   (mean body length, 14.8 cm) and October (mean body length, 14.5-14.8
   cm). We conclude that the Great Cormorant does not currently pose a
   significant threat to the whitefish stock in Poland, but we suggest that
   its diet should be monitored regularly to detect any changes of
