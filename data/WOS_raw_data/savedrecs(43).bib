@article{ ISI:000261389700010,
Author = {Crowder, A. and Harmsen, R. and Blatt, S. E.},
Title = {{Notes on Succession in Old Fields in Southeastern Ontario: the Herbs}},
Journal = {{CANADIAN FIELD-NATURALIST}},
Year = {{2007}},
Volume = {{121}},
Number = {{2}},
Pages = {{182-190}},
Month = {{APR-JUN}},
Abstract = {{Vegetation in abandoned hayfields was monitored during 1976-1998. An
   earlier successional stage followed ploughing. Changes in tree, shrub
   and vine Populations have been reported earlier and showed expected
   increases in species richness and cover. Highest species richness of
   herbs Occurred three years after Ploughing. Non-woody species richness
   trended irregularly downward. while non-woody cover was variable,
   peaking in 1987. Within the herbaceous community. year-to-year changes
   in cover and frequency of species in the following selected groups are
   reported here: 18 grasses including sown and adventive spccies 13
   legumes including two sown species: 14 macroforbs of the Compositae.
   including a goldenrod, Solidago canadensis, which dominated parts of the
   fields a rosette weed. Taraxacum officinalis; sedges, horsetails and
   some other minor components. Grasses and goldenrods were grazed,
   sometimes intensively and repeatedly, by insects; grasses were impacted
   by skipper larvae (Thymelicus lineola), and goldenrods by beetle larvae
   (Trirhabda spp.). Effects of repeated outbreaks on host plant cover are
