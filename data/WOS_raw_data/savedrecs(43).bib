@article{ ISI:000252083500019,
Author = {Withler, Ruth E. and Rundle, Tim and Beacham, Terry D.},
Title = {{Genetic identification of wild and domesticated strains of chinook
   salmon (Oncorhynchus tshawytscha) in southern British Columbia, Canada}},
