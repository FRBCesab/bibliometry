@article{ ISI:000246843200008,
Author = {Cleaveland, Sarah and Mlengeya, Titus and Kaare, Magai and Haydon, Dan
   and Lembo, Tiziana and Laurenson, M. Karen and Packer, Craig},
Title = {{The conservation relevance of epidemiological research into carnivore
   viral diseases in the Serengeti}},
Journal = {{CONSERVATION BIOLOGY}},
Year = {{2007}},
Volume = {{21}},
Number = {{3}},
Pages = {{612-622}},
Month = {{JUN}},
Note = {{Symposium on the Contribution of Long-Term Studies to Conservation,
   TANZANIA, DEC, 2003}},
Organization = {{Tanzania Wildlife Res Inst}},
Abstract = {{Recent outbreaks of rabies and canine distemper in wildlife populations
   of the Serengeti show that infectious disease constitutes a. significant
   cause of mortality that can result in regional extirpation of endangered
   species even within large, well-protected areas. Nevertheless, effective
   management of an infectious disease depends critically on understanding
   the epidemiological dynamics of the causative pathogen. Pathogens with
   short infection cycles cannot persist in small populations in the
   absence of a more permanent reservoir of infection. Development of
   appropriate interventions requires detailed data on transmission
   pathways between reservoirs and wildlife populations of conservation
   concern. Relevant data can be derived from long-term population
   monitoring, epidemic and case-surveillance patterns, genetic analyses of
   rapidly evolving pathogens, serological surveys, and intervention
   studies. We examined studies of carnivore diseases in the Serengeti.
   Epidemiological research contributes to wildlife conservation policy in
   terms of management of endangered populations and the integration of
   wildlife conservation with public health interventions. Longterm,
   integrative, cross-species research is essential for formulation of
   effective policy for disease control and optimization of ecosystem
   health.}},
