@incollection{ ISI:000311322500025,
Author = {Byun, Daewon W. and Jang, Meong-Do and Song, Chang-Keun and Kim, Soontae
   and Cheng, Fang-Yi and Perna, Ryan and Kim, Hyun-CHeol},
Editor = {{Borrego, C and Renner, E}},
Title = {{Operational evaluation of the Eastern Texas air quality (ETAQ)
   forecasting system based on MM5/SMOKE/CMAQ}},
Booktitle = {{AIR POLLUTION MODELING AND ITS APPLICATION XVIII}},
Series = {{Developments in Environmental Science}},
Year = {{2007}},
Volume = {{6}},
Pages = {{253-261}},
Abstract = {{An air quality forecasting system for Eastern Texas has been developed
   utilizing the customized MM5/SMOKE/CMAQ modeling system. We performed
   2-day air quality forecasting simulations for the 12km Eastern Texas
   regional domain, and the 4 km Houston-Galveston area (HGA) domain.
   Dynamic boundary conditions were provided by the 36km resolution
   conterminous US (CONUS) domain CMAQ simulations. Initial meteorological
   conditions were provided by the daily Eta forecast results. A set of
   complex operational scripts was used to allow automatic operation of the
   data download, sequencing processors, performing simulations and
   graphical analyses, building database archives, and presenting on the
   web. The public access web-based user interface has been developed to
   link the air quality forecasting results with other GIS databases
   including population and health effects databases. Three different
   streams of air quality forecasting with slightly different
   meteorological model options were performed during the summer and fall
   2005. The ozone forecasting results were evaluated daily with
   meteorological and air quality measurements from the surface continuous
   air monitoring sites in HGA.}},
