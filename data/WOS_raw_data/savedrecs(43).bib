Language = {{English}},
Affiliation = {{Heywood, JS (Reprint Author), Missouri State Univ, Dept Biol, 901 S Natl Ave, Springfield, MO 65897 USA.
   Missouri State Univ, Dept Biol, Springfield, MO 65897 USA.
   SW Missouri State Univ, Heartland Network Inventory \& Monitoring Program, Natl Pk Serv, Republic, MO 65802 USA.}},
DOI = {{10.2111/1551-5028(2007)60{[}426:OSDFMP]2.0.CO;2}},
