@article{ ISI:000244645600016,
Author = {Manefield, Mike and Whiteley, Andrew and Curtis, Tom and Watanabe,
   Kazuya},
Title = {{Influence of sustainability and immigration in assembling bacterial
   populations of known size and function}},
