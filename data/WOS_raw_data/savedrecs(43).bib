@article{ ISI:000245236200003,
Author = {Jorgensen, Terje and Lokkeborg, Svein and Ferno, Anders and Hufthammer,
   Marianne},
Title = {{Walking speed and area utilization of red king crab (Paralithodes
   camtschaticus) introduced to the Barents Sea coastal ecosystem}},
Journal = {{HYDROBIOLOGIA}},
Year = {{2007}},
Volume = {{582}},
Pages = {{17-24}},
Month = {{MAY}},
