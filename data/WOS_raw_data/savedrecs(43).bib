@article{ ISI:000246204300003,
Author = {Al-Zyoud, F. and Sengonca, C. and Al-Abbadi, S.},
Title = {{Evaluation of Serangium parcesetosum (Col.: Coccinellidae) for
   biological control of Bemisia tabaci under greenhouse conditions}},
Journal = {{JOURNAL OF PEST SCIENCE}},
