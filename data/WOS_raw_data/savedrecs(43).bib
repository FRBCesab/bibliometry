@article{ ISI:000248611000004,
Author = {Bogojevic, Mirta Sudaric and Hengl, Tomislav and Merdic, Enrih},
Title = {{Spatiotemporal monitoring of floodwater mosquito dispersal in Osijek,
   Croatia}},
