@article{ ISI:000374768300048,
Author = {Le Vaillant, Maryline and Ropert-Coudert, Yan and Le Maho, Yvon and Le
   Bohec, Celine},
Title = {{Individual parameters shape foraging activity in breeding king penguins}},
Journal = {{BEHAVIORAL ECOLOGY}},
