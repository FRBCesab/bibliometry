@article{ ISI:000368088300010,
Author = {Sanz-Aguilar, Ana and Manuel Igual, Jose and Oro, Daniel and Genovart,
   Meritxell and Tavecchia, Giacomo},
Title = {{Estimating recruitment and survival in partially monitored populations}},
