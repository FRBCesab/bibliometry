@inproceedings{ ISI:000370000900126,
Author = {Sidorovskaia, Natalia A. and Ackleh, Azmy S. and Tiemann, Christopher O.
   and Ma, Baoling and Ioup, Juliette W. and Ioup, George E.},
Editor = {{Popper, AN and Hawkins, A}},
Title = {{Passive Acoustic Monitoring of the Environmental Impact of Oil
   Exploration on Marine Mammals in the Gulf of Mexico}},
Booktitle = {{EFFECTS OF NOISE ON AQUATIC LIFE II}},
Series = {{Advances in Experimental Medicine and Biology}},
Year = {{2016}},
Volume = {{875}},
Pages = {{1007-1014}},
Note = {{3rd International Conference on the Effects of Noise on Aquatic Life,
   Budapest, HUNGARY, AUG, 2013}},
Organization = {{Fisheries Joint Management Comm; Natl Oceanog \& Atmospher Adm; Natl Sci
   Fdn; NAVFAC; Off Naval Res; Off Naval Res Global; Acoust Soc Amer; Aquat
   Noise Trust; Discovery Sound Sea; Co Biologists; Univ Maryland, Coll
   Chem \& Life Sci; Ctr Comparat \& Evolutionary Biol Hearing}},
Abstract = {{The Gulf of Mexico is a region densely populated by marine mammals that
   must adapt to living in a highly active industrial environment. This
   paper presents a new approach to quantifying the anthropogenic impact on
   the marine mammal population. The results for sperm and beaked whales of
