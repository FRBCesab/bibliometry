@article{ ISI:000382558100002,
Author = {Labuschagne, Christiaan and Nupen, Lisa and Kotze, Antoinette and
   Grobler, J. Paul and Dalton, Desire L.},
Title = {{Genetic monitoring of ex situ African Penguin (Spheniscus demersus)
   populations in South Africa}},
