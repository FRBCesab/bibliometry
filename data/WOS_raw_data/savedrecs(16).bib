Author = {Yang, Qi and Jing, Xiao-li and Luo, Kun and Yi, Jun and Zhong, Qiong and
   Li, Xiao-ming and Liu, Yang and Zeng, Guang-ming},
Title = {{HYDROLASE ACTIVITY DURING MICROAEROBIC THERMOPHILIC DIGESTION OF SLUDGE
   AFFECTED BY SODIUM DODECYLBENZENE SULFONATE (SDBS)}},
Journal = {{ENVIRONMENTAL ENGINEERING AND MANAGEMENT JOURNAL}},
