@incollection{ ISI:000399650300009,
Author = {Kerem, D. and Goffman, O. and Elasar, M. and Hadar, N. and Scheinin, A.
   and Lewis, T.},
Editor = {{DiSciara, GN and Podesta, M and Curry, BE}},
Title = {{The Rough-Toothed Dolphin, Steno bredanensis, in the Eastern
   Mediterranean Sea: A Relict Population?}},
