@article{ ISI:000371140700009,
Author = {Bernotas, P. and Vetemaa, M. and Saks, L. and Eschbaum, R. and Verliin,
   A. and Jaervalt, A.},
Title = {{Dynamics of European eel landings and stocks in the coastal waters of
   Estonia}},
