@article{ ISI:000370437500011,
Author = {Zupan, Ivan and Karaga, Antonio and Saric, Tomislav and Kanski, Danijel},
Title = {{Blue crab Callinectes sapidus Rathbun, 1896 continues invasion: first
   case of entering into freshwater ecosystem in the Mediterranean (Nature
   Park Vransko Lake, Adriatic Sea)}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2016}},
Volume = {{57}},
Number = {{1}},
Pages = {{81-84}},
Abstract = {{Three specimens of the adult blue crab (Callinectes sapidus) were caught
   inside the Nature Park Vransko Lake in May 2013, during a regular
   monitoring of ichtyofauna. All individuals were males and can be
   considered large. After the initial discovery, trap nets were placed on
   several locations within the lake, but no blue crabs were caught
   afterwards up to this date. Regardless, due to the invasive nature of
   this species and potential negative impact on indigenous species,
   further monitoring of the blue crab population in the Mediterranean and
   especially within the protected areas should gain more interest and
   support.}},
