@article{ ISI:000454999600010,
Author = {Canovas, Fernando and Gonzalez-Wanguemert, Mercedes},
Title = {{Determination of predation by the snakelock sea anemone Anemonia sulcata
   on the scyphozoan Pelagia noctiluca in the Mediterranean Sea}},
Journal = {{CAHIERS DE BIOLOGIE MARINE}},
Year = {{2018}},
Volume = {{59}},
Number = {{5}},
Pages = {{489-491}},
Abstract = {{Cnidarians comprise a large diversity of marine organisms. Some
   cnidarians are considered key members of ocean ecosystems that can also
   have socio-economic impacts in coastal communities across the world.
   This manuscript reports the predation between two species of cnidarians,
   the jellyfish Pelagia noctiluca (Medusozoa: Scyphozoa) and the anemone
   Anemonia sulcata (Anthozoa: Anemonia). Predation was observed in Cuatro
   Calas (37.37476 degrees N-1.63088 degrees W), during a summer sampling
   campaign (2015) from a four year monitoring project along the coast of
   Murcia (south-eastern coast of Iberian Peninsula, Spain). Predation
   between P. noctiluca and A. sulcata was observed in shallow waters (60
   cm depth), where high densities of both species were found. Anemones
   showed a firm grip on the jellyfish by using their siphonoglyph and did
   not lose their grip to jellyfish movements during approximately four
   hours of monitoring. To our knowledge, this is the first report of
   predator-prey interactions between a benthic and a pelagic cnidarian
   species in the Mediterranean Sea. Further studies should be carried out
   to a better understand A. sulcata feeding behaviour and its implications
   on jellyfish population dynamics.}},
