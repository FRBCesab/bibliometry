@article{ ISI:000423005200020,
Author = {Yang, Zhisong and Gu, Xiaodong and Nie, Yonggang and Huang, Feng and
   Huang, Yan and Dai, Qiang and Hu, Yibo and Yang, Yi and Zhou, Xiao and
   Zhang, Hemin and Yang, Xuyu and Wei, Fuwen},
Title = {{Reintroduction of the giant panda into the wild: A good start suggests a
   bright future}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{2018}},
Volume = {{217}},
Pages = {{181-186}},
Month = {{JAN}},
Abstract = {{Reintroduction is an important approach in the conservation of
   endangered species and in recent decades a number of reintroduction
   programs have been conducted for conservation purposes. Generally, the
   success rate of reintroduction is low, but long-term monitoring of most
   reintroduction programs remains uncommon and this may influence the
   evaluation of reintroduction. The giant panda is a flagship species of
   biodiversity conservation. The Chinese government runs a giant panda
   reintroduction program and here we present reintroduction monitoring
   data from 2009 to 2015 for three giant pandas released into a small
   isolated wild population in western Sichuan, China. The results indicate
   that all three giant pandas display similar activity patterns to wild
   giant pandas, however, the wild-caught female demonstrated better
   adaptive ability to the wild environment than the two captive-born
   individuals in that she established a stable home range in the quickest
   time and gave birth to a cub. Genetic analysis indicates that new
   genetic material has been introduced into the local population. Our
   study provides the latest update on the giant panda introduction program
   and indicates that it is progressing well.}},
