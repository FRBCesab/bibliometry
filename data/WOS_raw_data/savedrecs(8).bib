@article{ ISI:000417548600003,
Author = {Li, Zhiyuan and Che, Wenwei and Frey, H. Christopher and Lau, Alexis K.
   H.},
Title = {{Factors affecting variability in PM2.5 exposure concentrations in a
   metro system}},
