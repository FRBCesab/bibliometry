@article{ ISI:000456434200008,
Author = {Trigos, S. and Vicente, N.},
Title = {{POPULATION STATUS OF PINNA NOBILIS IN FOUR PROTECTED AREAS OF FRANCE AND
   MONACO}},
Journal = {{VIE ET MILIEU-LIFE AND ENVIRONMENT}},
Year = {{2018}},
Volume = {{68}},
Number = {{2-3}},
Pages = {{145-149}},
Abstract = {{Recent mortality events of Pinna nobilis registered in the western
   Mediterranean coasts highlight the need to monitor regularly populations
   of this endangered endemic bivalve. The present study has investigated a
   total surface of 6,400 m(2) of 4 protected areas from France and Monaco
   registering density of individuals and biometric data of each
   population. Herein, densities of 3.56 individuals.100 BO in the Embiez
   archipelago, 6.81 individuals.100 m(-2) in the Natural Reserve of
   Scandola, 1.37 individuals.100 m(-2) in the National Park of Port-Gros
   and 5.25 individuals.100 m(-2) in the Underwater Reserve of Larvotto.
   These values, with great number of adults and juvenile individuals,
   support the healthy and consolidated status of these populations.}},
