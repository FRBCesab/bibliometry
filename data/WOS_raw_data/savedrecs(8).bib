@article{ ISI:000414872800017,
Author = {Ingelido, Anna Maria and Abballe, Annalisa and Gemma, Simonetta and
   Dellatte, Elena and Iacovella, Nicola and De Angelis, Giovanna and
   Zampaglioni, Franco and Marra, Valentina and Miniero, Roberto and
   Valentini, Silvia and Russo, Francesca and Vazzoler, Marina and Testai,
