@article{ ISI:000422913000027,
Author = {Cai, Fei and Feng, Zhengjun and Zhu, Lizhong},
Title = {{Effects of biochar on CH4 emission with straw application on paddy soil}},
Journal = {{JOURNAL OF SOILS AND SEDIMENTS}},
