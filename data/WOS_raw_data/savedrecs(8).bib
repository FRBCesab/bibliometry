@incollection{ ISI:000437095400003,
Author = {Monteiro-Filho, Emygdio L. A. and Deconto, Lucimary S. and Louzada, Caio
   N. and Wanderley, Rebeca P. and Godoy, Daniela F. and Medeiros, Eric},
Editor = {{RossiSantos, MR and Finkl, CW}},
Title = {{Long-Term Monitoring of Dolphins in a Large Estuarine System of
   Southeastern Brazil}},
