@article{ ISI:000419675300022,
Author = {Reynoso, Mauricio A. and Pauluzzi, Germain C. and Kajala, Kaisa and
   Cabanlit, Sean and Velasco, Joel and Bazin, Jeremie and Deal, Roger and
   Sinha, Neelima R. and Brady, Siobhan M. and Bailey-Serres, Julia},
Title = {{Nuclear Transcriptomes at High Resolution Using Retooled INTACT}},
Journal = {{PLANT PHYSIOLOGY}},
Year = {{2018}},
Volume = {{176}},
Number = {{1}},
Pages = {{270-281}},
Month = {{JAN}},
Abstract = {{Isolated nuclei provide access to early steps in gene regulation
   involving chromatin as well as transcript production and processing.
   Here, we describe transfer of the isolation of nuclei from tagged
   specific cell types (INTACT) to the monocot rice (Oryza sativa L.). The
   purification of biotinylated nuclei was redesigned by replacing the
   outer nuclear-envelope-targeting domain of the nuclear tagging fusion
   (NTF) protein with an outer nuclear-envelope-anchored domain. This
   modified NTF was combined with codon-optimized Escherichia coli BirA in
   a single T-DNA construct. We also developed inexpensive methods for
   INTACT, T-DNA insertion mapping, and profiling of the complete nuclear
   transcriptome, including a ribosomal RNA degradation procedure that
   minimizes pre-ribosomal RNA (pre-rRNA) transcripts. A high-resolution
   comparison of nuclear and steady-state poly(A)+ transcript populations
   of seedling root tips confirmed the capture of pre-messenger RNA
   (pre-mRNA) and exposed distinctions in diversity and abundance of the
   nuclear and total transcriptomes. This retooled INTACT can enable
   high-resolution monitoring of the nuclear transcriptome and chromatin in
   specific cell types of rice and other species.}},
