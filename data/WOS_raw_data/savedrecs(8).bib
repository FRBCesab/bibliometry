@inproceedings{ ISI:000452662200001,
Author = {Sarelli, Anastasia and Sykas, Dimitris and Miltiadou, Milto and
   Bliziotis, Dimitris and Spastra, Yiota and Ieronymaki, Maria},
Editor = {{Themistocleous, K and Papadavid, G and Michaelides, S and Ambrosia, V and Hadjimitsis, DG}},
Title = {{A NOVEL AUTOMATED METHODOLOGY THAT ESTIMATES THE UNITED NATIONS (UN)
   SUSTAINABLE DEVELOPMENT GOAL (SDG) 14.1.1.: INDEX OF COASTAL
   EUTROPHICATION USING THE COPERNICUS MARINE ENVIRONMENT MONITORING
   SERVICE (CMEMS)}},
Booktitle = {{SIXTH INTERNATIONAL CONFERENCE ON REMOTE SENSING AND GEOINFORMATION OF
   THE ENVIRONMENT (RSCY2018)}},
Series = {{Proceedings of SPIE}},
