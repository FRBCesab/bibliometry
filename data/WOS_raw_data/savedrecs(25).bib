@article{ ISI:000320328600003,
Author = {Krueger, Eduardo and Drach, Patricia and Emmanuel, Rohinton and
   Corbella, Oscar},
Title = {{Assessment of daytime outdoor comfort levels in and outside the urban
   area of Glasgow, UK}},
Journal = {{INTERNATIONAL JOURNAL OF BIOMETEOROLOGY}},
