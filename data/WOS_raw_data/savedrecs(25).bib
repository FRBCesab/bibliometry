@article{ ISI:000323043000038,
Author = {Albernaz, K. C. and Merlin, B. L. and Martinelli, S. and Head, G. P. and
   Omoto, C.},
Title = {{Baseline Susceptibility to Cry1Ac Insecticidal Protein in Heliothis
   virescens (Lepidoptera: Noctuidae) Populations in Brazil}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
