@article{ ISI:000319420500023,
Author = {Vuarin, Pauline and Dammhahn, Melanie and Henry, Pierre-Yves},
Title = {{Individual flexibility in energy saving: body size and condition
   constrain torpor use}},
Journal = {{FUNCTIONAL ECOLOGY}},
Year = {{2013}},
Volume = {{27}},
