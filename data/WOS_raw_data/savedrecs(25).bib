@article{ ISI:000323758200015,
Author = {Fraser, Dylan J.},
Title = {{The emerging synthesis of evolution with ecology in fisheries science}},
Journal = {{CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES}},
Year = {{2013}},
Volume = {{70}},
