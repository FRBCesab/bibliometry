@article{ ISI:000318625700030,
Author = {Bellini, Claudio and Santos, Armando J. B. and Grossman, Alice and
   Marcovaldi, Maria A. and Barata, Paulo C. R.},
Title = {{Green turtle (Chelonia mydas) nesting on Atol das Rocas, north-eastern
   Brazil, 1990-2008}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
