@article{ ISI:000325072000011,
Author = {van Luijk, Judith and Cuijpers, Yvonne and van der Vaart, Lilian and de
   Roo, Tineke Coenen and Leenaars, Marlies and Ritskes-Hoitinga, Merel},
Title = {{Assessing the application of the 3Rs: a survey among animal welfare
   officers in The Netherlands}},
