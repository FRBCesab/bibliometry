@article{ ISI:000342875400005,
Author = {Parks, Amanda M. and Jenkins, Michael A. and Woeste, Keith E. and Ostry,
   Michael E.},
Title = {{Conservation Status of a Threatened Tree Species: Establishing a
   Baseline for Restoration of Juglans cinerea L. in the Southern
   Appalachian Mountains, USA}},
Journal = {{NATURAL AREAS JOURNAL}},
