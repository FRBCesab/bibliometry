@article{ ISI:000320843100002,
Author = {Abidli, Sami and Lahbib, Youssef and Rodriguez Gonzalez, Pablo and
   Garcia Alonso, Jose Ignacio and El Menif, Najoua Trigui},
Title = {{Imposex and butyltin burden in Bolinus brandaris (Mollusca, Gastropoda)
   and sediment from the Tunisian coast}},
Journal = {{HYDROBIOLOGIA}},
Year = {{2013}},
Volume = {{714}},
Number = {{1}},
Pages = {{13-24}},
Month = {{SEP}},
Abstract = {{The present study aimed at analyzing the imposex incidence and the
   presence of butyltins namely tributyltin (TBT) with its di- and
   mono-substituted metabolites in Bolinus brandaris whole tissues and in
   surface sediments at seven sites from the Tunisian coast during one
   campaign in May 2010. Butyltin levels were evaluated using isotope
   dilution GC-MS. Except the population collected from Zarat site, imposex
   was found in snails from the remained six sites with a maximal incidence
   and sterility (closure of the vaginal opening) registered in Carrier
   bay. Both imposex indices VDSI and RPLI showed a positive correlation
   with tissue concentrations of TBT. Total butyltin concentrations in
   sediments were higher in sites located in the vicinity of shipping areas
   with levels of TBT high enough to cause environmental concern if there
   is no legislative restriction and enforcement for the sale and use of
   these chemicals in Tunisia. These results further confirmed that B.
   brandaris is a good bioindicator of butyltin pollution in the studied
   areas. In addition, this study provided recent and new data on sediment
   butyltin concentrations that could serve for long-term monitoring of TBT
   pollution in Tunisia and the Mediterranean Sea.}},
