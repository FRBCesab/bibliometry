@article{ ISI:000329431400003,
Author = {Cavalcanti, Fernanda F. and Skinner, Luis Felipe and Klautau, Michelle},
Title = {{Population dynamics of cryptogenic calcarean sponges (Porifera,
   Calcarea) in Southeastern Brazil}},
Journal = {{MARINE ECOLOGY-AN EVOLUTIONARY PERSPECTIVE}},
