@article{ ISI:000209336000021,
Author = {Snow, Nathan P. and Andelt, William F.},
Title = {{Capture Success Higher Near Roads for San Clemente Island Foxes}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
