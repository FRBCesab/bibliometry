@article{ ISI:000323145400003,
Author = {Van Doornik, Donald M. and Eddy, Debra L. and Waples, Robin S. and Boe,
   Stephen J. and Hoffnagle, Timothy L. and Berntson, Ewann A. and Moran,
   Paul},
Title = {{Genetic Monitoring of Threatened Chinook Salmon Populations: Estimating
   Introgression of Nonnative Hatchery Stocks and Temporal Genetic Changes}},
Journal = {{NORTH AMERICAN JOURNAL OF FISHERIES MANAGEMENT}},
Year = {{2013}},
Volume = {{33}},
