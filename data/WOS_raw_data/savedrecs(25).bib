@article{ ISI:000342875200012,
Author = {Barrows, Cameron W.},
Title = {{An Ecosystem Approach to Defining Conservation Boundaries: Concepts and
   a Case Study}},
Journal = {{NATURAL AREAS JOURNAL}},
