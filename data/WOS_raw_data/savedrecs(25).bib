@article{ ISI:000324830500007,
Author = {Heidrich, Oliver and Dawson, Richard J. and Reckien, Diana and Walsh,
   Claire L.},
Title = {{Assessment of the climate preparedness of 30 urban areas in the UK}},
Journal = {{CLIMATIC CHANGE}},
Year = {{2013}},
Volume = {{120}},
