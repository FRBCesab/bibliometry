@article{ ISI:000296736200016,
Author = {Meyer, Michael W. and Rasmussen, Paul W. and Watras, Carl J. and Fevold,
   Brick M. and Kenow, Kevin P.},
Title = {{Bi-phasic trends in mercury concentrations in blood of Wisconsin common
   loons during 1992-2010}},
Journal = {{ECOTOXICOLOGY}},
Year = {{2011}},
Volume = {{20}},
Number = {{7, SI}},
Pages = {{1659-1668}},
Month = {{OCT}},
Abstract = {{We assessed the ecological risk of mercury (Hg) in aquatic systems by
   monitoring common loon (Gavia immer) population dynamics and blood Hg
   concentrations. We report temporal trends in blood Hg concentrations
   based on 334 samples collected from adults recaptured in subsequent
   years (resampled 2-9 times) and from 421 blood samples of chicks
   collected at lakes resampled 2-8 times 1992-2010. Temporal trends were
   identified with generalized additive mixed effects models and mixed
   effects models to account for the potential lack of independence among
   observations from the same loon or same lake. Trend analyses indicated
   that Hg concentrations in the blood of Wisconsin loons declined over the
   period 1992-2000, and increased during 2002-2010, but not to the level
   observed in the early 1990s. The best fitting linear mixed effects model
   included separate trends for the two time periods. The estimated trend
   in Hg concentration among the adult loon population during 1992-2000 was
   -2.6\% per year, and the estimated trend during 2002-2010 was +1.8\% per
   year; chick blood Hg concentrations decreased -6.5\% per year during
   1992-2000, but increased 1.8\% per year during 2002-2010. This bi-phasic
   pattern is similar to trends observed for concentrations of
   methylmercury and SO4 in lake water of an intensely studied seepage lake
   (Little Rock Lake, Vilas County) within our study area. A causeeffect
   relationship between these independent trends is hypothesized.}},
