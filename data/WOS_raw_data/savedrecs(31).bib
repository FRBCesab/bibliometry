@article{ ISI:000295437400008,
Author = {Persson, Sara and Backlin, Britt-Marie and Kindahl, Hans and Brunstrom,
   Bjorn and Magnusson, Ulf},
Title = {{Influence of age, nutritional status and season on the reproductive
   system in wild male mink (Neovison vison)}},
Journal = {{EUROPEAN JOURNAL OF WILDLIFE RESEARCH}},
