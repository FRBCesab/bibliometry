@article{ ISI:000296086500005,
Author = {Platts, Philip J. and Burgess, Neil D. and Gereau, Roy E. and Lovett,
   Jon C. and Marshall, Andrew R. and McClean, Colin J. and Pellikka, Petri
   K. E. and Swetnam, Ruth D. and Marchant, Rob},
Title = {{Delimiting tropical mountain ecoregions for conservation}},
Journal = {{ENVIRONMENTAL CONSERVATION}},
