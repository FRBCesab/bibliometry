@article{ ISI:000296138400003,
Author = {Lieb, David A. and Bouchard, Raymond W. and Carline, Robert F. and
   Nuttall, Ted R. and Wallace, John R. and Burkholder, Carrie L.},
Title = {{Conservation and Management of Crayfishes: Lessons from Pennsylvania}},
Journal = {{FISHERIES}},
