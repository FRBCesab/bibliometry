@article{ ISI:000295047200019,
Author = {Cubero, J. and Gell, I. and Johnson, E. G. and Redondo, A. and Graham,
   J. H.},
Title = {{Unstable green fluorescent protein for study of Xanthomonas citri subsp
   citri survival on citrus}},
Journal = {{PLANT PATHOLOGY}},
Year = {{2011}},
Volume = {{60}},
Number = {{5}},
Pages = {{977-985}},
Month = {{OCT}},
Abstract = {{Xanthomonas citri subsp. citri (Xac) is the causal agent of citrus
   bacterial canker, an important disease for the citrus industry. Studies
   of Xac survival in environments outside of the lesion performed in the
   past may have underestimated the viable population because the recovery
   was based on the ability of the bacterium to grow on culture media. This
   study monitored survival of Xac that express green fluorescent protein
   (GFP) in two different forms: the native protein, and a protein that is
   unstable due to a specific oligopeptide tail targeted by proteases
   within the bacterium. Transformed strains of Xac were verified to be
   stable in their expression of GFP and to show no differences in
   virulence and fitness compared to wild type strains. Evaluation of
   protein stability confirmed that strains with unstable GFP only
   expressed and fluoresced in metabolically active cells, and not in dead
   bacteria. Fluorescence of unstable GFP strains under confocal microscopy
   was used to track bacterial survival and biofilm formation on leaf and
   fruit surfaces. After spray inoculation, aggregates of fluorescing cells
   of unstable GFP strains formed biofilms on leaves and fruit. Bacterial
   cells that aggregated on the surfaces only survived when protected from
   desiccation. Aggregation of viable bacteria in biofilms confirms their
   role in pathogen survival outside of lesions and protection from
   bactericide treatments in the field or in the fruit disinfection
   process.}},
