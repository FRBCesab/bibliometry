ISSN = {{1432-9840}},
EISSN = {{1435-0629}},
Keywords = {{common tern; North Sea; herring; sprat; breeding; chick growth; Wadden
   Sea; Sterna hirundo; Clupea harengus; Sprattus sprattus; indicator;
