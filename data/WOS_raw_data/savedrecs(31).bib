@article{ ISI:000295437400012,
Author = {Fernandez-de-Simon, Javier and Diaz-Ruiz, Francisco and Cirilli,
   Francesca and Sanchez Tortosa, Francisco and Villafuerte, Rafael and
   Delibes-Mateos, Miguel and Ferreras, Pablo},
Title = {{Towards a standardized index of European rabbit abundance in Iberian
   Mediterranean habitats}},
