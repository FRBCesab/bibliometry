@article{ ISI:000301962800003,
Author = {Merz, Joseph E. and Hamilton, Scott and Bergman, Paul S. and Cavallo,
   Bradley},
Title = {{Spatial perspective for delta smelt: a summary of contemporary survey
   data}},
