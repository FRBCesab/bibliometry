@article{ ISI:000301270200012,
Author = {Wimberger, K. and Armstrong, A. J. and Penning, M.},
Title = {{Health checks of rehabilitated leopard tortoise, Stigmochelys pardalis,
   before release into the wild}},
Journal = {{SOUTH AFRICAN JOURNAL OF WILDLIFE RESEARCH}},
Year = {{2011}},
Volume = {{41}},
Number = {{2}},
Pages = {{229-235}},
Month = {{OCT}},
Abstract = {{The release of tortoises after confinement and treatment in
   rehabilitation centres includes the risk that released individuals may
   infect wild conspecifics with foreign parasites and other pathogens. The
   recent monitoring of the release of rehabilitated leopard tortoises
   (Stigmochelys pardalis) onto private land in KwaZulu-Natal province,
   South Africa, revealed that the health-checking protocol at the wildlife
   rehabilitation centre was inadequate. As a result the provincial
   conservation authority placed a moratorium on further leopard tortoise
   releases until a comprehensive health-checking protocol was in place.
   This protocol is detailed here to ensure the success of the release of
   confiscated and rehabilitated leopard tortoises, and that this does not
   come at cost to the wild leopard tortoise populations.}},
