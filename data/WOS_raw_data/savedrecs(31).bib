@article{ ISI:000296416500006,
Author = {Paterson, I. D. and Hoffmann, J. H. and Klein, H. and Mathenge, C. W.
   and Neser, S. and Zimmermann, H. G.},
Title = {{Biological control of Cactaceae in South Africa}},
Journal = {{AFRICAN ENTOMOLOGY}},
