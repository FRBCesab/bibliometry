@article{ ISI:000297202900006,
Author = {Torres-Mellado, G. A. and Jana, R. and Casanova-Katny, M. A.},
Title = {{Antarctic hairgrass expansion in the South Shetland archipelago and
   Antarctic Peninsula revisited}},
