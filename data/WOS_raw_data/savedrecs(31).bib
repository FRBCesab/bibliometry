@article{ ISI:000295203900006,
Author = {Megill, Laura and Walker, Lawrence R. and Vanier, Cheryl and Johnson,
   Debra},
Title = {{SEED BANK DYNAMICS AND HABITAT INDICATORS OF ARCTOMECON CALIFORNICA, A
   RARE PLANT IN A FRAGMENTED DESERT ENVIRONMENT}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
