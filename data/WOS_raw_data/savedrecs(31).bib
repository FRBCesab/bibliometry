@article{ ISI:000292871600005,
Author = {Strickland, Bronson K. and Dorr, Brian S. and Pogmore, Fred and
   Nohrenberg, Gary and Barras, Scott C. and Mcconnell, John E. and
   Gobeille, John},
Title = {{Effects of Management on Double-Crested Cormorant Nesting Colony
   Fidelity}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
