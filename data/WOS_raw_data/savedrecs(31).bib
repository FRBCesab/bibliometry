@article{ ISI:000295659500028,
Author = {Eager, P. T. and Allen, D. C. and Frair, J. L. and Fierke, M. K.},
Title = {{Within-Tree Distributions of the Sirex noctilio Fabricius (Hymenoptera:
   Siricidae) - Parasitoid Complex and Development of an Optimal Sampling
   Scheme}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{2011}},
Volume = {{40}},
