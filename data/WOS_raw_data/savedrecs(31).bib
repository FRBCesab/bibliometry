@article{ ISI:000295056700003,
Author = {Fragoso, F. P. and Varanda, E. M.},
Title = {{Flower-Visiting Insects of Five Tree Species in a Restored Area of
   Semideciduous Seasonal Forest}},
Journal = {{NEOTROPICAL ENTOMOLOGY}},
Year = {{2011}},
