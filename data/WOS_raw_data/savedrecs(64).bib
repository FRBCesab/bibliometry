@article{ ISI:A1992HL20000004,
Author = {DIMLING, JF},
Title = {{ANALYSIS OF THE BIOTIC FACTORS AFFECTING THE SEED SET OF A RARE
   PACIFIC-NORTHWEST ENDEMIC - SIDALCEA-MALVAEFLORA (DC) GRAY EX BENTH
   ELEGANS (GREENE) CL HITCHC}},
Journal = {{NORTHWEST SCIENCE}},
Year = {{1992}},
Volume = {{66}},
Number = {{1}},
Pages = {{35-39}},
Month = {{FEB}},
Abstract = {{Population of Sidalcea malvaeflora step, elegans, a plant on the Oregon
   Natural Heritage Data Base's 1987 Review List, were investigated to
   determine factors that limit its seed production.
   Viable, undeveloped and weevil damaged seeds from 100 racemes were
   monitored throughout the reproductive cycles of 1987 and 188.
   Experiments were conducted to examine potential limitation to seed
   production: hand cross-pollinations tested pollen limitation and
   water/nutrient treatments tested resource limitation.
   Production of viable was low (3.3\% of total ovules in 1987 and 7.1\% in
   1988), although 16.9 percent and 15.1 percent of ovules (in 1987 and
   1988 respectively) had dispersed before collection and could have
   contributed to viable seed set. The high incidence of undeveloped
   (aborted or unfertilized) ovules (41.1\% in 1987 and 51.5\% in 1988)
   suggested seed set was limited. Seed production was reduced by weevil
   predation (by 38.7\% of all ovules in 1987 and 26.5\% in 1988). Viable
   seed production in hand pollinated fruit was higher than in insect
   pollinated fruit. Racemes receiving extra water produced more viable
   seeds than controls.  Resource availability and insufficient pollination
   appear to affect seed production in S. malvaeflora ssp. elegans.}},
