@article{ ISI:A1991JM40300002,
Author = {WARNER, J and WATSON, A},
Title = {{SYNTHETIC VOLATILE LURES IMPROVE THE PERFORMANCE OF APPLE MAGGOT,
   RHAGOLETIS-POMONELLA (DIPTERA TEPHRITIDAE), TRAPS IN ONTARIO}},
Journal = {{PROCEEDINGS OF THE ENTOMOLOGICAL SOCIETY OF ONTARIO}},
Year = {{1991}},
Volume = {{122}},
Pages = {{9-13}},
Abstract = {{Field studies carried out over two years in Meaford and Trenton, Ontario
   showed that synthetic volatile lures increased apple maggot (AM),
   Rhagoletis pomonella (Walsh), catches on traps consisting of a yellow
   panel plus two red spheres by two to four times, compared to traps
   without lures. Traps baited with Consep(R) or Ladd(R) lures usually
   caught AM flies earlier in the season and when AM populations were low
   compared to unbaited traps. Ninety percent of the AM flies caught were
   on the red sphere part of the trap. More sensitive traps and improved
   treatment thresholds should increase the confidence in the AM monitoring
   system.}},
