@article{ ISI:A1990FA71000004,
Author = {CHAMBERS, J},
Title = {{OVERVIEW ON STORED-PRODUCT INSECT PHEROMONES AND FOOD ATTRACTANTS}},
Journal = {{JOURNAL OF THE KANSAS ENTOMOLOGICAL SOCIETY}},
Year = {{1990}},
Volume = {{63}},
