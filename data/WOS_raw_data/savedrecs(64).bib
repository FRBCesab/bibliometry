@article{ ISI:A1991FD21300013,
Author = {WHITEHURST, IT},
Title = {{THE GAMMARUS - ASELLUS RATIO AS AN INDEX OF ORGANIC POLLUTION}},
Journal = {{WATER RESEARCH}},
