@article{ ISI:A1991FR29100008,
Author = {FINCH, S},
Title = {{INFLUENCE OF TRAP SURFACE ON THE NUMBERS OF INSECTS CAUGHT IN WATER
   TRAPS IN BRASSICA CROPS}},
Journal = {{ENTOMOLOGIA EXPERIMENTALIS ET APPLICATA}},
Year = {{1991}},
Volume = {{59}},
Number = {{2}},
Pages = {{169-173}},
Month = {{MAY}},
Abstract = {{The contribution of the various surfaces of a water trap to the trap's
   overall effectiveness was studied in brassica crops by painting black
   different parts of fluorescent-yellow water-traps.  Three pest species,
   Delia radicum (L.), D. platura (Meig.), and Meligethes aeneus
   (Fab.)/Meligethes viridescens (Fab.), together with blowflies and
   syrphids, were caught in large numbers.  Each insect responded
   differently to the yellow/black traps.  The numbers of insect caught
   indicated that the area of trap involved in capturing D. radicum was
   effectively twice the surface area of the water.  Yellow traps for
   monitoring D. radicum populations could be made more selective by
   painting the inner wall black, as such traps caught similar numbers of
   D. radicum to all-yellow traps but 50\% fewer D. platura, Meligethes sp.
   and blowflies, and 95\% fewer syrphids.}},
