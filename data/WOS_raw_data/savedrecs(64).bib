@article{ ISI:A1991FK25300007,
Author = {DUFF, SR and FURGALA, B},
Title = {{SOME EFFECTS OF MENTHOL ON HONEY-BEE TRACHEAL MITE INFESTATIONS IN
   NONMIGRATORY HONEY-BEE COLONIES IN MINNESOTA}},
Journal = {{AMERICAN BEE JOURNAL}},
Year = {{1991}},
Volume = {{131}},
Number = {{5}},
Pages = {{315-317}},
Month = {{MAY}},
Abstract = {{The use of menthol for control of the honey bee tracheal mite (HBTM) was
   evaluated by comparing 4 treatments based on season of application
   (spring, spring and later summer, late summer, and none (check)). 
   Colonies were monitored during a 24 month period.  Spring application of
   menthol significantly reduced the percent honey bees infested by HBTM
   and prevented a resurgence of the HBTM population.  Late summer
   application of menthol did not have a significant effect on the percent
   honey bees infested.  Winter mortality of colonies in the second winter
   was significantly reduced by spring menthol application.}},
