@article{ ISI:A1992JD16700045,
Author = {CECCHI, F and PAVAN, P and MATAALVAREZ, J},
Title = {{FAST DIGESTER START-UP UNDER MESOPHILIC CONDITIONS USING THERMOPHILIC
   INOCULUM}},
