@article{ ISI:A1991GC72900034,
Author = {CARREIRO, JV and AVELAR, R},
Title = {{OCCUPATIONAL EXPOSURE IN MEDICAL AND PARAMEDICAL PROFESSIONS IN PORTUGAL}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{1991}},
Volume = {{36}},
Number = {{2-4}},
Pages = {{233-236}},
Abstract = {{Annual occupational radiation doses for external radiation in medical
   and paramedical professions in Portugal for the years 1986-1988 are
   reported.  The annual mean dose obtained for monitored workers is 0.53
   mSv and for exposed workers is 2.03 mSv.  The annual collective dose
   evaluated for the health fields is 2.47 man.Sv in 1988 and its
   contribution to the total collective dose is around 80\% when compared
   with industry and research laboratory fields.  The collective dose per
   million population is estimated to be 0.24 man.Sv.  The results point
   out the reduction of the occupational doses in all different working
   fields.}},
