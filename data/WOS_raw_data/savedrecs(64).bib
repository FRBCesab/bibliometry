@article{ ISI:A1992HZ72800010,
Author = {BOOIJ, CJH and NOORLANDER, J},
Title = {{FARMING SYSTEMS AND INSECT PREDATORS}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{1992}},
Volume = {{40}},
