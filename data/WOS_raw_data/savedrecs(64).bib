@article{ ISI:000207173100005,
Author = {Fleming, Jo-Ann G. W.},
Title = {{The Integration of Polydnavirus Genomes in Parasitoid Genomes:
   Implications for Biocontrol and Genetic Analyses of Parasitoid Wasps}},
Journal = {{BIOLOGICAL CONTROL}},
Year = {{1991}},
Volume = {{1}},
Number = {{2}},
Pages = {{127-135}},
Month = {{AUG}},
Abstract = {{Polydnaviruses appear to be essential for the establishment of
   compatible interactions between lepidopteran hosts and a select group of
   ichneumonid or braconid parasitoids. The symbiotic association of the
   wasps and viruses is efficiently maintained between wasp generations.
   Molecular analyses of the polydnavirus Campoletis sonorensis virus (CsV)
   and its parasitoid wasp host indicate that the CsV genome is integrated
   in the wasp's chromosomal DNA, suggesting that polydnaviruses like CsV
   probably are transmitted through the wasp germline and can be expected
   to follow Mendelian patterns of inheritance. Restriction fragment length
   polymorphisms have been detected in the genomes of polydnaviruses from
   many wasp species, including C. sonorensis, and are potentially useful
   as genetic markers that can be used to monitor inheritance patterns or
   populations within a wasp species. The implications of integrated
   polydnaviral DNAs for biocontrol are discussed with reference to the
   species specificity of the viruses and their potential for genetic
   mapping. (C) 1991 Academic Press, Inc.}},
