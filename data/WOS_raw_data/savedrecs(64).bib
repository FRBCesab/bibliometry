@article{ ISI:A1992HQ61200024,
Author = {KANE, DM and LITVAITIS, JA},
Title = {{AGE AND SEX COMPOSITION OF LIVE-CAPTURED AND HUNTER-KILLED SAMPLES OF
   BLACK BEARS}},
Journal = {{JOURNAL OF MAMMALOGY}},
Year = {{1992}},
Volume = {{73}},
Number = {{1}},
Pages = {{215-217}},
Month = {{FEB}},
Abstract = {{We compared age-class distribution and sex ratios of captured and
   hunter-killed samples of black bears (Ursus americanus) to examine the
   relative agreement between methods used to monitor regional populations
   of this species. Mean ages of captured bears were greater than
   hunter-killed bears, and females were less abundant in the captured
   sample. Temporal differences between sampling methods (live capture =
   diurnal + nocturnal versus hunters = diurnal) may explain age-class
   differences. Variation in mobility (live capture = mainly stationary
   versus hunters = stationary + mobile) may explain sex ratio differences.
   Although we could not determine which sample was more representative of
   the population, our results suggest that biologists apply several
   indices as a more reliable approach to monitor the response of a
   population of bears to environmental change.}},
