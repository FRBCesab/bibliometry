@article{ ISI:A1992KC05000012,
Author = {GRAY, JS},
Title = {{BIOLOGICAL AND ECOLOGICAL EFFECTS OF MARINE POLLUTANTS AND THEIR
   DETECTION}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{1992}},
Volume = {{25}},
Number = {{1-4}},
Pages = {{48-50}},
Abstract = {{Within the past 6 yr biological and ecological techniques for monitoring
   effects of pollutants have developed rapidly. Techniques are now
   available which approach chemical techniques in their sensitivity and
   which show pollution effects at sites hitherto not suspected of being
   polluted. Future developments of such techniques, it is suggested, are
   likely to involve development of better water and sediment bioassay
   techniques. Molecular biological techniques already show promise of
   revolutionizing approaches to effects monitoring. Finally, it is
   suggested that it is not necessary to demand that a given biological
   technique must show an effect at a higher level of organization than
   that at which it is tested (i.e. if induction of an enzyme in response
   to stress can be demonstrated, one does not need to show effects at
   population levels before regulatory action is taken).}},
