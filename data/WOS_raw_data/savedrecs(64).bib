@article{ ISI:A1991GP44500015,
Author = {AFOLAMI, SO and FAWOLE, B},
Title = {{EFFECT OF PRATYLENCHUS-SEFAENSIS FORTUNER 1973 ON GROWTH AND YIELD OF
   ZEA-MAYS L CV FARZ-7 UNDER CONTINUOUS CROPPING}},
Journal = {{PLANT AND SOIL}},
Year = {{1991}},
Volume = {{138}},
Number = {{1}},
Pages = {{133-138}},
Month = {{DEC}},
Abstract = {{Concrete microplots containing about 280 litres of sandy loam soil were
   used for growing three consecutive crops of maize at Ibadan in
   South-Western Nigeria, under four population regimes of Pratylenchus
   sefaensis. Nematode preplanting population rose up to 1,750 per 250 ml
   soil in the third experiment. This increase significantly reduced grain
   yield, but had no statistically significant effect on top weight and
   plant height. Cropping strategies that can prevent rapid population
   build-up of this nematode are advocated for fields infested by the
   nematode. Initial monitoring of soil nematode types and numbers before
   planting is recommended for appropriate decisions on cropping
   strategies.}},
