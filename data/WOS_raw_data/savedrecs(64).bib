@article{ ISI:A1992HP52400018,
Author = {PRINGLE, KL and GILIOMEE, JH},
Title = {{DISPERSION STATISTICS AND SAMPLE-SIZE ESTIMATES FOR MONITORING MITE
   POPULATIONS IN COMMERCIAL APPLE ORCHARDS}},
Journal = {{JOURNAL OF APPLIED ECOLOGY}},
Year = {{1992}},
Volume = {{29}},
Number = {{1}},
Pages = {{143-149}},
Abstract = {{1. Sample size estimates for monitoring phytophagous mite population
   densities in apple orchards were derived from dispersion statistics in a
   two-stage sampling system.
   2. Taylor's power law proved to be a more suitable model than the
   regression method of Iwao \& Kuno (1971).
   3. Using dummy variables in Taylor's power law it was found that in some
   cases spray applications affected the dispersion statistics and,
   therefore, sample size estimates.
   4. The background variances were unstable from farm to farm and season
   to season, contributing to unstable sample size estimates.
   5. As the unstable background variances were thought to be partly due to
   differences in the size of the orchards on the different farms, it was
   suggested that in future orchards be sub-divided into blocks not
   exceeding 2 ha for monitoring mite population densities.
   6. There was a trend suggesting that the sampling error decreased more
   rapidly when the number of trees sampled was increased than when the
   number of leaves per tree was increased.}},
