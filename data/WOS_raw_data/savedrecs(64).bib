@article{ ISI:A1991GG59800029,
Author = {KLEINJANS, JCS and ALBERING, HJ and MARX, A and VANMAANEN, JMS and
   VANAGEN, B and TENHOOR, F and SWAEN, GMH and MERTENS, PLJM},
Title = {{NITRATE CONTAMINATION OF DRINKING-WATER - EVALUATION OF GENOTOXIC RISK
   IN HUMAN-POPULATIONS}},
Journal = {{ENVIRONMENTAL HEALTH PERSPECTIVES}},
Year = {{1991}},
Volume = {{94}},
Pages = {{189-193}},
Month = {{AUG}},
