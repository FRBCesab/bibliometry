@article{ ISI:A1991GK82300032,
Author = {MINCHINTON, TE and SCHEIBLING, RE},
Title = {{THE INFLUENCE OF LARVAL SUPPLY AND SETTLEMENT ON THE
   POPULATION-STRUCTURE OF BARNACLES}},
Journal = {{ECOLOGY}},
