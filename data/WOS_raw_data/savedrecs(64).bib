@article{ ISI:A1991EV98100002,
Author = {GROUT, TG and RICHARDS, GI},
Title = {{VALUE OF PHEROMONE TRAPS FOR PREDICTING INFESTATIONS OF RED SCALE,
   AONIDIELLA-AURANTII (MASKELL) (HOM, DIASPIDIDAE), LIMITED BY NATURAL
