@article{ ISI:A1992HT81300006,
Author = {LUCAS, Z},
Title = {{MONITORING PERSISTENT LITTER IN THE MARINE-ENVIRONMENT ON SABLE-ISLAND,
   NOVA-SCOTIA}},
