@article{ ISI:A1991EY47200004,
Author = {AMINUKANO, M and MCNEILL, S and HAILS, RS},
Title = {{POLLUTANT, PLANT AND PEST INTERACTIONS - THE GRAIN APHID SITOBION-AVENAE
   (F)}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{1991}},
Volume = {{33}},
Number = {{3}},
Pages = {{233-243}},
Month = {{JAN}},
Abstract = {{The performance of the grain aphid, Sitobion avenae (F.), was monitored
   over three seasons in the field in an open-air fumigation experiment
   exposing cereal crops to elevated SO2 (mean levels 0.021-0.057-mu-l
   l-1).  In the first season winter wheat Triticum aestivum L. (cv.
   `Rapier') was grown, whereas in the following two seasons winter barley,
   Hordeum vulgare L. (cv. `Igri') was cultivated.  The results suggest
   that:  (1) the aphid population levels were enhanced by SO2 exposure;
   (2) natural enemy populations are not affected significantly by these
   SO2 levels, nor do they increase with enhanced prey levels.  Analysis of
   plant material suggests that changes in the food quality of the plants
   with respect to the aphids may be a factor causing enhanced aphid
   performance.}},
