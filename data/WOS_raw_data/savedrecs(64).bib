@article{ ISI:A1991GF00300017,
Author = {HASSE, V and VONKEYSERLINGK, N and TUN, USB},
Title = {{SIMPLE DECISION RULES DERIVED FROM RICE-MONITORING IN BURMA}},
Journal = {{ZEITSCHRIFT FUR PFLANZENKRANKHEITEN UND PFLANZENSCHUTZ-JOURNAL OF PLANT
   DISEASES AND PROTECTION}},
Year = {{1991}},
Volume = {{98}},
Number = {{4}},
Pages = {{444-448}},
Month = {{AUG}},
Abstract = {{A total of 139 and 206 rice fields in 12 Burmese states and divisions
   were monitored during the wet seasons of 1986 and 1987, respectively. 
   Pest populations were generally low.  No species exceeded the relatively
   conservative Burmese pest threshold levels in more than 10 \% of the
   fields each year.  Even though insecticides were not applied when
   infestations exceeded thresholds, rice yields were not significantly
   reduced.  As a consequence, higher action levels for selected Burmese
   rice pests are proposed.  The originally used decision system was based
   on enumerative sampling.  High correlations were found between this
   approach and the proposed binomial strategy which only counts absence or
   presence of pests or pest damage.  This method stands a good chance of
   being adopted by Burmese rice farmers.  Unfortunately, further studies
   had to be discontinued as a consequence of the political disturbances in
   Burma in August 1988.}},
