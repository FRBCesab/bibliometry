@article{ ISI:A1991EY04400027,
Author = {HARRIS, EJ and OLALQUIAGA, G},
Title = {{OCCURRENCE AND DISTRIBUTION PATTERNS OF MEDITERRANEAN FRUIT-FLY
   (DIPTERA, TEPHRITIDAE) IN DESERT AREAS IN CHILE AND PERU}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{1991}},
Volume = {{20}},
Number = {{1}},
Pages = {{174-178}},
Month = {{FEB}},
Abstract = {{The occurrence and distribution of the Mediterranean fruit fly,
   Ceratitis capitata (Wiedemann), in desert areas near the Peru-Chilean
   border were monitored with protein- and trimedlure-baited traps.  The
   results showed a distinct pattern of increase and decline related to C.
   capitata population trends in Tacna Valley, Peru.  C. capitata was most
   abundant in the Aeropuerto frontier location and the inland Gallinazo
   location.  All frontier locations showed similar seasonal population
   trends and yearly variation in total catches between trap sites related
   to long-range fly movement.  The synchronous population dispersal
   pattern of males and females suggests it is an important survival
   behavior mechanism for C. capitata populations in the arid areas of
   Chile and Peru.}},
