@article{ ISI:A1991GE59100004,
Author = {DUMERLE, P and CORNIC, JF},
Title = {{MONITORING THE REPRODUCTIVE CAPACITY OF CHORISTONEURA-MURINANA
   (LEPIDOPTERA, TORTRICIDAE) POPULATIONS BY MEASURING THE SIZE OF MALE
   MOTHS CAUGHT IN SEX-PHEROMONE TRAPS}},
Journal = {{ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY}},
Year = {{1991}},
Volume = {{12}},
Number = {{3}},
Pages = {{369-383}},
Abstract = {{The forewing length of fir budworm (Choristoneura murinana {[}Hb.])
   males caught with funnel sex-pheromone traps in two stands of Abies alba
   and one of Cedrus atlantica growing in southern France was found to vary
   considerably within each of the three populations, as well as varying
   fairly considerably from one population to another.  The mean size of
   the males decreased quite steadily throughout the flight period, but the
   rate of this decrease varied from one population to another.  The
   representativeness of the males captured with sex-pheromone traps, the
   relationship between the respective variations in size in the males and
   females, and that between the size of the females and their fecundity
   are discussed.  It is concluded that the variations in the size of the
   trapped males in all probability reflected similar, sometimes
   considerable, variations in the fecundity of the females both within and
   among the three populations under investigation.  Measuring the size of
   male C. murinana caught in sex-pheromone traps seems to be a
   particularly useful indirect method for assessing populations'
   reproductive potential and the nutritional quality of the host plant. 
   The numerical trends of the three populations investigated were not in
   agreement with their respective reproductive potentials.  The
   nutritional quality of Cedrus atlantica seems to be lower than that of
   Abies alba.}},
