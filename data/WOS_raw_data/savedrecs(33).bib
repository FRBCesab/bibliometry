@article{ ISI:000302824900187,
Author = {Cunhat, C. de L. da N. and Scudelari, A. C. and Goncalves, J. E. and
   Mercuri, E. G. F.},
Title = {{Assessment of sanitary sewage pollution in the Paranagua estuarine
   complex of Paranagua, Brazil using environmental modeling}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
Year = {{2011}},
Number = {{1, 64}},
Pages = {{912-916}},
Note = {{11th International Coastal Symposium (ICS), Szczecin, POLAND, MAY 09-13,
