@article{ ISI:000284959400008,
Author = {Burger, Joanna and Clarke, James and Gochfeld, Michael},
Title = {{Information needs for siting new, and evaluating current, nuclear
   facilities: ecology, fate and transport, and human health}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
