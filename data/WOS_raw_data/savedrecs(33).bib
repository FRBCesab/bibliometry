@inproceedings{ ISI:000392766200031,
Author = {Al-Mezori, Hassan Amin and Hawrami, Karzan A. M.},
Editor = {{Xuan, L}},
Title = {{Evaluation of Microbial quality of the drinking water of Duhok
   province/Kurdistan region of Iraq}},
Booktitle = {{Environmental Science and Development}},
Series = {{International Proceedings of Chemical Biological and Environmental
   Engineering}},
Year = {{2011}},
Volume = {{4}},
Pages = {{141-147}},
Note = {{2nd International Conference on Environmental Science and Development
   (ICESD 2011), Mumbai, INDIA, JAN 07-09, 2011}},
Organization = {{Asia Pacific Chem Biol \& Environm Engn Soc; Int Assoc Comp Sci \&
   Informat Technol}},
Abstract = {{The preservation and maintenance of our natural water resources is a
   very difficult task due to rapid growth of population and increased
   industrial activities, therefore quality of water resources is
   deteriorating day by day due to the continuous addition of undesirable
   chemicals. Among various organic and inorganic water pollutants,
   pathogenic bacteria are toxic, dangerous and harmful because of their
   tissue degradation in nature
   The aim of this study is to evaluate the Bacterial contamination of
   drinking water of Duhok province, and evaluate the efficiency of water
   purification project plants for disinfection of water
   The areas of the study were water projects those of supporting Duhok
   governorate with their districts (Sumeel, Chambarakat, Zakho, Akre,
   Shekhan and Amedi).Water samples were collected monthly from pre and
   post treated water at each station, which started from November 2008 to
   July 2009.
   The water samples for bacteriological analysis were collected from
   selected stations and analyzed for Fecal and total bacterial indicator
   by using Multiple-Tube fermentation Techniques. The results showed that
   the pre treated water of (Duhok, Chambarakat, Zakho, Akre and Amedy)
