@article{ ISI:000297214300016,
Author = {Ghia, D. and Fea, G. and Bernini, F. and Nardi, P. A.},
Title = {{Reproduction experiment on Austropotamobius pallipes complex under
   controlled conditions: Can hybrids be hatched?}},
Journal = {{KNOWLEDGE AND MANAGEMENT OF AQUATIC ECOSYSTEMS}},
Year = {{2011}},
Number = {{401}},
Abstract = {{The Austropotamobius pallipes complex is formed by the two species A.
   pallipes and A. italicus, recently defined only on genetic basis,
   without obvious morphological features for their differentiation.
   Dealing with this problem, we have conducted a breeding experiment
   between these two presumed species under controlled conditions, in order
   to investigate the possible existence of reproductive barriers. Using 80
   breeders (56 females and 24 males) collected from populations where
   their species identity was genetically well established, we performed
   four series of trials: F-ita x M-pal and F-pal x M-ita interspecific
   mating; F-ita x M-ita and F-pal x M-pal intraspecific mating as a
