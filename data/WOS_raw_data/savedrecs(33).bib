@article{ ISI:000287391800015,
Author = {Maharana, Mandakini and Swarnkar, M. and Chougaonkar, M. P. and Mayya,
   Y. S. and Sengupta, D.},
Title = {{AMBIENT GAMMA RADIATION LEVELS (INDOOR AND OUTDOOR) IN THE VILLAGES
   AROUND JADUGUDA (INDIA) USING CARD-BASED CaSO4: DY TL DOSEMETERS}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2011}},
Volume = {{143}},
Number = {{1}},
Pages = {{88-96}},
Month = {{JAN}},
Abstract = {{A systematic study of the gamma radiation levels (indoor and outdoor) in
   the villages surrounding the uranium-enriched regions around Jaduguda,
   India has been undertaken by monitoring selected dwellings in six
   villages. Each dwelling unit was monitored for a total duration of 1 y.
   The gamma radiation measurements were carried out using card-based
   CaSO4: Dy thermoluminescent dosemeters. The estimated average annual
   gamma dose values for indoor and outdoor were 980 and 924 (mu Gy y(-1)),
   respectively, for the entire region studied. The maximum indoor and
   outdoor gamma doses experienced in North Dungridih and South Dungridih
   villages were 1305 and 1223 (mu Gy y(-1)), respectively. The minimum
   indoor and outdoor gamma dose values observed in Chatikocha village were
   624 and 696 (mu Gy y(-1)), respectively. Seasonal variation of the
   indoor gamma values was not observed during the year; however, a small
   variation was seen with the type of building materials used for
   construction purposes. A statistical analysis was attempted to
   characterise the distribution of terrestrial gamma radiation obtained in
   the study area. The average quarterly indoor gamma values for spring,
   summer, monsoon and winter seasons as prevalent in the regions were 267
   +/- 71, 262 +/- 54, 213 +/- 91, 238 +/- 66 (mu Gy 90 d(-1)),
   respectively. The annual effective doses to the local population
   residing in the selected dwelling units were estimated to be 0.6 and 0.1
   (mSv y(-1)) for indoor and outdoor, respectively, using an occupancy
   factor of 0.8 and 0.2.}},
