Editor = {{Ceccaldi, HJ and Dekeyser, I and Girault, M and Stora, G}},
Title = {{Coastal Seawater Pollutants in the Coral Reef Lagoon of a Small Tropical
   Island in Development: The Mayotte Example (N Mozambique Channel, SW
   Indian Ocean)}},
Booktitle = {{GLOBAL CHANGE: MANKIND-MARINE ENVIRONMENT INTERACTIONS}},
Year = {{2011}},
