@article{ ISI:000285968100009,
Author = {Yott, Adelle and Rosatte, Rick and Schaefer, James A. and Hamr, Joe and
   Fryxell, John},
Title = {{Movement and Spread of a Founding Population of Reintroduced Elk (Cervus
   elaphus) in Ontario, Canada}},
Journal = {{RESTORATION ECOLOGY}},
