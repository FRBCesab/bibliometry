@article{ ISI:000284651000002,
Author = {Lee, Heung-Su and Gillespie, David R.},
Title = {{Life tables and development of Amblyseius swirskii (Acari: Phytoseiidae)
   at different temperatures}},
Journal = {{EXPERIMENTAL AND APPLIED ACAROLOGY}},
Year = {{2011}},
Volume = {{53}},
Number = {{1}},
Pages = {{17-27}},
Month = {{JAN}},
Abstract = {{Development time, reproduction, survival and sex ratio were determined
   for the omnivorous mite Amblyseius swirskii at nine constant
   temperatures (13, 15, 18, 20, 25, 30, 32, 34 and 36A degrees C) on
   pepper leaf disks with cattail, Typha latifolia, pollen for food. These
   data were used to derive life table parameters at these constant
   temperatures. No development was observed at 13A degrees C. The lower
   development threshold, based on the fit to the linear portion of the
   development curve, was 11.3A degrees C. The upper development threshold
   was 37.4 +/- A 1.12A degrees C, and the optimum temperature was
   calculated to be 31.5A degrees C. Average lifetime fecundity ranged from
   a low of 1.3 +/- A 0.24 eggs/female at 15A degrees C to a high of 16.1
   +/- A 0.34 eggs/female at 25A degrees C, and r (m) was greatest at 32A
   degrees C. Non-linear regression of the relationship between temperature
   and r (m) produced an estimate of 15.49 +/- A 0.905A degrees C for the
   lower threshold for population growth and 36.99 +/- A 0.816A degrees C
   for the upper threshold for population growth, and an optimum
   temperature of 30.1A degrees C. These values suggest that A. swiskii
   populations should grow quickly in response to food availability (pollen
   or prey) between 20 and 32A degrees C, but that, especially below 20A
   degrees C, population growth could be slow and impacts on prey
   populations should be monitored carefully.}},
