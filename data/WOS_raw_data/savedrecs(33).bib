@article{ ISI:000290629500005,
Author = {Hashimoto Soares, Lucy Satiko and Lopez, Juliana Pierrobon and Muto,
   Elizabeti Yuriko and Giannini, Roberto},
Title = {{CAPTURE FISHERY IN NORTHERN TODOS OS SANTOS BAY, TROPICAL SOUTHWESTERN
   ATLANTIC, BRAZIL}},
