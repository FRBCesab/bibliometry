@article{ ISI:000286600500006,
Author = {Haring, Herman J. and Blocksom, Karen A. and Smith, Mark E. and Angradi,
   Theodore and Wratschko, Melissa C. and Armstrong, Brandon and Bolgrien,
   David and Lazorchak, James M.},
Title = {{Sediment Toxicity in Mid-Continent Great Rivers (USA)}},
