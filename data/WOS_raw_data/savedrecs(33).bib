@inproceedings{ ISI:000300505700132,
Author = {Rashid, Yong Razidah and Sulaiman, Mohd Sabere and Aziz, Azlina and
   Selamat, Hilmilia and Yani, Abdul Halim Mat and Kandar, Mohd Zin},
Editor = {{Secondini, P and Wu, X and Tondelli, S and Wu, J and Xie, H}},
Title = {{Greening government's office buildings: PWD Malaysia experiences}},
Booktitle = {{2011 INTERNATIONAL CONFERENCE ON GREEN BUILDINGS AND SUSTAINABLE CITIES}},
