@inproceedings{ ISI:000398977000014,
Author = {Weaver, Peter L.},
Editor = {{Watson, A and MurrietaSaldivar, J and McBride, B}},
Title = {{El Toro Wilderness, Luquillo Experimental Forest, Puerto Rico}},
Booktitle = {{SCIENCE AND STEWARDSHIP TO PROTECT AND SUSTAIN WILDERNESS VALUES}},
Series = {{USDA Forest Service Rocky Mountain Research Station Proceedings}},
Year = {{2011}},
Volume = {{64}},
