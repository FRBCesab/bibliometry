@article{ ISI:000298333400018,
Author = {Rodriguez, Airam and Rodriguez, Beneharo and Lucas, Matthew P.},
Title = {{Trends in numbers of petrels attracted to artificial lights suggest
   population declines in Tenerife, Canary Islands}},
Journal = {{IBIS}},
Year = {{2012}},
Volume = {{154}},
Number = {{1}},
Pages = {{167-172}},
Month = {{JAN}},
Abstract = {{The secretive breeding behaviour of petrels makes monitoring their
   breeding populations challenging. To assess population trends of Corys
   Shearwater Calonectris diomedea, Bulwers Petrel Bulweria bulwerii and
   Macaronesian Shearwater Puffinus baroli in Tenerife from 1990 to 2010,
   we used data from rescue campaigns that aim to reduce the mortality of
   fledgling petrels attracted to artificial lights as proxies for trends
   in breeding population size. Despite increases in human population size
   and light pollution, the number of rescued fledglings of Corys
   Shearwater and Bulwers Petrel increased and remained stable,
   respectively, whereas numbers of rescued Macaronesian Shearwaters
   sharply declined. In the absence of more accurate population estimates,
   these results suggest a worrying decline in the Macaronesian Shearwaters
   breeding population.}},
