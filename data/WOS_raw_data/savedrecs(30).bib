@article{ ISI:000301983200006,
Author = {Lategan, Maria J. and Torpy, F. R. and Newby, S. and Stephenson, S. and
   Hose, G. C.},
Title = {{Fungal Diversity of Shallow Aquifers in Southeastern Australia}},
