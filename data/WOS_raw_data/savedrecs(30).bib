
@article{ ISI:000320412900003,
Author = {Duchamp, Christophe and Boyer, Jerome and Briaudet, Pierre-Emmanuel and
   Leonard, Yannick and Moris, Perrine and Bataille, Alain and Dahier,
   Thierry and Delacour, Gilles and Millisher, Gerard and Miquel, Christian
