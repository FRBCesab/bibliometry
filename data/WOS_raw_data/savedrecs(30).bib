@inproceedings{ ISI:000344125500015,
Author = {Johnson, Catherine L. and Hare, Jonathan A.},
Editor = {{Stephenson, RL and Annala, JH and Runge, JA and HallArber, M}},
Title = {{Zooplankton Monitoring in the Gulf of Maine: Past, Present, and Future}},
Booktitle = {{ADVANCING AN ECOSYSTEM APPROACH IN THE GULF OF MARINE}},
