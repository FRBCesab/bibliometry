@article{ ISI:000310034400009,
Author = {Descovich, Kristin A. and Lisle, Allan T. and Johnston, Stephen and
   Keeley, Tamara and Phillips, Clive J. C.},
Title = {{Intrasample variation and the effect of storage delay on faecal
   metabolite concentrations in the southern hairy-nosed wombat
   (Lasiorhinus latifrons)}},
Journal = {{AUSTRALIAN MAMMALOGY}},
