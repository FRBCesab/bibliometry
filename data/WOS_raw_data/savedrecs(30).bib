@article{ ISI:000303179500011,
Author = {Conceicao, Abel Augusto and Orr, Barron J.},
Title = {{Post-fire flowering and fruiting in Vellozia sincorana, a caulescent
   rosette plant endemic to Northeast Brazil}},
Journal = {{ACTA BOTANICA BRASILICA}},
