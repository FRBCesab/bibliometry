Title = {{Risks to Aquatic Ecosystems in Mountain Regions and Their Possible
   Management}},
Booktitle = {{CORRELATION BETWEEN HUMAN FACTORS AND THE PREVENTION OF DISASTERS}},
Series = {{Nato Science for Peace and Security Series E-Human and Societal Dynamics}},
Year = {{2012}},
Volume = {{94}},
