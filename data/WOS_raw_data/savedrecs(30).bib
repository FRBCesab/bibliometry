@incollection{ ISI:000319872100005,
Author = {Ellis-Felege, Susan N. and Carroll, John P.},
Editor = {{Ribic, CA and Thompson, FR and Pietz, PJ}},
Title = {{Gamebirds and Nest Cameras PRESENT AND FUTURE}},
Booktitle = {{VIDEO SURVEILLANCE OF NESTING BIRDS}},
