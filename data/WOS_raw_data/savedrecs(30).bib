@inproceedings{ ISI:000344125500026,
Author = {Lawton, Peter and Incze, Lewis S. and Ellis, Sara L.},
Editor = {{Stephenson, RL and Annala, JH and Runge, JA and HallArber, M}},
Title = {{Representation of Biodiversity Knowledge within Ecosystem-Based
   Management Approaches for the Gulf of Maine}},
