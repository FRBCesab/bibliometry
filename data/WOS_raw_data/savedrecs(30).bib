Title = {{Emergy-based urban dynamic modeling of long-run resource consumption,
   economic growth and environmental impact: conceptual considerations and
   calibration}},
Booktitle = {{18TH BIENNIAL ISEM CONFERENCE ON ECOLOGICAL MODELLING FOR GLOBAL CHANGE
   AND COUPLED HUMAN AND NATURAL SYSTEM}},
Series = {{Procedia Environmental Sciences}},
Year = {{2012}},
