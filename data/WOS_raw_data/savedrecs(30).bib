@article{ ISI:000298586200011,
Author = {Sanderlin, Jamie S. and Lazar, Nicole and Conroy, Michael J. and Reeves,
   Jaxk},
Title = {{Cost-efficient selection of a marker panel in genetic studies}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
