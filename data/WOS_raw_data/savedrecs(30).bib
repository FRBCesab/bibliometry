@article{ ISI:000298609300014,
Author = {Zhilyaev, G. G.},
Title = {{Vitality composition and its role in subpopulation loci of Soldanella
   hungarica Simonk. (Primulaceae) in the Carpathians}},
Journal = {{CONTEMPORARY PROBLEMS OF ECOLOGY}},
Year = {{2011}},
Volume = {{4}},
Number = {{6}},
Pages = {{651-657}},
Month = {{DEC}},
Abstract = {{A long-term monitoring (1974-2009) of the regularities of subpopulation
   organization of Soldanella hungarica Simonk. (Primulaceae) has been
   conducted in the plant community of Mugetum luzulosum in the
   Carpathians. The basic principles of formation and dynamics of the
   subpopulation loci have been revealed. The conclusion is made that the
   heterogeneity of luminance in the mountain-pine crooked forests creates
   a matrix of microhabitats, which serves as a basis for formation of
   subpopulation loci with their specific composition and dynamics. The
   arguments are given that the heterogeneity of vitality of the loci is
   the most important condition for the preservation of homeostasis and
   vitality of the natural historic populations of plants.}},
