@article{ ISI:000308113300006,
Author = {Nadel, Ryan L. and Noack, Ann E.},
Title = {{Current understanding of the biology of Thaumastocoris peregrinus in the
   quest for a management strategy}},
Journal = {{INTERNATIONAL JOURNAL OF PEST MANAGEMENT}},
