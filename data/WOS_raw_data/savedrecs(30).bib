@inproceedings{ ISI:000348536400069,
Author = {Ciomos, Vasile and Turdean, Marinella and Ciomos, Alina},
Book-Group-Author = {{SGEM}},
Title = {{IMPROVING WATER RESOURCES MANAGEMENT OF THE DANUBE RIVER BASIN USING
   MODERN MONITORING AND WARNING SYSTEMS}},
Booktitle = {{12TH INTERNATIONAL MULTIDISCIPLINARY SCIENTIFIC GEOCONFERENCE, SGEM
   2012, VOL. V}},
