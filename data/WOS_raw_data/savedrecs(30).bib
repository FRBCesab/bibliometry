@article{ ISI:000298944400013,
Author = {Miotto, Renata A. and Cervini, Marcelo and Begotti, Rodrigo A. and
   Galetti, Jr., Pedro M.},
Title = {{Monitoring a Puma (Puma concolor) Population in a Fragmented Landscape
   in Southeast Brazil}},
Journal = {{BIOTROPICA}},
