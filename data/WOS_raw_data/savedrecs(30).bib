@article{ ISI:000299009400006,
Author = {Raszka, Anna and Surmacz-Gorska, Joanna and Zabczynski, Sebastian and
   Miksch, Korneliusz},
Title = {{The Population Dynamics of Nitrifiers in Ammonium-Rich Systems}},
Journal = {{WATER ENVIRONMENT RESEARCH}},
