@article{ ISI:000300028500005,
Author = {Subchev, Mitko A. and Toshova, Teodora B. and Andreev, Radoslav A. and
   Petrova, Vilina D. and Maneva, Vasilina D. and Spasova, Teodora S. and
   Marinova, Nikolina T. and Minkov, Petko M. and Velchev, Dimitar I.},
Title = {{Employing Floral Baited Traps for Detection and Seasonal Monitoring of
   Tropinota (Epicometis) hirta (PoDA) (Coleoptera: Cetoniidae) in Bulgaria}},
Journal = {{ACTA ZOOLOGICA BULGARICA}},
Year = {{2011}},
Volume = {{63}},
Number = {{3}},
Pages = {{269-276}},
Month = {{DEC}},
Abstract = {{The potential of commercially available light blue VARb3k traps and
   baits for T hirta (Csalomon (R), Plant Protection Institute, Budapest,
   Hungary) as a new tool for detection and describing the seasonal flight
   patterns of Tropinota (Epicometis) hirta (PODA) was proved in eight
   sites in Bulgaria in 2009 and 2010. The traps showed very high
   efficiency in both cases of high and low population level of the pest.
   Significant catches of T hirta were recorded in Dryanovo, Karnobat,
   Knezha, Kyustendil, Petrich and Plovdiv. As a whole the beetles appeared
   in the very end of March beginning of April and reached their peak
   flight in the second half of April beginning of May; catches were
   recorded up to the middle of July. The bait/traps system used in our
   field work showed very high species selectivity. In nine out of ten
   cases the catches of T. hirta exceeded 90\% and in six of these this
   percent was equal or more than 99\%, including Petrich where only T
   hirta were caught in 2009.}},
