@inproceedings{ ISI:000393139700123,
Author = {Polya, David and Mondal, Debapriya and Sovann, Chansopheaktra and
   Bradford, William and Watts, Michael and Giri, Ashok and Mondal,
   Debapriya and Sovann, Chansopheaktra},
Editor = {{Ng, JC and Noller, BN and Naidu, R and Bundschuh, J and Bhattacharya, P}},
Title = {{Geogenic arsenic attributable health risks in Europe}},
Booktitle = {{UNDERSTANDING THE GEOLOGICAL AND MEDICAL INTERFACE OF ARSENIC, AS 2012}},
Series = {{Arsenic in the Environment}},
Year = {{2012}},
Pages = {{339-341}},
