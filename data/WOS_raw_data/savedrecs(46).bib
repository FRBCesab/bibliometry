@article{ ISI:000238275900006,
Author = {Yee, WL and Goughnour, RB},
Title = {{New host records for the apple maggot, Rhagoletis pomonella (Walsh)
   (Diptera : Tephritidae), in Washington State}},
Journal = {{PAN-PACIFIC ENTOMOLOGIST}},
Year = {{2006}},
Volume = {{82}},
Number = {{1}},
Pages = {{54-60}},
Month = {{JAN}},
Abstract = {{A study was conducted to establish new host records for the apple
   maggot, Rhagoletis pomonella (Walsh), 1867 (Diptera: Tephritidae), in
   southwestern Washington and to test the hypothesis that some new hosts
   are used consistently (annually) by this fly. Based on flies reared from
   fruit collected in 2001-2004, Asian pear, Pyrus serotina L., bitter
   cherry, Prunus emarginata (Doug. ex Hook.) D. Dietr., European pear,
   Pyrus communis L., garden plum, Prunus domestica L., and cranberry
   cotoneaster, Cotoneaster apiculatus Rehd. \& Wils., were R. pomonella
   hosts, in addition to apple, hawthorns, and crabapple. All hosts belong
   to the Rosaceae. Asian pear has never been recorded as a host for R.
   pomonella. Different Asian pear trees were infested for three
   consecutive years, suggesting this host is used consistently. Bitter
   cherry is also a new host record for R. pomonella, but it was rarely
   infested. European pear, plum, and cotoneaster are new published host
   records for R. pomonella in Washington State. European pear and plum
   were infested four and three consecutive years, respectively, suggesting
   they also were used consistently. Only one C. apiculatus was found, so
   it is unknown if it was used consistently. If R. pomonella populations
   increase in or near the major apple-growing areas of central Washington,
   Asian pear, European pear, and plum may occasionally be attacked and may
   need to be monitored in a fly detection and suppression program.}},
