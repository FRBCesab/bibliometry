Title = {{Adverse health effects of fluoro-edenitic fibers - Epidemiological
   evidence and public health priorities}},
Booktitle = {{LIVING IN A CHEMICAL WORLD: FRAMING THE FUTURE IN LIGHT OF THE PAST}},
Series = {{ANNALS OF THE NEW YORK ACADEMY OF SCIENCES}},
