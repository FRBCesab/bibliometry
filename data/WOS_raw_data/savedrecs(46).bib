@article{ ISI:000234236600013,
Author = {Hardman-Mountford, NJ and Allen, JI and Frost, MT and Hawkins, SJ and
   Kendall, MA and Mieszkowska, N and Richardson, KA and Somerfield, PJ},
Title = {{Diagnostic monitoring of a changing environment: An alternative UK
   perspective}},
