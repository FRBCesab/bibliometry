Author = {Daims, H. and Maixner, F. and Luecker, S. and Stoecker, K. and Hace, K.
   and Wagner, M.},
Title = {{Ecophysiology and niche differentiation of Nitrospira-like bacteria, the
   key nitrite oxidizers in wastewater treatment plants}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
