@article{ ISI:000236990200001,
Author = {Oikawa, S and Kanno, N and Sanada, T and Abukawa, J and Higuchi, H},
Title = {{A survey of indoor workplace radon concentration in Japan}},
Journal = {{JOURNAL OF ENVIRONMENTAL RADIOACTIVITY}},
Year = {{2006}},
Volume = {{87}},
Number = {{3}},
Pages = {{239-245}},
Abstract = {{Radon (Rn-222) concentration was measured at indoor workplaces in Japan
   to estimate effective dose to the public from Rn-222 and its progeny.
   Measurements were made from 2000 to 2003 at 705 sites in four categories
   of office, factory, school and hospital. Passive type Rn monitors
   equipped with two sheets of polycarbonate thin films for measuring radon
   concentrations were installed at observation sites and replaced every 3
   months to observe seasonal variations in Rn-222 concentrations. The
   range of annual man Rn-222 concentrations for all sites was 1.4-182 Bq
   m(-3), with the arithmetic mean and standard deviation were 20.8 and
   19.5 Bq m(-3). Annual mean Rn-222 concentration observed at office,
   factory, school and hospital were 22.6, 10.1, 28.4 and 19.8 Bq m(-3),
   respectively. Seasonal variations in Rn-222 con centrations at offices,
   schools and hospitals were similar to those found in dwellings, and
   variations in factories were similar to those found in outdoor
   environments. Rn-222 concentration observed in every quarter period was
   found to decrease as follows: school > office > hospital > factory. ne
   average effective dose to the public due to Rn-222 was estimated to be
   0.41 mSv y(-1) weighted by the working population. Considering the
   Rn-222 exposure in indoor workplaces, effective dose to the general
   public is estimated to be in the range from approximately 0.42 to 0.52
   mv y(-1). (c) 2005 Elsevier Ltd. All rights reserved.}},
