@article{ ISI:000231733300020,
Author = {Zhang, X and Wang, T and Li, C},
Title = {{Different responses of two contrasting wheat genotypes to abscisic acid
   application}},
Journal = {{BIOLOGIA PLANTARUM}},
Year = {{2005}},
Volume = {{49}},
Number = {{4}},
Pages = {{613-616}},
Month = {{DEC}},
Abstract = {{Purpose of this study was to investigate different responses of two
   wheat genotypes (Triticum aestivum L.) from the wet and dry climate
   regions to exogenous abscisic acid (ABA) application under well-watered
   and water-stressed conditions. Exogenous ABA was applied to the leaves
   by spraying and changes in dry matter accumulation and allocation,
   endogenous ABA content and carbon isotope ratio (delta C-13) were
   monitored. The ABA application significantly decreased stem height,
   total biomass, total leaf area, total grain mass and leaf area/mass
   ratio, and significantly increased root/aboveground biomass ratio,
   endogenous ABA content and delta C-13 under well-watered and
   water-stressed conditions. Compared with the wet climate genotype, the
   dry climate genotype was more responsive to exogenous ABA application,
   resulting in lower stem height, total biomass, total leaf area, total
   grain mass and leaf area/mass ratio, and higher root/aboveground biomass
   ratio, endogenous ABA content and delta C-13 under all experimental
   treatments.}},
