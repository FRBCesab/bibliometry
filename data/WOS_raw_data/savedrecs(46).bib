@article{ ISI:000239498100019,
Author = {Sin, G. and Govoreanu, R. and Boon, N. and Schelstraete, G. and
   Vanrolleghem, P. A.},
Title = {{Evaluation of the impacts of model-based operation of SBRs on activated
   sludge microbial community}},
