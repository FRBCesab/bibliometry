@article{ ISI:000233532400001,
Author = {Graef, F and Zughart, W and Hommel, B and Heinrich, U and Stachow, U and
   Werner, A},
Title = {{Methodological scheme for designing the monitoring of genetically
   modified crops at the regional scale}},
