@article{ ISI:000233778100011,
Author = {Rochet, MJ and Trenkel, V and Bellail, R and Coppin, F and Le Pape, O
   and Mahe, JC and Morin, J and Poulard, JC and Schlaich, I and Souplet, A
   and Verin, Y and Bertrand, J},
Title = {{Combining indicator trends to assess ongoing changes in exploited fish
   communities: diagnostic of communities off the coasts of France}},
Journal = {{ICES JOURNAL OF MARINE SCIENCE}},
Year = {{2005}},
Volume = {{62}},
Number = {{8}},
Pages = {{1647-1664}},
Month = {{DEC}},
Abstract = {{We present a method for combining individual indicator results into a
   comprehensive diagnostic of fishing impacts on fish populations and
   communities. A conceptual framework for interpreting combined trends in
   a set of simple indicators is proposed, relying beforehand on
   qualitative expectations anchored in ecological theory. The initial
   state of the community is first assessed using published information.
   Which combinations of trends are acceptable or undesirable is decided,
   depending on the initial status. The indicators are then calculated from
   a time-series and their time trends are estimated as the slopes of
   linear models. Finally, the test results are combined within the
   predefined framework, providing a diagnostic on the dynamics of fishing
   impacts on populations and communities. The method is demonstrated for
   nine coastal and shelf-sea fish communities monitored by French surveys.
   Most communities were persistently or increasingly impacted by fishing.
   In addition, climate change seems to have contributed to changes in East
   Atlantic communities.}},
