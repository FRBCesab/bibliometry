@article{ ISI:000233212100002,
Author = {Maeda-Martinez, AM and Rogers, DC and Worthington, RD},
Title = {{First records of the fairy shrimp Streptocephalus moorei (Branchiopoda :
   Anostraca) from the United States}},
Journal = {{JOURNAL OF CRUSTACEAN BIOLOGY}},
Year = {{2005}},
Volume = {{25}},
Number = {{4}},
Pages = {{547-550}},
Month = {{NOV}},
Abstract = {{We report the first two records of the fairy shrimp Streptocephalus
   moorei from the United States, previously known only from its type
   locality in southeastern Chihuahua, Mexico. An updated morphological
   diagnosis and habitat data are provided. Streptocephalus moorei,
   previously considered as a critically endangered species under the IUCN
   red list criteria, is now assessed as an endangered species on the basis
   of the following criteria: area of occupancy estimated to be less than
   500 km(2), known to exist at no more than five locations, and extreme
   fluctuations in the number of mature individuals. Appropriate measures
   should be taken to protect this species, such as additional survey
   efforts and monitoring of extant populations.}},
