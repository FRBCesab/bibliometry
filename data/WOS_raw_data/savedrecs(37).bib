@article{ ISI:000272650400007,
Author = {Kitchen, Lynn W. and Lawrence, Kendra L. and Coleman, Russell E.},
Title = {{The role of the United States military in the development of vector
   control products, including insect repellents, insecticides, and bed
   nets}},
Journal = {{JOURNAL OF VECTOR ECOLOGY}},
