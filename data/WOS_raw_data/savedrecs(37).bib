@article{ ISI:000270152300019,
Author = {Wulder, Michael A. and Ortlepp, Stephanie M. and White, Joanne C. and
   Coops, Nicholas C. and Coggins, Sam B.},
Title = {{Monitoring the impacts of mountain pine beetle mitigation}},
