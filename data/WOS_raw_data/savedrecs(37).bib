@article{ ISI:000268298800008,
Author = {Chen, Yanping and Evans, Jay D. and Zhou, Liang and Boncristiani,
   Humberto and Kimura, Kiyoshi and Xiao, Tieguang and Litkowski, A. M. and
   Pettis, Jeffery S.},
Title = {{Asymmetrical coexistence of Nosema ceranae and Nosema apis in honey bees}},
Journal = {{JOURNAL OF INVERTEBRATE PATHOLOGY}},
Year = {{2009}},
Volume = {{101}},
Number = {{3}},
