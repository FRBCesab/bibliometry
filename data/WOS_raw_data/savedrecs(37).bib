@article{ ISI:000265917200035,
Author = {Sesli, Faik Ahmet and Karsli, Fevzi and Colkesen, Ismail and Akyol,
   Nihat},
Title = {{Monitoring the changing position of coastlines using aerial and
   satellite image data: an example from the eastern coast of Trabzon,
   Turkey}},
