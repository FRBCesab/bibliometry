@article{ ISI:000268150600002,
Author = {Huang, Changjiang and Lin, Xiaoping and Lin, Junda and Du, Hong and
   Dong, Qiaoxiang},
Title = {{Population dynamics of Pseudo-nitzschia pungens in Zhelin Bay, China}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
