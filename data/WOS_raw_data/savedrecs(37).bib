@article{ ISI:000268138000060,
Author = {Gurney, Kevin R. and Mendoza, Daniel L. and Zhou, Yuyu and Fischer, Marc
   L. and Miller, Chris C. and Geethakumar, Sarath and Du Can, Stephane De
   La Rue},
Title = {{High Resolution Fossil Fuel Combustion CO2 Emission Fluxes for the
   United States}},
