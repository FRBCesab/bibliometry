@article{ ISI:000267824400016,
Author = {Rao, Harsha Tondoti Sathyanarayana and Yamakanamardi, Sadanand Mallappa
   and Mallaiah, Mahadeveswamy},
Title = {{Temporal variation in the specific growth rate of bacterioplankton in
   the River Cauvery and its four down stream tributaries in Karnataka
   State, India}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
