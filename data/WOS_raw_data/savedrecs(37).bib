@article{ ISI:000265515900012,
Author = {Weston, Nathaniel B. and Hollibaugh, James T. and Joye, Samantha B.},
Title = {{Population growth away from the coastal zone: Thirty years of land use
   change and nutrient export in the Altamaha River, GA}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
