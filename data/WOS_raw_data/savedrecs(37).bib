@article{ ISI:000266133900018,
Author = {Bloomquist, Craig K. and Nielsen, Clayton K.},
Title = {{A Remote Videography System for Monitoring Beavers}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{2009}},
Volume = {{73}},
Number = {{4}},
Pages = {{605-608}},
Month = {{MAY}},
Abstract = {{We developed a remote videography system for monitoring behavior and
   demography of beavers (Castor canadensis) inside lodges and bank dens.
   Videography systems taped 6 beaver colonies for 1,080 hours and recorded
   300 hours of beaver activity. Upon viewing videotape, we could
   characterize 86\% of beaver activity into 1 of 12 behavioral categories.
   Of 26 individually marked beavers, we observed 8 (31\%) on videotape and
   obtained a complete count of kits in 4 of 5 colonies. The remote
   videography system was generally useful and can provide wildlife
   biologists with demographic and behavioral information to support
   population modeling and management programs for beavers. (JOURNAL OF
   WILDLIFE MANAGEMENT 73(4): 605-608; 2009)}},
