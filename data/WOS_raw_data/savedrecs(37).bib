@article{ ISI:000267155600016,
Author = {Schiavone, Alessandra and Corsolini, Simonetta and Borghesi, Nicoletta
   and Focardi, Silvano},
Title = {{Contamination profiles of selected PCB congeners, chlorinated
   pesticides, PCDD/Fs in Antarctic fur seal pups and penguin eggs}},
Journal = {{CHEMOSPHERE}},
