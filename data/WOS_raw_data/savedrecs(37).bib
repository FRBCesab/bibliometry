@article{ ISI:000268907700011,
Author = {Hoerger, Corinne C. and Wettstein, Felix E. and Hungerbuehler, Konrad
   and Bucheli, Thomas D.},
Title = {{Occurrence and Origin of Estrogenic Isoflavones in Swiss River Waters}},
