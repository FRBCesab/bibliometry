@article{ ISI:000268741300004,
Author = {Tai, Vera and Palenik, Brian},
Title = {{Temporal variation of Synechococcus clades at a coastal Pacific Ocean
   monitoring site}},
Journal = {{ISME JOURNAL}},
Year = {{2009}},
