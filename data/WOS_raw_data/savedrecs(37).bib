@article{ ISI:000271408700001,
Author = {Greger, Paul D. and Hall, Derek B.},
Title = {{BURROW OCCUPANCY PATTERNS OF THE WESTERN BURROWING OWL IN SOUTHERN
   NEVADA}},
Journal = {{WESTERN NORTH AMERICAN NATURALIST}},
