@article{ ISI:000268014200013,
Author = {Giltrap, Michelle and Macken, Ailbhe and Davoren, Maria and Minchin, Dan
   and McGovern, Evin and Foley, Barry and Strand, Jakob and McHugh,
   Brendan},
Title = {{USE OF CAGED NUCELLA LAPILLUS AND CRASSOSTREA GIGAS TO MONITOR
   TRIBUTYLTIN-INDUCED BIOEFFECTS IN IRISH COASTAL WATERS}},
