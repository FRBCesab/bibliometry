@article{ ISI:000283412300014,
Author = {Molloy, Philip P. and Anticamara, Jonathan A. and Rist, Janna L. and
   Vincent, Amanda C. J.},
Title = {{Frugal conservation: What does it take to detect changes in fish
   populations?}},
Journal = {{BIOLOGICAL CONSERVATION}},
