@article{ ISI:000283406300016,
Author = {Furuya, Seiichi and Mochizuki, Mai and Saito, Seiya and Kobayashi,
   Hironori and Takayanagi, Tsutomu and Suzuki, Shunji},
Title = {{Monitoring of Qol fungicide resistance in Plasmopara viticola
   populations in Japan}},
Journal = {{PEST MANAGEMENT SCIENCE}},
Year = {{2010}},
Volume = {{66}},
Number = {{11}},
Pages = {{1268-1272}},
Month = {{NOV}},
Abstract = {{BACKGROUND: The increasing occurrence of Qol fungicide resistance in
   Plasmopara viticola (Berk. \& MA Curtis) Berl. \& De Toni populations is
   becoming a serious problem in the control of grapevine downy mildew. In
   Japan, the existence of Qol-fungicide-resistant P. viticola was reported
   in 2009.
   RESULTS: The Qol fungicide resistance in P. viticola samples collected
   from vineyards in Japan in 2008 and 2009 was monitored. Resistant P.
   viticola were detected in the regions where Qol fungicides have been
   introduced in accordance with the pest management programme, whereas in
   Hokkaido vineyards, where Qol fungicides have not yet been introduced,
   Qol-fungicide-resistant P. viticola were not found.
   CONCLUSION: Japan comprises thousands of islands and is physically
   isolated from other countries by the sea. Monitoring the emergence,
   incidence and distribution of Qol fungicide resistance in P. viticola
   populations in Japan is necessary to improve pest management strategies
   for downy mildew disease in Japanese vineyards. (C) 2010 Society of
   Chemical Industry}},
