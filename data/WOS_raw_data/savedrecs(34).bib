@article{ ISI:000282999300011,
Author = {Chang, Kyu Sik and Shin, E. Hyun and Jung, Jin Sung and Park, Chan and
   Ahn, Young-Joon},
Title = {{Monitoring for insecticide resistance in field-collected populations of
   Blattella germanica (Blattaria: Blattellidae)}},
Journal = {{JOURNAL OF ASIA-PACIFIC ENTOMOLOGY}},
Year = {{2010}},
Volume = {{13}},
Number = {{4}},
Pages = {{309-312}},
Month = {{DEC}},
Abstract = {{Seven field-collected populations of German cockroach, Blattella
   germanica (L.) (ICN, DNH, CHN, DGU, BSN, GSN, and GWJ), were tested for
   susceptibility to eight different insecticides by a topical application
   method, and were compared to an insecticide-susceptible KSS strain.
   Marked regional variations of insecticide susceptibility were observed.
   Extremely high to high levels of resistance were observed in bifenthrin
   {[}resistance ratio (RR), 46-159], deltamethrin (RR, 61-160), and
   esfenvalerate {[}RR, 70-270; except for BSN females (RR, 20) and GSN
   females (RR, 24)]. Low to moderate levels of resistance were observed in
   cypermethrin {[}RR, 16-29; except for DGU females (RR, 88) and BSN
   females (RR, 41)], permethrin ERR, 11-39; except for DNH females (RR,
   110) and BSN females (RR, 44)], chlorpyrifos {[}RR, 2-13; except for GSN
   and GWJ females (RR, ca. 140)], chlorpyrifos-methyl (RR, 2-8), and
   fenthion (RR, 8-17). All of the colonies were susceptible to one or more
   of the insecticides examined. These results indicate that careful
   selection and rotational use of these insecticides should result in
   continued satisfactory control against field populations of German
   cockroach. (C) Korean Society of Applied Entomology, Taiwan
   Entomological Society and Malaysian Plant Protection Society, 2010
   Published by Elsevier B.V. All rights reserved.}},
