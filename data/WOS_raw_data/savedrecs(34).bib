@article{ ISI:000284301400027,
Author = {Fabrick, Jeffrey A.},
Title = {{Insights into the Genetics and Molecular Mechanisms of Pink Bollworm
   Resistance to Cry Toxins}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
