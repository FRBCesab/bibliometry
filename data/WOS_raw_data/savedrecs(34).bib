@article{ ISI:000284301400022,
Author = {Gao, Y. L. and An, J. J. and Liu, C. X. and Wu, K. M.},
Title = {{Monitoring of Bt Resistance from Field-collected Helicoverpa armigera
   Populations in Northern China}},
Journal = {{SOUTHWESTERN ENTOMOLOGIST}},
