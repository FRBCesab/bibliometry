@article{ ISI:000282645700012,
Author = {Cooper, Tim F. and Paixao, Tiago and Heinemann, Jack A.},
Title = {{Within-host competition selects for plasmid-encoded toxin-antitoxin
   systems}},
Journal = {{PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2010}},
Volume = {{277}},
Number = {{1697}},
