@article{ ISI:000281657500009,
Author = {Laikre, Linda and Schwartz, Michael K. and Waples, Robin S. and Ryman,
   Nils and GeM Working Grp},
Title = {{Compromising genetic diversity in the wild: unmonitored large-scale
   release of plants and animals}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{2010}},
Volume = {{25}},
Number = {{9}},
Pages = {{520-529}},
Month = {{SEP}},
Abstract = {{Large-scale exploitation of wild animals and plants through fishing,
   hunting and logging often depends on augmentation through releases of
   translocated or captively raised individuals. Such releases are
   performed worldwide in vast numbers. Augmentation can be demographically
   and economically beneficial but can also cause four types of adverse
   genetic change to wild populations: (1) loss of genetic variation, (2)
   loss of adaptations, (3) change of population composition, and (4)
   change of population structure. While adverse genetic impacts are
   recognized and documented in fisheries, little effort is devoted to
   actually monitoring them. In forestry and wildlife management, genetic
   risks associated with releases are largely neglected. We outline key
   features of programs to effectively monitor consequences of such
   releases on natural populations.}},
