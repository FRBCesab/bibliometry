@article{ ISI:000282204000007,
Author = {Alcaide, Miguel and Cadahia, Luis and Lambertucci, Sergio A. and Negro,
   Juan J.},
Title = {{NONINVASIVE ESTIMATION OF MINIMUM POPULATION SIZES AND VARIABILITY OF
   THE MAJOR HISTOCOMPATIBILITY COMPLEX IN THE ANDEAN CONDOR}},
