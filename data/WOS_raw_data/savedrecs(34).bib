@article{ ISI:000281215100008,
Author = {Crawford, Justin A. and Anthony, Robert G. and Forbes, James T. and
   Lorton, Glenn A.},
Title = {{Survival and causes of mortality for pygmy rabbits (Brachylagus
   idahoensis) in Oregon and Nevada}},
Journal = {{JOURNAL OF MAMMALOGY}},
