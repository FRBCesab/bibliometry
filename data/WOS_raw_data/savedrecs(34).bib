@article{ ISI:000279290700017,
Author = {Ausband, David E. and Mitchell, Michael S. and Doherty, Kevin and Zager,
   Peter and Mack, Curt M. and Holyan, Jim},
Title = {{Surveying Predicted Rendezvous Sites to Monitor Gray Wolf Populations}},
