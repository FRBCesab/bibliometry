@article{ ISI:000279666700007,
Author = {Worrapimphong, Kobchai and Gajaseni, Nantana and Le Page, Christophe and
   Bousquet, Francois},
Title = {{A companion modeling approach applied to fishery management}},
Journal = {{ENVIRONMENTAL MODELLING \& SOFTWARE}},
