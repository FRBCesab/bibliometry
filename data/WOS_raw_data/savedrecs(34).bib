@article{ ISI:000285095400014,
Author = {Scarton, Francesco},
Title = {{Long Term Decline of a Common Tern (Sterna hirundo) Population Nesting
   in Salt Marshes in Venice Lagoon, Italy}},
Journal = {{WETLANDS}},
Year = {{2010}},
Volume = {{30}},
Number = {{6}},
Pages = {{1153-1159}},
Month = {{DEC}},
Abstract = {{Each year between 1989 and 2008, I monitored Common Tern colonies in the
   lagoon of Venice (Italy). This population nests only on salt marsh
   islets, which are subjected to flooding during high tides. The number of
   breeding pairs decreased significantly from 832 to 109, with an average
   rate of decrease of -10.1\%. Overall, 188 colonies were found, ranging
   from two to 583 pairs; median colony size was 57 pairs, and it decreased
   significantly. The mean turnover rate was high at 41.5\%, and it ranged
   each year between 7.7\% and 60\%. The observed trends could be
   attributed to the increase in mean sea level that occurred over the last
   20 years (0.5 cm/yr). In the May-June period, the number of days with
   high tides leading to a complete flooding of the breeding site
   increased. Between 1989 and 1998 there was on average 4.4 days with
   floodings. This increased to 8.4 days between 1999 and 2008. Therefore,
   sea level rise could be a major contributor to declining Common Tern
   populations in the lagoon of Venice.}},
