@article{ ISI:000280653200006,
Author = {Chaix, G. and Vigneron, P. and Razafimaharo, V. and Hamon, S.},
Title = {{IMPROVED MANAGEMENT OF MALAGASY EUCALYPTUS GRANDIS SEED ORCHARDS USING
   MICROSATELLITES AND PATERNITY ASSIGNMENT}},
Journal = {{JOURNAL OF TROPICAL FOREST SCIENCE}},
