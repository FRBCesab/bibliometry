@article{ ISI:000284339300008,
Author = {Madikiza, Zimkitha J. K. and Bertolino, Sandro and Baxter, Roderick M.
   and San, Emmanuel Do Linh},
Title = {{Seasonal, Sexual, and Age-Related Variations in the Live-Trapping
   Success of Woodland Dormice Graphiurus murinus}},
