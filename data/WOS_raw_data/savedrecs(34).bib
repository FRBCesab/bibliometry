@article{ ISI:000281474000023,
Author = {Ribic, Christine A. and Sheavly, Seba B. and Rugg, David J. and Erdmann,
   Eric S.},
Title = {{Trends and drivers of marine debris on the Atlantic coast of the United
   States 1997-2007}},
Journal = {{MARINE POLLUTION BULLETIN}},
