@article{ ISI:000301023700058,
Author = {Cowie, Christine T. and Rose, Nectarios and Gillett, Robert and Walter,
   Scott and Marks, Guy B.},
Title = {{Redistribution of Traffic Related Air Pollution Associated with a New
   Road Tunnel}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2012}},
