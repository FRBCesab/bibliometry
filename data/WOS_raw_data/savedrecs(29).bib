@article{ ISI:000300772500031,
Author = {Liu, Hong Yan and Xiong, Fei and Duan, Xin Bin and Chen, Da Qing},
Title = {{Isolation and characterization of polymorphic microsatellite loci from
   elongate loach (Leptobotia elongate), a threatened fish species endemic
   to the Yangtze River}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
Year = {{2012}},
Volume = {{4}},
Number = {{1}},
Pages = {{129-131}},
Month = {{MAR}},
Abstract = {{Elongate loach (Leptobotia elongate) is a threatened fish species
   endemic to the Yangtze River in China. A first set of 22 polymorphism
   microsatellite loci isolated from this species was characterized. The
   number of alleles, observed and expected heterozygosity ranged from 2 to
   12, from 0.2667 to 1.000 and from 0.2391 to 0.9011, respectively. Seven
   loci significantly deviated from Hardy-Weinberg equilibrium after
   Bonferroni correction. The polymorphism information content ranged from
   0.204 to 0.857. Ten loci were highly informative, 11 were moderately
   informative, and one was slightly informative. These polymorphic
   microsatellite markers will be useful for genetic monitoring of natural
   populations and management of breeding stocks in hatcheries.}},
