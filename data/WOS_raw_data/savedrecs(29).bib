@article{ ISI:000302804200015,
Author = {Muxagata, Erik and Amaral, Waldemar J. A. and Barbosa, Carla N.},
Title = {{Acartia tonsa production in the Patos Lagoon estuary, Brazil}},
Journal = {{ICES JOURNAL OF MARINE SCIENCE}},
Year = {{2012}},
Volume = {{69}},
Number = {{3}},
Pages = {{475-482}},
Month = {{MAY}},
Abstract = {{Acartid copepods are abundant in estuaries, harbours, and semi-enclosed
   waters. A monitoring programme with the objective of evaluating copepod
   production, particularly that of Acartia tonsa, the dominant taxon in
   the Patos Lagoon estuary, was implemented. Zooplankton samples are
   collected monthly from two fixed locations with bongo nets 30 cm in
   diameter with nets of 90 and 200 mm mesh. Over a full year, 33
   quantitative samples were analysed; copepods represented 40\% of the
   organisms collected with the 90-mu m mesh. Adults and copepodites of A.
   tonsa accounted for similar to 19\% of the total copepod fauna at both
   locations. Daily production rates of A. tonsa were calculated as 0.40 mg
