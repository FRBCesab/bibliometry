@article{ ISI:000299107900010,
Author = {Skuterud, Lavrans and Thorring, Havard},
Title = {{AVERTED DOSES TO NORWEGIAN SAMI REINDEER HERDERS AFTER THE CHERNOBYL
   ACCIDENT}},
Journal = {{HEALTH PHYSICS}},
Year = {{2012}},
Volume = {{102}},
Number = {{2}},
Pages = {{208-216}},
Month = {{FEB}},
Abstract = {{The Chernobyl fallout is an enduring challenge to reindeer husbandry in
   Norway, and South Sami reindeer herders in central and southern Norway
   are as contaminated by Cs-137 as inhabitants close to Chernobyl.
   Therefore, Norwegian authorities continuously recommend to these
   reindeer herders the use of countermeasures to reduce their intake of
   Cs-137. In this study, the authors have applied data on contamination
   levels in reindeer, results of dietary surveys, and whole body
   monitoring data in low and high contaminated areas to estimate the
   effectiveness of countermeasures and resulting averted doses to the
   reindeer herders. In the most contaminated area, the various
   countermeasures applied reduced radiocesium ingestion doses during
   1986-2009 by about 73\%, to an integrated dose of about 17 mSv. However,
   to comply with the recommended Cs-137 ingestion dose limit of 1 mSv
   y(-1), the study indicates that reindeer herders in the most
   contaminated areas will need to carry on with their countermeasures for
   another 10-15 y. Furthermore, the study indicates that whole body
   monitoring is an important tool to assess individual doses and
   countermeasure effectiveness in long-term management of a contamination
   situation and that such monitoring may be required to reach long-term
   reference levels. Health Phys. 102(2):208-216; 2012}},
