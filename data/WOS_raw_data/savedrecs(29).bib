@article{ ISI:000300436400001,
Author = {Young, Sam and Skerratt, Lee F. and Mendez, Diana and Speare, Rick and
   Berger, Lee and Steele, Mike},
Title = {{Using community surveillance data to differentiate between emerging and
   endemic amphibian diseases}},
Journal = {{DISEASES OF AQUATIC ORGANISMS}},
