@article{ ISI:000301087800044,
Author = {Martin, Florence and Torelli, Stephane and Le Paslier, Denis and
   Barbance, Agnes and Martin-Laurent, Fabrice and Bru, David and Geremia,
   Roberto and Blake, Gerard and Jouanneau, Yves},
Title = {{Betaproteobacteria dominance and diversity shifts in the bacterial
   community of a PAH-contaminated soil exposed to phenanthrene}},
Journal = {{ENVIRONMENTAL POLLUTION}},
Year = {{2012}},
Volume = {{162}},
Pages = {{345-353}},
Month = {{MAR}},
Abstract = {{In this study, the PAH-degrading bacteria of a constructed wetland
   collecting road runoff has been studied through DNA stable isotope
   probing. Microcosms were spiked with C-13-phenanthrene at 34 or 337 ppm,
   and bacterial diversity was monitored over a 14-day period. At 337 ppm,
   PAH degraders became dominated after 5 days by Betaproteobacteria,
   including novel Acidovorax, Rhodoferax and Hydrogenophaga members, and
   unknown bacteria related to Rhodocyclaceae. The prevalence of
   Betaproteobacteria was further demonstrated by phylum-specific
   quantitative PCR, and was correlated with a burst of phenanthrene
   mineralization. Striking shifts in the population of degraders were
   observed after most of the phenanthrene had been removed. Soil exposed
   to 34 ppm phenanthrene showed a similar population of degraders, albeit
   only after 14 days. Results demonstrate that specific Betaproteobacteria
   are involved in the main response to soil PAH contamination, and
   illustrate the potential of SIP approaches to investigate PAH
   biodegradation in soil. (C) 2011 Elsevier Ltd. All rights reserved.}},
