@article{ ISI:000300483500010,
Author = {Le, C. N. and Mendes, R. and Kruijt, M. and Raaijmakers, J. M.},
Title = {{Genetic and Phenotypic Diversity of Sclerotium rolfsii in Groundnut
   Fields in Central Vietnam}},
Journal = {{PLANT DISEASE}},
