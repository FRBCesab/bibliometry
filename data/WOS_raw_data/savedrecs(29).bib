@article{ ISI:000300465900007,
Author = {Shrout, Joshua D. and Nerenberg, Robert},
Title = {{Monitoring Bacterial Twitter: Does Quorum Sensing Determine the Behavior
   of Water and Wastewater Treatment Biofilms?}},
