@article{ ISI:000304543200030,
Author = {Sebrie, Ernesto M. and Schoj, Veronica and Travers, Mark J. and McGaw,
   Barbara and Glantz, Stanton A.},
Title = {{Smokefree Policies in Latin America and the Caribbean: Making Progress}},
