@article{ ISI:000309538300013,
Author = {Robinson, Robert A. and Baillie, Stephen R. and King, Ruth},
Title = {{Population processes in European Blackbirds Turdus merula: a state-space
   approach}},
Journal = {{JOURNAL OF ORNITHOLOGY}},
Year = {{2012}},
Volume = {{152}},
Number = {{2}},
Pages = {{S419-S433}},
