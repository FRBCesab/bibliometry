@article{ ISI:000336561400014,
Author = {Varikou, Kyriaki and Garantonakis, Nikos and Birouraki, Athanasia},
Title = {{Response of olive fruit fly Bactrocera oleae to various attractant
   combinations, in orchards of Crete}},
Journal = {{BULLETIN OF INSECTOLOGY}},
