@article{ ISI:000336390400011,
Author = {Starichenko, V. I. and Lyubashevskiy, N. M. and Modorov, M. V. and
   Chibiryak, M. V.},
Title = {{Skeletal Sr-90 as a marker of migration activity of murine rodents in
   the zone of the Eastern Ural Radioactive Trace}},
Journal = {{RUSSIAN JOURNAL OF ECOLOGY}},
Year = {{2014}},
Volume = {{45}},
Number = {{3}},
Pages = {{231-241}},
Month = {{MAY}},
Abstract = {{Analysis and generalization of long-term monitoring data on the
   accumulation of Sr-90 in the bone tissue of rodents inhabiting the
   East-Ural Radioactive Trace and the adjacent control area have shown
   that the skeletal content of Sr-90 depends on the level of radionuclide
   contamination of soil in animal habitats. An assessment of animal
   migration between contaminated and clean areas by this criterion has
   shown that the proportion of migrants does not exceed 10\%. On this
   basis, the conclusion is drawn concerning relative isolation of rodent
   populations in the EURT zone.}},
