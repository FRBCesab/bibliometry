@article{ ISI:000339872800002,
Author = {Singh, Randeep and Qureshi, Qamar and Sankar, Kalyanasundaram and
   Krausman, Paul R. and Goyal, Surendra Prakash},
Title = {{Reproductive characteristics of female Bengal tigers, in Ranthambhore
   Tiger Reserve, India}},
Journal = {{EUROPEAN JOURNAL OF WILDLIFE RESEARCH}},
