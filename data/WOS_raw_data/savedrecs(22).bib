@article{ ISI:000343214800001,
Author = {Mannion, Catharine M. and Derksen, Andrew I. and Seal, Dakshina R. and
   Osborne, Lance S. and Martin, Cliff G.},
Title = {{Population Dynamics of Scirtothrips dorsalis (Thysanoptera: Thripidae)
   and Other Thrips Species on Two Ornamental Host Plant Species in
   Southern Florida}},
