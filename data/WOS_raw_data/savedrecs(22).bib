@article{ ISI:000334176500014,
Author = {Schaeffler, Livia and Kappeler, Peter M.},
Title = {{Distribution and Abundance of the World's Smallest Primate, Microcebus
   berthae, in Central Western Madagascar}},
Journal = {{INTERNATIONAL JOURNAL OF PRIMATOLOGY}},
