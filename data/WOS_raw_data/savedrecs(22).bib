@article{ ISI:000341282700005,
Author = {Schulz, Andrea and Mathiassen, Solvejg K. and de Mol, Friederike},
Title = {{Approaches to early detection of herbicide resistance in Apera
   spica-venti regarding intra- and inter-field situations}},
Journal = {{JOURNAL OF PLANT DISEASES AND PROTECTION}},
