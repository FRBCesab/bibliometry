@article{ ISI:000337259100080,
Author = {Bijlsma, Lubertus and Serrano, Roque and Ferrer, Carlos and Tormos,
   Isabel and Hernandez, Felix},
Title = {{Occurrence and behavior of illicit drugs and metabolites in sewage water
   from the Spanish Mediterranean coast (Valencia region)}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2014}},
Volume = {{487}},
