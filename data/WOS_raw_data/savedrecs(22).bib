@article{ ISI:000333898300002,
Author = {De La Rosa-Reyna, Xochitl F. and Sifuentes-Rincon, Ana M. and
   Parra-Bracamonte, Gaspar M. and Arellano-Vera, Williams},
Title = {{Identification of Two Channel Catfish Stocks, Ictalurus punctatus,
   Cultivated in Northeast Mexico}},
Journal = {{JOURNAL OF THE WORLD AQUACULTURE SOCIETY}},
