@article{ ISI:000343641700009,
Author = {Walker, Ryan and Rafeliarisoa, Tsilavo and Currylow, Andrea and
   Rakotoniaina, Jean Claude and Louis, Jr., Edward},
Title = {{Short term monitoring reveals the rapid decline of southern Madagascar's
   Critically Endangered tortoise species}},
Journal = {{HERPETOLOGICAL JOURNAL}},
Year = {{2014}},
Volume = {{24}},
Number = {{3}},
Pages = {{193-196}},
Month = {{JUL}},
Abstract = {{Southern Madagascar supports two of the world's most threatened
   chelonians, Pyxis arachnoides and Astrochelys radiata, both thought to
   be rapidly declining as a result of habitat loss and poaching; however,
   to date quantitative data on this decline is lacking. We applied a
   conventional distance sampling procedure, monitoring populations twice
   over a 24-month period across the species' respective distributions.
   Population density for P. arachnoides dropped from 2.4 tortoises/ha
   (95\% Cl; 1.6-3.4) to 1.5/ha (95\% Cl; 0.8-2.8). Astrochelys radiata
   dropped from 2.1 tortoises/ha (95\% Cl 1.2-3.5) to 1.5/h. (95\% Cl
   0.7-3.3). Local community-based conservation initiatives need to be
   expanded to combat this decline.}},
