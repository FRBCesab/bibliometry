@article{ ISI:000337259100082,
Author = {Yargeau, Viviane and Taylor, Bryanne and Li, Hongxia and Rodayan, Angela
   and Metcalfe, Chris D.},
Title = {{Analysis of drugs of abuse in wastewater from two Canadian cities}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2014}},
Volume = {{487}},
