@article{ ISI:000337596000013,
Author = {Leclercq, E. and Grant, B. and Davie, A. and Migaud, H.},
Title = {{Gender distribution, sexual size dimorphism and morphometric sexing in
   ballan wrasse Labrus bergylta}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
Year = {{2014}},
Volume = {{84}},
Number = {{6}},
Pages = {{1842-1862}},
Month = {{JUN}},
Abstract = {{In wild ballan wrasse Labrus bergylta, mass-length relationships were
   not different between genders, and positive allometry was found in the
   mixed-gender population. Male-biased sexual size dimorphism was
   significant and the most effective morphometric method for sexing L.
   bergylta outside of the species spawning window used body mass (MB in
   g), total body length (LT in mm) and Fulton's condition factor (K) as
   discriminant variables to predict gender with 91\% accuracy. The
   discriminant score (SD) of a specimen can be calculated as SD=0 center
   dot 01MB-0 center dot 016LT-3 center dot 835K+6 center dot 252 to
   predict its gender as female or male if SD is<1 center dot 459 or SD
   is>1 center dot 504, respectively. There was a potential trend towards
   earlier sexual inversion compared to previous studies at comparable
   latitudes. Sex change is a phenotypically plastic trait under social
   control in haremic fishes and should be monitored in increasingly
   exploited L. bergylta. (C) 2014 The Fisheries Society of the British
   Isles}},
