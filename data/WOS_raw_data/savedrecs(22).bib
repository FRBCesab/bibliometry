@article{ ISI:000333931400007,
Author = {Jones, Hugh A. and Byrne, Maria},
Title = {{Changes in the distributions of freshwater mussels (Unionoida: Hyriidae)
   in coastal south-eastern Australia and implications for their
   conservation status}},
Journal = {{AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS}},
