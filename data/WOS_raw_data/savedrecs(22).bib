@article{ ISI:000342565000041,
Author = {Yang, Yajun and Dong, Biqin and Xu, Hongxing and Zheng, Xusong and Tian,
   Junce and Heong, Kongleun and Lu, Zhongxian},
Title = {{Decrease of Insecticide Resistance Over Generations Without Exposure to
   Insecticides in Nilaparvata lugens (Hemipteran: Delphacidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
