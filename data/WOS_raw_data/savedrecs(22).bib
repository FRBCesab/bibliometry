@article{ ISI:000336603600011,
Author = {Oppel, Steffen and Cassini, Andrew and Fenton, Calvin and Daley, James
   and Gray, Gerard},
Title = {{Population status and trend of the Critically Endangered Montserrat
   Oriole}},
