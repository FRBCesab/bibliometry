@article{ ISI:000338452000047,
Author = {Asadi, Somayeh and Hassan, Marwa and Nadiri, Ataallah and Dylla, Heather},
Title = {{Artificial intelligence modeling to evaluate field performance of
   photocatalytic asphalt pavement for ambient air purification}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
