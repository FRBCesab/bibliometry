@article{ ISI:000343664100008,
Author = {Vesterlund, Salla-Riikka and Sorvari, Jouni},
Title = {{Longevity of starved bumblebee queens (Hymenoptera: Apidae) is shorter
   at high than low temperatures}},
Journal = {{EUROPEAN JOURNAL OF ENTOMOLOGY}},
