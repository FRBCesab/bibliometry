@article{ ISI:000178032700011,
Author = {Satar, S and Yokomi, R},
Title = {{Effect of temperature and host on development of Brachycaudus schwartzi
   (Homoptera : Aphididae)}},
Journal = {{ANNALS OF THE ENTOMOLOGICAL SOCIETY OF AMERICA}},
