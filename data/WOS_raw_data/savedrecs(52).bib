@inproceedings{ ISI:000221379400022,
Author = {Mendes, JFG and Silva, L},
Editor = {{Beriatoss, E and Brebbia, CA and Coccossis, H and Kungolos, A}},
Title = {{Evaluating critical noise disturbance zones in a mid-sized city}},
