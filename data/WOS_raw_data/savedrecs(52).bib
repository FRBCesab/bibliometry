@article{ ISI:000178017600012,
Author = {Kindt, AC and Small, PF},
Title = {{Correlation between temperature, colonization rate, and population
   density of the diatom Cocconeis placentula in freshwater streams}},
Journal = {{JOURNAL OF FRESHWATER ECOLOGY}},
Year = {{2002}},
Volume = {{17}},
Number = {{3}},
Pages = {{441-445}},
Month = {{SEP}},
Abstract = {{We examined the correlation between water temperature, colonization
   rate, and population density of the freshwater diatom Cocconeis
   placentula over two complete annual cycles in two southeastern
   Pennsylvania streams. Previous research in these streams showed that
   differences in diatom community structure between spring and fall were
   not explained by day length, so water temperature was investigated as
   the controlling factor in the density of a single common genus in those
   communities. During the June 1992 to June 1993 period, the diatom
   population was monitored continuously at two-week intervals using 14-day
   colonized glass slides; during 2000-2001, colonization and monitoring
   was over 28-day intervals. Average monthly temperatures and diatom
   densities were analyzed with a Spearman rank-correlation test for each
   stream. There were significant positive correlations between the
   population density of C. placentula and water temperature at all
   population sizes in both streams in both years.}},
