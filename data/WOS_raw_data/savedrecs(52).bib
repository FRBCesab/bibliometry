@article{ ISI:000178338300009,
Author = {Lekve, K and Stenseth, NC and Gjosaeter, J and Doledec, S},
Title = {{Species richness and environmental conditions of fish along the
   Norwegian Skagerrak coast}},
Journal = {{ICES JOURNAL OF MARINE SCIENCE}},
Year = {{2002}},
Volume = {{59}},
