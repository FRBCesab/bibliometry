@article{ ISI:000180209000008,
Author = {Servin, R and Aguilar, R and Martinez, JL and Troyo, E and Ortega, A},
Title = {{Monitoring of resistance to three insecticides on pepper weevil
   (Anthonomus eugenii) in populations from Baja California Sur, Mexico}},
Journal = {{INTERCIENCIA}},
Year = {{2002}},
Volume = {{27}},
Number = {{12}},
Pages = {{691-694}},
Month = {{DEC}},
Abstract = {{Toxicity levels of three insecticides, carbaryl, endosulfan and
   methomyl, were determined in populations of the pepper weevil Anthonomus
   eugenii. Insect populations were obtained front three locations in Baja
   California Sur, Mexico: Benito Juarez (27degrees53'N, 113degrees46'W),
   San Jose Viejo (23degrees07'N, 109degrees43'W), and San Juan de los
   Planes (23degrees58'N, 109degrees56'W). The highest LC50 values were
   found for carbaryl (178.4, 635.4 and 5462.1 mug/ml), endosulfan was
   intermediate (36.5, 34.1 and 47.1 mug/ml, respectively); and the lowest
   LC50 values corresponded to methomyl (1.3, 1.7 and 2.7 mug/ml), to
   Benito Juarez, San Jose Viejo and San Juan de los Planes, respectively.
   It is concluded can be inferred that pepper weevil populations in the
   farming zone of San Juan de los Planes are resistant to the three
   insecticides used, and are significantly more resistant to carbaryl,
   than are pepper, weevil populations in San Jose Viejo and Benito Juarez.
   Levels of insecticide susceptibility correspond to the amounts of
   insecticides used against the pepper weevil in the three locations, and
   are a response to the selective pressure exerted by those chemicals.}},
