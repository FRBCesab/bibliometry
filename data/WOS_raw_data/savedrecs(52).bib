@article{ ISI:000177383400013,
Author = {Neckles, HA and Dionne, M and Burdick, DM and Roman, CT and Buchsbaum, R
   and Hutchins, E},
Title = {{A monitoring protocol to assess tidal restoration of salt marshes on
   local and regional scales}},
Journal = {{RESTORATION ECOLOGY}},
