
@article{ ISI:000185884200007,
Author = {Prise, KM and Folkard, M and Michael, BD},
Title = {{A review of the bystander effect and its implications for low-dose
   exposure}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2003}},
