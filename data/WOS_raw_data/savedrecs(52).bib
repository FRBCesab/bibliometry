@article{ ISI:000177448000027,
Author = {Forcioli, D and Frey, B and Frey, JF},
Title = {{High nucleotide diversity in the para-like voltage-sensitive sodium
   channel gene sequence in the western flower thrips (Thysanoptera :
   Thripidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
