@article{ ISI:000184016100016,
Author = {Fuerhacker, M},
Title = {{Bisphenol A emission factors from industrial sources and elimination
   rates in a sewage treatment plant}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2003}},
