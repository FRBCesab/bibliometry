@article{ ISI:000180932100004,
Author = {Beaty, BJ and Mackie, RS and Mattingly, KS and Carlson, JO and
   Rayms-Keller, A},
Title = {{The midgut epithelium of aquatic arthropods: A critical target organ in
   environmental toxicology}},
Journal = {{ENVIRONMENTAL HEALTH PERSPECTIVES}},
Year = {{2002}},
Volume = {{110}},
Number = {{6}},
Pages = {{911-914}},
