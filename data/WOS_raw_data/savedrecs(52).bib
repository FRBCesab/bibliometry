@article{ ISI:000178017600016,
Author = {Jia, QX and Xu, N and Zhu, J and Zheng, MP},
Title = {{Laboratory studies on the influence of temperature on the reproductive
   potential of Artemia sinica Cai (Crustacea : anostraca) from Yanchi
   Saltlake, China}},
Journal = {{JOURNAL OF FRESHWATER ECOLOGY}},
Year = {{2002}},
Volume = {{17}},
Number = {{3}},
Pages = {{467-473}},
Month = {{SEP}},
Abstract = {{The effects of temperature on population characteristics of Artemia
   sinica Cai from Yanchi Saltlake, Shanxi province, China, were studied in
   the laboratory at a salinity of 60parts per thousand. The developmental
   stages and generation time were dramatically shortened with increasing
   water temperature between 16degreesC and 34degreesC. Hatching and
   survival rates decreased rapidly above 30degreesC. The optimum
   temperature for reproduction and population growth was 26-28degreesC.
   The threshold temperatures for hatching of eggs and development of
   larvae were 9.07degreesC and 11.00degreesC, respectively. The effective
   cumulative temperature for a generation (time x temperature required
   from hatching to death) was 458.4+/-39.6 C degree-days.
   Based on field monitoring, Yanchi Saltlake provides favorable annual
   conditions of 2884.1 C degree-days, which would support an estimated
   6.3+/-0.5 A. sinica generations,per year. The intrinsic and finite rates
   of population increase were both increased with adapted water
   temperature. Extrapolating this data to Yanchi Saltlake, the period from
   the middle of May to September (second to fifth A. sinica generations)
   would be the most productive, with a generation time of < 42 d, a
   population doubling time of < 15 d, a finite rate of increase of > 1.0
   d(-1), and an intrinsic rate of natural increase of > 0.04 d(-1).}},
