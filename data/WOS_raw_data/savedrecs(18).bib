@article{ ISI:000356741000026,
Author = {Bonamano, S. and Madonia, A. and Borsellino, C. and Stefani, C. and
   Caruso, G. and De Pasquale, F. and Piermattei, V. and Zappala, G. and
   Marcelli, M.},
Title = {{Modeling the dispersion of viable and total Escherichia coli cells in
   the artificial semi-enclosed bathing area of Santa Marinella (Latium,
   Italy)}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2015}},
Volume = {{95}},
Number = {{1}},
Pages = {{141-154}},
Month = {{JUN 15}},
Abstract = {{Coastal areas are strongly affected by episodes of fecal contamination
   due to polluted water inflows from inadequately treated sewages. The
   present study aims to investigate the dispersion of Escherichia coli in
   the artificial semi-enclosed bathing area of Santa Marinella (Latium,
   Italy) through in situ samplings carried out in summer 2012 and the
   application of a dynamic model. Collected samples were analyzed by the
   Culture-Based technique and the Fluorescent Antibody method in order to
   estimate both the viable culturable cells and the total E. coli
   population, respectively. The in situ datasets were used to test the
   proposed modeling approach and simulate the behavior of bacteria as
   particles subjected, or not, to decay. Next, the flushing time and the
   computation of the Microbiological Potential Risk Area allowed the
   evaluation of the contribution of physical and biological processes to
   coliform dispersion and the related potential risk for bathers. (C) 2015
   Elsevier Ltd. All rights reserved.}},
