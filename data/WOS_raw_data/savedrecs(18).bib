@article{ ISI:000356632700007,
Author = {Clements, Christopher F. and Drake, John M. and Griffiths, Jason I. and
   Ozgul, Arpat},
Title = {{Factors Influencing the Detectability of Early Warning Signals of
   Population Collapse}},
Journal = {{AMERICAN NATURALIST}},
Year = {{2015}},
Volume = {{186}},
Number = {{1}},
Pages = {{50-58}},
Month = {{JUL}},
Abstract = {{The recent description of potentially generic early warning signals is a
   promising development that may help conservationists to anticipate a
   population's collapse prior to its occurrence. So far, the majority of
   such warning signals documented have been in highly controlled
   laboratory systems or in theoretical models. Data from wild populations,
   however, are typically restricted both temporally and spatially due to
   limited monitoring resources and intrinsic ecological
   heterogeneity-limitations that may affect the detectability of generic
   early warning signals, as they add additional stochasticity to
   population abundance estimates. Consequently, spatial and temporal
   subsampling may serve to either muffle or magnify early warning signals.
   Using a combination of theoretical models and analysis of experimental
   data, we evaluate the extent to which statistical warning signs are
   robust to data corruption.}},
