@article{ ISI:000360866600009,
Author = {Lonergan, Erin and Pasche, Julie and Skoglund, Linnea and Burrows, Mary},
Title = {{Sensitivity of Ascochyta Species Infecting Pea, Lentil, and Chickpea to
   Boscalid, Fluxapyroxad, and Prothioconazole}},
Journal = {{PLANT DISEASE}},
Year = {{2015}},
Volume = {{99}},
Number = {{9}},
Pages = {{1254-1260}},
Month = {{SEP}},
Abstract = {{Management of Ascochyta blight in pea, lentil, and chickpea relies on
   repeated fungicide applications, which has led to development of
   fungicide resistance and disease control failures in some systems. In
   vitro assays were conducted to determine baseline fungicide sensitivity
   in Mycosphaerella pinodes (Ascochyta pinodes), A. lends, and A. rabiei
   populations to the demethylation-inhibiting fungicide prothioconazole
   and the succinate dehydrogenase-inhibiting fungicides boscalid and
   fluxapyroxad by determining the effective concentration at which 50\% of
   germination or fungal growth was inhibited (EC50). Mean boscalid EC50
   values from conidial germination assays were 0.669, 0.639, and 0.171 mu
   g/ml and from mycelial growth assays were 0.258, 0.791, and 0.443 mu
   g/ml for M. pinodes, A. lends, and A. rabiei, respectively. Mean
   fluxapyroxad EC50 values were 0.050, 0.763, and 0.057 mu g/ml for M.
   pinodes, A. lentis, and A. rabiei, respectively. Mean baseline EC50
   values for prothioconazole with mycelial growth were 0.541, 0.604, and
   0.283 mu g/ml for M. pinodes, A. lentis, and A. rabiei, respectively. A
   single discriminatory fungicide concentration of 1 mu g/ml was selected
   for all species. Established sensitivity profiles and discriminatory
   concentrations will be used to monitor sensitivity shifts in populations
   of Ascochyta spp. and to make effective disease management
   recommendations.}},
