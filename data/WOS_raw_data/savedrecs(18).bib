@article{ ISI:000356200900003,
Author = {Dordevic, Sonja},
Title = {{Carapace peculiarities of Hermann's tortoises (Testudo hermanni) in
   several Balkan populations}},
Journal = {{NORTH-WESTERN JOURNAL OF ZOOLOGY}},
