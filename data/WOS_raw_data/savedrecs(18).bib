@article{ ISI:000356718500006,
Author = {Boakes, Elizabeth H. and Rout, Tracy M. and Collen, Ben},
Title = {{Inferring species extinction: the use of sighting records}},
Journal = {{METHODS IN ECOLOGY AND EVOLUTION}},
Year = {{2015}},
Volume = {{6}},
Number = {{6}},
Pages = {{678-687}},
Month = {{JUN}},
Abstract = {{Accurate measures of extinction are needed in biodiversity monitoring
   and conservation management, but ascertaining the exact time at which a
   species became extinct is difficult since a small population may go
   undetected for many years. For little-studied species, often the only
   information available is historical sighting data. Several statistical
   tests have been developed which use this information to make inferences
   about a species' extinction. The increasing array of methods can present
   a daunting choice. We review the more frequently cited methods, for each
   model explaining its assumptions, the data required, the scenarios it
   was developed for and power considerations, if known. We provide
   guidance on selecting the most appropriate method for a particular
   sighting record. We give examples from the literature to show how the
   methods have been usefully applied across conservation research,
   informing conservation decision-making and extinction inference.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Review}},
