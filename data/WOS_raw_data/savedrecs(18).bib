@article{ ISI:000357904100052,
Author = {Buteau, Stephane and Goldberg, Mark S.},
Title = {{Methodological issues related to pooling results from panel studies of
   heart rate variability and its association with ambient air pollution}},
Journal = {{ENVIRONMENTAL RESEARCH}},
