@article{ ISI:000360582700013,
Author = {Brzeski, Kristin E. and Harrison, Rebecca B. and Waddell, William T. and
   Wolf, Karen N. and Rabon, Jr., David R. and Taylor, Sabrina S.},
Title = {{Infectious disease and red wolf conservation: assessment of disease
   occurrence and associated risks}},
Journal = {{JOURNAL OF MAMMALOGY}},
