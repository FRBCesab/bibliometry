@article{ ISI:000356239000033,
Author = {Blount, J. L. and Buntin, G. D. and Sparks, Jr., A. N.},
Title = {{Host Preference of Megacopta cribraria (Hemiptera: Plataspidae) on
   Selected Edible Beans and Soybean}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{2015}},
Volume = {{108}},
Number = {{3}},
Pages = {{1094-1105}},
Month = {{JUN}},
Abstract = {{Megacopta cribraria (F.) (Hemiptera: Plataspidae) is an Old World pest
   of legumes in Asia. Since its 2009 discovery in Georgia, it has become
   an economic pest of soybeans in the southeastern United States. The
   objective of this study was to determine the host preference of
   Megacopta cribraria on edible legumes that might incur economic damage
   from injury of this pest. From 2012 to 2013 choice, no-choice, and field
   trials were conducted to evaluate the host suitability of several beans
   of commercial interest including pinto bean, lima bean, winter pea, and
   black-eyed pea. Choice and no-choice studies were conducted under
   greenhouse conditions. Plants in greenhouse trials were infested with
   adults and egg masses collected from kudzu and soybean and monitored for
   similar to 2 wk. Field trials were allowed to be infested by naturally
   occurring Megacopta cribraria populations. Sweep and whole plant counts
   of adults, egg masses, and nymphs were used to quantify field
   infestations. The legume crops found to be suitable developmental hosts
   are soybean, edamame, and pigeon pea. Low levels of development were
   seen on fava bean and none on the remaining entries.}},
