@article{ ISI:000369116800002,
Author = {Bucci, Melanie E. and Krausman, Paul R.},
Title = {{A fortuitous mitigation for desert mule deer along the All-American
   Canal}},
Journal = {{CALIFORNIA FISH AND GAME}},
