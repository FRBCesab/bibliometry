@article{ ISI:000422634900007,
Author = {Welsh, Stuart A. and Loughman, Zachary J.},
Title = {{Upstream dispersal of an invasive crayfish aided by a fish passage
   facility}},
Journal = {{MANAGEMENT OF BIOLOGICAL INVASIONS}},
