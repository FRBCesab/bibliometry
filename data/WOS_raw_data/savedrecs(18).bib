@article{ ISI:000368890500022,
Author = {Sidie-Slettedahl, Anna M. and Jensen, Kent C. and Johnson, Rex R. and
   Arnold, Todd W. and Austin, Jane E. and Stafford, Joshua D.},
Title = {{Evaluation of Autonomous Recording Units for Detecting 3 Species of
   Secretive Marsh Birds}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
