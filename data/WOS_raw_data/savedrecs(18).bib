@article{ ISI:000356529000006,
Author = {Anthony, Travis and Riedle, J. Daren and East, Mitchell B. and Fillmore,
   Brian and Ligon, Day B.},
Title = {{Monitoring of a Reintroduced Population of Juvenile Alligator Snapping
   Turtles}},
Journal = {{CHELONIAN CONSERVATION AND BIOLOGY}},
