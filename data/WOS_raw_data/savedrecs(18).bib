@article{ ISI:000360418900006,
Author = {Della Libera, Matteo and Passilongo, Daniela and Reby, David},
Title = {{Acoustics of male rutting roars in the endangered population of Mesola
   red deer Cervus elaphus italicus}},
Journal = {{MAMMALIAN BIOLOGY}},
