@article{ ISI:000413622000010,
Author = {Barriga-Vallejo, C. and Aguilera, C. and Cruz, J. and Banda-Leal, J. and
   Lazcano, D. and Mendoza, R.},
Title = {{Ecotoxicological Biomarkers in Multiple Tissues of the Neotenic
   Ambystoma spp. for a Non-lethal Monitoring of Contaminant Exposure in
   Wildlife and Captive Populations}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
