@article{ ISI:000418290100003,
Author = {Cox, W. Andrew and Sghwarzer, Amy and Kiltie, Richard and Paul, Ann and
   Rachal, Mark and Kent, Gina M. and Meyer, Kenneth D. and Lorenz, Jerome
   J. and Frezza, Peter E. and Rafferiy, Heather and Roebling, Suzy},
Title = {{Development of a Survey Protocol for Monitoring Reddish Egrets
   (\&ITEgretta rufescens\&IT) in Florida, USA}},
