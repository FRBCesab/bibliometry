@article{ ISI:000418292500023,
Author = {Ferreira-Filho, Pedro J. and Prado, Marcelle C. N. and Ghiotto, Thais C.
   and Giuliani, Graziella K. F. and Dias, Luiz P. and Silva, Matheus M.
   and Guerreiro, Julio C. and Zanuncio, Jose C.},
Title = {{The native psyllid Mastigimas anjosi (Hemiptera: Calophyidae): first
   record and abundance on pink cedar, Cedrela fissilis (Meliaceae), trees
   in Sao Paulo, Brazil}},
Journal = {{FLORIDA ENTOMOLOGIST}},
Year = {{2017}},
Volume = {{100}},
Number = {{4}},
Pages = {{817-819}},
Month = {{DEC}},
Abstract = {{The objective of this study was to identify and report on the occurrence
   of Psylloidea damaging pink cedar, Cedrela fissilis Vell. (Meliaceae),
   plants in the municipality of Sorocaba, in the southern region of Sao
   Paulo State, Brazil. Psyllids were collected on C. fissilis plants, and
   sent to authorities for identification. Psyllids damaging C. fissilis
   plants in urban areas and fragments of semideciduous seasonal forest
   were identified as Mastigimas anjosi Burckhardt, Queiroz D., Queiroz E.,
   Andrade, Zanol Rezende \& Kotrba (Hemiptera: Psylloidea: Calophyidae),
   which is native to Brazil. Cedrela fissilis also is endemic to Brazil,
   and has high ecological and economic value. It is harvested for timber
   production and is on the endangered species list. This is the first
   report of M. anjosi damaging C. fissilis plants in Sao Paulo State,
   Brazil and the importance of this plant for conservation and ecological
   restoration areas in Brazil justifies research to develop management
   programs for this psyllid.}},
