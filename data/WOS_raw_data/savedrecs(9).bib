@article{ ISI:000415391200074,
Author = {Kumar, S. Barath and Mohanty, A. K. and Das, N. P. I. and Satpathy, K.
   K. and Sarkar, S. K.},
Title = {{Impingement of marine organisms in a tropical atomic power plant cooling
   water system}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2017}},
Volume = {{124}},
Number = {{1}},
Pages = {{555-562}},
Month = {{NOV 15}},
Abstract = {{A one-year impingement monitoring was conducted at Madras Atomic Power
   Station (MAPS), Kalpakkam, southeastern coast of India and identified a
   total of 67 species of marine organisms in the cooling water system.
   Estimates of total annual impingement contributed about 1.47 x 10(6)
   individuals and 142.5 t of biomass.. Jellyfish contributed about 6.8 x
   10(5 )individuals and 135.6 t of biomass. Crabs, shrimps and fish were
   the most vulnerable organisms contributing about 4.29 x 10(5)
   individuals, 1.39 x 10(5 )individuals and 2.16 x 10(5) individuals
   respectively. Commercially important species namely Trichiurus lepturus,
   Sardinella longiceps and Portunus pelagicus were found to be impinged
   1.88\% and 0.29\% by number and weight of the total biomass
   respectively. Out of similar to 327 fish species recorded at Kalpakkam,
   only about 9.4\% of species were impinged at MAPS. Multispecies
   impingement at MAPS poses the problem of finding the best mitigation
   options for tropical conditions.}},
