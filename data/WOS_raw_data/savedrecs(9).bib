@article{ ISI:000413539800004,
Author = {Bergmann, Alan J. and North, Paula E. and Vasquez, Luis and Bello,
   Hernan and Gastanaga Ruiz, Maria del Carmen and Anderson, Kim A.},
Title = {{Multi-class chemical exposure in rural Peru using silicone wristbands}},
