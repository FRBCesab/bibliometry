@article{ ISI:000415690400004,
Author = {Chen, Qiong and Tomlinson, Kyle W. and Cao, Lin and Wang, Bo},
Title = {{Effects of fragmentation on the seed predation and dispersal by rodents
   differ among species with different seed size}},
Journal = {{INTEGRATIVE ZOOLOGY}},
