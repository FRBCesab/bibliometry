Author = {Zhao, Yang-Guo and Zhang, Yi and She, Zonglian and Shi, Yue and Wang,
   Min and Gao, Mengchun and Guo, Liang},
Title = {{Effect of Substrate Conversion on Performance of Microbial Fuel Cells
   and Anodic Microbial Communities}},
Journal = {{ENVIRONMENTAL ENGINEERING SCIENCE}},
