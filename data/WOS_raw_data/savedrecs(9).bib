@article{ ISI:000410940500011,
Author = {Ahmad, Mushtaq and Gull, Sanobar},
Title = {{Susceptibility of armyworm Spodoptera litura (Lepidoptera: Noctuidae) to
   novel insecticides in Pakistan}},
