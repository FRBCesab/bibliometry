@article{ ISI:000418700500008,
Author = {Mibroda, Julie E. and McNeil, Jr., Darin J. and Duchamp, Joseph E. and
   Townsend, Josiah H. and Larkin, Jeffery L.},
Title = {{ROADSIDE HABITAT USE BY THE ENDEMIC SHORT-HEADED GARTERSNAKE (THAMNOPHIS
   BRACHYSTOMA) IN NORTHWESTERN PENNSYLVANIA, USA}},
Journal = {{HERPETOLOGICAL CONSERVATION AND BIOLOGY}},
