Author = {Andrade, E. and Miro, C. and Reis, M. and Santos, M. and Madruga, M. J.},
Title = {{ASSESSMENT OF RADIUM ACTIVITY CONCENTRATION AND RADON EXHALATION RATES
   IN IBERIAN PENINSULA BUILDING MATERIALS}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2017}},
Volume = {{177}},
Number = {{1-2}},
Pages = {{31-35}},
