@article{ ISI:000410980400001,
Author = {Suffice, Pauline and Asselin, Hugo and Imbeau, Louis and Cheveau,
   Marianne and Drapeau, Pierre},
Title = {{More fishers and fewer martens due to cumulative effects of forest
