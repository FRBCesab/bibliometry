@article{ ISI:000410727800014,
Author = {Hatter, Ian W. and Dielman, Patrick and Kuzyk, Gerald W.},
Title = {{An Integrated Modeling Approach for Assessing Management Objectives for
   Mule Deer in Central British Columbia}},
