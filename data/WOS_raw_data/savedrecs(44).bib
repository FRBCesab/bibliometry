@article{ ISI:000242494300027,
Author = {Richardson, Elizabeth A. and Kaiser, Michel J. and Edwards-Jones, Gareth
   and Ramsay, Kirsten},
Title = {{Trends in sea anglers' catches of trophy fish in relation to stock size}},
Journal = {{FISHERIES RESEARCH}},
