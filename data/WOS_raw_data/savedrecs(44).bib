@incollection{ ISI:000310875400014,
Author = {Prudente, Maricar S. and Malarvannan, Govindan and Tanabe, Shinsuke},
Editor = {{Li, A and Tanabe, S and Jiang, G and Giesy, JP and Lam, PKS}},
Title = {{Persistent Toxic Substances in the Philippine Environment}},
