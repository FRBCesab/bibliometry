@article{ ISI:000242398700029,
Author = {Choate, David M. and Wolfe, Michael L. and Stoner, David C.},
Title = {{Evaluation of cougar population estimators in Utah}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
