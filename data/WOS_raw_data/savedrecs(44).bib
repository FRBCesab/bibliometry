@inproceedings{ ISI:000250404400002,
Author = {Hudson, J. Michael and Jackson, Julie A.},
Editor = {{Brouder, MJ and Scheurer, JA}},
Title = {{Population abundance estimates for humpback chub and roundtail chub in
   Westwater Canyon, Colorado River, Utah, 1998-2000}},
