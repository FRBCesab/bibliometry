@article{ ISI:000241704600006,
Author = {Werner, Julieta and Palace, Vince P. and Wautier, Kerry G. and Mills,
   Kenneth H. and Chalanchuk, Sandra M. and Kidd, Karen A.},
Title = {{Reproductive fitness of lake trout (Salvelinus namaycush) exposed to
   environmentally relevant concentrations of the potent estrogen
   ethynylestradiol (EE2) in a whole lake exposure experiment}},
Journal = {{SCIENTIA MARINA}},
Year = {{2006}},
Volume = {{70}},
Number = {{2}},
