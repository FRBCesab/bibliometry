@article{ ISI:000241240400022,
Author = {Showler, A. T. and Robacker, D. and Salgado, E.},
Title = {{Grandlure dosage and attraction of boll weevils (Coleoptera :
   Curculionidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
