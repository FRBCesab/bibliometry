@article{ ISI:000241050000013,
Author = {Penna, A. and Fusco, G. and Bertozzini, E. and Giacobbe, M. G. and Vila,
   M. and Galluzzi, L. and Garces, E. and Luglie, A. and Maso, M. and
   Magnani, M.},
Title = {{Monitoring of Alexandrium species in the Mediterranean Sea using a
   combined filter system-PCR assay detection method}},
