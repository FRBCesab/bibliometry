@article{ ISI:000242565000014,
Author = {Smith, Tara P. and Hammond, Abner M.},
Title = {{Comparative susceptibility of sweetpotato weevil (Coleoptera :
   Brentidae) to selected insecticides}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{2006}},
Volume = {{99}},
