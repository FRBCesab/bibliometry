@article{ ISI:000241748900014,
Author = {Ozdilek, Hasan Goksel},
Title = {{An analogy on assessment of urban air pollution in Turkey over the turn
   of the millennium (1992-2001)}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
