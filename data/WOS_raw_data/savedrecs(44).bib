@article{ ISI:000256840200082,
Author = {Griciene, B. and Ladygiene, R. and Morkunas, G. and Pilkyte, L. and
   Urboniene, A.},
Title = {{The overview of internal exposure monitoring in Lithuania}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2007}},
Volume = {{127}},
Number = {{1-4, SI}},
Pages = {{398-401}},
Note = {{Workshop on Internal Dosimetry, Montpellier, FRANCE, OCT 02-05, 2006}},
Abstract = {{This study deals with analysis of situation and results of internal
   exposure monitoring of radiation workers and population in Lithuania.
   Radiation workers are assessed for internal exposures by direct
   methods-whole body counting or organ counting by gamma spectrometry at
   the Radiation Protection Centre and Ignalina Nuclear Power Plant (INPP).
   Results of monitoring of INPP and nuclear medicine workers show that no
   significant activities were detected. The annual committed effective
   doses of workers are < 1 mSv. The measured average activity of K-40 in
   males and females was 3.7 +/- 1.0 and 2.5 +/- 0.7 kBq, respectively.
   Mixed diet sampled at hospitals in 2001-5 was analysed for Sr-90 and
   Cs-137 activity concentrations. Average effective dose due to 90Sr and
   Cs-137 in mixed diet was 0.6 +/- 0.2 and 0.47 +/- 0.13 mu Sv,
   respectively. Indoor radon measurements were done in multi-storey
   houses. Average concentration was 15.1 +/- 1.0 Bq m(-3). The annual
   effective dose aused by radon was 0.38 mSv.}},
