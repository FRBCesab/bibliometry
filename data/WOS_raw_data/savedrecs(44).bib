@incollection{ ISI:000268468400020,
Author = {Chouliara, Eirini and Kontominas, Michael G.},
Editor = {{Singh, VK and Govil, JN and Ahmad, K and Sharma, RK}},
Title = {{Combined Effect of Thyme Essential Oil and Modified Atmosphere Packaging
   to Extend Shelf-Life of Fresh Chicken Meat}},
Booktitle = {{NATURAL PRODUCTS I}},
Series = {{Recent Progress in Medicinal Plants}},
Year = {{2007}},
Volume = {{15}},
Pages = {{423-441}},
Abstract = {{The combined effect of thyme essential oil (0.5\%, 1\%) and modified
   atmosphere packaging (MAP) (30\% CO(2)/70\% N(2) and 70\% CO(2)/30\%
   N(2)) on shelf-life extension of fresh breast meat stored at 4 degrees C
   was investigated. The following parameters were monitored over a period
   of 25 days: Microbiological (total viable counts, Pseudomonas, lactic
   acid bacteria, Brochothrix thermosphacta, and Enterobacteriaceae),
   physico-chemical (pH, TBA, color) and sensory (odor and taste)
   attributes. All microbial species were affected primarily by thyme oil
   and secondarily by MAP. Microbial populations were reduced by 1-5 log
   cfu/g for a given sampling day. TBA values in all cases remained lower
   than 1 mg malondialdehyde (MDA)kg(-1) throughout the entire storage
   period. Likewise pH varied between 6.4 (day 0) and 5.9 (day 25) with
   higher values being recorded for samples containing thyme oil. Color
   parameters L{*}, a{*} and b{*} were not significantly affected by MAP or
   thyme oil during storage. Finally sensory analysis showed a shelf-life
   extension of breast chicken meat by ca. 5 days for samples containing
   0.5\% thyme oil, 1-2 days for samples under MA. P and 6 days for samples
   under MAP containing 0.5\% thyme oil. Thus thyme oil and MAP exhibited
   an additive protective effect.}},
