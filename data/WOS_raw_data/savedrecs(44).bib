@article{ ISI:000252847100029,
Author = {Martins, M. B. and Alves, J. G. and Abrantes, J. N. and Roda, A. R.},
Title = {{Occupational exposure in nuclear medicine in Portugal in the 1999-2003
   period}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
