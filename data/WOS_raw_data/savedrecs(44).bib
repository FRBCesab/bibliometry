@article{ ISI:000242139300009,
Author = {Jaenike, John and Elizondo, Susan and Delahanty, Nicholas},
Title = {{Mushrooms as potential sources of Wolbachia-curing antibiotics}},
Journal = {{EVOLUTIONARY ECOLOGY RESEARCH}},
Year = {{2006}},
Volume = {{8}},
Number = {{7}},
Pages = {{1291-1299}},
Month = {{NOV}},
Abstract = {{Background: Drosophila innubila is infected with maternally transmitted
   male-killing Wolbachia. The mushrooms on which mycophagous Drosophila
   feed are chemically diverse. It is possible that some species contain
   antibiotics that could cure flies of their Wolbachia infections. Such
   curing would result in an immediate doubling of offspring production,
   would affect the prevalence of infection in natural populations, and
   would influence where Drosophila prefer to oviposit.
   Methods: Drosophila innubila were reared on similar to 100 diverse
   species of mushrooms and their subsequent offspring sex ratios monitored
   for evidence of antibiotic curing of Wolbachia infection.
   Results: None of the mushrooms tested provided compelling evidence of
   antibiotic activity against Wolbachia.}},
