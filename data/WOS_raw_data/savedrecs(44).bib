@article{ ISI:000242860400011,
Author = {Sharew, Hailu and Kays, Jonathan S.},
Title = {{Vole damage control in forest plantations}},
Journal = {{NORTHERN JOURNAL OF APPLIED FORESTRY}},
Year = {{2006}},
Volume = {{23}},
Number = {{4}},
Pages = {{297-300}},
Month = {{DEC}},
Abstract = {{The effects of five treatments (mowing with broadcast baiting, mowing
   with hand baiting, hand baiting with no mowing, herbicide, and untreated
   control) on vole population and damage to forest plantations were
   evaluated. All treatments caused reductions in vole, feeding, as well as
   reduced seedling damage and potential seedling mortality. This study
   shows the practical importance of monitoring planting sites for vole
   populations in the fall prior to and after planting and suggests tin
   immediate control of high and severe vole population ratings with
   rodenticide with or without mowing to minimize potential seedling
   mortality.}},
