@article{ ISI:000315701700014,
Author = {Dellinger, Timothy A. and Folk, Martin J. and Spalding, Marilyn G.},
Title = {{COPULATORY BEHAVIOR OF NON-MIGRATORY WHOOPING CRANES IN FLORIDA}},
Journal = {{WILSON JOURNAL OF ORNITHOLOGY}},
