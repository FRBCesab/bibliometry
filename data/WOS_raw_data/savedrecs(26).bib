
@article{ ISI:000317685500022,
Author = {Brink, Henry and Smith, Robert J. and Skinner, Kirsten},
Title = {{Methods for lion monitoring: a comparison from the Selous Game Reserve,
   Tanzania}},
Journal = {{AFRICAN JOURNAL OF ECOLOGY}},
Year = {{2013}},
Volume = {{51}},
