@article{ ISI:000330415200007,
Author = {Sharma, Sandeep and Dutta, Trishna and Maldonado, Jesus E. and Wood,
   Thomas C. and Panwar, Hemendra Singh and Seidensticker, John},
Title = {{Selection of microsatellite loci for genetic monitoring of sloth bears}},
Journal = {{URSUS}},
Year = {{2013}},
Volume = {{24}},
Number = {{2}},
Pages = {{164-169}},
Abstract = {{The sloth bear (Melursus ursinus) is a threatened species endemic to the
   Indian subcontinent. To date, no reliable method has been developed for
   identifying individuals or monitoring their populations. Here we
   describe a non-invasive genetic monitoring technique for individual
   identification of sloth bears. After testing 18 micro satellites
   developed for other carnivore species, including ursids, we optimized a
   panel of 7 highly polymorphic microsatellite loci that yielded a
   cumulative Probability of Identity between siblings value of 2.15E-03.
   We used this panel to identify 55 individual sloth bears from 190 fecal
   and 4 hair samples collected in tiger reserves in central India. This
   panel can be used for population genetic studies and population
   monitoring of sloth bears.}},
