@article{ ISI:000316694600009,
Author = {Doak, Daniel F. and Bakker, Victoria J. and Vickers, Winston},
Title = {{Using Population Viability Criteria to Assess Strategies to Minimize
   Disease Threats for an Endangered Carnivore}},
Journal = {{CONSERVATION BIOLOGY}},
