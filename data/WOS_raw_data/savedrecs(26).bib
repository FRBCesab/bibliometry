@article{ ISI:000316526300037,
Author = {Rebolledo, Elisa L. Bravo and Van Franeker, Jan A. and Jansen, Okka E.
   and Brasseur, Sophie M. J. M.},
Title = {{Plastic ingestion by harbour seals (Phoca vitulina) in The Netherlands}},
Journal = {{MARINE POLLUTION BULLETIN}},
