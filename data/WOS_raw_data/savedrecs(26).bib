@article{ ISI:000317335400013,
Author = {Greco, E. B. and Wright, M. G.},
Title = {{Dispersion and Sequential Sampling Plan for Xylosandrus compactus
   (Coleoptera: Curculionidae) Infesting Hawaii Coffee Plantations}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{2013}},
Volume = {{42}},
Number = {{2}},
Pages = {{277-282}},
Month = {{APR}},
Abstract = {{The black twig borer, Xylosandrus compactus (Eichhoff) (Coleoptera:
   Curculionidae: Scolytinae), is a serious pest of coffee (Coffea arabica
   L.) in the Kona region of the island of Hawaii, the center of the
   largest area of coffee production within the state of Hawaii. This study
   indirectly characterizes the spatial distribution of X. compactus in
   coffee plantations through assessment of twig borer damage, and presents
   a sequential sampling plan for monitoring X. compactus population
   densities. Taylor's Power Law (TPL) and Iwao's mean crowding index
   showed that X. compactus infestations were highly aggregated within
   plantations, with b and beta values significantly larger than 1. The TPL
   linear regression of log variance against log mean (R-2 = 0.92) provided
   a better fit to the data than the linear regression of mean crowding on
   the mean (R-2 = 0.68). Subsequently, Taylor's power law parameters were
   used to develop the Green's sequential plan to estimate densities of X.
   compactus at the 90 and 75\% precision levels.}},
