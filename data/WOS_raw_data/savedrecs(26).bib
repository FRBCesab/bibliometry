@article{ ISI:000313816600019,
Author = {Fahrentrapp, Johannes and Broggini, Giovanni A. L. and Kellerhals,
   Markus and Peil, Andreas and Richter, Klaus and Zini, Elena and Gessler,
   Cesare},
Title = {{A candidate gene for fire blight resistance in Malus x robusta 5 is
   coding for a CC-NBS-LRR}},
Journal = {{TREE GENETICS \& GENOMES}},
Year = {{2013}},
Volume = {{9}},
Number = {{1}},
Pages = {{237-251}},
Month = {{FEB}},
Abstract = {{Fire blight is the most important bacterial disease in apple (Malus x
   domestica) and pear (Pyrus communis) production. Today, the causal
   bacterium Erwinia amylovora is present in many apple- and pear-growing
   areas. We investigated the natural resistance of the wild apple Malus x
   robusta 5 against E. amylovora, previously mapped to linkage group 3.
   With a fine-mapping approach on a population of 2,133 individuals
   followed by phenotyping of the recombinants from the region of interest,
   we developed flanking markers useful for marker-assisted selection. Open
   reading frames were predicted on the sequence of a BAC spanning the
   resistance locus. One open reading frame coded for a protein belonging
   to the NBS-LRR family. The in silico investigation of the structure of
   the candidate resistance gene against fire blight of M. x robusta 5,
   FB\_MR5, led us hypothesize the presence of a coiled-coil region
   followed by an NBS and an LRR-like structure with the consensus
   `LxxLx{[}IL]xxCxxLxxL'. The function of FB\_MR5 was predicted in
   agreement with the decoy/guard model, that FB\_MR5 monitors the
   transcribed RIN4\_MR5, a homolog of RIN4 of Arabidopsis thaliana that
   could interact with the previously described effector AvrRpt2(EA) of E.
   amylovora.}},
