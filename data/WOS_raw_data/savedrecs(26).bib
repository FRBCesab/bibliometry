@article{ ISI:000316368200014,
Author = {Cotteaux-Lautard, Christelle and Berenger, Jean-Michel and Fusca,
   Francois and Chardon, Hubert and Simon, Fabrice and Pages, Frederic},
Title = {{A NEW CHALLENGE FOR HOSPITALS IN SOUTHEAST FRANCE: MONITORING LOCAL
   POPULATIONS OF AEDES ALBOPICTUS TO PREVENT NOSOCOMIAL TRANSMISSION OF
   DENGUE OR CHIKUNGUNYA}},
Journal = {{JOURNAL OF THE AMERICAN MOSQUITO CONTROL ASSOCIATION}},
Year = {{2013}},
Volume = {{29}},
Number = {{1}},
Pages = {{81-83}},
Month = {{MAR}},
Abstract = {{Aedes albopictus was first identified in southern France in 2004,
   inducing an emerging risk for autochthonous transmission around imported
   cases of dengue or chikungunya, and also for mosquito-borne nosocomial
   transmission in hospitals. Aedes albopictus has been present in
   Marseille since September 2009 and in Aix-en-Provence since August 2010.
   Because of the possible admission of viremic patients with dengue or
   chikungunya in the hospitals of these cities, a mosquito survey was
   conducted in 2011 in 2 of the hospitals, with the use of mosquito traps.
