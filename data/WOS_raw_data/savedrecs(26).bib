@article{ ISI:000316209500011,
Author = {Evans, Tyler G. and Chan, Francis and Menge, Bruce A. and Hofmann,
   Gretchen E.},
Title = {{Transcriptomic responses to ocean acidification in larval sea urchins
   from a naturally variable pH environment}},
Journal = {{MOLECULAR ECOLOGY}},
