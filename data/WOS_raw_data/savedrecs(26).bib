@article{ ISI:000315008600005,
Author = {Ciarrocca, Manuela and Capozzella, Assunta and Tomei, Francesco and
   Tomei, Gianfranco and Caciari, Tiziana},
Title = {{Exposure to cadmium in male urban and rural workers and effects on FSH,
   LH and testosterone}},
