@article{ ISI:000338206900005,
Author = {Brown, Donald J. and Swannack, Todd M. and Forstner, Michael R. J.},
Title = {{Predictive Models for Calling and Movement Activity of the Endangered
   Houston Toad}},
Journal = {{AMERICAN MIDLAND NATURALIST}},
