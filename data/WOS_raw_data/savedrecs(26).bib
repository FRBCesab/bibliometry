@article{ ISI:000331514500027,
Author = {Taniguchi, Ian K. and Stein, Derek and Lampson, Kai and Rogers-Bennett,
   Laura},
Title = {{TESTING TRANSLOCATION AS A RECOVERY TOOL FOR PINK (HALIOTIS CORRUGATA)
   AND GREEN (HALIOTIS FULGENS) ABALONE IN SOUTHERN CALIFORNIA}},
Journal = {{JOURNAL OF SHELLFISH RESEARCH}},
