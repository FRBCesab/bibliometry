@article{ ISI:000314712700004,
Author = {Pulg, Ulrich and Barlaup, Bjorn T. and Sternecker, Katharina and Trepl,
   Ludwig and Unfer, Guenther},
Title = {{RESTORATION OF SPAWNING HABITATS OF BROWN TROUT (Salmo trutta) IN A
   REGULATED CHALK STREAM}},
Journal = {{RIVER RESEARCH AND APPLICATIONS}},
