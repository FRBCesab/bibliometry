@article{ ISI:000316913000009,
Author = {Stefanescu, Constanti and Paramo, Ferran and Akesson, Susanne and
   Alarcon, Marta and Avila, Anna and Brereton, Tom and Carnicer, Jofre and
   Cassar, Louis F. and Fox, Richard and Heliola, Janne and Hill, Jane K.
