@article{ ISI:000313030500006,
Author = {Recart, Wilnelia and Ackerman, James D. and Cuevas, Ana A.},
Title = {{There goes the neighborhood: apparent competition between invasive and
   native orchids mediated by a specialist florivorous weevil}},
Journal = {{BIOLOGICAL INVASIONS}},
Year = {{2013}},
Volume = {{15}},
Number = {{2}},
Pages = {{283-293}},
Month = {{FEB}},
Abstract = {{The exotic orchid, Spathoglottis plicata, has naturalized and spread
   rapidly over Puerto Rico where it is generally considered to be
   innocuous. It is abundant and occupies the same habitat as the native
   orchid, Bletia patula. The two are hosts to the same native weevil,
   Stethobaris polita, a specialist on orchid flowers. We ask whether the
   weevils mediate apparent competition between the two orchids. We
   monitored weevil populations, floral damage and fruit set in B. patula
   in the presence and absence of S. plicata. We also experimentally tested
   whether weevils preferred one species over the other. Finally, we
   modeled the distribution of both orchid species to predict the extent by
   which the two species may interact in Puerto Rico. We found a
   significantly lower number of weevils and a higher fruit set for B.
   patula where S. plicata is absent, indicative that apparent competition
   is occurring. The choice experiments show that weevils prefer flowers of
   S. plicata over those of B. patula, but B. patula still sustained
   considerable damage. The current distribution of the native B. patula is
   nearly limited to the northern karst region of Puerto Rico. The
   naturalized S. plicata has a broader range and the models predict that
   its distribution will strongly overlap with that of B. patula. We expect
   the S. plicata invasion to continue and affect native orchids through
   apparent competition as long as the presence of S. plicata maintains
   elevated weevil populations. Thus, even seemingly harmless invasive
   orchids can have subtle but significant negative consequences.}},
