@article{ ISI:A1992JY05100020,
Author = {WEISSER, PJ and WHITFIELD, AK and HALL, CM},
Title = {{THE RECOVERY AND DYNAMICS OF SUBMERGED AQUATIC MACROPHYTE VEGETATION IN
   THE WILDERNESS LAKES, SOUTHERN CAPE}},
Journal = {{BOTHALIA}},
Year = {{1992}},
Volume = {{22}},
Number = {{2}},
Pages = {{283-288}},
Month = {{OCT}},
Abstract = {{Between 1979 and 1981, the submerged aquatic macrophyte vegetation in
   the Wilderness lakes died back significantly, and in some areas
   disappeared altogether. This study documents the senescent phase and
   describes the recovery of the plant populations between May 1982 and May
   1983. In two lakes, namely Langvlei and Eilandvlei, the plant biomass
   approximately doubled between the winters of 1982 and 1983. Seasonal
   changes in species composition are documented arid possible factors
   accounting for the collapse and recovery of the plant populations are
   discussed.}},
