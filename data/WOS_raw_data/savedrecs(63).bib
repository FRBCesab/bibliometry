@article{ ISI:A1993NQ37600009,
Author = {DWYER, CP and BALDASSARRE, GA},
Title = {{SURVIVAL AND NEST SUCCESS OF SYMPATRIC FEMALE MALLARDS,
   ANAS-PLATYRHYNCHOS, AND AMERICAN BLACK DUCKS, A-RUBRIPES, BREEDING IN A
   FORESTED ENVIRONMENT}},
Journal = {{CANADIAN FIELD-NATURALIST}},
Year = {{1993}},
Volume = {{107}},
Number = {{2}},
Pages = {{213-216}},
Month = {{APR-JUN}},
Abstract = {{Inverse population trends of Mallards (Anas platyrhynchos) and American
   Black Ducks (A. rubripes) in eastern Canada and northeastern United
   States have led many to speculate whether differential productivity and
   survival exist between these closely related forms. Monitoring of
   radio-marked female Mallards and Black Ducks breeding sympatrically in
   the western Adirondack region of northern New York in 1990-1991 showed
   that nest success rates of Mallards (S = 0.51) and Black Ducks (S =
   0.61) were similar (P > 0.10), as was the survival rates of females
   during the 107-day breeding period (5 April - 20 July; Mallard S = 0.88.
   Black Duck S = 0.77). Despite these similarities, a higher proportion of
   Mallards successfully hatched a clutch than did Black Ducks (67\% vs.
   44\%). Thus. Mallards may be more productive than Black Ducks,
   contributing to their increasing numbers throughout Black Duck breeding
   range.}},
