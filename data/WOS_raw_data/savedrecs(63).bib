@article{ ISI:A1993LP26000016,
Author = {MEYERSSCHONE, L and SHUGART, LR and BEAUCHAMP, JJ and WALTON, BT},
Title = {{COMPARISON OF 2 FRESH-WATER TURTLE SPECIES AS MONITORS OF RADIONUCLIDE
   AND CHEMICAL CONTAMINATION - DNA-DAMAGE AND RESIDUE ANALYSIS}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY}},
Year = {{1993}},
Volume = {{12}},
Number = {{8}},
Pages = {{1487-1496}},
Month = {{AUG}},
Abstract = {{Two species of turtles that occupy different ecological niches were
   compared for their usefulness as monitors of freshwater ecosystems where
   both low-level radioactive and nonradioactive contaminants are present.
   The pond slider (Trachemys scripta) and common snapping turtle (Chelydra
   serpentina) were analyzed for the presence of Sr-90, Cs-137, Co-60, and
   Hg, radionuclides and chemicals known to be present at the contaminated
   site, and single-strand breaks in liver DNA. The integrity of the DNA
   was examined by the alkaline unwinding assay, a technique that detects
   strand breaks as a biological marker of possible exposure to genotoxic
   agents. This measure of DNA damage was significantly increased in both
   species of turtles at the contaminated site compared with turtles of the
   same species at a reference site, and shows that contaminant-''posed
   populations were under more severe genotoxic stress than those at the
   reference site. The level of strand breaks observed at the contaminated
   site was high and in the range reported for other aquatic species
   exposed to deleterious concentrations of genotoxic agents such as
   chemicals and ionizing radiation. Statistically significantly higher
   concentrations of radionuclides and Hg were detected in the turtles from
   the contaminated area. Mercury concentrations were significantly higher
   in the more carnivorous snapping turtle compared with the slider;
   however, both species were effective monitors of the contaminants.}},
