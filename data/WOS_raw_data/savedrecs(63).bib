@article{ ISI:A1993MW86500017,
Author = {SCUDAMORE, KA},
Title = {{MYCOTOXINS IN STORED PRODUCTS - MYTH OR MENACE}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{1993}},
