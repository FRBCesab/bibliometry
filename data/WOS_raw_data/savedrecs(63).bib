@article{ ISI:A1993MQ28200002,
Author = {KADIRI, MO and REYNOLDS, CS},
Title = {{LONG-TERM MONITORING OF THE CONDITIONS OF LAKES - THE EXAMPLE OF THE
   ENGLISH LAKE DISTRICT}},
Journal = {{ARCHIV FUR HYDROBIOLOGIE}},
Year = {{1993}},
Volume = {{129}},
Number = {{2}},
Pages = {{157-178}},
Month = {{DEC}},
Abstract = {{The paper describes an approach to the monitoring of the condition of a
   relatively well-studied series of lakes, the purpose of which scheme
   sought the optimum combination of high sensitivity and low logistic
   effort. It involves the collection of three samples, corresponding with
   spring, early- and late-summer phytoplankton production, in every fifth
   or sixth year, phytoplankton is enumerated and categorised according to
   size, being < 65 mum or > 65 mum in unit (cell or colony) dimension and
   with an additional category for (larger) `cyanophytes'. Biomass scores
   averaged over the year prove to be very conservative in the lakes of the
   English Lake District, unless they have undergone significant
   eutrophication though (mainly) receiving sewage effluent (i.e.
   mineralised but not nutrient stripped). The method is shown to be
   sensitive to broad differences in trophic status but insufficiently so
   to more subtle changes over a five-year period, unless these are either
   anticipated or confirmed by more intensive study. For background
   monitoring of a previously well-studied lake or lake series, the present
   method should prove adequate.}},
Publisher = {{E SCHWEIZERBART'SCHE VERLAGS}},
Address = {{NAEGELE U OBERMILLER JOHANNESSTRASSE 3A, D 70176 STUTTGART, GERMANY}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{NERC,INST FRESHWATER ECOL,AMBLESIDE LA22 OLP,CUMBRIA,ENGLAND.}},
DOI = {{10.1127/archiv-hydrobiol/129/1993/157}},
ISSN = {{0003-9136}},
Keywords-Plus = {{POPULATIONS; PERIODICITY; VARIABILITY}},
