@article{ ISI:A1993NA60800009,
Author = {SU, NY and BAN, PM and SCHEFFRAHN, RH},
Title = {{A SHALLOW SUBSURFACE MONITORING STATION FOR SUBTERRANEAN TERMITES
   (ISOPTERA)}},
Journal = {{SOCIOBIOLOGY}},
Year = {{1993}},
Volume = {{23}},
Number = {{2}},
Pages = {{175-182}},
Abstract = {{A monitoring station consisting of a flat plastic container with
   numerous holes on the sides and a cut-out bottom was installed near the
   soil surface for monitoring subterranean termites. Termite foraging
   tunnels in the soil were quickly reconstructed and extended to the wood
   block in the station through the holes on the side of the station. The
   new shallow design was more attractive to the eastern subterranean
   termite, Reticulitermes flavipes (Kollar), than the previous version of
   the underground monitoring station in which foraging tunnels may be
   hindered by its solid plastic collar.}},
