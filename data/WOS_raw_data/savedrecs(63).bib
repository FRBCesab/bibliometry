@article{ ISI:A1994MW30600015,
Author = {LEMASTER, ET and TROST, RE},
Title = {{SUMMER SURVIVAL RATE ESTIMATES OF ADULT WOOD DUCKS - IMPLICATIONS FOR
   BANDING PROGRAMS}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{1994}},
Volume = {{58}},
Number = {{1}},
Pages = {{107-114}},
Month = {{JAN}},
Abstract = {{Wood duck (Aix sponsa) preseason banding data are essential to monitor
   the population and to evaluate management programs. Historically,
   numbers of banded wood ducks have not reached established banding goals,
   particularly for the adult female cohort. The lack of data has led to
   difficulties in accurately determining annual survival and recovery
   rates. We hypothesized that these estimates may be improved by
   incorporating additional banding records from a period (15 Apr-30 Jun)
   prior to the traditional preseason banding period (1 Jul-15 Sep). We
   examined recovery distributions to determine if early and late-banded
   wood ducks were representative of the same populations. We next
   developed models that allowed for combining early (15 Apr-30 Jun) and
   late (1 Jul-15 Sep) preseason banding data for adult wood ducks to
   estimate summer survival rates, and annual survival and recovery rates.
   Early and late-banded female wood duck recovery distributions were
   different (P < 0.05); however, differences were less than the
   differences between sex and age classes (P < 0.01) banded during the
   same period. There was no difference (P > 0.19) in recovery
   distributions for early and late-banded male wood ducks. We concluded
   that, data from wood ducks banded early and late were representative of
   the same populations. Adult summer survival rates varied among
   geographic regions (P < 0.02). The mean summer survival rate for adult
   female southern wood ducks was estimated as 58.00\% (SE = 5.96\%), while
   for female wood ducks in northern areas it averaged 85.46\% (SE =
   4.49\%). Annual survival and recovery rate estimates realized gains in
   precision when early and late-banding data were combined.}},
