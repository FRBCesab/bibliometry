@article{ ISI:A1993MW09900005,
Author = {BOGDANOWICZ, SM and WALLNER, WE and BELL, J and ODELL, TM and HARRISON,
   RG},
Title = {{ASIAN GYPSY MOTHS (LEPIDOPTERA, LYMANTRIIDAE) IN NORTH-AMERICA -
   EVIDENCE FROM MOLECULAR-DATA}},
Journal = {{ANNALS OF THE ENTOMOLOGICAL SOCIETY OF AMERICA}},
Year = {{1993}},
Volume = {{86}},
Number = {{6}},
Pages = {{710-715}},
Month = {{NOV}},
Abstract = {{Gypsy moths, Lymantria dispar L., were accidentally introduced into the
   United States from Europe more than a century ago and have become a
   major forest pest in eastern North America. Recently, concern has arisen
   over possible establishment of gypsy moths of Asian origin in western
   North America. Because Asian moths have distinctive ecological and
   behavioral characteristics, it is essential to monitor their presence.
   Identification of pheromone-trapped males based on morphology is not
   straightforward, but comparisons of mitochondrial DNA (mtDNA)
   restriction fragment length polymorphisms (RFLPs) and sequences reveal
   diagnostic differences between Asian and North American haplotypes. An
   mtDNA survey of moths trapped in the Pacific Northwest in 1990 and.1991
   documents the presence of moths of Asian origin at several sites in
   North America. These results provided the basis for undertaking efforts
   in 1992 to eradicate gypsy moth populations of Asian origin in both the
   United States and Canada:}},
Publisher = {{ENTOMOL SOC AMER}},
Address = {{9301 ANNAPOLIS RD, LANHAM, MD 20706}},
Type = {{Article}},
