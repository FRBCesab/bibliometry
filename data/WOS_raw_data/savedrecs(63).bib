@article{ ISI:A1992KD17900001,
Author = {DUNSON, WA and WYMAN, RL and CORBETT, ES},
Title = {{A SYMPOSIUM ON AMPHIBIAN DECLINES AND HABITAT ACIDIFICATION}},
