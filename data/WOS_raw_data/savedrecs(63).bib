@article{ ISI:A1992KJ41900008,
Author = {ANDERSON, NH},
Title = {{INFLUENCE OF DISTURBANCE ON INSECT COMMUNITIES IN PACIFIC-NORTHWEST
   STREAMS}},
Journal = {{HYDROBIOLOGIA}},
