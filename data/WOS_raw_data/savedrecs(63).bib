@article{ ISI:A1993MN13000018,
Author = {WELTON, JS and LADLE, M},
Title = {{THE EXPERIMENTAL TREATMENT OF THE BLACKFLY, SIMULIUM-POSTICATUM IN THE
   DORSET-STOUR USING THE BIOLOGICALLY PRODUCED INSECTICIDE
   BACILLUS-THURINGIENSIS VAR ISRAELENSIS}},
Journal = {{JOURNAL OF APPLIED ECOLOGY}},
Year = {{1993}},
Volume = {{30}},
Number = {{4}},
Pages = {{772-782}},
Abstract = {{1. The blackfly Simulium posticatum causes a severe biting problem in
   the valley of the river Stour in Dorset, UK. Various control measures
   have been attempted including egg removal and weed cutting to remove
   associated larvae. 2. Following a small-scale trial of the larvicide
   Bacillus thuringiensis var. israelensis (Bti) in a side-stream of the
   Stour during 1989, permission was obtained to treat four sites on the
   main river between Blandford and Wimborne in 1991. 3. The effect of Bti
   treatment was monitored at sites up to 1300 m downstream of the
   application points. Mortality of S. posticatum was between 82\% and 98\%
   except for the site 1300 m downstream of Blandford where the kill was
   68\%. 4. Extensive sampling of Chironomidae and other benthic
   invertebrates over a period of 84 days failed to show any effect of
   treatment on non-target organisms. 5. Diurnal drift sampling was
   undertaken to assess any possible change in behaviour of invertebrates
   and to sample any dead animals in the water body. Again, no effect was
   found with the exception of dead larvae of S. posticatum within 1 day of
   treatment. 6. By careful monitoring of simuliid populations it was
   possible to treat the river when only S. posticatum larvae were present,
   between winter and summer generations of other species.}},
