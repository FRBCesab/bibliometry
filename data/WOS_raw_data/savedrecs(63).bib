@article{ ISI:A1993LG88100004,
Author = {VINCENT, LS},
Title = {{THE NATURAL-HISTORY OF THE CALIFORNIA TURRET SPIDER ATYPOIDES-RIVERSI
   (ARANEAE, ANTRODIAETIDAE) - DEMOGRAPHICS, GROWTH-RATES, SURVIVORSHIP,
   AND LONGEVITY}},
Journal = {{JOURNAL OF ARACHNOLOGY}},
Year = {{1993}},
Volume = {{21}},
Number = {{1}},
Pages = {{29-39}},
Abstract = {{A large and dense population of over 500 burrows of Atypoides riversi in
   a 2.0 x 3.2 m area was monitored for two years to indirectly determine
   demographics, growth rates, survivorship and longevity of the spiders.
   Twelve size classes of spiders were designated by correlating spider
   size to burrow size. All size classes were present simultaneously
   throughout the year. Variable growth rates were recorded for spiders in
   each size class, and survivorship was lowest for spiders in the smallest
   size classes. It is estimated, based in large part on growth rates, that
   A. riversi can live at least 16 years in the field.}},
