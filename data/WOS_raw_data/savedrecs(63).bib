@article{ ISI:A1993NA60800006,
Author = {HAAGSMA, KA and RUST, MK},
Title = {{2 MARKING DYES USEFUL FOR MONITORING FIELD POPULATIONS OF
   RETICULITERMES-HESPERUS (ISOPTERA, RHINOTERMITIDAE)}},
Journal = {{SOCIOBIOLOGY}},
Year = {{1993}},
Volume = {{23}},
Number = {{2}},
Pages = {{155-164}},
Abstract = {{Four histological dyes, Nile Blue A, Neutral Red, Sudan Red 7B, and
   Sudan IV were evaluated as markers for population studies on the western
   subterranean termite, Reticulitermes hesperus. Neutral Red and Nile Blue
   A provided marks easily visible for up to 11 and 15 weeks, respectively.
   Mortality resulting from treatment with these dyes at the lowest
   concentrations tested was not significant. Sudan Red 7B and Sudan IV
   were considered inadequate markers because coloration of termites faded
   rapidly (<18 days) following treatment. Nile Blue and Neutral Red were
   not passed in sufficient quantities via trophallaxis following treatment
   to be visible in non-treated worker termites.}},
