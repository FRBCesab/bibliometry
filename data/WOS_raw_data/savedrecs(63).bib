@article{ ISI:A1992JG53100013,
Author = {NIEMEYER, H},
Title = {{MONITORING IPS-TYPOGRAPHUS AND PITYOGENES-CHALCOGRAPHUS (COL,
   SCOLYTIDAE) IN LOWER SAXONY AND SCHLESWIG-HOLSTEIN}},
Journal = {{JOURNAL OF APPLIED ENTOMOLOGY-ZEITSCHRIFT FUR ANGEWANDTE ENTOMOLOGIE}},
Year = {{1992}},
Volume = {{114}},
Number = {{1}},
Pages = {{98-102}},
Month = {{JUL}},
Abstract = {{Due to the intensive management of German forests, the main purpose of
   bark beetle monitoring is not to detect stands of high infestation risk.
   Much more, monitoring shall give a rough idea of regional population
   trends. Some years experience show that single mo tor traps more than 50
   m apart from each other use to cause pheromone induced infestations in
   nearby standing spruce.  Thus, in 1988 a system of monitoring has been
   established in Lower Saxony and Schleswig-Holstein which consists of
   mass trapping lines along stand margins as usual (distances of 30-50 m
   between traps and 10-15 m between traps and next spruce) but each of
   them with one fortnightly checked trap:  the monitor trap. This system
   helps reducing infestations and provides records of semimonthly data
   from April to September. The analysis of 3 years of flight activity
   patterns and data tables did not reveal a method to use these data for a
   long term prediction. However, a short term prediction based on monthly
   warning messages referring to regions seems to be feasible, provided
   that corresponding short term thresholds of infestation risks will be
   elaborated.}},
