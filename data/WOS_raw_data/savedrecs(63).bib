Title = {{Effects of vegetation composition and land-use variables on bait station
   visitations by black bears}},
Booktitle = {{PROCEEDINGS OF THE FORTY-EIGHTH ANNUAL CONFERENCE - SOUTHEASTERN
   ASSOCIATION OF FISH AND WILDLIFE AGENCIES}},
Year = {{1994}},
Pages = {{252-260}},
