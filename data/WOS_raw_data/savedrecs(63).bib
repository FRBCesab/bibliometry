@article{ ISI:A1992JW98600004,
Author = {MAKI, AW},
Title = {{OF MEASURED RISKS - THE ENVIRONMENTAL IMPACTS OF THE PRUDHOE BAY,
   ALASKA, OIL-FIELD}},
