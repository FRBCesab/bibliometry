@article{ ISI:A1993MC69900007,
Author = {TEFERI, T and MILLAR, JS},
Title = {{EARLY MATURATION BY NORTHERN PEROMYSCUS-MANICULATUS}},
Journal = {{CANADIAN JOURNAL OF ZOOLOGY-REVUE CANADIENNE DE ZOOLOGIE}},
Year = {{1993}},
Volume = {{71}},
Number = {{9}},
Pages = {{1743-1747}},
Month = {{SEP}},
Abstract = {{Female Peromyscus maniculatus rarely breed as young of the year in the
   Kananaskis Valley, southwestern Alberta. We monitored breeding by
   young-of-the-year females in unmanipulated sites from 1985 to 1991 and
   on manipulated sites (females removed, males removed, and food added)
   from 1989 to 1991. Breeding by young females was uncommon on all
   manipulated sites (16\% when food was added, 3 \% when females were
   removed, 0\% when males were removed). Females that matured as young of
   the year tended to have relatively high growth rates as nestlings. We
   conclude that individual quality is more important than social cues in
   regulating maturation by young females in this population.}},
