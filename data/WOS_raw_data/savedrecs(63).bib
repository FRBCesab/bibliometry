@article{ ISI:A1993ME67600005,
Author = {RASMUSSEN, LEL and CROW, GL},
Title = {{SERUM CORTICOSTERONE CONCENTRATIONS IN IMMATURE CAPTIVE WHITETIP REEF
   SHARKS, TRIAENODON OBESUS}},
Journal = {{JOURNAL OF EXPERIMENTAL ZOOLOGY}},
Year = {{1993}},
Volume = {{267}},
Number = {{3}},
Pages = {{283-287}},
Month = {{NOV 1}},
Abstract = {{Serum concentrations of the steroid hormone corticosterone were
   monitored over a four-year period in two male and two female immature
   captive whitetip reef sharks, Triaenodon obesus. Corticosterone
   concentrations in the serum ranged from 69-999 pg/ml (319.8 +/- 31.9; n
   = 36) for males and 3-246 pg/ml (116.0 +/- 15.1; n = 32) for females.
   Males had significantly higher values than females (P < 0.001). No
   significant difference was detected between females; however, a
   significant difference was observed between the two males. No
   seasonality was observed in corticosterone levels. Corticosterone levels
   in elasmobranchs are only 10\% the levels of the principal
   glucocorticoid, 1 alpha-hydroxycorticosterone, but the assay is
   inexpensive and readily available, and, once individual ranges are
   established, the corticosterone concentrations have utility. Our data
   suggest that, after these baseline levels are established for individual
   sharks, long-term monitoring of serum corticosterone in captive
   populations may be utilized to monitor health or, conversely, stress.
   (C) 1993 Wiley-Liss, Inc.}},
