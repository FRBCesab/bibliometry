@article{ ISI:A1993LX81700005,
Author = {MORENO, CA and REYES, A and ASENCIO, G},
Title = {{HABITAT AND MOVEMENTS OF THE RECRUITS OF CONCHOLEPAS-CONCHOLEPAS
   (MOLLUSCA, MURICIDAE) IN THE ROCKY INTERTIDAL OF SOUTHERN CHILE}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
Year = {{1993}},
Volume = {{171}},
Number = {{1}},
Pages = {{51-61}},
Abstract = {{The habitat choice of the recruits of Concholepas concholepas Brugiere
   (Mollusca; Gastropoda; Muricidae) was studied during the recruitment
   seasons of 1989 and 1991. From June to December 1989, the new cohort was
   monitored measuring individual size, type of microhabitat utilized and
   density, using 45 permanent plots scattered on the rocks of the Marine
   Reserve of Mehuin, at three tidal levels (high, mid and low intertidal).
   The low tide fringe of the surf zone was found to be the main settlement
   habitat. In such environmental conditions, the post-settled individuals
   are associated with a number of sea anemone species, remaining attached
   to the base of their columns from June to August. An anemone removal
   experiment conducted during the recruitment season of 1991, showed that
   this association did not have significant influence on the density of
   recruits. In September 1989, the mean size of the individuals reached 8
   mm of peristomatic length; then, the young recruits began to move from
   low intertidal and submerged habitats towards the emergent
   mid-intertidal habitats and also into the high intertidal in rocks
   subject to high levels of wave exposure. These movements were tested
   during the 1991 season using 500 marked individuals released in the low
   intertidal; we observed some of the marked individuals living in the mid
   and high intertidal during the summer, in spite of the recapture rate
   being very low (4\% for the last observation). This displacement of the
   individuals into different habitats, explains why in previous studies
   recruitment in southern Chile was detected mainly in August to December
   in the mid-intertidal, with individuals whose sizes were near to 10 mm
   of peristomatic length.}},
