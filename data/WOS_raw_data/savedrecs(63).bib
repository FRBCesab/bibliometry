@article{ ISI:A1993LA40700004,
Author = {HAWKINS, PR and GRIFFITHS, DJ},
Title = {{ARTIFICIAL DESTRATIFICATION OF A SMALL TROPICAL RESERVOIR - EFFECTS UPON
   THE PHYTOPLANKTON}},
Journal = {{HYDROBIOLOGIA}},
