@article{ ISI:A1993LZ75200005,
Author = {BELANGER, SE and DAVIDSON, DH and FARRIS, JL and REED, D and CHERRY, DS},
Title = {{EFFECTS OF CATIONIC SURFACTANT EXPOSURE TO A BIVALVE MOLLUSK IN STREAM
   MESOCOSMS}},
