@article{ ISI:000262033600001,
Author = {Brunton, Dianne H. and Evans, Barbara A. and Ji, Weihong},
Title = {{Assessing natural dispersal of New Zealand bellbirds using song type and
   song playbacks}},
Journal = {{NEW ZEALAND JOURNAL OF ECOLOGY}},
Year = {{2008}},
Volume = {{32}},
Number = {{2}},
Pages = {{147-154}},
Abstract = {{New Zealand's managed offshore islands provide Sanctuary to endangered
   and rare fauna but also benefit common native species. These productive
   islands may facilitate the expansion of mobile species back to the
   mainland. In northern New Zealand. many mainland protected sites are
   located oil coastal headlands within short distances of these offshore
   islands. Bellbirds (Anthornis melanura), locally extinct oil the
   mainland of this region for > 100 years, are capable of dispersing these
   distances and are occasionally sighted along the coast. Nonetheless, it
   was unknown whether they had established breeding populations. Natural
   dispersal events are difficult to assess in terms of their Source.
   structure and likelihood Of Succeeding. Females are generally more
   difficult to detect but when present provide conservation practitioners
   with confidence that a population may establish. Here we test a
   non-invasive monitoring method for a self-reintroduced Population of
   bellbirds at Tawharanui, a managed coastal headland Situated equal
   distances (20 km) from two potential Source populations. Little Barrier
   and Tiritiri Matangi islands (LBI and Tiri, respectively), Bellbird son,
   playbacks effectively confirmed the presence of both male and female
   bellbirds. The male and female song, types recorded at Tawharanui were
   not found oil Tiri but matched those of LBI and we propose this as the
   Source Population. We tested our playback protocol at other coastal
   parks and advocate annual playback surveys for detecting new populations
   at potential mainland Sites.}},
