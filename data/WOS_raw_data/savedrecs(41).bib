@inproceedings{ ISI:000258728300013,
Author = {Jackson, L. Scott and Parsons, Gary H. and Davis, Glen and Crosby, Ken},
Editor = {{McLaughlin, KD}},
Title = {{Assessment of nearshore artificial reefs in Okaloosa County, Florida by
   volunteers using side scan sonar}},
