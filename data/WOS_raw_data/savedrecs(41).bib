@article{ ISI:000253003600024,
Author = {Blanco, Carlos A. and Storer, Nicholas P. and Abel, Craig A. and
   Jackson, Ryan and Leonard, Rogers and Lopez, Jr., Juan D. and Payne,
   Gregory and Siegfried, Blair D. and Spencer, Terence and Teran-Vargas,
   Antonio P.},
Title = {{Baseline susceptibility of tobacco budworm (Lepidoptera : Noctuidae) to
   Cry1F toxin from Bacillus thuringiensis}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{2008}},
Volume = {{101}},
Number = {{1}},
Pages = {{168-173}},
Month = {{FEB}},
Abstract = {{Transgenic cotton, Gossypium hirsutum L., lines expressing both Cry1F
   and Cry1Ac insecticidal proteins from Bacillus thuringiensis (Bt) have
   been commercially available in the United States since 2005. Both Bt
   proteins are highly effective against tobacco budworm, Heliothis
   virescens (F.), and other lepidopteran pests of cotton. Although Cry1Ac
   has been available in Bt cotton since 1996, the Cry1F component is
   relatively new. As part of the proactive resistance management program
   for Cry1F/Cry1Ac cotton, a susceptibility-monitoring program is being
   implemented. Baseline variation in the susceptibility to Cry1F in field
   populations of tobacco budworm was measured. There was a three-fold
   variation in the amount of Cry1F needed to kill 50\% of the neonates
   from 15 different field populations from the southern and central United
   States. Future variation in susceptibility of tobacco budworm
   populations to Cry1F or even resistance evolution could be documented
   based on this baseline data. A candidate diagnostic concentration was
   determined that may be efficiently used to identify individuals that
   potentially carry major alleles conferring field-relevant resistance to
   Cry1F before such alleles spread through field populations.}},
