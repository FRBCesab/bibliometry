@inproceedings{ ISI:000254264800069,
Author = {Molls, Frank and Nemitz, Armin},
Editor = {{Nielsen, J and Dodson, JJ and Friedland, K and Hamon, TR and Musick, J and Verspoor, E}},
Title = {{Restoration of Atlantic salmon and other diadromous fishes in the Rhine
   River system}},
