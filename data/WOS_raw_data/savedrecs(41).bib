@inproceedings{ ISI:000263597300037,
Author = {Bezold, Justin and Peterson, Douglas L.},
Editor = {{Allen, MS and Sammons, S and Maceina, MJ}},
Title = {{Assessment of Lake Sturgeon Reintroduction in the Coosa River System,
   Georgia-Alabama}},
Booktitle = {{BALANCING FISHERIES MANAGEMENT AND WATER USES FOR IMPOUNDED RIVER
   SYSTEMS}},
Series = {{American Fisheries Society Symposium}},
Year = {{2008}},
Volume = {{62}},
Pages = {{571-586}},
Note = {{4th North American Reservoir Symposium on Balancing Fisheries Management
   and Water Uses for Impounded River Systems, Atlanta, GA, JUN 07-09, 2007}},
Organization = {{Amer Fisheries Soc, SE Div Reservoir Comm}},
Abstract = {{At the extreme Southern boundary of its range, the lake Sturgeon
   Acipenser fulvescens was once an important biological component of the
   Coosa River system of Georgia-Alabama. During the 1970s, the Population
   was extirpated by the combined effects of overfishing and degraded water
   quality. Over the past 20 years, water quality has improved, and in
   2002, the Georgia Department of Natural Resources initiated a lake
   Sturgeon reintroduction program with the goal of establishing a
   self-sustaining population within 20 years. Front 2004, to 2007, we
   evaluated the initial phase of this reintroduction program by
   quantifying poststocking survival and seasonal habitat use of Juvenile
   lake sturgeon in the Coosa River. We used gill nets and trammel nets to
   capture juveniles at several locations in both riverine and reservoir
   habitats. Fourteen individual Juveniles were randomly selected for
   surgical implantation of radio tags to monitor their seasonal movements
   and habitat use. Over the 3 years of the Study, we captured a total of
   597 Juvenile lake sturgeon measuring 231-790 nun total length. Using
   capture probabilities calculated from Program MARK (White and Burnham
   1999), we estimated a total abundance of 789 (690-889, 95\% confidence
   interval) juvenile lake sturgeon in 2006. Survival of each cohort from
   date stocked to Summer 2006 varied from 1\% to 14\%, depending on year
   and size of fish stocked. Seasonal movements of Juveniles varied;
   however, most fish occupied a relatively short reach in the lower river
   (hiring summer months when water temperatures were more than 25 degrees
   C. At least 1\% of fish stocked in each cohort have survived. and the
   population appears to be gradually increasing with each additional year
   of stocking. Further studies are needed to monitor annual recruitment
   and to evaluate reproductive success as first cohorts reach maturity.}},
