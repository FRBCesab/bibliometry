@article{ ISI:000256390800013,
Author = {Mendel, Jan and Lusk, Stanislav and Kosco, Jan and Vetesnik, Lukas and
   Halacka, Karel and Papousek, Ivo},
Title = {{Genetic diversity of Misgurnus fossilis populations from the Czech
   Republic and Slovakia}},
Journal = {{FOLIA ZOOLOGICA}},
Year = {{2008}},
Volume = {{57}},
Number = {{1-2}},
Pages = {{90-99}},
Note = {{3rd International Conference on Loaches of the Genus Cobitis and Related
   Genera, Sibenik, CROATIA, SEP 24-29, 2006}},
Organization = {{Croatia Ichtythyol Soc; Univ Zagreb, Dept Zool, Fac Sci}},
Abstract = {{Monitoring of the analysed populations of the endangered fish species
   Misgurnus fossilis was conducted using microsatellite analysis and
   sequencing of a part of the control region. Absolutely first
   microsatellite markers for weather loach were found. Six polymorphic
   microsatellite loci were prepared, five of which were tested. Number of
   alleles per a locus ranged from 3 to 5. All studied populations are
   differentiated one from another (F(ST) = 0.205 - 0.367). All sampled
   populations Contained unique alleles. Sequential analysis of the
   mitochondrial control region showed great haplotype similarity of the
   studied populations which come from one widely spread haplotype H\_1,
   and thus suggested possible hypothesis of recent spreading from one
   source.}},
