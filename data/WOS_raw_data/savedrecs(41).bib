@article{ ISI:000261822400001,
Author = {Mengak, Michael T. and Castleberry, Steven B.},
Title = {{Influence of Acorn Mast on Allegheny Woodrat Population Trends in
   Virginia}},
Journal = {{NORTHEASTERN NATURALIST}},
