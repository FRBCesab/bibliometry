@article{ ISI:000256746400007,
Author = {Bryan, Jr., A. Lawrence and Brooks, William B. and Taylor, Jimmy D. and
   Richardson, David M. and Jeske, Clinton W. and Brisbin, Jr., I. Lehr},
Title = {{Satellite tracking large-scale movements of Wood Storks captured in the
   Gulf Coast region}},
Journal = {{WATERBIRDS}},
Year = {{2008}},
Volume = {{31}},
Number = {{1}},
Pages = {{35-41}},
Note = {{Wood Stork Symposium held at the Annual Meeting of the
   Waterbird-Society, Jekyll Isl, GA, 2005}},
Organization = {{Waterbird Soc}},
Abstract = {{Wood Storks (Mycteria americana) breeding in the southeastern United
   States exhibit lengthy inter- and intra-regional movements in response
   to resource availability. One type of movement., post-breeding
   dispersal, has resulted in storks from this population temporarily
   moving into the Gulf Coast states of Alabama and eastern Mississippi.
   Concurrently, other Wood Storks, presumably of Mexican/Central American
   origin, are frequently observed in large numbers in the Mississippi
   River Valley and nearby western states during the late-summer months and
   the proximity of dispersing storks from the two regions might result in
   population mixing. In a preliminary examination of the origins of Wood
   Storks observed in Gulf Coast states and the likelihood of population
   mixing, we deployed ten satellite transmitters on storks in those areas
   in June-July of 2003 and monitored their movements. All storks captured
   in eastern Mississippi moved into southern Florida. Storks captured in
   western Mississippi went to eastern Mexico or western Guatemala. One
   stork from Louisiana went to Mexico and the other went to southern
   Florida, suggesting that population mixing may be occurring on their
   non-breeding season habitats.}},
