@article{ ISI:000259160600002,
Author = {Reis, Marcelo de Almeida and Zanetti, Ronald and Soares Scolforo, Jose
   Roberto and Ferreira, Maria Zelia and Zanuncio, Jose Cola},
Title = {{Sampling of leaf-cutting ant nests (Hymenoptera : Formicidae) in
   eucalyptus plantations using quadrant and Prodan methods}},
Journal = {{SOCIOBIOLOGY}},
Year = {{2008}},
Volume = {{51}},
Number = {{1}},
Pages = {{21-29}},
Abstract = {{This study was carried out in the Municipality of Belo Oriente, Minas
   Gerais State, Brazil from January 2002 to February 2005 in five
   eucalyptus stands with a total area of 160.22 hectares. The objectives
   were to develop sampling plans for leaf-cutting ant nests with the
   quadrant and Prodan methods and to determine the best estimator for
   their population density in eucalyptus plantations in areas with steep
   relief in Minas Gerais State, Brazil. The eucalyptus stands were divided
   into contiguous sample units with two rows long by one plant wide. The
   leaf-cutting ant nests were located, identified and measured in each
   sample. Maps were prepared with the location of these nests and of the
   eucalyptus trees per stand to simulate the sampling plans. The estimate
   of the number of leaf-cutting ant nests by the Pollard estimator was
   more precise than that of the Cottam \& Curtis for the quadrant method.
   The Pollard and Prodan estimators are adequate to be used in programs of
   monitoring leaf-cutting ant nests in eucalyptus plantations in this
   region.}},
