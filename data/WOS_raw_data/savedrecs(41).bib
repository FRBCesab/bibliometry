@article{ ISI:000256887000006,
Author = {Wilhelm, Michael and Kraft, Martin and Rauchfuss, Knut and Hoelzer,
   Juergen},
Title = {{Assessment and management of the first German case of a contamination
   with perfluorinated compounds (PFC) in the region Sauerland, North
   Rhine-Westphalia}},
Journal = {{JOURNAL OF TOXICOLOGY AND ENVIRONMENTAL HEALTH-PART A-CURRENT ISSUES}},
