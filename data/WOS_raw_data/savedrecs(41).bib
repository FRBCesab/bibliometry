@inproceedings{ ISI:000293693900004,
Author = {Bianchi, Gabriella},
Editor = {{Bianchi, G and Skjoldal, HR}},
Title = {{The Concept of the Ecosystem Approach to Fisheries in FAO}},
Booktitle = {{ECOSYSTEM APPROACH TO FISHERIES}},
