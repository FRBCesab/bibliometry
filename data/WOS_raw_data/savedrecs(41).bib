@article{ ISI:000263036700003,
Author = {Kochubovski, M. and Janevski, J. and Dimovska, M. and Simjanoski, P. and
   Ristova, V.},
Title = {{MONITORING OF THE AMBIENT AIR QUALITY IN SKOPJE AND VELES AND EVALUATION
   OF THE HEALTH EFFECTS IN 1990-2006}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2008}},
Volume = {{9}},
Number = {{4}},
Pages = {{743-752}},
Abstract = {{The aim of the study was to evaluate ambient air quality in Skopje and
   Veles with parameters sulphur dioxide and black smoke and respiratory
   morbidity in the period 1990-2006. According to the monitoring results,
   it has been Found significant high statistical difference (the Student
   t-test t = 10.657, p < 0.0001) between the average annual concentrations
   of sulphur dioxide in the ambient air during the period of examination,
   with higher values in Veles (average = 0.092 mg/m(3)), compared to
   Skopje (average = 0.030 mg/m(3)). Decreasing trend of sulphur dioxide
   concentrations has been registered in both cities, with stress on
   Skopje. The same situation has been noticed concerning black smoke, with
   significant lower levels in Veles (average = 0.02 mg/m(3)), compared to
   Skopje (average = 0.03 mg/m(3)). Decreasing trend has been found in both
   cities, with accent to Skopje. There has not been registered significant
   statistical difference (the Student t-test t = -0.6T p > 0.05) between
   the average annual concentrations of black smoke in the ambient air
   during the period of examination, with lower values in Veles (average =
   0.029 mg/m(3)), compared on Skopje (average = 0.032 mg/m(3)). Decreasing
   trend of black smoke concentrations has been registered in both cities,
   with accent on Skopje. Incidence rate of respiratory, diseases was
   higher in Veles, compared to Skopje and the country level. There has
   been registered decreasing trend of incidence rate of respiratory
   diseases (ICD-10 J00-J99) on 1000 Population in the period 1990-2005 in
   Skopje and Veles as well in the whole Republic of Macedonia. But, it has
   to be stressed that the trend in Veles is with a lower declination
   compared to Skopje and to the whole country.}},
