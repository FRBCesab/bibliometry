@article{ ISI:000261194700002,
Author = {Dean, Andrew P. and Nicholson, James M. and Sigee, David C.},
Title = {{Impact of phosphorus quota and growth phase on carbon allocation in
   Chlamydomonas reinhardtii: an FTIR microspectroscopy study}},
Journal = {{EUROPEAN JOURNAL OF PHYCOLOGY}},
