@inproceedings{ ISI:000076629600018,
Author = {Mahmood, F},
Editor = {{Rupp, HR}},
Title = {{Detection of eastern equine encephalitis virus in wild-caught Culiseta
   melanura females by the reverse transcriptase polymerase chain reaction:
   The 1995 trials}},
Booktitle = {{PROCEEDINGS OF THE 83RD ANNUAL MEETING OF THE NEW JERSEY MOSQUITO
   CONTROL ASSOCIATION INC}},
Year = {{1996}},
Pages = {{84-94}},
Note = {{83rd Annual Meeting of the New-Jersey-Mosquito-Control-Association-Inc,
   ATLANTIC CITY, NJ, MAR 05-08, 1996}},
Organization = {{New Jersey Mosquito Control Assoc}},
Abstract = {{This study monitored activity of eastern equine encephalitis virus
   (EEEV) in bird populations in southern NJ. Culiseta melanura was used as
   an index for the EEEV levels in birds. Reverse transcriptase polymerase
   chain reaction (RT-PCR) and wet-chick/IFA techniques were employed for
   EEEV detection. A total of 5,174 females comprising 501 pools were
   tested from coastal and inland sites. Sixty-three pools tested positive
   by the RT-PCR compared to 6 pools tested positive by the wet-chick/IFA
   technique. Significantly, more pools tested positive from the coastal
   sites by both techniques, but a higher minimum field infection rate
   (MFIR)/1,000 females was detected by RT-PCR from the inland sites. A
   maximum MFIR was detected by the IFA technique from the coastal sites,
   suggesting more infectious virus was transmitted to birds at these
   sites. Early EEEV activity was detected in June from Dennisville and the
   1(st) positive pool of the season consisting of gravid females was
   collected on 6/7/96. The 1(st) positive pool detected by the
   wet-chick/IFA technique was from Dennisville on 7/5/96 and was the
   earliest detection of EEEV by this method in 15 years. A comparison of
   the 2 techniques showed RT-PCR detected a maximum percentage of positive
   pools in late-blood-fed females; whereas the maximum percentage of
   positive unfed pools was detected by the wet-chick/IFA technique. The
   1995 vector surveillance season differed ii-om 1994 in having peak
   mosquito populations in June and July and the populations greatly
   reduced from August-October.}},
