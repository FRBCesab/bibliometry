@article{ ISI:A1996VL82400017,
Author = {Wilkinson, SB and Zheng, WZ and Allen, JR and Fielding, NJ and Wanstall,
   VC and Russell, G and Hawkins, SJ},
Title = {{Water quality improvements in Liverpool docks: The role of filter
   feeders in algal and nutrient dynamics}},
Journal = {{MARINE ECOLOGY-PUBBLICAZIONI DELLA STAZIONE ZOOLOGICA DI NAPOLI I}},
Year = {{1996}},
Volume = {{17}},
Number = {{1-3}},
Pages = {{197-211}},
