@inproceedings{ ISI:A1996BJ71G00012,
Author = {Shaw, PW and Walker, JTS},
Book-Group-Author = {{NEW ZEALAND PLANT PROTECT SOC INC}},
Title = {{Biological control of woolly apple aphid by Aphelinus mali in an
   integrated fruit production programme in Nelson}},
