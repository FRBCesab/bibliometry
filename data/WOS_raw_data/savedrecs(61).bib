@inproceedings{ ISI:A1996BG89S00005,
Author = {Daly, JC and Trowell, S},
Editor = {{Symondson, WOC and Liddell, JE}},
Title = {{Biochemical approaches to the study of ecological genetics: The role of
   selection and gene flow in the evolution of insecticide resistance}},
Booktitle = {{ECOLOGY OF AGRICULTURAL PESTS: BIOCHEMICAL APPROACHES}},
Series = {{SYSTEMATICS ASSOCIATION SPECIAL VOLUMES}},
Year = {{1996}},
Number = {{53}},
Pages = {{73-92}},
Note = {{International Symposium on the Ecology of Agricultural Pests -
   Biochemical Approaches, UNIV WALES CARDIFF, CARDIFF, WALES, SEP 21-23,
   1994}},
Organization = {{Systemat Assoc; Biochem Soc, Tech Grp; Univ Wales Cardiff, Sch Molec \&
   Med Biosci; Univ Wales Cardiff, Sch Pure \& App Biol}},
Abstract = {{The selection and spread of insecticide resistance is an evolutionary
   phenomenon. Resistance becomes a problem for pest control when an allele
   conferring resistance increases in frequency within populations and then
   spreads more widely. Ecological genetics studies the interaction between
   population structure and the factors - mutation, selection, genetic
   drift and gene flow - that produce these changes. Traditionally,
   resistance studies identified only two phenotypes, susceptible or
   resistant, and made simplifying assumptions about the inheritance of
   resistance. Such an approach has impeded our ability to test alternative
   hypotheses about the evolution of resistance because the resistance
   phenotype can involve more than one major gene; the genetic basis of
   resistance can change over time; and the expression of resistance can
   vary throughout the life-cycle. More detailed evolutionary studies of
   resistance require suitable tools. Biochemical techniques, such as
   electrophoresis, antibody, protein and DNA-based technologies, provide
   the means to identify unambiguously gene products, or genes and their
   allelic variants. The techniques can address a wide range of issues such
   as: separation of sibling species, detection and monitoring of resistant
   individuals, and the distribution of resistance genes in populations.}},
