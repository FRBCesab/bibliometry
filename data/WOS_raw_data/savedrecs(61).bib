@article{ ISI:A1996VB13300004,
Author = {Haarhoff, J and VanderMerwe, B},
Title = {{Twenty-five years of wastewater reclamation in Windhoek, Namibia}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
