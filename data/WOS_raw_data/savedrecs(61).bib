@article{ ISI:A1996TU16500003,
Author = {Vincent, JP and Hewison, AJM and Angibault, JM and Cargnelutti, B},
Title = {{Testing density estimators on a fallow deer population of known size}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{1996}},
Volume = {{60}},
Number = {{1}},
Pages = {{18-28}},
Month = {{JAN}},
Abstract = {{We tested 1 index of density (a kilometric index: number of sightings
   per km of transect) and 2 estimators of population number
   (Petersen-Lincoln, strip transect), all based on direct observation
   data, against actual animal number in an enclosed population of fallow
   deer (Danza dama) of known size. The study included phases of population
   increase, stability and decrease, and took place in woodland habitat
   typical of that inhabited by wild temperate ungulates. All 3 estimators
   were good reflectors of population trend, explaining 75-96\% of the
   variation in actual population number during the e-year study. Using the
   Petersen-Lincoln method and the strip transect method, reasonably
   unbiased estimates of absolute population size were obtained (percent
   relative bias: between -8.1 and 8.2\%, and -19.5 and -3.4\%,
   respectively), however, some bias was detected in each measure. Little
   difference in observability among sex-age classes or among marked
   individuals was found, suggesting such data may be used to determine
   population structure. For management purposes, a kilometric index based
   on highly standardized methods may provide a powerful, cost-effective
   tool to monitor population trend. Where an estimate of absolute animal
   number is required and habitat structure is sufficiently homogeneous to
   aid measurement of the width of the observation band, these same data
   may be used in a strip transect approach, extrapolating to an estimate
   of population size. Whatever the method employed, use of data from
   summer and autumn periods should be avoided because of problems of
   differential visibility during the growing season and altered behavior
   patterns during the autumn rut.}},
Publisher = {{WILDLIFE SOC}},
Address = {{5410 GROSVENOR LANE, BETHESDA, MD 20814-2197}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Vincent, JP (Reprint Author), INRA,INST RECH GRANDS MAMMIFERES,BP 27,F-31326 CASTANET TOLOSAN,FRANCE.}},
DOI = {{10.2307/3802035}},
ISSN = {{0022-541X}},
Keywords = {{Dama dama; density indices; enclosure; fallow deer; Petersen estimate;
