@article{ ISI:A1995TM82100001,
Author = {Goerzen, DW and Richards, KW and Rank, GH},
Title = {{Drifting in populations of the alfalfa leafcutting bee, Megachile
   rotundata (Fab) (Hym, Megachilidae)}},
Journal = {{JOURNAL OF APPLIED ENTOMOLOGY-ZEITSCHRIFT FUR ANGEWANDTE ENTOMOLOGIE}},
Year = {{1995}},
Volume = {{119}},
Number = {{9}},
Pages = {{575-579}},
Month = {{NOV}},
Abstract = {{The within-field drifting of Megachile rotundata (Fab.) females was
   evaluated at three field sites in Saskatchewan and one site in Alberta,
   Canada. Marked females were released at field shelters and bee movement
   was monitored throughout the field season. High levels of bee drifting
   were observed at all locations. An average of 32.3\% (Saskatchewan) or
   51.8\% (Alberta) of marked females were observed to have drifted away
   from their release site. The distance female bees drifted was predicted
   using exponential regression equations and expressed using contour
   response patterns. This high level of within-held movement must be taken
   into account when evaluating held treatments in subpopulations of
   alfalfa leafcutting bees.}},
