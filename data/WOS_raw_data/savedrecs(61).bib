@article{ ISI:A1996WZ96600004,
Author = {Pradel, R},
Title = {{Animal dispersal within subdivided populations: An approach based on
   monitoring individuals}},
Journal = {{ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY}},
Year = {{1996}},
Volume = {{17}},
Number = {{6}},
Pages = {{475-483}},
Note = {{Symposium on the Ecology and Conservation of Spatially Structured
   Populations, CTR ETUDES BIOLOG CHIZE, CHIZE, DEUX-SEVRES, FRANCE, OCT
   18-20, 1996}},
Abstract = {{Many hypotheses have been advanced for the proximate causes of
   dispersal, and the theoretical consequences of dispersal have been
   investigated from the points of view of both population dynamics and
   genetics. However, the theory has not yet been put convincingly to the
   test because of the lack of suitable methods of measuring and analysing
   dispersal in the wild. Monitoring individuals by capture-recapture
   throughout the range occupied by a subdivided population is a good
   candidate provided that 1) entirely new statistical models for the study
   of natal dispersal are developed, 2) the current models for the study of
   breeding dispersal are substantially improved and 3) spatial models of
   population dynamics currently developed are implemented in user-friendly
   software. Given these prerequisites, bird colonies may be an excellent
   biological model because 1) birds are easily marked when young and 2)
   colonies are clearly separated and often conspicuous so that dispersers
   are identified unequivocally. Moreover, some species present a
   manageably small number of colonies.}},
