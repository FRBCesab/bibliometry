@article{ ISI:A1995RY86400030,
Author = {EVERITT, JH and ANDERSON, GL and ESCOBAR, DE and DAVIS, MR and SPENCER,
   NR and ANDRASCIK, RJ},
Title = {{USE OF REMOTE-SENSING FOR DETECTING AND MAPPING LEAFY SPURGE
   (EUPHORBIA-ESULA)}},
Journal = {{WEED TECHNOLOGY}},
Year = {{1995}},
Volume = {{9}},
Number = {{3}},
Pages = {{599-609}},
Month = {{JUL-SEP}},
Abstract = {{Leafy spurge is a troublesome, exotic weed in the northern Great Plains
   of the United States. Leafy spurge produces showy yellow bracts during
   June that give this weed a conspicuous appearance, A study was conducted
   to determine the feasibility of using remote sensing techniques to
   detect leafy spurge in this phenological stage, Study sites were located
   in North Dakota and Montana, Plant canopy reflectance measurements
   showed that leafy spurge had higher visible (0.63- to 0.69-mu m)
   reflectance than several associated plant species, The conspicuous
   yellow bracts of leafy spurge gave it distinct yellow-green and pink
   images on conventional color and color-infrared aerial photographs,
   respectively. Leafy spurge also could be distinguished on conventional
   color video imagery where it had a golden yellow image response.
   Quantitative data obtained from digitized video images showed that leafy
   spurge had statistically different digital values from those of
   associated vegetation and soil, Computer analyses of video images showed
   that light reflected from leafy spurge populations could be quantified
   from associated vegetation, This technique permits area estimates of
   leafy spurge populations. Large format conventional color photographs of
   Theodore Roosevelt National Park near Medora, ND were digitized and
   integrated with a geographic information system to produce a map of
   leafy spurge populations within the park that can be useful to monitor
   the spread or decline of leafy spurge.}},
