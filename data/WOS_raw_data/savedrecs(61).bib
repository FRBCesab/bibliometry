@article{ ISI:A1996UC05400010,
Author = {Sangster, GI and Lehmann, K and Breen, M},
Title = {{Commercial fishing experiments to assess the survival of haddock and
   whiting after escape from four sizes of diamond mesh cod-ends}},
Journal = {{FISHERIES RESEARCH}},
Year = {{1996}},
Volume = {{25}},
Number = {{3-4}},
Pages = {{323-345}},
Month = {{MAR}},
Abstract = {{An investigation is described into the survival of haddock
   (Melanogrammus aeglifinus (L.)) and whiting (Merlangius merlangus (L.)),
   as a result of escape from 70, 90, 100 and 110 mm diamond mesh cod-ends
   towed at 1.5 m s(-1) by a 550 hp commercial fishing vessel. God-end
   escapes were collected and transferred by divers into 35 m(3) cages on
   the seabed where their survival against control fish was monitored over
   a period of 60 days. Triplicated cage experiments showed a range of
   results for the survival of 15-38 cm haddock and 17-35 cm whiting. The
   survival rates for the haddock and whiting experimental groups were
   48-67\% and 52-60\% (70 mm codend), 79-82\% and 73-78\% (90 mm cod-end).
   73-83\% and 67-77\% (100 mm cod-end) and 85-89\% and 83-86\% (110 mm
   cod-end), respectively. The survival rates of the controls were 100\%
   for both species. These percentages relate only to the numbers of
   survivors from the total escapes in a particular cod-end mesh size
   category, regardless of fish length. Further analysis of the data
   revealed that the survival of the smaller cod-end escapes was much worse
   than for larger fish of either species. This suggests that survival may
   be a more complex function of fish length. Furthermore, there was no
   clear relationship between survival and mesh size for haddock and
   whiting of a given length over the diamond mesh range of 70-110 mm. The
   data from each of the haddock and whiting survival/mortality probability
   curves were applied to the four mesh size selectivity gives obtained on
   the actual haddock and whiting Populations studied. This produced data
   to predict the Fate of haddock and whiting of given length after
   entering the trawl for each of the four diamond mesh sizes, in terms of
   survival, mortality, discards and marketable catch.}},
