@article{ ISI:A1995RP24400020,
Author = {EVENDEN, ML and BORDEN, JH and VANSICKLE, GA and GRIES, G},
Title = {{DEVELOPMENT OF A PHEROMONE-BASED MONITORING-SYSTEM FOR WESTERN HEMLOCK
   LOOPER (LEPIDOPTERA, GEOMETRIDAE) - EFFECT OF PHEROMONE DOSE, LURE AGE,
   AND TRAP TYPE}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{1995}},
Volume = {{24}},
Number = {{4}},
Pages = {{923-932}},
Month = {{AUG}},
Abstract = {{A two-component pheromone blend containing a 1:1 (vol:vol) ratio of
   isomeric 5,11-dimethylheptadecane and isomeric 2,5-dimethylheptadecane
   was tested in high-capacity Unitraps to monitor populations of the
   western hemlock looper, Lambdina fiscellaria lugubrosa (Hulst),
   throughout the coastal western hemlock and interior cedar hemlock
   biogeoclimatic zones of British Columbia. Rubber septa were loaded with
   each component at doses of 10, 100, 1,000, and 10,000 mu g (1992) and 1
   and 10 mu g (1993). Males demonstrated a dose-dependent response, except
   that captures in traps with the 2 highest doses (1,000 and 10,000 mu g)
   did not differ. Unitraps with 10-mu g lures captured significantly more
   males than similarly baited sticky traps at the beginning of the flight
   and with 1- and 10-mu g-baited lures at midflight. Both 1- and 10-mu g
   lures maintained their attractiveness over the 3-mo flight season, which
   extends from mid-August through October, but catches in traps with 1-mu
   g lures were low. In all cases, >80\% of the males were captured by the
   beginning of October. Seasonal trends in trap catches were not closely
   related io temperature. This research suggests that traps baited with
   10-mu g lures could be used to monitor populations and detect incipient
   outbreaks of the western hemlock looper.}},
