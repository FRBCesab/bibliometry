@article{ ISI:A1995TB12400025,
Author = {NAKAMURA, Y and SUZUKI, SY and HIROMI, J},
Title = {{POPULATION-DYNAMICS OF HETEROTROPHIC DINOFLAGELLATES DURING A
   GYMNODINIUM-MIKIMOTOI RED TIDE IN THE SETO INLAND SEA}},
Journal = {{MARINE ECOLOGY PROGRESS SERIES}},
Year = {{1995}},
Volume = {{125}},
Number = {{1-3}},
Pages = {{269-277}},
Month = {{SEP}},
Abstract = {{Populations of heterotrophic dinoflagellates, phytoplankton and ciliates
   were monitored daily during summer 1994 in the Seto Inland Sea, Japan.
   The growth rates of heterotrophic dinoflagellates in screened seawater
   (<50 mu m) were also measured. During the survey period, a Gymnodinium
   mikimotoi red tide occurred, continued for 1 wk and disappeared.
   Following the outbreak of the red tide, abundance and growth rates of
   heterotrophic dinoflagellates (mainly composed of Gyrodinium dominans
   and Gyrodinium spirale) increased rapidly. Calculations of the impact of
   grazing by the heterotrophic dinoflagellates on the G. mikimotoi red
   tide strongly suggest that they played an important role in the
   disappearance of the red tide. On the other hand, the ciliate population
   (mainly composed of oligotrichs of ca 45 mu m) did not respond to the
   red tide but covaried with the population of small (2 to 8 mu m)
   flagellates, suggesting that ciliates are not direct competitors of
   heterotrophic dinoflagellates in this region.}},
