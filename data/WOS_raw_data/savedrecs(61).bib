@article{ ISI:A1996VY83100001,
Author = {Jungmann, D and Ludwichowski, KU and Faltin, V and Benndorf, J},
Title = {{Field study to investigate environmental factors that could effect
   microcystin synthesis of a Mycrocystis population in the Bautzen
   reservoir}},
Journal = {{INTERNATIONALE REVUE DER GESAMTEN HYDROBIOLOGIE}},
Year = {{1996}},
Volume = {{81}},
Number = {{4}},
Pages = {{493-501}},
Abstract = {{Toxicity of Microcystis blooms to warm-blooded animals generated by
   microcystins has been reported world wide. The ecological relevance of
   microcystin production for cyanobacteria remains unknown. The
   microcystin concentration in Microcystis blooms occurring in the Bautzen
   reservoir was investigated. The microcystin content of samples were
   determined by HPLC and ranged from undetectable to 14.7 mu g mg(-1).
   Various chemical and physical parameters were monitored at the same time
   as Microcystis sampling, however, there was no correlation between these
   parameters and microcystin dynamics. The spatial distribution of
   microcystin in the Microcystis population was investigated once and
   showed no difference between samples taken at five stations. The
   microcystin concentration in the cell free water from the reservoir was
   below the detection threshold (<1 mu g L(-1)). Size dependent fractions
   of the Microcystis population analyzed for microcystin concentration
   correlated with colony size. In the small fraction (>30 <66 mu m)
   microcystin was not detected. In the medium fraction (>66 <100 mu m)
   lower microcystin concentrations were detected than in large fraction
   (>100 mu m) in which the highest microcystin concentrations were found.}},
Publisher = {{AKAD VERLAG GMBH}},
