@article{ ISI:000317047500002,
Author = {Mu, Li and Wen, Jianping},
Title = {{Spatial distribution characteristics of polycyclic musks as a chemical
   marker in river water and sediment compared with other typical
   pollutants}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
