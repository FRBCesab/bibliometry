Title = {{Assessment and conservation of whitefish (Coregonus lavaretus (L.)) in
   the UK}},
Booktitle = {{BIOLOGY AND MANAGEMENT OF COREGONID FISHES - 2011}},
Series = {{Advances in Limnology}},
