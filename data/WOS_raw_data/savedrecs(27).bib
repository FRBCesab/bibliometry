@article{ ISI:000310232900023,
Author = {Park, Bum Soo and Baek, Seung Ho and Ki, Jang-Seu and Cattolico, Rose
   Ann and Han, Myung-Soo},
Title = {{Assessment of EvaGreen-based quantitative real-time PCR assay for
   enumeration of the microalgae Heterosigma and Chattonella
   (Raphidophyceae)}},
Journal = {{JOURNAL OF APPLIED PHYCOLOGY}},
