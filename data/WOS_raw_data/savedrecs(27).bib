@article{ ISI:000314539400002,
Author = {Conard, Jonathan M. and Sandercock, Brett K. and Gipson, Philip S. and
   Ballard, Warren B.},
Title = {{Factors Influencing Survival of Female Elk in a Harvested Population}},
Journal = {{JOURNAL OF FISH AND WILDLIFE MANAGEMENT}},
