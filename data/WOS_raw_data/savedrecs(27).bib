@article{ ISI:000313406400015,
Author = {Paes E Lima, Eron and Wanderlinde, Jucara and de Almeda, Daniella Torres
   and Lopez, Gustave and Goldberg, Daphne Wrobel},
Title = {{Nesting Ecology and Conservation of the Loggerhead Sea Turtle (Caretta
   caretta) in Rio de Janeiro, Brazil}},
Journal = {{CHELONIAN CONSERVATION AND BIOLOGY}},
Year = {{2012}},
Volume = {{11}},
Number = {{2}},
Pages = {{249-254}},
Month = {{DEC}},
Abstract = {{Loggerhead turtle (Caretta caretta) nesting in the Southwest Atlantic
   has been monitored for decades, but information from northern Rio de
   Janeiro State (Brazil) has been lacking until now. In this study, we
   documented 11,086 nests laid between the 1992/1993 and 2010/2011 nesting
   seasons (similar to 1000 nests per season) and found significant
   variations in incubation period and hatching success among in situ and
   relocated nests. Because loggerhead nests in Rio de Janeiro are presumed
   to produce a large proportion of male hatchlings because of lower
   average incubation temperatures, this area is an important component of
   loggerhead population biology and conservation in Brazil and the
   southwest Atlantic region.}},
