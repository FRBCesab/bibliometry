@article{ ISI:000329412300002,
Author = {Farhad, Alipour and Varsha, Wankhade},
Title = {{THE SNAKE FAUNA OF MULSHI TALUKA, PUNE DISTRICT, INDIA}},
Journal = {{RUSSIAN JOURNAL OF HERPETOLOGY}},
Year = {{2013}},
Volume = {{20}},
Number = {{1}},
Pages = {{16-18}},
Month = {{JAN-MAR}},
Abstract = {{Western Ghats of India in general and Pune district in particular, have
   unique geographical and climatic conditions that support a rich flora
   and fauna. In view of the lack of in-depth studies of snakes of this
   region, an investigation was initiated in Mulshi areas of Pune district
   for an inventory of snakes species and their habitats. A total of 158
   specimens were monitored and identified from June 2009 to August 2011.
   Twenty six species belonging 7 families were represented, including:
   Family Typhlopidae, Uropeltidae, Pythonidae, Boidae, Colubridae,
   Elapidae, and Viperidae.
