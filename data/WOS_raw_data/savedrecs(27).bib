@article{ ISI:000315628000001,
Author = {Sedlarova, Michaela and Trojanova, Zuzana and Lebeda, Ales},
Title = {{Distribution and Harmfulness of Plasmopara halstedii on Sunflower in the
   Czech Republic}},
