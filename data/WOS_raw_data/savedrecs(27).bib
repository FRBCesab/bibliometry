@article{ ISI:000313793900009,
Author = {McClelland, M. A. and Irons, K. S. and Sass, G. G. and O'Hara, T. M. and
   Cook, T. R.},
Title = {{A COMPARISON OF TWO ELECTROFISHING PROGRAMMES USED TO MONITOR FISH ON
   THE ILLINOIS RIVER, ILLINOIS, USA}},
