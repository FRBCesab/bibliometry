@article{ ISI:000310425700006,
Author = {Hobbs, James A. and Castillo, Gonzalo and Tigan, Galen and Lindberg,
   Joan and Ikemiyagi, Naoaki and Ramos, Georgia},
Title = {{Tagging the next generation: validation of trans-generational chemical
   tagging for an endangered fish}},
Journal = {{ENVIRONMENTAL BIOLOGY OF FISHES}},
