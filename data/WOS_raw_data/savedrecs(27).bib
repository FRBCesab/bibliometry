@inproceedings{ ISI:000326475400073,
Author = {Khan, S. and Tubino, M. and Vila, M. M. D. C. and Olaf, E. and
   Rodrigues, T. C. and da Silvae, M. P.},
Editor = {{Pirrone, N}},
Title = {{Determination of the Organophosphorus Pesticide Diclorvos by Enzymatic
   Flow- Injection Analysis with Conductimetric Detection}},
