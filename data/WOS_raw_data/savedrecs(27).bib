@article{ ISI:000313382700004,
Author = {Bolenbaugh, Jason R. and Cooper, Tom and Brady, Ryan S. and Willard,
   Karen L. and Krementz, David G.},
Title = {{Population Status and Habitat Associations of the King Rail in the
   Midwestern United States}},
