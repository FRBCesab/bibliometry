@inproceedings{ ISI:000346067900257,
Author = {Cheristanidis, S. and Habilomatis, G. and Diapouli, E. and Chaloulakou,
   A.},
Editor = {{Lekkas, TD}},
Title = {{PM MASS, NUMBER CONCENTRATION AND EC SURROGATE MEASUREMENTS IN AN ATHENS
   RESIDENTIAL AREA. IMPACT OF RELATIVE SOURCES AND ACTIVITIES.}},
Booktitle = {{PROCEEDINGS OF THE 13TH INTERNATIONAL CONFERENCE ON ENVIRONMENTAL
   SCIENCE AND TECHNOLOGY}},
Series = {{Proceedings of the International Conference on Environmental Science and
