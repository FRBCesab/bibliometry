@article{ ISI:000312304800003,
Author = {Britton, John R. and Davies, Gareth D. and Pegg, Josephine},
Title = {{Spatial variation in the somatic growth rates of European barbel Barbus
   barbus: a UK perspective}},
Journal = {{ECOLOGY OF FRESHWATER FISH}},
