Title = {{Study of growth, yield and radiation energy conversion efficiency on
   varieties and different plant population of peanut}},
Booktitle = {{3RD INTERNATIONAL CONFERENCE ON SUSTAINABLE FUTURE FOR HUMAN SECURITY,
   SUSTAIN 2012}},
Series = {{Procedia Environmental Sciences}},
Year = {{2013}},
Volume = {{17}},
