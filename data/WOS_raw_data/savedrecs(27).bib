Author = {Galland, Claire and Dupuy, Celie and Capitaine, Carole and Auffret,
   Michel and Quiniou, Louis and Laroche, Jean and Pichereau, Vianney},
Title = {{Comparisons of liver proteomes in the European flounder Platichthys
   flesus from three contrasted estuaries}},
Journal = {{JOURNAL OF SEA RESEARCH}},
Year = {{2013}},
Volume = {{75}},
Number = {{SI}},
Pages = {{135-141}},
Month = {{JAN}},
