@article{ ISI:000358885500004,
Author = {Zhang, L. and Huanqiong, N. and Gapare, W. J. and Dillon, S. K. and Li,
   X. and Wu, H. X.},
Title = {{Comparison of allelic diversity between native gene resource plantings
   and selections in open-pollinated progeny test of Pinus radiata D. Don}},
Journal = {{SILVAE GENETICA}},
