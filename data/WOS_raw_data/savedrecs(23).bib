@article{ ISI:000330088400011,
Author = {Emaresi, Guillaume and Bize, Pierre and Altwegg, Res and Henry, Isabelle
   and van den Brink, Valentijn and Gasparini, Julien and Roulin, Alexandre},
Title = {{Melanin-Specific Life-History Strategies}},
