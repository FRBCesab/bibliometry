@article{ ISI:000330494900002,
Author = {Crane, Michael A. and Cho, Hyunje G. and Landrigan, Phillip J.},
Title = {{Implications of the World Trade Center Health Program (WTCHP) for the
   Public Health Response to the Great East Japan Earthquake}},
Journal = {{INDUSTRIAL HEALTH}},
