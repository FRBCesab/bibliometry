@article{ ISI:000341181200002,
Author = {Gori, Riccardo and Balducci, Alice and Caretti, Cecilia and Lubello,
   Claudio},
Title = {{Monitoring the oxygen transfer efficiency of full-scale aeration
   systems: investigation method and experimental results}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{70}},
Number = {{1}},
Pages = {{8-14}},
Abstract = {{This paper reports the results of a series of off-gas tests aimed at
   monitoring the evolution of the oxygen transfer efficiency in an urban
   wastewater treatment plant (3,500 population equivalent) located in
   Tuscany (Italy). The tests were conducted over a 2-year period starting
   with the testing of the aeration system. It was found that in the
   absence of membrane-panel cleaning operations, the oxygen transfer
   efficiency under standard conditions in process water (alpha SOTE)
   dropped from 18 to 9.5\% in 2 years. This gives rise to a 40\% increase
   in the wastewater treatment plant annual energy costs. The on-site
   chemical cleaning of the diffusers allowed for an almost total recovery
   of the transfer efficiency (alpha SOTE equal to 16\%). The use of the
   off-gas method for monitoring the oxygen transfer efficiency over time
   is therefore essential for enabling correct planning of the cleaning
   operations of the diffusers and for cutting the energy consumption and
   operating costs of the aeration system.}},
