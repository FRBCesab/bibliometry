@article{ ISI:000332350200008,
Author = {Sobrero, Raul and Ly Prieto, Alvaro and Ebensperger, Luis A.},
Title = {{Activity, overlap of range areas, and sharing of resting locations in
   the moon-toothed degu, Octodon lunatus}},
Journal = {{JOURNAL OF MAMMALOGY}},
Year = {{2014}},
Volume = {{95}},
Number = {{1}},
Pages = {{91-98}},
Month = {{FEB 19}},
Abstract = {{The evolution of sociality across octodontid rodents remains puzzling
   Although basal species are solitary living, the most derived octodontids
   studied so far are social, implying that sociality evolved recently from
   solitaryliving ancestors. However, the social behavior of some
   octodontids remains anecdotal. We aimed to provide the 1st systematic
   data on activity, space use, and social behavior of the moon-toothed
   degu (Octodon lunatus), a derived octodontid rodent. We used
   livetrapping and radiotelemetry to monitor patterns of aboveground
   activity, aboveground range areas and overlap, and use of resting
   locations in a coastal population in north-central Chile. Activity of O.
   lunatus was statistically similar during nighttime and daytime, implying
   no clear diurnal or nocturnal activity. During daytime the animals used
   resting locations that were associated with high shrub cover and
   Pouteria splendens. Radiocollared males and females shared resting
   locations on multiple occasions. There was a nonsignificant trend in
   degus that used same resting locations to exhibit greater range overlap
   than degus using different resting locations. Associations based on
   resting locations revealed a total of 5 social groups. Taken together,
   these results indicate that adult O. lunatus exhibit some sociality, a
   finding consistent with a trend in which group living is more frequent
   in the most derived compared with basal octodontids.}},
