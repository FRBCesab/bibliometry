@inproceedings{ ISI:000346761600008,
Author = {Omar, N. Q. and Sanusi, S. A. M. and Hussin, W. M. W. and Samat, N. and
   Mohammed, K. S.},
Book-Group-Author = {{IOP}},
Title = {{Markov-CA model using analytical hierarchy process and multi-regression
   technique}},
Booktitle = {{7TH IGRSM INTERNATIONAL REMOTE SENSING \& GIS CONFERENCE AND EXHIBITION}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2014}},
