@article{ ISI:000331151400039,
Author = {Su, Jianya and Zhang, Zhenzhen and Wu, Min and Gao, Congfen},
Title = {{Changes in Insecticide Resistance of the Rice Striped Stem Borer
   (Lepidoptera: Crambidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
