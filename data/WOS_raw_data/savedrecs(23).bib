@article{ ISI:000332968800015,
Author = {Li Wei and Tan Rui and Yang Yu-ming and Wang Juan},
Title = {{Plant diversity as a good indicator of vegetation stability in a typical
   plateau wetland}},
Journal = {{JOURNAL OF MOUNTAIN SCIENCE}},
