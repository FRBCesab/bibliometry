@article{ ISI:000349495700042,
Author = {Hong-Min, Li and Ji-Hong, Ma and Ping, Wang},
Title = {{Cotton Bollworm Resistance to the Bt Cotton and Management Strategy in
   Xinjiang, China}},
Journal = {{EGYPTIAN JOURNAL OF BIOLOGICAL PEST CONTROL}},
Year = {{2014}},
Volume = {{24}},
Number = {{2}},
Pages = {{533-541}},
Abstract = {{Xinjiang is one of the most important cotton production regions
   worldwide, where at present more than 11\% of the global total yield of
   cotton lint was produced. Bt cotton was widely introduced to Xinjiang in
   year 2000, and now more than half of the total acreage of cotton
   plantations was a transgenic cotton. Compared to other cotton regions of
   the world, in Xinjiang only a few investigations on the resistance
   monitoring and management have been done because of policy constraints.
   Moreover, factors responsible for the cause and rapid increase of the
   resistance of the cotton bollworm (Helicovetpa armigera (Hubner))
   population to the Bt cotton, such as lacking of standardization of seed
   supply, reasonable resistance management method and overall resistance
   monitoring have become serious threats to sustainable use of this
   transgenic cotton. Focusing on such problems, the present status of the
   research on resistance of the cotton bollworm to Bt cotton in the
   Xinjiang region has been evaluated. Some strategies for providing
   scientific basis for reasonable using of the transgenic crops and
   integrated management against cotton bollworms are recommended.}},
