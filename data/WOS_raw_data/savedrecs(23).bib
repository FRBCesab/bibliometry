@article{ ISI:000333891100006,
Author = {Gibbs, W. Keith and Miller, Jason E. and Cook, S. Bradford and Kulp,
   Matt A.},
Title = {{Habitat Use and Dispersal of a Reintroduced Etheostoma sitikuense
   (Citico Darter) Population}},
Journal = {{SOUTHEASTERN NATURALIST}},
