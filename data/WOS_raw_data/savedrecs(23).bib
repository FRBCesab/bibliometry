@article{ ISI:000338211800009,
Author = {White, Shannon L. and Orth, Donald J.},
Title = {{Distribution and Habitat Correlates of Clinch Dace (Chrosomus sp cf.
   saylori) in the Upper Clinch River Watershed}},
Journal = {{AMERICAN MIDLAND NATURALIST}},
