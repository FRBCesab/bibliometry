@article{ ISI:000340483500005,
Author = {Hopkins, III, John B. and Whittington, Jesse and Clevenger, Anthony P.
   and Sawaya, Michael A. and St Clair, Colleen Cassady},
Title = {{Stable isotopes reveal rail-associated behavior in a threatened
   carnivore}},
Journal = {{ISOTOPES IN ENVIRONMENTAL AND HEALTH STUDIES}},
Year = {{2014}},
Volume = {{50}},
Number = {{3, SI}},
Pages = {{322-331}},
Abstract = {{Human-wildlife conflict is a leading cause of adult mortality for large
   carnivores worldwide. Train collision is the primary cause of mortality
   for threatened grizzly bears (Ursus arctos) in Banff National Park. We
   investigated the use of stable isotope analysis as a tool for
   identifying bears that use the railway in Banff. Rail-associated bears
   had higher delta N-15 and delta S-34 values than bears sampled away from
   the rail, but similar delta C-13 values. Because elevated delta N-15
   values are indicative of higher animal protein consumption,
   rail-associated bears likely preyed on ungulates that foraged along the
   rail or scavenged on train-killed animals. The higher delta S-34 values
   in bear hair could have resulted from bears consuming sulfur pellets
   spilled on the rail or through the uptake of sulfur in the plants bears
   or animals consumed. Similar delta C-13 values suggest that the two
   types of bears had generally similar plant-based diets. Results from
   this study suggest that stable isotopes analysis could be used as a
   non-invasive, affordable, and efficient technique to identify and
   monitor bears that forage on the railway in Banff and potentially other
   transportation corridors worldwide.}},
