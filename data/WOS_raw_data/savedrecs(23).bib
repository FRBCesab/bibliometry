@article{ ISI:000340512500018,
Author = {Morais, Joao and Martins, Antonio and Vale, Micaela and Vasconcelos,
   Vitor},
Title = {{ASSESSMENT OF CYANOBACTERIAL TOXINOGENIC GENOTYPES AND ESTIMATION OF
   TOXIN CONTENT IN URBAN LAKES}},
