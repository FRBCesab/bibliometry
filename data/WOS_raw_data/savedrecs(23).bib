@article{ ISI:000340127200004,
Author = {Doucet, J. P. and Doucet-Panaye, A.},
Title = {{Structure-activity relationship study of trifluoromethylketone
   inhibitors of insect juvenile hormone esterase: Comparison of several
   classification methods}},
