@article{ ISI:000326930000015,
Author = {Peck, David V. and Olsen, Anthony R. and Weber, Marc H. and Paulsen,
   Steven G. and Peterson, Carol and Holdsworth, Susan M.},
Title = {{Survey design and extent estimates for the National Lakes Assessment}},
