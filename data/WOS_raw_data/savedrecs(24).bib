@article{ ISI:000326387800032,
Author = {Kamioki, Masayoshi and Ando, Haruko and Isagi, Yuji and Inoue-Murayama,
   Miho},
Title = {{Development of microsatellite markers for the Asian Stubtail Urosphena
   squameiceps by using next-generation sequencing technology}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
Year = {{2013}},
Volume = {{5}},
Number = {{4}},
Pages = {{1027-1029}},
Month = {{DEC}},
Abstract = {{The Asian Stubtail Urosphena squameiceps winters in Southeast Asia which
   is facing a serious threat from deforestation and the species is
   categorized as a threatened species at some of the breeding populations.
   Monitoring population dynamics of the species in the field is difficult
   because they are secretive and prefer dense undergrowth. Therefore,
   developing genetic markers from the species is useful for detecting
   appropriate conservational management units and to understanding their
   unique breeding system. We isolated and characterized 13 microsatellite
   loci of the Asian Stubtail by using a next-generation sequencer. The
   number of alleles and the expected heterozygosity at each locus were
   2-26 (mean 10.31) and 0.14-0.92 (mean 0.64), respectively. The combined
   first-parent non-exclusion probability was < 0.005.}},
