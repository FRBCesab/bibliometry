@inproceedings{ ISI:000368235000006,
Author = {Story, Georgeanna and Driscoll, Don and Banks, Sam},
Editor = {{Ballard, G and Banks, P and Claridge, A and Sanderson, J and Swann, D}},
Title = {{What can camera traps tell us about the diurnal activity of the
   nocturnal bare-nosed wombat (Vombatus ursinus)?}},
Booktitle = {{CAMERA TRAPPING: WILDLIFE MANAGEMENT AND RESEARCH}},
Year = {{2014}},
Pages = {{35-43}},
Note = {{1st International Colloquium on Camera Trapping in Wildlife Management
   and Research, Sydney, AUSTRALIA, SEP 13-14, 2012}},
Abstract = {{The bare-nosed or common wombat (Vombatus ursinus) is considered a
   nocturnal species that spends the day resting underground in burrows.
   Diurnal activity does occur, however it has been linked primarily to
   increased forage requirements during winter or droughts. Using
   ScoutGuard (R) SG550 camera traps we monitored the activity of wombats
   at three major burrow entrances during 12 months of above average
   rainfall. Activity rates were influenced by season and time of day.
   Diurnal activity accounted for 36\% of all activity, with seasonal
   variation ranging from 56\% in autumn to 33\% in spring. Sub-adults and
   females with young performed the majority of this diurnal activity. Some
   diurnal behaviour included prolonged periods near a burrow and involved
   resting, grooming, potentially basking and investigation of the burrow
   entrance. The behavioural patterns observed indicate that diurnal
   activity in wombats can be substantial and is not solely focused on
   meeting foraging requirements. We suggest that diurnal activity may
   result from social constraints and development within a population,
   particularly in sub-adult wombats. Camera traps have proven to be
   effective in identifying and quantifying wombat activity and provide a
   valuable tool for continuing research into the behaviour of the
   bare-nosed wombat.}},
