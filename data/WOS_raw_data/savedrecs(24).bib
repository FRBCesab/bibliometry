@article{ ISI:000328305100006,
Author = {Yih, Wonho and Kim, Hyung Seop and Myung, Geumog and Park, Jong Woo and
   Yoo, Yeong Du and Jeong, Hae Jin},
Title = {{The red-tide ciliate Mesodinium rubrum in Korean coastal waters}},
Journal = {{HARMFUL ALGAE}},
