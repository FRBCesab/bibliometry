@incollection{ ISI:000459771400028,
Author = {Ostrovsky, Ilia and Yacobi, Yosef Z. and Koren, Nir},
Editor = {{Zohary, T and Sukenik, A and Berman, T and Nishri, A}},
Title = {{Sedimentation Processes}},
