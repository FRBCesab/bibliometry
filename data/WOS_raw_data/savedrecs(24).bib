@article{ ISI:000323887300029,
Author = {Bartell, Steven M. and Brain, Richard A. and Hendley, Paul and Nair,
   Shyam K.},
Title = {{Modeling the potential effects of atrazine on aquatic communities in
   midwestern streams}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY}},
