@article{ ISI:000328873500010,
Author = {White, Michael and Boura, Liza and Venizelos, Lily},
Title = {{Population Structure for Sea Turtles at Drini Bay: An Important
   Nearshore Foraging and Developmental Habitat in Albania}},
Journal = {{CHELONIAN CONSERVATION AND BIOLOGY}},
