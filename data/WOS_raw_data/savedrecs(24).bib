@article{ ISI:000344813200014,
Author = {Fancourt, Bronwyn A.},
Title = {{Rapid decline in detections of the Tasmanian bettong (Bettongia
   gaimardi) following local incursion of feral cats (Felis catus)}},
Journal = {{AUSTRALIAN MAMMALOGY}},
Year = {{2014}},
Volume = {{36}},
Number = {{2}},
Pages = {{247-253}},
Abstract = {{An abrupt decline in the number of Tasmanian bettongs (Bettongia
   gaimardi) was observed as part of a study investigating population
   declines in the eastern quoll (Dasyurus viverrinus). Seven remote camera
   surveys were undertaken at a monitoring site between February 2012 and
   October 2013. An 11\% reduction in bettong detections was observed
   immediately following the first appearance of feral cats (Felis catus)
   (at least three individuals) at the site. Within four months, bettong
   detections had fallen by 58\% and by 100\% within six months. No
   bettongs were detected in subsequent surveys undertaken 10, 12 and 16
   months after cats were first observed. Cat predation and toxoplasmosis
   are discussed as mechanisms possibly responsible for the local
   disappearance of bettongs from this site, together with implications for
   the future management and conservation of the species.}},
