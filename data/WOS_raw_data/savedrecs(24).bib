@article{ ISI:000209336100008,
Author = {Timmer, Jennifer M. and Butler, Matthew J. and Ballard, Warren B. and
   Boal, Clint W. and Whitlaw, Heather A.},
Title = {{Abundance and Density of Lesser Prairie-Chickens and Leks in Texas}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
