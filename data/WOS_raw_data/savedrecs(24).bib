@article{ ISI:000328871300010,
Author = {Sanchez, Alberto and Concepcion Ortiz-Hernandez, Ma and Talavera-Saenz,
   Ana and Aguiniga-Garcia, Sergio},
Title = {{Stable nitrogen isotopes in the turtle grass Thalassia testudinum from
   the Mexican Caribbean: Implications of anthropogenic development}},
Journal = {{ESTUARINE COASTAL AND SHELF SCIENCE}},
Year = {{2013}},
Volume = {{135}},
Pages = {{86-93}},
Month = {{DEC 20}},
Abstract = {{Nutrient inputs associated with population growth threaten the integrity
   of coastal ecosystems. To assess the rapid increase in tourism, we
   compared the delta N-15 from Thalassia testudinum collected at sites
   with different levels of tourism development to detect the N inputs of
   wastewater discharge (WD) along the coast of Quintana Roo. The
   contributions of nitrogen enriched in N-15 are directly related to the
   increase of WD inputs in areas of tourism development (Nichupte Lagoon
   in Cancun) and decreased toward Bahia Akumal and Tulum. The delta N-15
   from T testudinum was significantly lower at Mahahual and Puerto
   Morelos. In areas of the lowest development and with tourist activity
   restricted, such as the Yum Balam Reserve and Sian Ka'an Biosphere
   Reserve, the delta N-15 values were relatively enriched compared to
   Mahahual and Puerto Morelos. Therefore, Puerto Morelos and Mahahual may
   be used for baseline isotopic monitoring where tourist activities are
   growing and can lead to environmental pressure on the reef lagoon
   ecosystem. The anthropogenic N input has the potential to impact, both
   environmentally and economically, the seagrass meadows and the coral
   reefs along the coast of Quintana Roo and the Caribbean. (C) 2013
   Elsevier Ltd. All rights reserved.}},
