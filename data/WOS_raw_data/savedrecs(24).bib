@article{ ISI:000328842300008,
Author = {O'Connor, Nancy J.},
Title = {{Invasion dynamics on a temperate rocky shore: from early invasion to
   establishment of a marine invader}},
Journal = {{BIOLOGICAL INVASIONS}},
