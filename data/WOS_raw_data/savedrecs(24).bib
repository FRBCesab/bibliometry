@article{ ISI:000328822800010,
Author = {Hayamizu, Masato and Hosokawa, Ichijitsu and Kimura, Osamu and Ohara,
   Masashi},
Title = {{Intraspecific variation in life history traits of Viola brevistipulata
   (Violaceae) in Hokkaido}},
Journal = {{PLANT SPECIES BIOLOGY}},
Year = {{2014}},
Volume = {{29}},
Number = {{1}},
Pages = {{85-91}},
Month = {{JAN}},
Abstract = {{Intraspecific life-history variations of Viola brevistipulata var.
   brevistipulata were examined with special reference to reproductive
   features based on quantitative and qualitative monitoring of
   chasmogamous (CH) and cleistogamous (CL) flowers. We selected 10
   populations in southwestern Hokkaido in common deciduous forests, and
   marked 40-70 individual plants at each population. Marked individuals
   were monitored every 1-2 weeks. The expression of CH and CL flowers was
   distinct among the populations. That is, there were populations
   consisting of plants with only CH flowers, while other populations
   produced CL flowers after CH flowers with greater shoot longevity This
   phenomenon was observed even in populations located at the same site.
   The results suggest that V.brevistipulata var. brevistipulata shows
   distinct life-history variations among the populations.}},
