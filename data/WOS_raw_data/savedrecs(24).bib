@article{ ISI:000328005800032,
Author = {Aubail, A. and Mendez-Fernandez, P. and Bustamante, P. and Churlaud, C.
   and Ferreira, M. and Vingada, J. V. and Caurant, F.},
Title = {{Use of skin and blubber tissues of small cetaceans to assess the trace
   element content of internal organs}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2013}},
Volume = {{76}},
Number = {{1-2}},
Pages = {{158-169}},
Month = {{NOV 15}},
Abstract = {{In order to evaluate the use of biopsy samples as non-destructive tool
   for assessing trace element concentrations in small cetaceans, the
   concentrations of 14 trace elements were determined in skin, blubber,
   liver and kidneys of four species of small cetaceans (i.e. common
   dolphin Delphinus delphis, harbour porpoise Phocoena phocoena,
   bottlenose dolphin Tursiops truncatus and striped dolphin Stenella
   coeruleolba), stranded and/or by-caught along the NE Atlantic Ocean
   coast between 2001 and 2008. Only Cu, Fe, Hg, Mn, Ni and Zn were above
   the detection limit of the instruments and showed recoveries
   satisfactory enough to be interpreted. Among these trace elements, Hg
   was the only one showing a significant correlation between
   concentrations in and those in liver and kidneys. In consequence skin
   and blubber can only be used as non-invasive monitoring tissues to
   investigate Hg bioaccumulation in internal tissues for cetacean
   populations. (C) 2013 Elsevier Ltd. All rights reserved.}},
