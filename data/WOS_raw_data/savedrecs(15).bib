@article{ ISI:000384300800012,
Author = {Rhodes, K. L. and Taylor, B. M. and Hernandez-Ortiz, D. and
   Cuetos-Bueno, J.},
Title = {{Growth and reproduction of the highfin grouper Epinephelus maculatus}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
