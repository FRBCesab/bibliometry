@article{ ISI:000376022300013,
Author = {Monroe, E. M. and Alexander, K. D. and Britten, H. B.},
Title = {{Still here after all these years: the persistence of the Uncompahgre
   fritillary butterfly}},
Journal = {{JOURNAL OF INSECT CONSERVATION}},
