@article{ ISI:000377476700064,
Author = {Lei, Xiaoning and Xiu, Guangli and Li, Bo and Zhang, Kun and Zhao,
   Mengfei},
Title = {{Individual exposure of graduate students to PM2.5 and black carbon in
   Shanghai, China}},
