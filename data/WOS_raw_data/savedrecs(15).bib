@article{ ISI:000373722700024,
Author = {Bu, Hongliang and Hopkins, III, John B. and Zhang, Dan and Li, Sheng and
   Wang, Rongjiang and Yao, Meng and Wang, Dajun},
Title = {{An evaluation of hair-snaring devices for small-bodied carnivores in
   southwest China}},
Journal = {{JOURNAL OF MAMMALOGY}},
