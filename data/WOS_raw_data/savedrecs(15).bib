@article{ ISI:000372354700011,
Author = {Franssen, N. R. and Durst, S. L. and Gido, K. B. and Ryden, D. W. and
   Lamarra, V. and Propst, D. L.},
Title = {{Long-term Dynamics of Large-bodied Fishes Assessed from Spatially
   Intensive Monitoring of a Managed Desert River}},
Journal = {{RIVER RESEARCH AND APPLICATIONS}},
