@article{ ISI:000383375400004,
Author = {Kaminski, Lucas A. and Iserhard, Cristiano A. and Freitas, Andre V. L.},
Title = {{Thisbe silvestre sp nov (Lepidoptera: Riodinidae): a new myrmecophilous
   butterfly from the Brazilian Atlantic Forest}},
Journal = {{AUSTRAL ENTOMOLOGY}},
