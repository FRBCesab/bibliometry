@article{ ISI:000379882900019,
Author = {Gjerdrum, Carina and Bolduc, Francois},
Title = {{Non-breeding Distribution of Herring Gull (Larus argentatus) and Great
   Black-backed Gull (Larus marinus) in Eastern Canada from Ship-based
   Surveys}},
