@article{ ISI:000378082600019,
Author = {Albers, Geriann and Edwards, John W. and Rogers, Richard E. and Mastro,
   Lauren L.},
Title = {{Natality of Yearling Coyotes in West Virginia}},
Journal = {{JOURNAL OF FISH AND WILDLIFE MANAGEMENT}},
