@article{ ISI:000372548900005,
Author = {Coy, Monique R. and Bin, Liu and Stelinski, Lukasz L.},
Title = {{Reversal of insecticide resistance in Florida populations of Diaphorina
   citri (Hemiptera: Liviidae)}},
Journal = {{FLORIDA ENTOMOLOGIST}},
