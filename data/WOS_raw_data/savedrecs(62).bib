@article{ ISI:A1994MX61400004,
Author = {GILLANDERS, BM and BROWN, MT},
Title = {{SEASONAL-VARIATION IN STANDING CROP, REPRODUCTION AND
   POPULATION-STRUCTURE OF XIPHOPHORA-GLADIATA (PHAEOPHYCEAE, FUCALES)}},
Journal = {{BOTANICA MARINA}},
Year = {{1994}},
Volume = {{37}},
Number = {{1}},
Pages = {{35-41}},
Month = {{JAN}},
Abstract = {{Changes in standing crop, reproductive status and structure of a
   population of Xiphophora gladiata at Katiki Point, Otago, New Zealand
   were monitored over a 12 month period. Plants were reproductive for 8
   months of the year with the peak reproductive activity being found over
   the winter (July/August) months. The greatest number of reproductive
   plants were found in the largest size classes with plants usually being
   15 cm in length before forming receptacles. Peak reproductive activity
   increased at the same time as tissue nitrogen and mannitol levels and
   may be linked. A significant seasonal variation in biomass was found
   with maximal levels in autumn (March-May). Biomass levels were
   correlated with tissue mannitol levels and with seawater ammonia and
   phosphate levels. This study provides a platform for describing patterns
   on broader temporal and spatial scales and has suggested some factors
   for targeted experiments.}},
