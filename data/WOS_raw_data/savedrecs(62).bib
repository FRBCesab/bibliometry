@inproceedings{ ISI:A1995BJ63Q00046,
Author = {Todd, JW and Culbreath, AK and Brown, SL},
Editor = {{Kuo, CG}},
Title = {{Dynamics of vector populations and progress of spotted wilt disease
   relative to insecticide use in peanuts}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON TOSPOVIRUSES AND THRIPS OF FLORAL AND
   VEGETABLE CROPS}},
