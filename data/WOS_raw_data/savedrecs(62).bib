@inproceedings{ ISI:A1995BH94E00021,
Author = {Topping, CJ and Sunderland, KD},
Editor = {{Toft, S and Riedel, W}},
Title = {{Methods for monitoring aerial dispersal by spiders}},
Booktitle = {{ARTHROPOD NATURAL ENEMIES IN ARABLE LAND I: DENSITY, SPATIAL
   HETEROGENEITY AND DISPERSAL}},
Series = {{ACTA JUTLANDICA}},
Year = {{1995}},
Volume = {{70}},
Number = {{2}},
Pages = {{245-256}},
Note = {{1st EU Workshop on Enhancement, Dispersal and Population Dynamics of
   Beneficial Insects in Integrated Agroecosystems, UNIV AARHUS, AARHUS,
   DENMARK, OCT 21-23, 1993}},
Organization = {{European Community}},
Abstract = {{A suction trap, a rotary trap and water filled tray traps (deposition
   traps) were used to monitor aerial dispersal in a population of spiders
   in and around a field of winter wheat between April and November 1991.
   All three methods proved adequate for describing trends in aerial
   activity during this period. Suction trapping and rotary sampling were
   less labour intensive than using deposition traps and provided the
   facility to record daily fluctuation in activity. Deposition traps had
   the advantage that they could be left unattended for longer periods of
   time. The main factor affecting the efficiency of the rotor trap was
   considered to be the vertical velocities of ballooning spiders.
   Calibration of the rotor trap was attempted but the difficulties of
   accurately assessing the number of spiders ballooning from an area of
   ground rendered this task problematical. A method of assessing the
   effects of aerial dispersal on the density of spiders on the ground is
   presented. This method relied on inclusion caging to prevent dispersal
   and was only partially successful due to statistical problems resulting
   from the aggregated dispersion of spiders in the field. There was no
   indication from this study of any targe net immigration or emigration of
   spiders from the field.}},
