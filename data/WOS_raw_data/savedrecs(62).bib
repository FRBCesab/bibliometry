@article{ ISI:A1994NJ80600007,
Author = {APLET, GH and LAVEN, RD and SHAW, RB},
Title = {{APPLICATION OF TRANSITION MATRIX MODELS TO THE RECOVERY OF THE RARE
   HAWAIIAN SHRUB, TETRAMOLOPIUM-ARENARIUM (ASTERACEAE)}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{1994}},
Volume = {{14}},
Number = {{2}},
Pages = {{99-106}},
Month = {{APR}},
Abstract = {{The size-class structure of the only known population of the extremely
   rare (<100 individuals) Hawaiian shrub Tetramolopium arenarium was
   monitored semi-annually for two years. Mortality exceeded recruitment
   over the observation period, and the absence of seedlings suggests a
   population headed for extinction. Survival of this species likely will
   depend on artificial population augmentation with propagated seedlings.
   Three risk scenarios were evaluated with the use of stage-based
   transition matrix models, and results indicate that recovery of T.
   arenarium will require a massive program of artificial propagation and
   outplanting. Transition matrix models are shown to be potentially
   valuable to managers designing augmentation programs for other species.}},
