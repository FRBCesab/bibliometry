@article{ ISI:A1995RL89100032,
Author = {SOMLYODY, L},
Title = {{WATER-QUALITY MANAGEMENT - CAN WE IMPROVE INTEGRATION TO FACE
   FUTURE-PROBLEMS}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
