@article{ ISI:A1995QC02500010,
Author = {GALPER, S and EDEN, LM and STIRLING, GR and SMITH, LJ},
Title = {{SIMPLE SCREENING METHODS FOR ASSESSING THE PREDACIOUS ACTIVITY OF
   NEMATODE-TRAPPING FUNGI}},
Journal = {{NEMATOLOGICA}},
