Title = {{Monitoring the density of Lake Puulavesi vendace (Coregonus albula (L))
   by hydroacoustics, catch per unit effort, virtual population and catch
   per swept area}},
Booktitle = {{ADVANCES IN LIMNOLOGY 46: BIOLOGY AND MANAGEMENT OF COREGONID FISHES -
   1993}},
Series = {{ERGEBNISSE DER LIMNOLOGIE}},
Year = {{1995}},
