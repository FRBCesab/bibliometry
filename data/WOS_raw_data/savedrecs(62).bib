@article{ ISI:A1995QU62500008,
Author = {KRAUS, B and PAGE, RE},
Title = {{POPULATION-GROWTH OF VARROA-JACOBSONI OUD IN MEDITERRANEAN CLIMATES OF
   CALIFORNIA}},
Journal = {{APIDOLOGIE}},
Year = {{1995}},
Volume = {{26}},
Number = {{2}},
Pages = {{149-157}},
Abstract = {{In April and October mite-free honey-bee colonies were artificially
   infested with 50 individuals of Varroa jacobsoni each and treated with
   the pesticide Apisian(R) after a period of 24 weeks. Population growth
   was studied in 24 colonies from April to October and in 6 colonies from
   October to April. The proportion of Varroa jacobsoni that invaded the
   test colonies after the initial inoculation was monitored in colonies
   that were constantly treated with Apistan(R). Conservative calculations
   suggested that the initial mite population in a honey-bee colony
   increases on average about 300-fold during 1 year in central California.
   This result excludes the contribution of additional mites that invaded
   the colonies. The result of the present study demonstrates the high
   virulence of Varroa jacobsoni in California, where beekeepers are forced
   to treat infested colonies twice a year.}},
