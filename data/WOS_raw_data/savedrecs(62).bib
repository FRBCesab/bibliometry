@article{ ISI:A1994NY20700007,
Author = {LESICA, P and STEELE, BM},
Title = {{PROLONGED DORMANCY IN VASCULAR PLANTS AND IMPLICATIONS FOR MONITORING
   STUDIES}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{1994}},
Volume = {{14}},
Number = {{3}},
Pages = {{209-212}},
Month = {{JUL}},
Abstract = {{Many species of geophyte vascular plants are reported to remain dormant
   during the growing season. We examined data from our own and published
   studies and found that, generally, 25\% or less of the plants in these
   populations demonstrated prolonged dormancy during any one year.
   Individual plants usually remained dormant for 1-2 years. Population
   estimates of plants with prolonged dormancy based on random sampling
   methods will often underestimate density. Permanent plot methods with
   repeated measures analysis can be used to monitor change in population
   density with a reduced risk of bias. Demographic monitoring studies of
   species with prolonged dormancy will require longer periods of time to
   obtain useful information.}},
