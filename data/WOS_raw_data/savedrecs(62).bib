@article{ ISI:A1994NJ78400001,
Author = {TOPPING, CJ and SUNDERLAND, KD},
Title = {{A SPATIAL POPULATION-DYNAMICS MODEL FOR LEPTHYPHANTES-TENUIS (ARANEAE,
   LINYPHIIDAE) WITH SOME SIMULATIONS OF THE SPATIAL AND TEMPORAL EFFECTS
   OF FARMING OPERATIONS AND LAND-USE}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{1994}},
Volume = {{48}},
Number = {{3}},
Pages = {{203-217}},
Month = {{APR}},
Abstract = {{A simple spatial model of the population dynamics of the agriculturally
   important spider Lepthyphantes tenuis (Blackwall) is described. The
   model was constructed to identify priority areas for further population
   dynamics research and to determine how populations may react to changes
   in agricultural practices. The model incorporates all present knowledge
   concerning reproduction, population growth and migration of this
   species.
   Simulations using the model suggest: The life-history strategy exhibited
   by this species allows it to survive in regularly perturbed systems;
   simultaneous destruction of local populations over a large contiguous
   area would not easily be ameliorated by re-colonisation by migrating
   spiders. In contrast population destruction on the same scale of
   mortality had little effect on the metapopulation if separated in space
   or time; relatively un-perturbed areas were found to be important
   sources for re-colonisation of agricultural habitats. The spatial scale
   at which populations are monitored should be relatively large to
   overcome any possible unpredictable changes in local populations as a
   result of spatial interactions caused by local habitat heterogeneity.}},
