@article{ ISI:A1994PG14600004,
Author = {LAGARDERE, JP and BEGOUT, ML and LAFAYE, JY and VILLOTTE, JP},
Title = {{INFLUENCE OF WIND-PRODUCED NOISE ON ORIENTATION IN THE SOLE
   (SOLEA-SOLEA)}},
Journal = {{CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES}},
Year = {{1994}},
Volume = {{51}},
Number = {{6}},
Pages = {{1258-1264}},
Month = {{JUN}},
Abstract = {{Sole (Solea solea), telemetered in an enclosure using an acoustic
   positioning system, changed their swimming trajectories and orientation
   behaviour as a function of wind strength and direction. Monitoring of
   the spatial variation in both wind-generated currents and noise spectra
   in the enclosure indicates that these behavioural changes correspond to
   patterns in the spatial distribution of noise and to sound intensity.
   Thus, our observations indicate that sole perceives and reacts to
   horizontal variability in ambient noise levels. Such behaviour may be
   important in determining movements of fish populations at sea during
   poor weather conditions.}},
