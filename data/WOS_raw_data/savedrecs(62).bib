@article{ ISI:A1994PM07600011,
Author = {BENDER, MA and SETLOW, RB},
Title = {{SPONTANEOUS AND INDUCED MICRONUCLEI AND UDS IN PERIPHERAL LYMPHOCYTES}},
Journal = {{MUTATION RESEARCH-ENVIRONMENTAL MUTAGENESIS AND RELATED SUBJECTS}},
Year = {{1994}},
Volume = {{313}},
Number = {{2-3}},
Pages = {{209-214}},
Month = {{OCT-DEC}},
Abstract = {{We have examined the distributions both of spontaneous and X-ray-induced
   micronuclei (MN) and of spontaneous and UV-induced unscheduled DNA
   synthesis (autoradiographic grains; UDS) in cultures of peripheral blood
   lymphocytes from normal, healthy human volunteers. While the spontaneous
   MN and UDS do not differ significantly from the expected Poisson
   distributions, both the induced MN and UDS are strongly over distributed
   (i.e., variance much greater than mean). This not only must be allowed
   for in statistical tests used for population monitoring, but offers
   suggestive evidence that there are large differences in radiation
   response from sample to sample, some of which may reflect true
   differences among normal, healthy subjects.}},
