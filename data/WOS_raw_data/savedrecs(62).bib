@article{ ISI:A1994PY43500002,
Author = {EDGAR, DR and KERSHAW, GP},
Title = {{THE DENSITY AND DIVERSITY OF THE BIRD POPULATIONS IN 3 RESIDENTIAL
   COMMUNITIES IN EDMONTON, ALBERTA}},
Journal = {{CANADIAN FIELD-NATURALIST}},
Year = {{1994}},
Volume = {{108}},
Number = {{2}},
Pages = {{156-161}},
Month = {{APR-JUN}},
Abstract = {{Over a one-year period, bird populations were monitored in three urban
   residential communities in Edmonton, Alberta.  The communities were
   approximately 35, 20, and 10 years old.  The bird population density,
   number of species, and evenness of distribution declined from the oldest
   to newest area.  The only bird species common to all three areas
   throughout was the House Sparrow, comprising between 70\% and 100\% of
   the total bird population.  Except in the winter, the number of species
   and population density of birds were less in the urban areas than in a
   number of wildland studies, and also less than were found in a number of
   other urban bird studies.}},
