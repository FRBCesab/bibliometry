@article{ ISI:A1994ML47600010,
Author = {MORIN, MP and CONANT, S},
Title = {{VARIABLES INFLUENCING POPULATION ESTIMATES OF AN ENDANGERED PASSERINE}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{1994}},
Volume = {{67}},
Number = {{1}},
Pages = {{73-84}},
Abstract = {{The population of an endangered, endemic Hawaiian honeycreeper, the
   Laysan Finch Telespiza cantans, has been censused over 23 years using
   strip transects. During 1986-1988, multiple censuses were made.
   Vegetation type, breeding cycle stage, and variability among individual
   counters influenced the transect counts during those three years. During
   the breeding peak, the population appeared to be undercounted
   (presumably because females were not seen) and was most evenly
   distributed throughout the major vegetation types. Variability among
   individual counters was significant, but was not explained by previous
   census experience.
   The breeding season population estimate declined by one-half in the year
   following a drought. There was no overall population trend over the 23
   years. Major refinement of census techniques would be difficult to
   justify due to logistic constraints and the need to minimize ecosystem
   damage. We conclude that strip transects are adequate for monitoring
   gross changes in this population, especially if breeding stage is known.}},
