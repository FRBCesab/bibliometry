@article{ ISI:A1995RM44200008,
Author = {SKARE, I},
Title = {{MASS-BALANCE AND SYSTEMIC UPTAKE OF MERCURY RELEASED FROM DENTAL AMALGAM
   FILLINGS}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{1995}},
Volume = {{80}},
Number = {{1-4}},
Pages = {{59-67}},
Month = {{FEB}},
Note = {{3rd International Conference on Mercury as a Global Pollutant, WHISTLER,
   CANADA, JUL 10-14, 1994}},
Organization = {{Elect Power Res Inst; Environm Canada, Atmospher Environm Serv; Frontier
   Geosci; Hlth Canada; Ontario Hydro; Dept Environm Protect, Florida;
   Wisconsin Dept Nat Resources; US FDA}},
Abstract = {{The release of mercury (Hg) from dental amalgam fillings has been
   verified by several authors. In this study, the emission rate of
   Hg-o-vapor from the oral cavity (O-Hg) and the urinary Hg-excretion rate
   (U-Hg) have been studied with 34 healthy individuals. In ten cases, the
   urinary excretions of silver (U-Ag) and the fecal excretions of Hg and
   Ag (F-Hg, F-Ag) were also monitored. All variables, except U-Ag, were
   significantly related to the load of amalgam. According to this study,
   an individual with a moderate load of amalgam, i.e. 30 restored
   surfaces, is predicted to exhibit the following emission rates: O-Hg=22,
   U-Hg=3, F-Hg=60 and F-Ag=27 mu g/d (d=24 hours), consistent with a gross
   mass balance for Hg of approximately 60 mu g/d. The corresponding
   systemic uptake of Hg was estimated to 12 mu g/d based on external data
   relating air Hg-o-exposures to urinary Hg-excretions. The worst case
   individual showed a gross mass balance of 200 mu g Hg/d connected to a
   systemic uptake of 70 mu g Hg/d. These values were compared to the
   average intake of total-Hg by a Swedish diet (2 mu g/d) and to the WHO's
   tolerable value for intake of total-Hg by food (45 mu g/d). Upscaled to
   the entire Swedish population (8 mill.), the data suggests a
   fecal/urinary emission to the environment of 100 kg Hg yearly
   originating from a population load of amalgam fillings containing 90,000
   kg of Hg.}},
