@article{ ISI:A1995QQ87600003,
Author = {FFRENCHCONSTANT, R and ARONSTEIN, K and ANTHONY, N and COUSTAU, C},
Title = {{POLYMERASE CHAIN REACTION-BASED MONITORING TECHNIQUES FOR THE DETECTION
   OF INSECTICIDE RESISTANCE-ASSOCIATED POINT MUTATIONS AND THEIR POTENTIAL
   APPLICATIONS}},
