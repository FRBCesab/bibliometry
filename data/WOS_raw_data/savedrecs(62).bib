@inproceedings{ ISI:A1994BB42H00010,
Author = {DAVIS, LC and MURALIDHARAN, N and VISSER, VP and CHAFFIN, C and FATELEY,
   WG and ERICKSON, LE and HAMMAKER, RM},
Editor = {{Anderson, TA and Coats, JR}},
Title = {{ALFALFA PLANTS AND ASSOCIATED MICROORGANISMS PROMOTE BIODEGRADATION
   RATHER THAN VOLATILIZATION OF ORGANIC-SUBSTANCES FROM GROUND-WATER}},
Booktitle = {{BIOREMEDIATION THROUGH RHIZOSPHERE TECHNOLOGY}},
