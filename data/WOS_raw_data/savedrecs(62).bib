@article{ ISI:A1995QH63000004,
Author = {KOHL, M and SCOTT, CT and ZINGG, A},
Title = {{EVALUATION OF PERMANENT SAMPLE-SURVEYS FOR GROWTH AND YIELD STUDIES - A
   SWISS EXAMPLE}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
Year = {{1995}},
Volume = {{71}},
Number = {{3}},
Pages = {{187-194}},
Month = {{FEB}},
Note = {{IUFRO conference on Growth and Yield Estimation from Successive Forest
   Inventories, COPENHAGEN, DENMARK, JUN 14-17, 1993}},
Organization = {{IUFRO}},
Abstract = {{This paper compares experimental growth and yield study plots with
   sample plots from forest inventories.  The difference in research
   objectives leads to a situation where sample plots are available that
   are representative of the total population, but give only limited
   information on site conditions and management history.  However,
   detailed information on site condition and management history is
   available for growth and yield plots, but does not represent the total
   population.  As an example, Swiss growth and yield plots are compared
   with survey plots in terms of species distribution, stem form, slope
   class and elevation.
   A critical review is given of the two most common samping designs for
   regional forest surveys.  Continuous forest inventory is recommended
   over the more efficient but more complex sampling with partial
   replacement design for use in conjunction with growth and yield studies.
   The limitations of both growth and yield plots and sample plots are
   discussed.  Recommendations for linking both types of plots to close
   information gaps are given to improve both forest surveys and growth and
   yield studies.}},
