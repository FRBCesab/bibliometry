@article{ ISI:A1994NN32000004,
Author = {WILLIAMSON, DT},
Title = {{SOCIAL-BEHAVIOR AND ORGANIZATION OF RED LECHWE IN THE LINYANTI SWAMP}},
Journal = {{AFRICAN JOURNAL OF ECOLOGY}},
Year = {{1994}},
Volume = {{32}},
Number = {{2}},
Pages = {{130-141}},
Month = {{JUN}},
Abstract = {{The social behaviour and organization of red lechwe (Kobus leche leche
   Gray 1850) were studied for 2.5 years (January 1975-June 1977) and were
   monitored for 7.5 years thereafter (until December 1984). Agonistic,
   territorial, sexual and maternal-filial behaviour in lechwe were
   documented. The salient feature of red lechwe social organization in the
   Linyanti Swamp was that breeding occurred on clustered mating
   territories, which were resource based. Other findings included: (1)
   significant intraspecific differences in group size, which were
   attributed to factors other than those used by Jarman (1974) to explain
   interspecific variation in group size; (2) differences in the dynamics
   of territoriality in the lechwe populations of the Linyanti Swamp and
   the Kafue Flats, which were associated with ecololgical and demographic
   differences; (3) a prediction that lek-breeding members of the genus
   Kobus would show a higher degree of sexual dimorphism than non-lek
   breeding members was not unequivocally confirmed by a comparison of the
   Kafue and Linyanti lechwe populations.}},
