@article{ ISI:A1994PZ14900012,
Author = {CULBREATH, AK and TODD, JW and BRANCH, WD and BROWN, SL and DEMSKI, JW
   and BEASLEY, JP},
Title = {{EFFECT OF NEW PEANUT CULTIVAR GEORGIA BROWNE ON EPIDEMICS OF SPOTTED
   WILT}},
