@article{ ISI:A1994MX24100019,
Author = {LARTER, NC and GATES, CC},
Title = {{HOME-RANGE SIZE OF WOOD BISON - EFFECTS OF AGE, SEX, AND FORAGE
   AVAILABILITY}},
