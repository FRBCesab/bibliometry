@article{ ISI:A1995QG44600031,
Author = {SHARP, R},
Title = {{BIRD CONSERVATION AND THE UK BIODIVERSITY ACTION PLAN}},
Journal = {{IBIS}},
Year = {{1995}},
Volume = {{137}},
Number = {{1}},
Pages = {{S219-S223}},
Month = {{JAN}},
