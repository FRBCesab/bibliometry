@article{ ISI:000399966600046,
Author = {Zhu, Honglei and Li, Ying and Liu, Zhaoli and Shi, Xiaoliang},
Title = {{ESTIMATING AFFECTED AND NEARBY RESETTLED POPULATION CAUSED BY THE NIERJI
   DAM PROJECT USING REMOTE SENSING}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2017}},
Volume = {{26}},
Number = {{3}},
Pages = {{2221-2230}},
Abstract = {{Resettlement caused by dam construction is an unsolved problem all over
   the world. Studies conducted on resettlement programmes, however, are
   limited, and the exact numbers of resettlers worldwide are not fully
   known. Therefore, timely and accurate estimation of resettlers in dam
   construction area is critical for public planning and researching.
   Remote sensing is an effective method in monitoring land use/cover
   changes in dam construction and estimating population distribution. In
   this paper, the affected population and resettled population near the
   reservoir caused by the Nierji Dam project were estimated with limited
   demographic information and impervious surfaces. Three counties were
   influenced by the Nierji Dam: Nehe City (county-level city), Nenjiang
   County and Molidawadawoer (Moqi) County. The estimated affected
   populations of Moqi and Nenjiang County were 17969 and 3259, with
   relative errors of -8.1\% and-19.5\%. The estimated affected population
   of Nehe County was 15490, while the actual affected population of Nehe
   County was not obtained. Moreover, the estimated relocated population
   was 28475, with a relative error of 14.8\%.The results indicated that
   the developed approach showed a promising performance on estimating the
   affected, and resettled populations caused by large dam project.}},
