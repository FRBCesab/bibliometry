@article{ ISI:000397595000011,
Author = {Durden, Wendy Noke and Stolen, Eric D. and Jablonski, Teresa A. and
   Puckett, Stefanie A. and Stolen, Megan K.},
Title = {{Monitoring Seasonal Abundance of Indian River Lagoon Bottlenose Dolphins
   (Tursiops truncatus) Using Aerial SurveysWendy}},
