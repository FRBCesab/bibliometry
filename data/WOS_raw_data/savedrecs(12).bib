@article{ ISI:000397136400022,
Author = {Elliott, Anna and Hobson, Victoria and Tang, Kam W.},
Title = {{Balancing fishery and conservation: a case study of the barrel jellyfish
   Rhizostoma octopus in South Wales}},
Journal = {{ICES JOURNAL OF MARINE SCIENCE}},
Year = {{2017}},
Volume = {{74}},
