@inproceedings{ ISI:000400157500057,
Author = {Wang, K. and Bai, L. Y. and Feng, J. Z.},
Book-Group-Author = {{IOP}},
Title = {{Urbanization Process Monitoring in Northwest China based on DMSP/OLS
   Nighttime Light Data}},
Booktitle = {{INTERNATIONAL SYMPOSIUM ON EARTH OBSERVATION FOR ONE BELT AND ONE ROAD
   (EOBAR)}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2017}},
Volume = {{57}},
