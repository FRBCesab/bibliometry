Title = {{Land Subsidence Monitoring by InSAR Time Series Technique Derived From
   ALOS-2 PALSAR-2 over Surabaya City, Indonesia}},
Booktitle = {{5TH GEOINFORMATION SCIENCE SYMPOSIUM 2017 (GSS 2017)}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2017}},
Volume = {{98}},
