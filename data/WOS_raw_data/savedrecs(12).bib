@article{ ISI:000413090300003,
Author = {Arab, Sihem and Arab, Abdeslem},
Title = {{EFFECT OF THE PHYSICO-CHEMICAL PARAMETERS ON THE DISTRIBUTION OF THE
   FECAL FLORA IN A DAM RESERVOIR (ALGERIA)}},
Journal = {{REVUE D ECOLOGIE-LA TERRE ET LA VIE}},
Year = {{2017}},
Volume = {{72}},
Number = {{3}},
Pages = {{269-280}},
Abstract = {{The present study was conducted to assess fluctuations in the
   environmental parameters influencing the concentration and distribution
   of the various bacterial populations, specifically those of fecal
   origin, in Boukourdane Dam (Algeria). Monitoring was conducted for 2
   years (January 2013 - January 2015) at four stations. Enumeration of
   fecal bacteria at the surface and various depths and comparison of
   bacteriological parameters revealed seasonal variations, with high
   bacterial concentrations during colder seasons (winter and autumn).
   Canonical correspondence analysis (CCA) of the entire dataset revealed
   that conductivity, ortho-phosphates and pH played a significant role in
   the development of fecal streptococci. Additionally, CCA indicated that
   seasonal variations and sulfate ions influenced the distribution of
   sulfite-reducing anaerobes. Total coliform levels were found to be
   related to water temperature, while fecal coliform levels were related
   to nitrate ions and dissolved oxygen levels.}},
