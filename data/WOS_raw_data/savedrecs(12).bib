@article{ ISI:000393322600010,
Author = {Khalil, Mohammed I. and Gibson, David J. and Baer, Sara G.},
Title = {{Phylogenetic diversity reveals hidden patterns related to population
   source and species pools during restoration}},
Journal = {{JOURNAL OF APPLIED ECOLOGY}},
