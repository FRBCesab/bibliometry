@inproceedings{ ISI:000422891800006,
Author = {Page, Kevin S. and Lewis, Edward},
Editor = {{Kapuscinski, KL and Simonson, TD and Crane, DP and Kerr, SJ and Diana, JS and Farrell, JM}},
Title = {{Integrating Voluntary Angler Catch Reports with Mark-Recapture Data to
   Model a Muskellunge Fishery in Clear Fork Reservoir, Ohio}},
Booktitle = {{MUSKELLUNGE MANAGEMENT: FIFTY YEARS OF COOPERATION AMONG ANGLERS,
   SCIENTISTS, AND FISHERIES BIOLOGISTS}},
Series = {{American Fisheries Society Symposium}},
Year = {{2017}},
