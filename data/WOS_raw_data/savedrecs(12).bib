@article{ ISI:000417037900008,
Author = {Siegel, Jared E. and McPhee, Megan V. and Adkison, Milo D.},
Title = {{Evidence that Marine Temperatures Influence Growth and Maturation of
   Western Alaskan Chinook Salmon}},
