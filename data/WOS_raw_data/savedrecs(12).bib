@article{ ISI:000402414700011,
Author = {Braccini, Matias},
Title = {{Temporal patterns in the size of the main commercial shark species of
   Western Australia}},
Journal = {{MARINE AND FRESHWATER RESEARCH}},
Year = {{2017}},
Volume = {{68}},
Number = {{6}},
Pages = {{1112-1117}},
Abstract = {{Declines in the mean size of harvested organisms may indicate
   overexploitation. In the present study, temporal patterns in the mean
   size of the four main commercial shark species of Western Australia were
   evaluated. Unlike commonly observed for other shark populations, there
   were no strong temporal patterns in the mean size of gummy, dusky and
   whiskery sharks, whereas the mean size of sandbar sharks showed a
   gradual increase since 1993. These observations add further evidence
   that sharks can be fished sustainably when adequate monitoring,
   assessment and management actions are implemented.}},
