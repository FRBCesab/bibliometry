@article{ ISI:000396367400005,
Author = {Velez-Arellano, Nurenskaya and Garcia-Dominguez, Federico A. and
   Lluch-Cota, Daniel B. and Gutierrez-Gonzalez, Jose L. and Antonio
   Salcido-Guevara, Luis and Sanchez-Cardenas, Rebeca},
Title = {{Morphophysiological Indices of the Green Abalone Haliotis fulgens
   Philippi, 1845 at Mexican Ocean Pacific Coast}},
