@article{ ISI:000230215600002,
Author = {Groves, PA and Chandler, JA},
Title = {{Habitat quality of historic snake river fall Chinook salmon spawning
   locations and implications for incubation survival. Part 2: Intra-gravel
   water quality}},
Journal = {{RIVER RESEARCH AND APPLICATIONS}},
