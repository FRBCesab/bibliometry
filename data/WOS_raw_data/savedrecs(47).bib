@article{ ISI:000226354600001,
Author = {do Amaral, MCR and Rebelo, MD and Torres, JPM and Pfeiffer, WC},
Title = {{Bioaccumulation and depuration of Zn and Cd in mangrove oysters
   (Crassostrea rhizophorae, Guilding, 1828) transplanted to and from a
   contaminated tropical coastal lagoon}},
Journal = {{MARINE ENVIRONMENTAL RESEARCH}},
Year = {{2005}},
Volume = {{59}},
Number = {{4}},
Pages = {{277-285}},
Month = {{MAY}},
Abstract = {{In order to study Zn and Cd accumulation and depuration, a set of
   oysters, Crassostrea rhizophorae, were transplanted to a metal
   contaminated coastal lagoon and another one was harvested there and
   transplanted to a non-polluted site. C. rhizophorae oysters and Perna
   perna mussels native from both sites were collected in order to monitor
   variability of metal concentration in resident populations. After three
   months exposure, oysters transplanted to the polluted site accumulated
   fourfold Zn (307-1319 mug g(-1)) without reaching the concentration
   level of resident oysters (9770 mug g(-1)). Cadmium concentrations had a
   slight but significant decrease during the same period (1.25-0.54 mug
   g(-1)). Oysters transplanted to the non-polluted site, showed threefold
   Zn deputation (6727-2404 mug g(-1)), while Cd had no significant
   variation (0.90-1.45 mug g(-1)). Results showed that transplanted
   oysters do not reach heavy metal concentrations in indigenous
   populations suggesting transplanted organisms would be better used to
   evaluate bioavailability instead of environmental concentrations. (C)
   2004 Elsevier Ltd. All rights reserved.}},
