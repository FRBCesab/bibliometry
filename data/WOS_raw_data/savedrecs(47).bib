@article{ ISI:000236478700004,
Author = {Barrows, CW and Swartz, MB and Hodges, WL and Allen, MF and Rotenberry,
   JT and Li, BL and Scott, TA and Chen, XW},
Title = {{A framework for monitoring multiple-species conservation plans}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
