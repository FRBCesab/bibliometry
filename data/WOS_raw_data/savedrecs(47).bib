@article{ ISI:000229867500116,
Author = {Gutierrez-Gonzalez, JL and Perez-Enriquez, R},
Title = {{A genetic evaluation of stock enhancement of blue abalone Haliotis
   fulgens in Baja California, Mexico}},
Journal = {{AQUACULTURE}},
Year = {{2005}},
Volume = {{247}},
