@article{ ISI:000232410700010,
Author = {Kavallieratos, NG and Athanassiou, CG and Balotis, GN and Tatsi, GT and
   Mazomenos, BE},
Title = {{Factors affecting male Prays oleae (Lepidoptera : Yponomeutidae)
   captures in pheromone-baited traps in olive orchards}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{2005}},
Volume = {{98}},
Number = {{5}},
Pages = {{1499-1505}},
Month = {{OCT}},
Abstract = {{Effects of trap design, height and side of trap placement on olive
   trees, pheromone doses in dispensers, aging of dispensers in the field,
   and secondary pheromone components were evaluated for the development of
   an effective pheromone monitoring system for Prays oleae (Bernard) Lesne
   (Lepidoptera: Yponomeutidae) in olive orchards. Field trials showed that
   trap design, pheromone dose, and trapping side affected male captures,
   whereas dispenser age, trap height, and secondary components had no
   influence. Pherocon 1C and Delta traps baited with I mg of
   (Z)-7-tetradecenal captured more male moths than Pherocon H or Funnel
   traps. Placement of traps at different cardinal directions significantly
   affected captures, but this trend was not consistent and varied with
   flight period and trap position internal or external to the tree canopy.
   Moth phenology as determined by pheromone traps from early April to
   mid-October was consistent with published field data. Results indicate
   that Pherocon 1C or Delta traps baited with I mg of (Z)-7-tetradecenal
   provide an effective tool for monitoring the flight activity of P. oleae
   and the time of application of control measures.}},
