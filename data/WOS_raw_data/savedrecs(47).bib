@article{ ISI:000233403500008,
Author = {Ingraldi, MF},
Title = {{A skewed sex ratio in Northern Goshawks: Is it a sign of a stressed
   population?}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{3}},
Pages = {{247-252}},
Month = {{SEP}},
Note = {{International Symposium on Ecology and Management of Northern Goshawks
   held in Conjunction with Annual Meeting of the
   Raptor-Research-Foundation, Anchorage, AK, SEP 04-05, 2003}},
Organization = {{Raptor Res Fdn}},
Abstract = {{I examined 6 yr (1993-98) of data on Northern Goshawk (Accipiter
   gentilis) reproductive performance in east-central Arizona. Forty-four
   breeding territories were monitored over the 6-yr period, yielding 109
   nesting attempts and 141 fledglings produced from 76 Successful broods.
   Among the 63 broods from which sex of nestlings could be determined by
   measurements, 29\% fledged one Young, 54\% fledged two young, and 17\%
   fledged three young. The mean sex ratio across years was 1.93 +/- 0.70 (
   95\% CI) males/females (annual range 1.1:1-3:1). When combining all
   fledglings of known sex (76 males and 43 females), a 1.77:1 male to
   female sex ratio was significantly different from 1:1 (P = 0.002). A
   male-biased fledgling sex ratio may be explained by one or more of the
   following hypotheses: (1) more males were produced due to nutritional
   stress resulting in minimization of investment in the larger sex
   (females) and (2) fewer females were produced because of differential
   mortality due to exposure to the elements during the nestling stage. I
   propose that environmental stress in the form of exposure to the
   elements during a critical life stage (e.g., rainfall during the
   nestling stage), combined with limited food availability, may be driving
   the skewed sex ratios observed in this local Northern Goshawk
   population.}},
