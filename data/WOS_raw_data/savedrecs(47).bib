@article{ ISI:000228346100015,
Author = {Phillips, BF and Melville-Smith, R},
Title = {{Sustainability of the western rock lobster fishery: A review of past
   progress and future challenges}},
Journal = {{BULLETIN OF MARINE SCIENCE}},
