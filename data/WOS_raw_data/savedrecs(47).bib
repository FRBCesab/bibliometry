@article{ ISI:000229416200021,
Author = {Keefer, ML and Peery, CA and Daigle, WR and Jepson, MA and Lee, SR and
   Boggs, CT and Tolotti, KR and Burke, BJ},
Title = {{Escapement, harvest, and unknown loss of radio-tagged adult salmonids in
   the Columbia River - Snake River hydrosystem}},
Journal = {{CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES}},
Year = {{2005}},
Volume = {{62}},
Number = {{4}},
