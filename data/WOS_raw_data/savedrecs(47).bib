@article{ ISI:000228346100018,
Author = {Prince, J},
Title = {{Combating the tyranny of scale for haliotids: Micro-management for
   microstocks}},
Journal = {{BULLETIN OF MARINE SCIENCE}},
Year = {{2005}},
Volume = {{76}},
Number = {{2}},
Pages = {{557-577}},
Month = {{MAR}},
Note = {{World Conference on the Scientific and Technical Bases for the
   Sustainability of Fisheries, Univ Miami, Miami, FL, NOV 26-30, 2001}},
Abstract = {{Global production of Haliotids (abalone) declined more than 60\% since
   the 1960s and continues to decline, indicating a failure to manage these
   resources sustainably. This paper discusses facets of abalone biology
   which make sustainable management problematic. Abalone resources are
   comprised of many small (100s-1000s in) self-recruiting microstocks with
   highly variable sizes-at-maturity. This causes a mismatch between the
   scale of management and the scale of component units of stock. This
   ``tyranny of scale{''} leaves component microstocks vulnerable to the
   ``tragedy of the commons{''} and resources vulnerable to serial
   depletion despite apparently rigorous regional scale management. I
   postulate that beyond abalone, and across a range of spatial scales,
   unrecognized spatial complexity compromises monitoring, assessment, and
   management in many fisheries. I suggest that a major challenge for
   abalone fisheries and the field of fisheries more generally, is to adapt
   the scale of fisheries monitoring, assessment, and management to the
   actual scale of component units of stocks, and to mobilize the resources
   required to adequately address the global needs of a myriad of
   microstocks. An increased use of territorially based fishing rights and
   a new class of fisheries practitioners are proposed as part of the
   answer to this challenge.}},
