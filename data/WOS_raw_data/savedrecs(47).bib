@article{ ISI:000229082200014,
Author = {Weitzel, DL and Pyron, M},
Title = {{Status and ecology of walleye (Sander vitreus) in the Upper Wabash
   River, Indiana}},
Journal = {{JOURNAL OF FRESHWATER ECOLOGY}},
Year = {{2005}},
Volume = {{20}},
Number = {{2}},
Pages = {{327-334}},
Month = {{JUN}},
Abstract = {{Habitat use of 20 tagged walleye (Sander vitreus) was monitored using
   ultrasonic biotelemetry under varying flow conditions in the Wabash
   River from June through September 2003. The study area was a
   free-flowing 20-km reach of the upper Wabash River from Lagro (rkm 633)
   to Rich Valley Indiana (rkm 612). Boat electrofishing was used to
   estimate the population and to collect individuals for biotelemetry.
   Summer walleye movements were short in distance and use of cover type,
   lateral river position, and substrata varied with river discharge.
   Electrofishing surveys indicated low walleye abundance in the upper
   Wabash River. Three walleye-sauger hybrids were captured, suggesting
   natural reproduction. Tagged individuals showed versatility in habitat
   use.}},
