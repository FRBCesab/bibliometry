@article{ ISI:000248655400006,
Author = {Moynahan, Brendan J. and Lindberg, Mark S. and Rotella, Jay J. and
   Thomas, Jack Ward},
Title = {{Factors affecting nest survival of greater sage-grouse in northcentral
   Montana}},
