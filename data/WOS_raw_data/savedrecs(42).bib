@article{ ISI:000246732600029,
Author = {Mohan, Manju and Dagar, Lalit and Gurjar, B. R.},
Title = {{Preparation and validation of gridded emission inventory of criteria air
   pollutants and identification of emission hotspots for megacity Delhi}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
