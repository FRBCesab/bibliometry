@article{ ISI:000207806800009,
Author = {Langford, Gabriel J. and Borden, Joel A. and Major, C. Smoot and Nelson,
   David H.},
Title = {{EFFECTS OF PRESCRIBED FIRE ON THE HERPETOFAUNA OF A SOUTHERN MISSISSIPPI
   PINE SAVANNA}},
Journal = {{HERPETOLOGICAL CONSERVATION AND BIOLOGY}},
Year = {{2007}},
Volume = {{2}},
Number = {{2}},
Pages = {{135-143}},
Month = {{SEP}},
Abstract = {{Despite the recent popularity of prescribed burns in the southeast USA,
   little research is available on the effects of prescribed fire on
   herpetofauna in the western coastal plain (Gulf Coast, USA). We assessed
   the effects of a restoration prescribed burn on herpetofauna and
   composed an inventory of amphibians and reptiles on the Grand Bay
   National Estuarine Research Reserve (GBNERR) and surrounding Grand Bay
   National Wildlife Refuge (GBNWR), Jackson County, Mississippi. We used
   visual encounter surveys (VES), minnow traps, and polyvinyl chloride
   (PVC) tubes to sample herpetofauna. We recorded 429 individuals and 29
   species from January-June, 2004. We found greater numbers of
   herpetofauna in burned than unburned stands, while species diversity
   indices were equal between burn treatments. Our results provide the
   first herpetological survey of the reserve, providing a baseline for
   monitoring herpetofauna population and community trends.}},
