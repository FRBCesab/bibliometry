@article{ ISI:000250128900023,
Author = {Fenoglio, M. S. and Trumper, E. V.},
Title = {{Influence of weather conditions and density of Doru luteipes (Dermaptera
   : forficulidae) on Diatraea saccharalis (Lepidoptera : Crambidae) egg
   mortality}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{2007}},
Volume = {{36}},
Number = {{5}},
Pages = {{1159-1165}},
Month = {{OCT}},
Abstract = {{The influence of weather factors and the predator Doru luteipes Scudder
   density on Diatraea saccharalis (F.) egg predation was studied.
   Mortality of D. saccharalis eggs was determined by artificially
   infesting maize plots with egg masses at various times within each of
   two maize-growing seasons. Each egg cohort was monitored every 24-48 h
   to determine the fate of eggs, and predation rates were calculated. Doru
   luteipes were sampled every 7-10 d, and the mean air temperature, the
   minimum percentage of relative humidity, and rainfall accumulations were
   recorded during the egg exposure period. To test the effects of abiotic
   and biotic variables on egg predation, we used a generalized linear
   model (GLM). Diatraea saccharalis egg predation was negatively
   associated with rainfall, whereas D. luteipes density and mean
   temperature were positively correlated with mortality. The implications
   of these findings for the management of D. saccharalis are discussed.}},
