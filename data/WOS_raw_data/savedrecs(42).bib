@article{ ISI:000249883100010,
Author = {Van De Werfhorst, Laurie C. and Pearse, John S.},
Title = {{Trampling in the rocky intertidal of central California: A follow-up
   study}},
Journal = {{BULLETIN OF MARINE SCIENCE}},
Year = {{2007}},
Volume = {{81}},
