@article{ ISI:000250813900008,
Author = {Rius, Marc},
Title = {{The effect of protection on fish populations in the Ses Negres Marine
   Reserve (NW Mediterranean, Spain)}},
Journal = {{SCIENTIA MARINA}},
Year = {{2007}},
Volume = {{71}},
Number = {{3}},
Pages = {{499-504}},
Month = {{SEP}},
Abstract = {{This work is the result of a 3-year monitoring programme for assessing
   the effect that the Ses Negres marine reserve has had on fish
   Populations. Species richness (based on fish species that are considered
   to be reserve effect indicators), and the size structure of 4 species
   vulnerable to angling and spear fishing were Studied by visual censuses
   conducted both inside and Outside the marine reserve. Analysis of fish
   Populations inside the reserve showed a reserve effect (larger sizes,
   higher abundance and richer fish assemblages) when compared to the
   non-reserve sites, especially by the end of the Study period. The
   results highlight the importance of marine reserves for the recovery of
   fish populations. Future management plans are discussed for this marine
   reserve.}},
