@article{ ISI:000249909100005,
Author = {Francis, Antonio and Bloem, Kenneth A. and Roda, Amy L. and Lapointe,
   Stephen L. and Zhang, Aijun and Onokpise, Oghenekome},
Title = {{Development of trapping methods with a synthetic sex pheromone of the
   pink hibiscus mealybug, Maconellicoccus hirsutus (Hemiptera :
   Pseudococcidae)}},
