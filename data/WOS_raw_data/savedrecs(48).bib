@article{ ISI:000224037500005,
Author = {Chang, SC and Rihana, A and Bahrman, S and Gruden, CL and Khijniak, AI
   and Skerlos, SJ and Adriaens, P},
Title = {{Flow cytometric detection and quantification of mycobacteria in
   metalworking fluids}},
