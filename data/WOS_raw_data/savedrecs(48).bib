@article{ ISI:000225377000005,
Author = {Chomsky, O and Kamenir, Y and Hyams, M and Dubinsky, Z and
   Chadwick-Furman, NE},
Title = {{Effects of temperature on growth rate and body size in the Mediterranean
   Sea anemone Actinia equina}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
Year = {{2004}},
Volume = {{313}},
Number = {{1}},
Pages = {{63-73}},
Month = {{DEC 15}},
Abstract = {{Actinia equina is the most common sea anemone in the rocky intertidal
   zone of the Mediterranean coast of Israel, yet little is known about its
   biology in this habitat. We examined variation in polyp growth at
   several temperatures within the local range. Under laboratory
   conditions, only polyps at low temperatures (15 and 20degreesC) grew,
   whereas those at higher temperatures (25 and 30degreesC) lost body mass.
   Seasonal monitoring of pedal disk diameter over 18 months at field sites
   showed that polyps shrank significantly during the summer when
   temperatures were high. We conclude that at summer seawater temperatures
   along the coast of Israel (28.7-29.5degreesC), polyps of A. equina are
   unable to balance their metabolic requirements with energy input,
   resulting in a seasonal reduction in biomass. Polyps appear able to
   acclimate to high temperatures, but not sufficiently to avoid shrinkage
   of tissues. (C) 2004 Elsevier B.V. All rights reserved.}},
