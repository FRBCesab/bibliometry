@inproceedings{ ISI:000230647300022,
Author = {Nemer, N and Demolin, G and Kawar, N and Kfoury, L and Zakhour, E},
Editor = {{Lieutier, F and Ghaioule, D}},
Title = {{Monitoring of the new cedar web-spinning sawfly, Cephalcia
   tannourinensis n. sp., in cedar forests of Lebanon}},
Booktitle = {{Entomological Research in Mediterranean Forest Ecosystems}},
Series = {{SCIENCE UPDATE}},
Year = {{2005}},
Pages = {{247-255}},
Note = {{International Symposium on Entomological Research in Mediterranean
   Forest Ecosystems, Rabat, MOROCCO, MAY 06-11, 2002}},
Organization = {{Minist Water \& Forests; INRA; DREF; IUFRO}},
Abstract = {{The cedar web-spinning sawfly, Cephalcia tannourinensis n.sp is an
   extremely voracious insect, which feeds on the new cedar needles and
   causes extensive damage. Attacked trees of Cedrus libani (A. Rich) turn
   brown in color and look as if fire has swept through the forest. Larval
   populations as high as 900 larvae/m(2) were recorded in some spots
   during 1998-2001. To remedy the situation, a management plan was
   established by developing monitoring programs and spraying the forest
   with diflubenzuron (Dimilin (R) SC 48). Monitoring and evaluation
   methods involved sampling prepupae in soil and catching adults with
   yellow sticky traps. The relationship between adult catches and the
   number of prepupae ready to emerge in both treated and control areas was
   studied. A significant linear relationship between estimates of the two
   variables was found in both areas. High adult catches were always
   associated with high numbers of prepupae in the soil; however lower
   adult catches showed variability. While trap catches was a less time
   consuming method of sampling, it did not reflect the actual situation at
   lower densities. In this case, soil sampling of prepupae proved to be a
   more reliable method.}},
