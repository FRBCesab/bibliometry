@article{ ISI:000230054700025,
Author = {Li, H and Yang, M and Zhang, Y and Liu, X and Gao, M and Kamagata, Y},
Title = {{Comparison of nitrification performance and microbial community between
   submerged membrane bioreactor and conventional activated sludge system}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
