@article{ ISI:000225717600014,
Author = {Eriksson, L and Damborsky, J and Earll, M and Johansson, E and Trygg, J
   and Wold, S},
Title = {{Three-block bi-focal PLS (3BIF-PLS) and its application in QSAR}},
