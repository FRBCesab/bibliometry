@inproceedings{ ISI:000232152900027,
Author = {Link, J and Almeida, F and Valentine, P and Auster, P and Reid, R and
   Vitaliano, J},
Editor = {{Barnes, BW and Thomas, JP}},
Title = {{The effects of area closures on Georges Bank}},
