@article{ ISI:000233557400030,
Author = {GrayMerod, R and Hendrickx, L and Mueller, LN and Xavier, JB and Wuertz,
   S},
Title = {{Effect of nucleic acid stain Syto9 on nascent biofilm architecture of
   Acinetobacter sp BD413}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2005}},
Volume = {{52}},
Number = {{7}},
