@inproceedings{ ISI:000237753600014,
Author = {Andreadakis, A. and Gavalakis, E. and Tzimas, A. and Kaliakatsos, L.},
Editor = {{Lekkas, TD}},
Title = {{The implementation of the Water Framework Directive (WFD) at the River
   Basin of Anthemountas with emphasis on the pressures and impacts
   analysis}},
Booktitle = {{Proceedings of the 9th International Conference on Environmental Science
   and Technology, Vol A - Oral Presentations, Pts A and B}},
Series = {{Proceedings of the International Conference on Environmental Science and
   Technology}},
Year = {{2005}},
