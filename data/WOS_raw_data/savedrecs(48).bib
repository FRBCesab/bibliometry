@article{ ISI:000233557400035,
Author = {Collins, G and Mahony, T and McHugh, S and Gieseke, A and de Beer, D and
   O'Flaherty, V},
Title = {{Distribution, dynamics and in situ ecophysiology of Crenarchaeota in
   anaerobic wastewater treatment granular biofilms}},
