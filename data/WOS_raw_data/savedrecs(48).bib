@inproceedings{ ISI:000228755600032,
Author = {Charlot, G and Edin, M and Floc'hlay, F and Soing, P and Boland, C},
Editor = {{Lang, GA}},
Title = {{Tabel (R) edabriz: A dwarf rootstock for intensive cherry orchards}},
Booktitle = {{Proceedings of the IVth International Cherry Symposium, Vols 1 and 2}},
