@article{ ISI:000226322800022,
Author = {Oka, T and Miura, S and Masaki, T and Suzuki, W and Osumi, K and Saitoh,
   S},
Title = {{Relationship between changes in beechnut production and Asiatic black
   bears in northern Japan}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
Year = {{2004}},
Volume = {{68}},
Number = {{4}},
Pages = {{979-986}},
Month = {{OCT}},
Abstract = {{Minimizing human-caused mortality by providing alternative solutions to
   human-bear conflicts is important for the conservation and management of
   Asiatic black bear (Ursus thibetanus) populations. We found a positive
   correlation (P < 0.05) between the number of nuisance bears killed and a
   beechnut (Fagus crenata) crop failure index in 5 of 7 zones in the
