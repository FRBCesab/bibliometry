@article{ ISI:000227281400006,
Author = {Cataldo, D and Boltovskoy, D and Hermosa, JL and Canzi, C},
Title = {{Temperature-dependent rates of larval development in Limnoperna fortunei
   (Bivalvia : Mytilidae)}},
Journal = {{JOURNAL OF MOLLUSCAN STUDIES}},
Year = {{2005}},
Volume = {{71}},
Number = {{1}},
Pages = {{41-46}},
Month = {{FEB}},
Abstract = {{Laboratory-reared larvae of Limnoperna fortunei (Dunker) were monitored
   until the morula stage at 26degreesC, and from there on until settlement
   at 20, 25 and 28degreesC. The first polar lobe is produced c. 40 min
   after spawning, and the first division occurs 14 min later. Slightly
   over an hour after spawning, the second polar lobe appears and the
   second division yields a 4-cell stage. The third division occurs 90 min
   after spawning, and the fourth 115 min after spawning. The morula stage
   is reached 3.5 h after spawning. The trochophore stage (size: 95-110
   mum) is reached 20, 6.3 and 6 h after spawning at 20, 25 and 28degreesC,
   respectively. Straight-hinged veligers (115-175 mum) start appearing at
   45, 26, and 24 h after spawning; umboned veligers (156-220 mum) at 287,
   165 and 118 h after spawning; and pediveligers (> 200 mum) at 480, 313
   and 265 h after spawning. Implications of these findings for the
   dispersion of the species and for its fouling impact are discussed.}},
