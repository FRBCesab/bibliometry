@inproceedings{ ISI:000237753600011,
Author = {Ammary, Bashaar Y.},
Editor = {{Lekkas, TD}},
Title = {{Wastewater reuse in Jordan: Present status and future plans}},
Booktitle = {{Proceedings of the 9th International Conference on Environmental Science
   and Technology, Vol A - Oral Presentations, Pts A and B}},
Series = {{Proceedings of the International Conference on Environmental Science and
   Technology}},
Year = {{2005}},
