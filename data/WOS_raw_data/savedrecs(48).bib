@inproceedings{ ISI:000237173700006,
Author = {Billingsley, PF and Charlwood, JD and Knols, BGJ},
Editor = {{Takken, W and Martens, P and Bogers, RJ}},
Title = {{Rapid assessment of malaria risk using entomological techniques: taking
   an epidemiological snapshot}},
Booktitle = {{Environmental Change and Malaria Risk: Global and Local Implications}},
Series = {{WAGENINGEN UR FRONTIS SERIES}},
Year = {{2005}},
Volume = {{9}},
