Title = {{Environmental exposure of thallium and potential health risk in an area
   of high natural concentrations of thallium: southwest Guizhou, China}},
Booktitle = {{Environmental Exposure and Health}},
Series = {{WIT TRANSACTIONS ON ECOLOGY AND THE ENVIRONMENT}},
