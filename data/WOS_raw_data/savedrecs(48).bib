@article{ ISI:000232707300021,
Author = {Venegas, LE and Mazzeo, NA},
Title = {{Application of atmospheric dispersion models to evaluate population
   exposure to NO2 concentration in Buenos Aires}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENT AND POLLUTION}},
