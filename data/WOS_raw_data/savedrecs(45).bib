@article{ ISI:000247653800015,
Author = {Armstrong, Doug P. and Raeburn, Elizabeth H. and Lewis, Rebecca M. and
   Ravine, Don},
Title = {{Estimating the viability of a reintroduced New Zealand robin population
   as a function of predator control}},
