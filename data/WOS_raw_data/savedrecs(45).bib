@article{ ISI:000240963300001,
Author = {Uysal, Zahit and Koksalan, Irem},
Title = {{The annual cycle of Synechococcus (cyanobacteria) in the northern
   Levantine Basin shelf waters (Eastern Mediterranean)}},
Journal = {{MARINE ECOLOGY-AN EVOLUTIONARY PERSPECTIVE}},
