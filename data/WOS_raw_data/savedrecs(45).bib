@article{ ISI:000238701600001,
Author = {Elmberg, Johan and Nummi, Petri and Poysa, Hannu and Sjoberg, Kjell and
   Gunnarsson, Gunnar and Clausen, Preben and Guillemain, Matthieu and
   Rodrigues, David and Vaananen, Veli-Matti},
Title = {{The scientific basis for new and sustainable management of migratory
   European ducks}},
Journal = {{WILDLIFE BIOLOGY}},
Year = {{2006}},
Volume = {{12}},
Number = {{2}},
Pages = {{121-127}},
Month = {{JUN}},
Abstract = {{It is an axiom in ecology that knowing the sheer number of individuals
   in a population is of very little help if the objective is to understand
   future and past changes in population size. Yet, this is exactly how
   migratory European ducks are monitored, many of which are important
   quarry species in several countries. We argue that present monitoring is
   insufficient to address objectives of wise use and sustainability such
   as those emphasised in recent management directives and multilateral
   international agreements. The two main problems are the almost total
   lack of reliable data on recruitment and mortality. We advocate a
   pan-European monitoring system based on undisputed scientific
   principles; i.e. a long-term, coordinated and standardised scheme that
   produces data about vital rates of duck populations as well as about
   harvest size. Data from such a scheme can be used by game biologists to
   produce predictive tools, thus providing a functional basis for
   management decisions for adaptive harvesting and conservation alike.}},
