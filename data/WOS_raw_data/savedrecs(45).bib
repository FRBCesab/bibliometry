@article{ ISI:000240691100088,
Author = {Goeksu, H. Y. and Bailiff, I. K.},
Title = {{Luminescence dosimetry using building materials and personal objects}},
Journal = {{RADIATION PROTECTION DOSIMETRY}},
Year = {{2006}},
Volume = {{119}},
Number = {{1-4}},
