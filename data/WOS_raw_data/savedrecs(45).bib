@article{ ISI:000238687100015,
Author = {Baudrimont, Magalie and de Montaudouin, Xavier and Palvadeau, Audrey},
Title = {{Impact of digenean parasite infection on metallothionein synthesis by
   the cockle (Cerastoderma edule): A multivariate field monitoring}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2006}},
Volume = {{52}},
Number = {{5}},
Pages = {{494-502}},
Month = {{MAY}},
Abstract = {{Metallothioneins (MT) are proteins that play an important role in
   metabolism of essential metals and detoxification of trace metals from
   living organisms. Their synthesis is induced by metal pollution but can
   also be exacerbated by other factors such as reproduction processes. In
   this context, we monitored NIT concentrations in a cockle Cerastoderma
   edule (marine bivalve) population and highlighted the effect of a
   castrating digenean parasite, Labratrema minimus. In spent cockles, NIT
   levels were low (ca. 5 nmol sites g(-1), fresh weight) but slightly
   higher in parasitized individuals. During gametogenesis, NIT synthesis
   increased in all cockles, but concentrations were lower in parasitized
   individuals (18 against 27 nmol sites g(-1), fw in unparasitized
   cockles) in relation with gonad damage by parasites. Therefore, it is
   suggested that parasite infection in cockles can modulate MT synthesis
   that could consequently interfere with the response of these protective
   proteins in case of metal contamination. (c) 2005 Elsevier Ltd. All
   rights reserved.}},
