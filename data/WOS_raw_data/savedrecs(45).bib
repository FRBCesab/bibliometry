Author = {Muthuramkumar, S and Ayyappan, N and Parthasarathy, N and Mudappa, D and
   Raman, TRS and Selwyn, MA and Pragasan, LA},
Title = {{Plant community structure in tropical rain forest fragments of the
   western Ghats, India}},
