@article{ ISI:000239155500008,
Author = {Siebert, Ursula and Gilles, Anita and Lucke, Klaus and Ludwig, Martje
   and Benke, Harald and Kock, Karl-Hermann and Scheidat, Meike},
Title = {{A decade of harbour porpoise occurrence in German waters - Analyses of
   aerial surveys, incidental sightings and strandings}},
Journal = {{JOURNAL OF SEA RESEARCH}},
