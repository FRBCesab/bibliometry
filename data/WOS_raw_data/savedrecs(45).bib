@article{ ISI:000239084600005,
Author = {Heupel, M. R. and Simpfendorfer, Colin A. and Collins, Angela B. and
   Tyminski, John P.},
Title = {{Residency and movement patterns of bonnethead sharks, Sphyrna tiburo, in
   a large Florida estuary}},
Journal = {{ENVIRONMENTAL BIOLOGY OF FISHES}},
