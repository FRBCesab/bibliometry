@article{ ISI:000255625200033,
Author = {Coops, Nicholas C. and Wulder, Michael A. and Duro, Dennis C. and Han,
   Tian and Berry, Sandra},
Title = {{The development of a Canadian dynamic habitat index using multi-temporal
   satellite estimates of canopy light absorbance}},
Journal = {{ECOLOGICAL INDICATORS}},
