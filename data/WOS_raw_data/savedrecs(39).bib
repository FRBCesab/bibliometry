@article{ ISI:000262607600003,
Author = {Roy, Caroline S. and Talbot, Guylaine and Topp, Edward and Beaulieu,
   Carole and Palin, Marie-France and Masse, Daniel I.},
Title = {{Bacterial community dynamics in an anaerobic plug-flow type bioreactor
   treating swine manure}},
Journal = {{WATER RESEARCH}},
