@article{ ISI:000258376800008,
Author = {Kang, Ming and Huang, Hongwen and Jiang, Mingxi and Lowe, Andrew J.},
Title = {{Understanding population structure and historical demography in a
   conservation context: population genetics of an endangered fern}},
Journal = {{DIVERSITY AND DISTRIBUTIONS}},
