@article{ ISI:000264575400002,
Author = {Dunlop, R. A. and Mellor, A.},
Title = {{PROBABLE ACOUSTIC RECORDINGS OF SPERM WHALES (PHYSETER MACROCEPHALUS)
   ALONG THE NORTH ANTRIM COAST, NORTHERN IRELAND}},
Journal = {{BIOLOGY AND ENVIRONMENT-PROCEEDINGS OF THE ROYAL IRISH ACADEMY}},
Year = {{2008}},
Volume = {{108B}},
Number = {{3}},
Pages = {{135-141}},
Month = {{DEC}},
Abstract = {{Despite increasing numbers of stranding events of sperm whales in the
   British Ides, to date relatively little is known about the population
   status of these whales in the North Atlantic. Acoustic survevs of this
   deep-water species are advantageous over visual surveys as they have a
   greater detection range, can operate 24 hours a day in most weather
   conditions and are less affected by observer subjectivity. An acoustic
   survey to assess potential monitoring sites was carried out on the North
   Antrim coast of Northern Ireland, where sperm whale clicks were
   successfully detected and recorded. This study identifies potential for
   future acoustic monitoring of whales off the north coast of Ireland and
   highlights the potential value of the technique in population studies.}},
