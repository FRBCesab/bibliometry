@article{ ISI:000260938000021,
Author = {Schmeller, Dirk S. and Gruber, Bernd and Bauch, Bianca and Lanno, Kaire
   and Budrys, Eduardas and Babij, Valerija and Juskaitis, Rimvydas and
   Sammul, Marek and Varga, Zoltan and Henle, Klaus},
Title = {{Determination of national conservation responsibilities for species
   conservation in regions with multiple political jurisdictions}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
Year = {{2008}},
Volume = {{17}},
