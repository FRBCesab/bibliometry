@article{ ISI:000264365100015,
Author = {Lasier, Peter J.},
Title = {{Alternative Substrates for Culturing the Freshwater Oligochaete
   Lumbriculus variegatus}},
Journal = {{NORTH AMERICAN JOURNAL OF AQUACULTURE}},
