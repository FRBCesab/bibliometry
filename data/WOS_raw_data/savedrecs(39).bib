@article{ ISI:000264416900011,
Author = {Zupo, Valerio and Messina, Patrizia and Carcaterra, Andrea and Aflalo,
   Eli D. and Sagi, Amir},
Title = {{Experimental evidence of a sex reversal process in the shrimp Hippolyte
   inermis}},
Journal = {{INVERTEBRATE REPRODUCTION \& DEVELOPMENT}},
