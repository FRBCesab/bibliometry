@article{ ISI:000263197000010,
Author = {Martin, J. L. and Hanke, A. R. and LeGresley, M. M.},
Title = {{Long term phytoplankton monitoring, including harmful algal blooms, in
   the Bay of Fundy, eastern Canada}},
Journal = {{JOURNAL OF SEA RESEARCH}},
