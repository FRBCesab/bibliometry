@article{ ISI:000258243700004,
Author = {Johnson, Glen D.},
Title = {{Prospective spatial prediction of infectious disease: experience of New
   York State (USA) with West Nile Virus and proposed directions for
   improved surveillance}},
Journal = {{ENVIRONMENTAL AND ECOLOGICAL STATISTICS}},
Year = {{2008}},
Volume = {{15}},
