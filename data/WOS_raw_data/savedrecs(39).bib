@article{ ISI:000260056300019,
Author = {Akpinar, S. and Oztop, Hakan F. and Akpinar, Ebru Kavak},
Title = {{Evaluation of relationship between meteorological parameters and air
   pollutant concentrations during winter season in Elazig, Turkey}},
