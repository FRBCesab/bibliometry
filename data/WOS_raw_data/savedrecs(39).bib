@article{ ISI:000260627500022,
Author = {McCarthy, Kyle P. and Fuller, Todd K. and Ming, Ma and McCarthy, Thomas
   M. and Waits, Lisette and Jumabaev, Kubanych},
Title = {{Assessing Estimators of Snow Leopard Abundance}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
