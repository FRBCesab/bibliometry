@article{ ISI:000262607600017,
Author = {Lee, Changsoo and Kim, Jaai and Hwang, Kwanghyun and O'Flaherty, Vincent
   and Hwang, Seokhwan},
Title = {{Quantitative analysis of methanogenic community dynamics in three
   anaerobic batch digesters treating different wastewaters}},
