@article{ ISI:000260993900006,
Author = {Whipps, Christopher M. and Matthews, Jennifer L. and Kent, Michael L.},
Title = {{Distribution and genetic characterization of Mycobacterium chelonae in
   laboratory zebrafish Danio rerio}},
Journal = {{DISEASES OF AQUATIC ORGANISMS}},
