@article{ ISI:000258542400026,
Author = {Argov, Yael and Gazit, Yoav},
Title = {{Biological control of the Mediterranean fruit fly in Israel:
   Introduction and establishment of natural enemies}},
Journal = {{BIOLOGICAL CONTROL}},
