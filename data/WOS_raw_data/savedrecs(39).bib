@article{ ISI:000260286300009,
Author = {Daniela Gomez, Maria and Priotto, Jose and Cecilia Provensal, Maria and
   Steinmann, Andrea and Castillo, Ernesto and Polop, Jaime J.},
Title = {{A population study of house mice (Mus musculus) inhabiting different
   habitats in an Argentine urban area}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2008}},
Volume = {{62}},
Number = {{3}},
Pages = {{270-273}},
Month = {{OCT}},
Abstract = {{The aim of this study was to determine temporal variations of abundance,
   reproductive condition, and demography of Mus musculus in different
   habitats in the urban area of Rio Cuarto, Cordoba, Argentina. House mice
   were monitored seasonally from spring 2000 to winter 2003 on 35 vacant
   lots and 32 vacant areas. A total of 1122 house mice were captured. M.
   musculus abundance and the percentages of active females were greater in
   vacant lots than in vacant areas. In the latter habitats, there were
   more males than females, and juveniles and sub-adults were barely
   represented in some seasons. Vacant lots seem to provide more suitable
   habitats to support mouse populations than do vacant areas. (C) 2008
   Elsevier Ltd. All rights reserved.}},
