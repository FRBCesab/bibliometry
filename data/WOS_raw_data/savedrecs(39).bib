@article{ ISI:000262017900022,
Author = {Fieberg, John and Kuehn, David W. and DelGiudice, Glenn D.},
Title = {{UNDERSTANDING VARIATION IN AUTUMN MIGRATION OF NORTHERN WHITE-TAILED
   DEER BY LONG-TERM STUDY}},
Journal = {{JOURNAL OF MAMMALOGY}},
