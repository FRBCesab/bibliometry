@article{ ISI:000265278500001,
Author = {Mathewson, Brooks},
Title = {{The Relative Abundance of Eastern Red-backed Salamanders in Eastern
   Hemlock-dominated and Mixed Deciduous Forests at Harvard Forest}},
Journal = {{NORTHEASTERN NATURALIST}},
