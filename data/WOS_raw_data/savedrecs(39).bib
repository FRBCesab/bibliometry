@article{ ISI:000260316800012,
Author = {Bartel, Rebecca A. and Knowlton, Frederick F. and Stoddart, L. Charles},
Title = {{LONG-TERM PATTERNS IN MAMMALIAN ABUNDANCE IN NORTHERN PORTIONS OF THE
   GREAT BASIN}},
Journal = {{JOURNAL OF MAMMALOGY}},
Year = {{2008}},
Volume = {{89}},
Number = {{5}},
Pages = {{1170-1183}},
Month = {{OCT}},
Abstract = {{We collected long-term indices of mammalian abundance at 2 sites in the
   Great Basin: Curlew Valley in northern Utah, and the Idaho National
   Laboratory in Southeastern Idallo. Abundance patterns were examined for
   I predator, the coyote (Canis latrans), and its prey community including
   9 species of rodents and 3 species of lagomorphs. Our results suggest
   cycles with a period of 10-11 years among the main prey species, Lepus
   californicus. Responses of C. latrans to prey fluctuations were variable
   among study areas., with abundance levels of coyotes remaining high
   during declines in prey Populations in Curlew Valley. Abundance indices
   were generally low for all species of rodents except Peromyscus
   maniculatus. Although many of the rodents demonstrated consistent
   biannual fluctuations, we could not statistically confirm multiseasonal
   cyclic patterns. Population levels of coyotes seem to reflect a
   combination of factors including abundance of both monitored species of
   prey and alternate prey groups, and human harvest.}},
