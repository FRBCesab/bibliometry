@article{ ISI:000260938000022,
Author = {Schmeller, Dirk S. and Bauch, Bianca and Gruber, Bernd and Juskaitis,
   Rimvydas and Budrys, Eduardas and Babij, Valerija and Lanno, Kaire and
   Sammul, Marek and Varga, Zoltan and Henle, Klaus},
Title = {{Determination of conservation priorities in regions with multiple
   political jurisdictions}},
