@article{ ISI:000261846900009,
Author = {Nozawa, Yoko},
Title = {{Micro-crevice structure enhances coral spat survivorship}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
