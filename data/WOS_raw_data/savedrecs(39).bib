Title = {{QoI Fungicide Resistance: Current Status and the Problems Associated
   with DNA-Based Monitoring}},
Booktitle = {{RECENT DEVELOPMENTS IN MANAGEMENT OF PLANT DISEASES}},
Series = {{Plant Pathology in the 21st Century}},
Year = {{2009}},
