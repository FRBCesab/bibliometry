@inproceedings{ ISI:000305619200003,
Author = {Hanner, R. H. and Lima, J. and Floyd, R.},
Editor = {{Pitblado, R and Routledge, J}},
Title = {{DNA Barcoding and Its Relevance to Pests, Plants and Biological Control}},
