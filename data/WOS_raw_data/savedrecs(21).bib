@article{ ISI:000345946500005,
Author = {Broggio, D. and Navarro, E. and Viltard, D. and Challeton-de Vathaire,
   C. and Bo, R. and Debrose, L. and Moya, X. and Franck, D.},
Title = {{An experimental study of detection limits and corresponding doses of
   IRSN in vivo monitoring vehicles in the case of post-accident scenarios}},
Journal = {{RADIOPROTECTION}},
Year = {{2014}},
Volume = {{49}},
Number = {{4}},
Pages = {{261-268}},
Month = {{OCT}},
Abstract = {{The measurement performances of in vivo monitoring vehicles were studied
   considering an increase in the radiological background in areas where
   the population is not evacuated after a nuclear accident. The study
   focused on Cs-137 and I-131 Detection Limits (DLs) and corresponding
   doses, for adults and one-year-old children. These DLs for ground
   contamination were obtained experimentally using a grid of point
   sources. Then, the DLs and doses were calculated using the experimental
   data and a safety factor for two accident scenarios. For these scenarios
   the Cs-137 DL corresponds to a committed effective dose of 0.02 mSv. The
   I-131 DL corresponds to committed equivalent thyroid doses of 3 mSv
   (adult) and 30 mSv (one-year-old child). To guarantee a 45 mSv thyroid
   equivalent dose assessment for the child the surface activity of I-131 +
   I-132 + I-133 should be below 1.6 MBq/m(2). This study shows that the
   vehicles can operate in a contaminated area where the population is not
   evacuated. However, in such a case, the contamination level outside and
   inside the vehicle should be kept stable to guarantee efficient body
   counting.}},
