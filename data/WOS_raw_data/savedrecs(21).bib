@article{ ISI:000349472500014,
Author = {Nemer, Nabil and Bal, Jane and Bechara, Elyas and Frerot, Brigitte},
Title = {{Pheromone identification of the cedar shoot moth Dichelia cedricola
   Diakonoff (Lepidoptera: Tortricidae)}},
Journal = {{ANNALES DE LA SOCIETE ENTOMOLOGIQUE DE FRANCE}},
Year = {{2014}},
Volume = {{50}},
Number = {{3-4}},
Pages = {{367-371}},
Month = {{OCT 2}},
Abstract = {{The cedar shoot moth, Dichelia cedricola Diakonoff, is considered as an
   important defoliator of the Lebanese cedars, Cedrus libani in Lebanon
   and Turkey. A recent outbreak revealed the need for a new tool to
   monitor the population within an integrated forest health management
   approach. To achieve this objective, a sex pheromone was identified by
   using solid phase microextraction (SPME) of calling female head space
   and female gland extracts, analyzed by gas chromatography/mass
   spectrometry. The biological activity of the identified components was
   assessed by field trapping in the Lebanese cedar forests.
   (Z)-11-tetradecenyl acetate and (Z)-11-tetradecen-1-ol were identified
   as the sex pheromone components of D. cedricola. The field studies
   showed that (Z)-11-tetradecenyl acetate and (Z)-11-tetradecen-1-ol
   (65:35) is an efficient formulation. The chemical composition is
   discussed in the view of biosystematics within Archipini tribe. These
   findings will contribute to the implementation of an integrated
   management program using the pheromone of D. cedricola and the
   assessment of the pest presence or absence in the cedar forest
   ecosystems of the Mediterranean regions.}},
