@article{ ISI:000345532000008,
Author = {Aina, Yusuf A. and van der Merwe, Johannes H. and Alshuwaikhat, Habib M.},
Title = {{Spatial and Temporal Variations of Satellite-Derived Multi-Year
   Particulate Data of Saudi Arabia: An Exploratory Analysis}},
