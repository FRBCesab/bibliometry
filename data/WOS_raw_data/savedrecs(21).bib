@article{ ISI:000340404900011,
Author = {Kapetanakos, Y. A. and Lovette, I. J. and Katzner, T. E.},
Title = {{Development of microsatellite markers and a restriction endonuclease
   digest assay for non-invasive sampling of endangered White-rumped,
   Slender-billed and Red-headed vultures}},
Journal = {{CONSERVATION GENETICS RESOURCES}},
Year = {{2014}},
Volume = {{6}},
Number = {{3}},
Pages = {{539-542}},
Month = {{SEP}},
Abstract = {{Southeast Asian vultures have been greatly reduced in range and
   population numbers, but it is challenging to use traditional tagging and
   monitoring techniques to track changes in their populations. Genotypes
   derived from non-invasively collected feather samples provide an
   alternative and effective means to `capture' individual vultures for
   mark-recapture analyses. We describe a restriction endonuclease digest
   assay that distinguishes the visually similar feathers of three species
   of critically endangered Asian vultures (Gyps bengalensis, G.
   tenuirostris, and Sarcogyps calvus). In addition, we describe a panel of
   eight polymorphic microsatellite loci. In combination, the restriction
   endonuclease assay and microsatellite marker set developed here are
   powerful molecular tools for investigating the genetic and demographic
   status of these Asian vultures species.}},
