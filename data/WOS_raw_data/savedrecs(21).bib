@article{ ISI:000341644600003,
Author = {Chaves, L. S. M. and Laporta, G. Z. and Sallum, M. A. M.},
Title = {{Effectiveness of Mosquito Magnet in Preserved Area on the Coastal
   Atlantic Rainforest: Implication for Entomological Surveillance}},
Journal = {{JOURNAL OF MEDICAL ENTOMOLOGY}},
