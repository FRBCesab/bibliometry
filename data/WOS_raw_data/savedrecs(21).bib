@article{ ISI:000345540700043,
Author = {de Hoogh, Kees and Korek, Michal and Vienneau, Danielle and Keuken,
   Menno and Kukkonen, Jaakko and Nieuwenhuijsen, Mark J. and Badaloni,
   Chiara and Beelen, Rob and Bolignano, Andrea and Cesaroni, Giulia and
