@article{ ISI:000339827500031,
Author = {Machado, A. and Bordalo, A. A.},
Title = {{Diversity and dynamics of the Vibrio community in well water used for
   drinking in Guinea-Bissau (West Africa)}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
