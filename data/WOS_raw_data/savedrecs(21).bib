@article{ ISI:000342213800003,
Author = {Zheng, L. X. and Zheng, Y. and Wu, W. J. and Fu, Y. G.},
Title = {{Field Evaluation of Different Wavelengths Light-Emitting Diodes as
   Attractants for Adult Aleurodicus dispersus Russell (Hemiptera:
   Aleyrodidae)}},
Journal = {{NEOTROPICAL ENTOMOLOGY}},
