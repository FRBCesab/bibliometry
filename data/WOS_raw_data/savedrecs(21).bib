@article{ ISI:000342027500069,
Author = {Cullen, Elizabeth and Evans, David S. and Davidson, Fred and Burke,
   Padraig and Burns, Damien and Flanagan, Andrew and Griffin, Chris and
   Kellegher, Anne and Mannion, Rory and Mulcahy, Maurice and Ryan, Michael
   and Biot, Pierre and Casteleyn, Ludwine and Castano, Argelia and
   Angerer, Juergen and Koch, Holger M. and Esteban, Marta and Schindler,
   Birgit K. and Navarro, Carmen and Kolossa-Gehring, Marike and Fiddicke,
   Ulrike and Schoeters, Greet and Den Hond, Elly and Sepai, Ovnair and
   Exley, Karen and Bloemen, Louis and Knudsen, Lisbeth E. and Joas,
   Reinhard and Joas, Anke and Aerts, Dominique},
Title = {{Mercury Exposure in Ireland: Results of the DEMOCOPHES Human
   Biomonitoring Study}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH}},
Year = {{2014}},
Volume = {{11}},
Number = {{9}},
Pages = {{9761-9775}},
Month = {{SEP}},
Abstract = {{Background: Monitoring of human exposure to mercury is important due to
   its adverse health effects. This study aimed to determine the extent of
   mercury exposure among mothers and their children in Ireland, and to
   identify factors associated with elevated levels. It formed part of the
   Demonstration of a study to Coordinate and Perform Human Biomonitoring
   on a European Scale (DEMOCOPHES) pilot biomonitoring study. Methods:
   Hair mercury concentrations were determined from a convenience sample of
   120 mother/child pairs. Mothers also completed a questionnaire. Rigorous
   quality assurance within DEMOCOPHES guaranteed the accuracy and
   international comparability of results. Results: Mercury was detected in
   79.2\% of the samples from mothers, and 62.5\% of children's samples.
   Arithmetic mean levels in mothers (0.262 mu g/g hair) and children
   (0.149 mu g /g hair) did not exceed the US EPA guidance value. Levels
   were significantly higher for those with higher education, and those who
   consumed more fish. Conclusions: The study demonstrates the benefit of
   human biomonitoring for assessing and comparing internal exposure
   levels, both on a population and an individual basis. It enables the
   potential harmful impact of mercury to be minimised in those highly
   exposed, and can therefore significantly contribute to population
   health.}},
