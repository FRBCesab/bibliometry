@article{ ISI:000345588500036,
Author = {Winders, K. R. and Steffensen, K. D.},
Title = {{Population size of pallid sturgeon, Scaphirhynchus albus (Forbes \&
   Richardson, 1905), in the lower Missouri River near Kansas City,
   Missouri, USA}},
Journal = {{JOURNAL OF APPLIED ICHTHYOLOGY}},
