@article{ ISI:000342174700019,
Author = {Nussberger, B. and Wandeler, P. and Weber, D. and Keller, L. F.},
Title = {{Monitoring introgression in European wildcats in the Swiss Jura}},
Journal = {{CONSERVATION GENETICS}},
