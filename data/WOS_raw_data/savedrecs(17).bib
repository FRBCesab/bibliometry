@article{ ISI:000364341400014,
Author = {Diefenbach, Duane and Hansen, Leslie and Bohling, Justin and
   Miller-Butterworth, Cassandra},
Title = {{Population and genetic outcomes 20years after reintroducing bobcats
   (Lynx rufus) to Cumberland Island, Georgia USA}},
Journal = {{ECOLOGY AND EVOLUTION}},
