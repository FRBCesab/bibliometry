@article{ ISI:000365864900002,
Author = {Watts, Bryan D. and Clark, Kathleen E. and Koppie, Craig A. and Therres,
   Glenn D. and Byrd, Mitchell A. and Bennett, Karen A.},
Title = {{ESTABLISHMENT AND GROWTH OF THE PEREGRINE FALCON BREEDING POPULATION
   WITHIN THE MID-ATLANTIC COASTAL PLAIN}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
