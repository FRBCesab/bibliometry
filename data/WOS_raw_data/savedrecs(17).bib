@article{ ISI:000366406200009,
Author = {Faassen, Elisabeth J. and Veraart, Annelies J. and Van Nes, Egbert H.
   and Dakos, Vasilis and Lurling, Miquel and Scheffer, Marten},
Title = {{Hysteresis in an experimental phytoplankton population}},
