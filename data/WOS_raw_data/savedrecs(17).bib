@article{ ISI:000363322100009,
Author = {Massimino, Dario and Johnston, Alison and Pearce-Higgins, James W.},
Title = {{The geographical range of British birds expands during 15 years of
   warming}},
Journal = {{BIRD STUDY}},
Year = {{2015}},
Volume = {{62}},
Number = {{4}},
Pages = {{523-534}},
Month = {{OCT 2}},
Abstract = {{Capsule Abundance monitoring data suggest that the short-term response
   of breeding birds to recent warming in Great Britain has been range
   expansion, caused by poleward shifts of leading range margins and no
   significant shifts of trailing range margins.Aims To quantify
   latitudinal and elevational shifts of breeding bird populations in Great
   Britain and test for differential shifts in range margins during a
   period of warming (1994-2009).Methods We modelled the population density
   of 80 species as a smooth function of latitude, longitude, elevation and
   year. Reference points on the distribution curve were used to describe
   latitudinal and elevational shifts.Results Across species, poleward
   shifts in the leading range margin were greater than in the
   range-centre. The trailing range margin was largely static, providing
   evidence for significant range expansion. The magnitude of latitudinal
   range shift lagged behind the equivalent shift in temperature,
   suggesting that species may be accumulating a climatic debt. There was
   no evidence for consistent elevational shifts.Conclusion Contrary to the
   generally expected long-term consequences of climate change of range
   contraction, we show that the short-term response to recent warming has
   been range expansion. This suggests the mechanisms of short-term and
   long-term consequences of climate change may differ.}},
