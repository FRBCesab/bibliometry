@article{ ISI:000365269200008,
Author = {Picarelli, Luciano and Damiano, Emilia and Greco, Roberto and Minardo,
   Aldo and Olivares, Lucio and Zeni, Luigi},
Title = {{Performance of slope behavior indicators in unsaturated pyroclastic
   soils}},
Journal = {{JOURNAL OF MOUNTAIN SCIENCE}},
Year = {{2015}},
Volume = {{12}},
Number = {{6}},
Pages = {{1434-1447}},
Month = {{NOV}},
Abstract = {{Landslide risk is increasing in many parts of the world due to growth of
   population and infrastructures. Therefore, an effort has to be made in
   developing new and cheap sensors for areas susceptible of landslides to
   continuously control the slope behaviour, until approaching failure
   conditions. The paper reported experimental data from small-scale
   physical models about the performance of Time Domain Reflectometry (TDR)
   and optical fibres, which act as the indicators of the incoming failure
   of slopes covered by unsaturated granular soils. Obtained results appear
   encouraging, since both sensors provide continuous information about the
   state of the slope, in terms of water content profiles and ongoing
   deformations, induced by rainwater infiltration, even immediately before
   the triggering of a fast landslide.}},
