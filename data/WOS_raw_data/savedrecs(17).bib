@article{ ISI:000362672100003,
Author = {Hunt, Kathleen E. and Rolland, Rosalind M. and Kraus, Scott D.},
Title = {{Conservation Physiology of an Uncatchable Animal: The North Atlantic
   Right Whale (Eubalaena glacialis)}},
Journal = {{INTEGRATIVE AND COMPARATIVE BIOLOGY}},
