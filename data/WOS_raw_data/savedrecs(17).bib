@article{ ISI:000362828300021,
Author = {Koch, Robert L. and Rich, Walter A.},
Title = {{Stink Bug (Hemiptera: Heteroptera: Pentatomidae) Feeding and Phenology
   on Early-Maturing Soybean in Minnesota}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
