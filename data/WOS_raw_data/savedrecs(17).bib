@article{ ISI:000368721800008,
Author = {Hansen, Angela M. K. and Bryan, Colleen E. and West, Kristi and Jensen,
   Brenda A.},
Title = {{Trace Element Concentrations in Liver of 16 Species of Cetaceans
   Stranded on Pacific Islands from 1997 through 2013}},
