@article{ ISI:000362629100009,
Author = {Propper, Ralph and Wong, Patrick and Bui, Son and Austin, Jeff and
   Vance, William and Alvarado, Alvaro and Croes, Bart and Luo, Dongmin},
Title = {{Ambient and Emission Trends of Toxic Air Contaminants in California}},
