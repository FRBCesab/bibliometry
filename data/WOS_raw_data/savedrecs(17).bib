@article{ ISI:000365791300001,
Author = {Suda, Cecilia N. K. and Vani, Gannabathula S. and de Oliveira, Mariana
   Feijo and Rodrigues, Jr., Edson and Rodrigues, Edson and Lavrado, Helena
   P.},
Title = {{The biology and ecology of the Antarctic limpet Nacella concinna}},
