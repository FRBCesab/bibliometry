@article{ ISI:000364341400001,
Author = {Miller, David A. W. and Grant, Evan H. Campbell},
Title = {{Estimating occupancy dynamics for large-scale monitoring networks:
   amphibian breeding occupancy across protected areas in the northeast
   United States}},
Journal = {{ECOLOGY AND EVOLUTION}},
