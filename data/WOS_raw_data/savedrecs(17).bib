@article{ ISI:000360776100031,
Author = {Heady, Walter N. and Clark, Ross P. and O'Connor, Kevin and Clark, Cara
   and Endris, Charles and Ryan, Sierra and Stoner-Duncan, Sara},
Title = {{Assessing California's bar-built estuaries using the California Rapid
   Assessment Method}},
Journal = {{ECOLOGICAL INDICATORS}},
