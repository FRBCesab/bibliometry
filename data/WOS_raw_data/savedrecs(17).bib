@article{ ISI:000365594500025,
Author = {Steketee, Ann K. and Wood, Petra Bohall and Gregg, Ian D.},
Title = {{American Woodcock Habitat Changes in Canaan Valley and Environs}},
Journal = {{SOUTHEASTERN NATURALIST}},
