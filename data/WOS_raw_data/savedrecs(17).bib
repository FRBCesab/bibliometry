@article{ ISI:000366782800016,
Author = {Babu, Talluri Kiran and Sharma, Rajan and Thakur, R. P. and Upadhyaya,
   Hari D. and Reddy, P. Narayan and Girish, A. G.},
Title = {{Selection of Host Differentials for Elucidating Pathogenic Variation in
   Magnaporthe grisea Populations Adapted to Finger Millet (Eleusine
   coracana)}},
Journal = {{PLANT DISEASE}},
