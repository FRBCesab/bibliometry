@article{ ISI:000364274400016,
Author = {Kiesewetter, Gregor and Schoepp, Wolfgang and Heyes, Chris and Amann,
   Markus},
Title = {{Modelling PM2.5 impact indicators in Europe: Health effects and legal
   compliance}},
Journal = {{ENVIRONMENTAL MODELLING \& SOFTWARE}},
