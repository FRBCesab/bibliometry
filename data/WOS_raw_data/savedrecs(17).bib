@article{ ISI:000364610900002,
Author = {Voss, Christine M. and Browder, Joan A. and Wood, Andrew and Michaelis,
   Adriane},
Title = {{Factors driving the density of derelict crab pots and their associated
   bycatch in North Carolina waters}},
