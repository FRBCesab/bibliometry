@article{ ISI:000171363200009,
Author = {Warrick, GD and Harris, CE},
Title = {{Evaluation of spotlight and scent-station surveys to monitor kit fox
   abundance}},
Journal = {{WILDLIFE SOCIETY BULLETIN}},
Year = {{2001}},
Volume = {{29}},
Number = {{3}},
Pages = {{827-832}},
Month = {{FAL}},
Abstract = {{Spotlight and scent-station surveys have been used to monitor
   populations of the endangered San Joaquin kit fox (Vulpes macrotis
   mutica), but reliability of these methods has been tested rarely. We
   evaluated effectiveness of these techniques to monitor long-term (8- to
   13-yr) trends in a population of kit foxes over a 216-km(2) area. Counts
   of foxes during fall and winter spotlight surveys were correlated
   positively with population estimates of kit foxes (r greater than or
   equal to0.59, P less than or equal to0.06). Fall scent-station indices
   and population estimates were correlated positively (r=0.65, P=0.08).
   However, spotlight and scent-station indices were imprecise and detected
   only large changes in population size. Further studies are needed to
   determine whether replicating surveys within seasons or increasing
   sampling intensity can provide more reliable indices of kit fox
   abundance.}},
