@article{ ISI:000171793700011,
Author = {Owiunji, I},
Title = {{Bird recovery in a recently logged forest of Budongo}},
Journal = {{OSTRICH}},
Year = {{2001}},
Number = {{15}},
Pages = {{56-59}},
Month = {{JUL}},
Note = {{10th Pan-African Ornithological Congress, MAKERERE UNIV, KAMPALA,
   UGANDA, SEP 01-08, 2000}},
Organization = {{BirdLife Int; Barclays; RSPB; Wetlands Int; MacArthur Fdn; ABC;
   NatureUganda; Agence Intergovn Francophonie; NABU; WWF}},
Abstract = {{This study presents results of the recovery of bird populations in a
   logged compartment in Budongo Forest Reserve. Compartment W21 was first
   logged in 1963-64 and again in 1996-97. Between 1993-95 studies were
   conducted to establish the long-term effects of selective logging
   covering five compartments, including W21. A followup study in W21 alone
   was undertaken in April-September 1998. Data were collected using the
   same methods (point counts and mistnetting) used in 1993-95 to allow
   comparison of results for the two studies. The results showed that most
   forest specialist species were present in W21 after the 1996-97 logging,
   and there was no evidence of species loss as a result of logging.
   Assessment of species diversity by use of the logarithmic series showed
   that W21 was more diverse before the 1996-97 logging took place. There
   was a significant increase in abundance of birds after logging. All
   feeding guilds also showed significant increases, except for
   frugivore-insectivore and frugivore-granivore guilds. This study, where
   a logged compartment is monitored before and after logging, was the
   first of its kind in Uganda. It is hoped that the study will act as a
   model for long-term monitoring and future efforts should aim at
   detecting changes in the population, especially changes in abundance and
   feeding guilds.}},
