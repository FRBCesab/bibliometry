@article{ ISI:000169162600005,
Author = {Caparroz, R and Miyaki, CY and Bampi, MI and Wajntal, A},
Title = {{Analysis of the genetic variability in a sample of the remaining group
   of Spix's Macaw (Cyanopsitta spixii, Psittaciformes : Aves) by DNA
   fingerprinting}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{2001}},
Volume = {{99}},
Number = {{3}},
Pages = {{307-311}},
Month = {{JUN}},
Abstract = {{The Spix's Macaw Cyanopsitta spixii is the world's most endangered
   parrot. There is only one male in the wild and 39 birds maintained in
   captivity. Among these, only nine were originally from the wild. The
   genetic variability of seven captive birds (six originally from the
   wild) was estimated using Jeffreys' human minisatellite probes 33.15 and
   33.6. The similarity indexes obtained were close to those observed
   between fullsibs in other bird species. This result supports the
   evidence of its historical rarity in the wild and suggest that the birds
   studied belong to a small population. Thus, the technique applied here
