@article{ ISI:000169326200001,
Author = {O'Neill, GA and Dawson, I and Sotelo-Montes, C and Guarino, L and
   Guariguata, M and Current, D and Weber, JC},
Title = {{Strategies for genetic conservation of trees in the Peruvian Amazon}},
Journal = {{BIODIVERSITY AND CONSERVATION}},
