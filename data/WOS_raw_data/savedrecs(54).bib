@article{ ISI:000168474200006,
Author = {Aiken, JJ and Godley, BJ and Broderick, AC and Austin, T and
   Ebanks-Petrie, G and Hays, GC},
Title = {{Two hundred years after a commercial marine turtle fishery: the current
   status of marine turtles nesting in the Cayman Islands}},
Journal = {{ORYX}},
Year = {{2001}},
Volume = {{35}},
Number = {{2}},
Pages = {{145-151}},
Month = {{APR}},
Abstract = {{Large populations of marine turtles breeding in the Cayman Islands were
   drastically reduced in the early 1800s. However, marine turtle nesting
   still occurs in the islands. The present-day status of this nesting
   population provides insight into the conservation of marine turtles, a
   long-lived species. In 1998 and 1999, the first systematic survey of
   marine turtle nesting in the Cayman Islands found 38 nests on 22 beaches
   scattered through the three islands. Three species were found: the green
   Chelonia mydas, hawksbill Eretmochelys imbricata and loggerhead Caretta
   caretta turtles. Comparison with other rookeries suggests that the small
   number of sexually mature adults surviving Cayman's huge perturbations
   may be impeding population recovery. This shows the need to implement
   conservation measures prior to massive reductions in population size.}},
