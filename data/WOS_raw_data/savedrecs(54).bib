@article{ ISI:000170602100006,
Author = {James, DG and Cosse, A and Wright, LC and Perez, J},
Title = {{Pheromone trapping of Synanthedon tipuliformis (Lepidoptera : Sesiidae)
   in Washington red currants}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
Year = {{2001}},
Volume = {{30}},
Number = {{4}},
Pages = {{663-666}},
Month = {{AUG}},
Abstract = {{Collections of volatiles, ovipositor extracts, and electoantennography
   showed the sex pheromone of female currant borer moths, Synanthedon
   tipuliformis (Clerck), from Washington to be a 2-component (100:3) blend
   of (E,Z) -2, 13-octadecadienyl acetate and (E,Z) -3, 13-octadecadienyl
   acetate. Pheromone-baited sticky traps (rubber septa dispensers)
   captured male S. tipuliformis at one abandoned and two commercial red
   currant sites (one treated with insecticide, one untreated) in south
   central Washington from 19 May to 16 August 2000. Peak catches occurred
   during late May and June with up to 200-300 moths/trap/wk. Lowest
   numbers (overall mean: 4.8 +/- 0.9 moths/trap/visit) were recorded at
   the insecticide-treated site and largest numbers (39.6 +/- 5.5
   moths/trap/visit) occurred at the untreated, commercial site.}},
