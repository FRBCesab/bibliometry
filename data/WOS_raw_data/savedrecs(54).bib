@article{ ISI:000168140100004,
Author = {Webster, CR and Jenkins, MA and Parker, GR},
Title = {{A field test of herbaceous plant indicators of deer browsing intensity
   in mesic hardwood forests of Indiana, USA}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{2001}},
Volume = {{21}},
Number = {{2}},
Pages = {{149-158}},
Month = {{APR}},
Abstract = {{Excessive browsing by white-tailed deer (Odocoileus virginianus
   Zimmerman) reduces the height of plants and may cause reallocation of
   resources normally used for sexual reproduction. With time, the cover of
   herbaceous species may be altered as browsing creates a competitive
   disadvantage for preferred species. The development of equations that
   predict herbaceous cover based on plant height and/or flowering status
   allows managers to identify excessive browsing before lasting damage to
   plant populations occurs. We measured the heights of three herbaceous
   species-sweet cicely (Osmorhiza claytonii {[}Michx.] C.B. Clarke),
   jack-in-the-pulpit (Arisaema triphyllum {[}L.] Schott), and white
   baneberry (Actaea pachypoda Ell.)-in Indiana state parks to determine if
   existing equations effectively predict deer browse damage based on the
   relationship between plant height and herbaceous cover. Flowering status
   was also recorded to test the relationship between sexual maturity and
   plant height. Based upon this data-intensive evaluation, regression
   models using plant height were effective in predicting percent cover of
   herbaceous species as an index of browse damage. While flowering status
   of jack-in-the-pulpit and white baneberry were strongly related to plant
   height (R-2 = 0.91 and R-2 = 0.91, respectively), sweet cicely flowering
   status showed no relationship to height (R-2 = 0.02). Examining the
   heights of these three species may provide an efficient and accurate
   method of evaluating the impacts of deer abundance on forest plant
   communities in the lower Midwest.}},
