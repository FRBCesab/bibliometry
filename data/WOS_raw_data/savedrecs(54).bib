@article{ ISI:000172475900002,
Author = {Budde, C},
Title = {{Individual features in the calls of the Grey Crowned Crane, Balearica
   regulorum gibbericeps}},
Journal = {{OSTRICH}},
Year = {{2001}},
Volume = {{72}},
Number = {{3-4}},
Pages = {{134-139}},
Month = {{SEP}},
Abstract = {{This study investigates if vocal monitoring of the Grey Crowned Crane,
   Balearica regulorum gibbericeps, is a feasible method to monitor their
   population. The vocal repertoire of the Grey Crowned Crane was analysed
   for individual, pair and sexual differences. Wild crones could not be
   differentiated by individuals and sexes, and were therefore investigated
   for identification of pairs. Results were supplemented with data on
   captive birds for individuals and sexes. The results show that
   individuals and sex identification are possible in the unison call. Pair
   identification is feasible in the unison call, Vocal monitoring can,
   therefore, be used as a possible monitoring tool in Grey Crowned Crane
   populations.}},
