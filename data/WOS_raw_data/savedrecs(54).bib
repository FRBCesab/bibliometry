@inproceedings{ ISI:000172548900043,
Author = {Cowan, IM and Hellawell, EE and Hughes, SJ},
Editor = {{Latini, G and Brebbia, CA}},
Title = {{The relationship between traffic throughput and the associated primary
   pollutants in Surrey}},
Booktitle = {{AIR POLLUTION IX}},
Series = {{ADVANCES IN AIR POLLUTION SERIES}},
Year = {{2001}},
Volume = {{10}},
Pages = {{431-438}},
Note = {{9th International Conference on Modelling, Monitoring and Management of
   Air Pollution, ANCONA, ITALY, SEP 12-14, 2001}},
Organization = {{Wessex Inst Technol; Ancona Univ}},
Abstract = {{This paper investigates the link between traffic-related primary
   pollutants and traffic volume in Surrey. This County, in the South East
   of England, was selected for the study as it has one of the highest
   levels of population density within the United Kingdom, hence elevated
   levels of car ownership and traffic flow. In addition, there were very
   few major industrial sources within the County, thus air pollutants were
   likely to be predominantly caused by traffic. This assumption was
   confirmed by comparing diurnal patterns of pollution at one location on
   a typical weekday, and Christmas Day, when the traffic was minimal.
   A further investigation compared the diurnal patterns of pollution with
   the levels of traffic provided by the local transportation model. This
   demonstrated a direct link between the primary pollutant, nitrogen oxide
   (NO) and traffic volume. The results showed two daily peaks in pollutant
   levels, which coincided with morning and afternoon `rush hours'. Similar
   distributions were also obtained for the other primary pollutants,
   carbon monoxide (CO) and particulate matter (PM10), measured at the same
   monitoring site. The study was then extended to two further locations
   within Surrey. Both of these were found to produce similar daily
   distributions of NO and PM10 although the actual magnitude of pollutants
   varied. Lower values were measured at the background site, due to its
   distance from the pollutant source.}},
