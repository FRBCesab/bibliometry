@article{ ISI:000172011900109,
Author = {Erkinaro, H and Erkinaro, J and Rask, M and Niemela, E},
Title = {{Status of zoobenthos and fish populations in subarctic rivers of the
   northernmost Finland: Possible effects of acid emissions from Russian
   Kola Peninsula}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{2001}},
Volume = {{130}},
Number = {{1-4, 2}},
Pages = {{831-836}},
