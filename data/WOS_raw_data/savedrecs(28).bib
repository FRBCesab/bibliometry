@article{ ISI:000324043900006,
Author = {Vaccari, Mentore and Torretta, Vincenzo and Collivignarelli, Carlo},
Title = {{Effect of Improving Environmental Sustainability in Developing Countries
   by Upgrading Solid Waste Management Techniques: A Case Study}},
Journal = {{SUSTAINABILITY}},
Year = {{2012}},
