@article{ ISI:000307760300004,
Author = {Rassati, D. and Toffolo, E. Petrucco and Battisti, A. and Faccoli, M.},
Title = {{Monitoring of the pine sawyer beetle Monochamus galloprovincialis by
   pheromone traps in Italy}},
Journal = {{PHYTOPARASITICA}},
