@article{ ISI:000306500600009,
Author = {Refsnider, Jeanine M. and Strickland, Jeramie and Janzen, Fredric J.},
Title = {{Home Range and Site Fidelity of Imperiled Ornate Box Turtles (Terrapene
   ornata) in Northwestern Illinois}},
