@article{ ISI:000305306600001,
Author = {Bravo, Mercedes A. and Fuentes, Montserrat and Zhang, Yang and Burr,
   Michael J. and Bell, Michelle L.},
Title = {{Comparison of exposure estimation methods for air pollutants: Ambient
   monitoring data and regional air quality simulation}},
Journal = {{ENVIRONMENTAL RESEARCH}},
