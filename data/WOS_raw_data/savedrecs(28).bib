@article{ ISI:000304626600005,
Author = {Miranda, Lucilia S. and Morandini, Andre C. and Marques, Antonio C.},
Title = {{Do Staurozoa bloom? A review of stauromedusan population biology}},
Journal = {{HYDROBIOLOGIA}},
