@article{ ISI:000308624400019,
Author = {Hane, Matthew E. and Kroll, Andrew J. and Johnson, Josh R. and Rochelle,
   Mike and Arnett, Edward B.},
Title = {{Experimental effects of structural enrichment on avian nest survival}},
