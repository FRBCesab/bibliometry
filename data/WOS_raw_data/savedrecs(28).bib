Author = {Clucas, R. and Moller, H. and Bragg, C. and Fletcher, D. and Lyver, P.
   O'B and Newman, J.},
Title = {{Rakiura Maori muttonbirding diaries: monitoring trends in titi (Puffinus
   griseus) abundance in New Zealand}},
Journal = {{NEW ZEALAND JOURNAL OF ZOOLOGY}},
