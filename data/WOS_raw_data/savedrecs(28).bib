@article{ ISI:000312839500002,
Author = {Hett, Cornelia and Heinimann, Andreas and Epprecht, Michael and
   Messerli, Peter and Hurni, Kaspar},
Title = {{Carbon Pools and Poverty Peaks in Lao PDR Spatial Data Inform
   Policy-making for REDD+ at the National Level}},
