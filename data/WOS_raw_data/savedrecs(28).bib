@article{ ISI:000306559300011,
Author = {Hatfield, Jeff S. and Reynolds, Michelle H. and Seavy, Nathaniel E. and
   Krause, Crystal M.},
Title = {{Population Dynamics of Hawaiian Seabird Colonies Vulnerable to Sea-Level
   Rise}},
