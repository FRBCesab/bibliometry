@article{ ISI:000305473500030,
Author = {Gordon, Jennifer R. and Ottea, James},
Title = {{Association of Esterases With Insecticide Resistance in Culex
   quinquefasciatus (Diptera: Culicidae)}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
