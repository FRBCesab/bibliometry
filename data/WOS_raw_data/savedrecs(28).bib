@article{ ISI:000307927700016,
Author = {van Bers, N. E. M. and Crooijmans, R. P. M. A. and Groenen, M. A. M. and
   Dibbits, B. W. and Komen, J.},
Title = {{SNP marker detection and genotyping in tilapia}},
Journal = {{MOLECULAR ECOLOGY RESOURCES}},
