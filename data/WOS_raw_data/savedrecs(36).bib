@article{ ISI:000271437400003,
Author = {Flynn, Rodney W. and Schumacher, Thomas V.},
Title = {{Temporal Changes in Population Dynamics of American Martens}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
