@article{ ISI:000271106300021,
Author = {Smeaton, Christina M. and Fryer, Brian J. and Weisener, Christopher G.},
Title = {{Intracellular Precipitation of Pb by Shewanella putrefaciens CN32 during
   the Reductive Dissolution of Pb-Jarosite}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2009}},
Volume = {{43}},
Number = {{21}},
Pages = {{8086-8091}},
Month = {{NOV 1}},
Abstract = {{Jarosites (MFe(3)(SO(4))(2)(OH)(6)) are precipitated in the Zn industry
   to remove impurities during the extraction process and contain metals
   such as Pb and Ag. Jarosite wastes are often confined to capped tailings
   ponds, thereby creating potential for anaerobic reductive dissolution by
   microbial populations. This study demonstrates the reductive dissolution
   of synthetic Pb-jarosite (PbFe(6)(SO(4))(4)(OH)(12)) by a subsurface
   dissimilatory Fe reducing bacterium (Shewanella putrefaciens CN32) using
   batch experiments under anaerobic circumneutral conditions. Solution
   chemistry, pH, Eh, and cell viability were monitored over time and
   illustrated the reduction of released structural Fe(III) from the
   Pb-jarosite to Fe(II). Inoculated samples containing Pb-jarosite also
   demonstrated decreased cellular viability coinciding with increased Pb
