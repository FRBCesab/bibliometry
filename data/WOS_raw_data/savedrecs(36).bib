@incollection{ ISI:000278828200004,
Author = {Zappala, Lucia},
Editor = {{Ciancio, A and Mukerji, KG}},
Title = {{CITRUS INTEGRATED PEST MANAGEMENT IN ITALY}},
Booktitle = {{INTEGRATED MANAGEMENT OF ARTHROPOD PESTS AND INSECT BORNE DISEASES}},
