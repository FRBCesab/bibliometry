@article{ ISI:000273462000004,
Author = {Gleason, Jeffrey S. and Rode, Karyn D.},
Title = {{Polar Bear Distribution and Habitat Association Reflect Long-term
   Changes in Fall Sea Ice Conditions in the Alaskan Beaufort Sea}},
Journal = {{ARCTIC}},
