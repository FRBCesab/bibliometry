@inproceedings{ ISI:000288481100017,
Author = {Soares, Amilcar},
Editor = {{Atkinson, PM and Lloyd, CD}},
Title = {{Geostatistical Methods for Polluted Sites Characterization}},
Booktitle = {{GEOENV VII - GEOSTATISTICS FOR ENVIRONMENTAL APPLICATIONS}},
Series = {{QUANTITATIVE GEOLOGY AND GEOSTATISTICS}},
Year = {{2010}},
