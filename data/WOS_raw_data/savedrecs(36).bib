@article{ ISI:000273718500005,
Author = {Costall, Jessica A. and Death, Russell G.},
Title = {{Population structure and habitat use by the spider Latrodectus katipo
   along the Manawatu-Wanganui coastline}},
Journal = {{NEW ZEALAND JOURNAL OF ZOOLOGY}},
Year = {{2009}},
Volume = {{36}},
Number = {{4}},
Pages = {{407-415}},
Month = {{DEC}},
Abstract = {{A survey of habitats likely to support the widow spider, Latrodectus
   katipo (Theridiidae), endemic to New Zealand, was carried out at seven
   locations along the Manawatu-Wanganui coastline. Altogether, 151
   juvenile, 74 female and 14 male katipo were found, with considerable
   differences in population structure between sites. The lack of juveniles
   at some sites suggests there is low reproductive output, but continued
   monitoring will be needed to confirm this. Most individuals were found
   on driftwood. Fewer were found occupying debris and vegetation. There is
   substantial overlap between the sizes of driftwood refuges occupied by
   katipo and by Steatoda capensis, an introduced theridiid spider from
   South Africa. We conclude that katipo populations along the
   Manawatu-Wanganui coastline are threatened by a range of human
   activities, and that management action may be required to prevent local
   extinctions of this charismatic invertebrate.}},
