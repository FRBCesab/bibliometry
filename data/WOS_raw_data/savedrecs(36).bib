@article{ ISI:000272993400004,
Author = {Steenhof, Karen and Peterson, Brit E.},
Title = {{AMERICAN KESTREL REPRODUCTION IN SOUTHWESTERN IDAHO: ANNUAL VARIATION
   AND LONG-TERM TRENDS}},
Journal = {{JOURNAL OF RAPTOR RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{4}},
Pages = {{283-290}},
Month = {{DEC}},
Abstract = {{We monitored American Kestrels (Falco sparverius) nesting in boxes from
   1986-2006 in southwestern Idaho. The number of boxes available to
   kestrels each year varied from 34 to 126. Approximately 20\% of boxes
   were on highway signs along Interstate 84, 20\% were oil trees in rural
   residential properties, and 60\% were oil wooden poles in rangeland,
   irrigated agricultural, and exurban areas. Occupancy rates increased
   over the 21-year period, despite increasing human populations, housing
   developments, and vehicular traffic near the boxes. The percent. of
   boxes occupied each year averaged 48 +/- 14, and the percent of nesting
   attempts that raised young to 22 d old averaged 64 +/- 10. Clutch Size
   averaged 4.8 and exhibited no significant trend over time. Nesting
   success and productivity also showed no trends over time. klean number
   of young fledged by kestrel pairs each year ranged from 1.3 to 3.5 and
   averaged 2.6. Brood sizes at successful boxes averaged 4.0 Nests in
   boxes on trees near farmsteads and in boxes oil interstate highway signs
   were less successful than nests in boxes oil poles in less-inhabited
   areas, suggesting that signs Oil interstate highways may not, be optimal
   nesting habitat for kestrels. Kestrel nest-initiation and hatching dates
   became earlier during the study period, and mean winter temperatures
   increased significantly during the study period, suggesting a possible
   effect of climate change.}},
