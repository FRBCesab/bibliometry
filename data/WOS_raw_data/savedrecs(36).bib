@article{ ISI:000274419200011,
Author = {Stuart-Smith, Rick D. and Barrett, Nevilles S. and Stevenson, David G.
   and Edgar, Graham J.},
Title = {{Stability in temperate reef communities over a decadal time scale
   despite concurrent ocean warming}},
