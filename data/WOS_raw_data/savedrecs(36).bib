@article{ ISI:000274506400017,
Author = {Venegas, Laura E. and Mazzeo, Nicolas A.},
Title = {{An ambient air quality monitoring network for Buenos Aires city}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENT AND POLLUTION}},
Year = {{2010}},
