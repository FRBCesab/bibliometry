@article{ ISI:000279233700017,
Author = {Li, Weizhong and Tong, Yanyan and Xiong, Qiang and Huang, Qiuying},
Title = {{Efficacy of Three Kinds of Baits Against the Subterranean Termite
   Reticulitermes chinensis (Isoptera: Rhinotermitidae) in Rural Houses in
   China}},
Journal = {{SOCIOBIOLOGY}},
Year = {{2010}},
Volume = {{56}},
Number = {{1}},
Pages = {{209-221}},
Abstract = {{The Subterranean termite Reticulitermes chinensis Snyder (Isoptera:
   Rhinotermitidae), an important pest damaging trees and wooden
   structures, has a wide distribution in China. Population density of R.
   chinensis has been increasing in cities and rural areas in China. In
   mountainous rural areas, peasant houses made of civil structures are
   being destroyed by R. chinensis. About 30\% of 400 peasant houses made
   of civil structures were damaged by R. chinensis in Xiangshi village,
   Yichang city, China. Since April 25, 2008, three types of monitoring
   stations were used to monitor termites in nine houses in Xiangshi
   village. The results showed that only above-ground monitoring boxes
   detected the activity of R. chinensis in three houses. These houses were
   treated by using sulfluramid baits, hexaflumuron baits and fipronil
   baits. By September 7, 2009, 43.5mg sulfluramid, 15.0mg hexaflumuron and
   31.3mg fipronil were consumed during the total baiting period of 100
   days. After applying the three kinds of baits, both numbers of termite
   workers and wood consumption in the three houses significantly declined.
   Furthermore, the flight of R. chinensis in the two houses was stopped by
   hexaflumuron baits and fipronil baits. Therefore, the three kinds of
   baits used in the study could suppress the activity of the termite R.
   chinensis in rural houses.}},
