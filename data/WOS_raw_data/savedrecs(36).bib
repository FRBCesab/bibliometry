@article{ ISI:000270980100001,
Author = {Nagendra, Harini and Paul, Somajita and Pareeth, Sajid and Dutt, Sugato},
Title = {{Landscapes of Protection: Forest Change and Fragmentation in Northern
   West Bengal, India}},
