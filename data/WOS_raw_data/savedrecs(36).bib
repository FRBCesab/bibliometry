@article{ ISI:000272492800021,
Author = {Hayes, Christopher J. and Fettig, Christopher J. and Merrill, Laura D.},
Title = {{Evaluation of Multiple Funnel Traps and Stand Characteristics for
   Estimating Western Pine Beetle-Caused Tree Mortality}},
