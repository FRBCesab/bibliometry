Title = {{THE GREY PARTRIDGE IN THE CZECH REPUBLIC: CHALLENGES FOR RESEARCHERS AND
   GAME MANAGERS. A REVIEW}},
Booktitle = {{UCLIO 2010: UNIVERSITY CONFERENCE IN LIFE SCIENCES - PROCEEDINGS}},
Year = {{2010}},
Pages = {{226-236}},
