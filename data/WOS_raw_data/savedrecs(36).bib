@inproceedings{ ISI:000313619900033,
Author = {Schulz, H.},
Editor = {{Baricevic, D and Novak, J and Pank, F}},
Title = {{Application of Rapid Analytical Methods for Efficient Evaluation of
   Plant Genetic Resources and Breeding Material}},
Booktitle = {{IV INTERNATIONAL SYMPOSIUM ON BREEDING RESEARCH ON MEDICINAL AND
   AROMATIC PLANTS - ISBMAP2009}},
