@article{ ISI:000279053400007,
Author = {de Guzman, Lilia I. and Frake, Amanda M. and Rinderer, Thomas E.},
Title = {{Seasonal population dynamics of small hive beetles, Aethina tumida
   Murray, in the south-eastern USA}},
Journal = {{JOURNAL OF APICULTURAL RESEARCH}},
Year = {{2010}},
Volume = {{49}},
Number = {{2}},
Pages = {{186-191}},
Abstract = {{The population of small hive beetles, Aethina tumida (SHB), was
   monitored from 2005 to 2008 in colonies of Italian and Russian honey
   bees located near St. Gabriel, Louisiana, USA. SHB populations differed
   between honey bee strains (only in one site out of two), with Italian
   colonies supporting more beetles (7.45 +/- 0.98 SHB per colony) than the
   Russian colonies (4.48 +/- 0.51 SHB per colony). No difference between
   the two strains was observed at site 1 where the SHB population was
   generally low (Italian = 2.73 +/- 0.36 SHB; Russian = 2.69 +/- 0.57 SHB
   per colony). Our results also revealed that SHB populations varied
   throughout the year, with peak infestations observed in the autumn
   (September and November). SHB abundance was significantly correlated
   with the proportion of hot days, but not with the proportions of cool,
   dry, or humid days, or the percentage of days with rainfall. Our results
   suggest that in-hive autumn trapping of SHB in the south eastern USA may
   reduce springtime numbers of SHB.}},
