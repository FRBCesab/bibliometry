@article{ ISI:000173501800014,
Author = {Imsland, AK and Jonassen, TM and Langston, A and Hoare, R and Wergeland,
   H and FitzGerald, R and Mulcahy, M and Stefansson, SO},
Title = {{The interrelation of growth and disease resistance of different
   populations of juvenile Atlantic halibut (Hippoglossus hippoglossus L.)}},
Journal = {{AQUACULTURE}},
Year = {{2002}},
Volume = {{204}},
Number = {{1-2}},
Pages = {{167-177}},
Month = {{JAN 21}},
Abstract = {{Growth of juvenile Atlantic halibut from three areas of the North
   Atlantic (Canada, Iceland and Norway) was studied in an experiment using
   individual tagged fish reared at 15degreesC for 85 days. Fish from each
   population were subsequently split into two groups and acclimatised to
   either 12degreesC or 18degreesC. The fish were then injected
   intra-peritoneally with a Vibrio anguillarum bacteria suspension and
   mortality monitored for 4 weeks. Growth rates of the Canadian population
   ranked lowest, whereas the Norwegian population had the highest mean
   growth rates (SGR = 1.70\% day(-1), 1.62\% day(-1) and 1.53\% day(-1)
   for the Norwegian, Icelandic and Canadian populations, respectively).
   The halibut from Norway had the best survival following bacterial
   challenge (80\%, 50\% and 55\% survival for the Norwegian, Icelandic and
   Canadian populations, respectively). Mortality was higher at 18degreesC
   than at 12degreesC in the Icelandic (62\% at 12degreesC and 27\% at
   18degreesC) and Canadian (56\% at 12degreesC and 32\% at 18degreesC)
   fish, whereas a smaller difference between temperatures was observed in
   the Norwegian fish (25\% at 12degreesC and 13\% at 18degreesC). Fish
   that survived the challenge test were those that had grown fastest in
   the growth trial. Low, but significant, correlations between survival
   and size and growth were seen, but these correlations varied between
   populations. In the Canadian population, no correlation between size and
   growth and survival were seen; only size was correlated (r = 0.27) with
   survival in the Icelandic population, whereas both size (r = 0.18) and
   growth (r = 0.17) were correlated with survival in the Norwegian
   population. (C) 2002 Elsevier Science B.V. All rights reserved.}},
