@inproceedings{ ISI:000180304900019,
Author = {Fryer, G and Dunn, G and Anderson, P},
Editor = {{GoodrichMahoney, JW and Mutrie, DF and Guild, CA}},
Title = {{Rare plant impact mitigation for the Alliance Pipeline Project}},
