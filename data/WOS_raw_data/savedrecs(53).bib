Title = {{Life history characteristics of smallmouth bass populations in Nova
   Scotia based on tournament monitoring}},
Booktitle = {{BLACK BASS: ECOLOGY, CONSERVATION, AND MANAGEMENT}},
Series = {{AMERICAN FISHERIES SOCIETY SYMPOSIUM}},
