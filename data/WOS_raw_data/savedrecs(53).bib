@inproceedings{ ISI:000226029600034,
Author = {Carignan, V and Villard, MA},
Editor = {{Chamberlain, D and Wilson, A}},
Title = {{A simple method to select avian indicators of ecological integrity: A
   case study in eastern New Brunswick (Canada)}},
Booktitle = {{AVIAN LANDSCAPE ECOLOGY: PURE AND APPLIED ISSUES IN THE LARGE-SCALE
   ECOLOGY OF BIRDS}},
Year = {{2002}},
Pages = {{228-232}},
Note = {{11th Annual Conference of the
   International-Association-for-Landscape-Ecology, Univ E Anglia, Norwich,
   ENGLAND, SEP 10-13, 2002}},
Organization = {{Int Assoc Landscape Ecol, UK Chapter}},
Abstract = {{As part of a broader program to assess and monitor the ecological
   integrity of the Greater Kouchibouguac Ecosystem (eastern New Brunswick,
   Canada), we surveyed birds at 259 stations to identify potential
   indicator species. Stations were distributed among four forest and bog
   ecosystem types in a relatively undisturbed area (Kouchibouguac National
   Park) and in an adjacent area disturbed by forestry and human
   settlement. We compared the level of disturbance (as assessed from
   landscape structure) around study sites within versus outside the Park
   but found no statistically significant difference. Potential indicator
   species were selected based on their degree of ecological
   specialization, specialists being considered more susceptible to
   environmental disturbances. Combined with the monitoring of other taxa
   and indicators at higher levels (e.g. landscape change), the population
   trends of the 16 avian indicator species proposed here should provide
   Park managers with the information required to maintain the ecological
   integrity of the Greater Kouchibouguac Ecosystem.}},
