@inproceedings{ ISI:000180884300015,
Author = {Dorland, E and Willems, JH},
Editor = {{Kindlmann, P and Willems, JH and Whigham, DF}},
Title = {{Light climate and plant performance of Ophrys insectifera; a four-year
   field experiment in The Netherlands (1998-2001)}},
Booktitle = {{TRENDS AND FLUCTUATIONS AND UNDERLYING MECHANISMS IN TERRESTRIAL ORCHID
   POPULATIONS}},
Year = {{2002}},
Pages = {{225-238}},
Note = {{Conference on Trends and Fluctuations and Underlying Mechanisms in
   Terrestrial Orchid Populations, CESKE BUDEJOVICE, CZECH REPUBLIC, MAY
   06-13, 2001}},
Abstract = {{Reduced light availability as a consequence of changed management is
   considered to be the main cause of the rapid decline of Ophrys
   insectifera L. in The Netherlands. Cessation of coppicing of woodlands
   and increased growth of shade-tolerant understorey species have
   decreased light conditions in the habitat of this endangered species. To
   restore suitable light conditions, coppicing was reintroduced in the
   woodland uphill of chalk grassland in The Netherlands in 1996. Coppicing
   increased light availability for individuals of O. insectifera on the
   upper part of the grassland. Flowering was significantly positively
   correlated with increased light availability. Individuals in plots with
   the highest light intensity had a higher percentage of flowering plants
   and were able to flower for more consecutive years, compared to plants
   receiving less light. However, only very small positive correlations
   between increased light availability and inflorescence production or
   plant size were found. Percentage of pollinated flowers showed a
   negative correlation with increased light conditions, maybe because of
   changed wind conditions as a result of the coppiced woodland uphill the
   grassland. The importance of short-term experiments on an individual
   plant basis is stressed by the outcome of this study, since flowering
   frequency can only be measured by monitoring individual year-to-year
   performance.}},
