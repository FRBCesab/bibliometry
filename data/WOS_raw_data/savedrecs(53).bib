@inproceedings{ ISI:000180355400058,
Author = {Valkeajarvi, P and Auvinen, H and Riikonen, R and Salmi, P},
Editor = {{Todd, T and Fleischer, G}},
Title = {{Monitoring of vendace stocks in Finland}},
Booktitle = {{ADVANCES IN LIMNOLOGY 57: BIOLOGY AND MANAGEMENT OF COREGONID FISHES -
   1999}},
Series = {{ERGEBNISSE DER LIMNOLOGIE}},
Year = {{2002}},
Volume = {{57}},
