@inproceedings{ ISI:000180304900084,
Author = {Scarborough, GC and Robertson, T},
Editor = {{GoodrichMahoney, JW and Mutrie, DF and Guild, CA}},
Title = {{Evaluation of low technology large woody debris as a technique to
   augment fish habitat in streams crossed by transmission corridors}},
Booktitle = {{SEVENTH INTERNATIONAL SYMPOSIUM ON ENVIRONMENTAL CONCERNS IN
   RIGHTS-OF-WAY-MANAGEMENT}},
Year = {{2002}},
