@inproceedings{ ISI:000226029600050,
Author = {Kingdon, NG and Darling, EAD},
Editor = {{Chamberlain, D and Wilson, A}},
Title = {{Using satellite imagery to aid the implementation of a recovery plan for
   a declining farmland bird}},
Booktitle = {{AVIAN LANDSCAPE ECOLOGY: PURE AND APPLIED ISSUES IN THE LARGE-SCALE
   ECOLOGY OF BIRDS}},
Year = {{2002}},
Pages = {{313-317}},
