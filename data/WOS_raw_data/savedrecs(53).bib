@article{ ISI:000178571000002,
Author = {Lopez, L and Pozo, C and Gomez, MA and Calvo, C and Lopez, JG},
Title = {{Studies on the effects of the insecticide aldrin on aquatic microbial
   populations}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2002}},
Volume = {{50}},
Number = {{2}},
Pages = {{83-87}},
Abstract = {{The effects of aldrin at rates 10 and 50 mg ml(-1) on microbial function
   were studied under aerobic conditions. Parameters monitored included
   total culturable population of heterotrophic bacteria, number of
   culturable phosphate-solubilizing microllora, number of bacteria which
   decompose protein with liberation Of H2S, nitrifying bacteria,
   denitrifying bacteria and denitrification activity. Culturable
   heterotrophic bacteria, phosphate-solubilizing bacteria, bacteria which
   decompose proteins with liberation of H2S and denitrifying bacteria were
   significantly increased at dose rates of 10 and 50 mg ml(-1) of aldrin.
   However, the presence of aldrin decreased denitrification activity at
   concentration of 50 mg ml(-1). Nitrifying population were not affected
   as a consequence of the addition of aldrin, showing that these
   microorganisms can tolerate the concentrations of aldrin tested. (C)
   2002 Elsevier Science Ltd. All rights reserved.}},
