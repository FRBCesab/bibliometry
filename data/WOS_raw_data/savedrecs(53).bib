Title = {{A comparison of first and third year growth of two strains of largemouth
   bass in South Carolina}},
Booktitle = {{BLACK BASS: ECOLOGY, CONSERVATION, AND MANAGEMENT}},
Series = {{AMERICAN FISHERIES SOCIETY SYMPOSIUM}},
Year = {{2002}},
