@article{ ISI:000172738200012,
Author = {Pine, WE and Allen, MS and Dreitz, VJ},
Title = {{Population viability of the gulf of Mexico sturgeon: Inferences from
   capture-recapture and age-structured models}},
Journal = {{TRANSACTIONS OF THE AMERICAN FISHERIES SOCIETY}},
