@inproceedings{ ISI:000226029600004,
Author = {Brotons, L and Monkkonen, M and Martin, JL},
Editor = {{Chamberlain, D and Wilson, A}},
Title = {{Fragments are not like real islands: Landscape context and population
   trends of birds in boreal forests}},
Booktitle = {{AVIAN LANDSCAPE ECOLOGY: PURE AND APPLIED ISSUES IN THE LARGE-SCALE
   ECOLOGY OF BIRDS}},
Year = {{2002}},
Pages = {{39-47}},
Note = {{11th Annual Conference of the
   International-Association-for-Landscape-Ecology, Univ E Anglia, Norwich,
   ENGLAND, SEP 10-13, 2002}},
Organization = {{Int Assoc Landscape Ecol, UK Chapter}},
Abstract = {{We tested the role of matrix type as a determinant of changes in bird
   densities associated with patch area (patch size effect) by studying
   bird distribution in two different landscape contexts: forested islands
   in archipelagos (low quality matrix) and mature forest patches in
   landscape managed by forestry (high quality matrix). We predicted that
   patch area effects (PAE) will be stronger in patches surrounded by a low
   quality matrix in which movements of individuals are more restricted and
   alternative resources not available. We assessed the role of species
   sensitivity to human induced landscape change (estimated from species
   population trends in long term monitoring data) as a predictor of patch
   size effect in forest. birds in different landscape scenarios. Overall
   patch area effects were significantly higher than 0 in real islands but
   not in patches surrounded by a higher quality matrix. Species more
   sensitive to large-scale landscape change showed strong patch size
   effects on islands but not in fragments. Our results suggest that forest
   fragments do not function as real islands mainly because of the
   compensatory effects of the surrounding matrix. However, the significant
   association between large-scale population trends and species responses
   at a local level in patches surrounded by a low quality matrix suggests
   that strong changes in landscape matrix quality might be behind recent
   population decreases of some boreal forest birds.}},
