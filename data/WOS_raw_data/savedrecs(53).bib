@article{ ISI:000179985900014,
Author = {Sinclair, A and Catling, PM},
Title = {{Recent trends in stem numbers in Goldenseal, Hydrastis canadensis,
   populations at the northern limit of its range}},
Journal = {{CANADIAN FIELD-NATURALIST}},
Year = {{2002}},
Volume = {{116}},
Number = {{1}},
Pages = {{112-114}},
Month = {{JAN-MAR}},
Abstract = {{A 15\% increase in stem number occurred in 14 natural Ontario Goldenseal
   populations surveyed between 1998 and 2000, based on maps of stems in
   1m(2) quadrats. Increase occurred in 10 of the 14 populations and was
   significant. The increase mostly involved small stems, but neither
   seedlings nor very small plants were observed suggesting that stem
   increase was largely due to vegetative reproduction by rhizomes and/or
   root tips. These data correspond to expectation since populations have
   not expanded rapidly nor spread substantially over the past few decades,
   but rather appear to have expanded slightly and slowly. Corresponding to
   earlier speculation, evidence is accumulating to suggest that
   substantial spreading and large population increases may be dependent on
   specific kinds and levels of disturbance some of which are no longer
   prevalent or frequent.}},
