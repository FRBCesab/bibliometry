@article{ ISI:000175613400002,
Author = {Kovacova, M and Schieber, B},
Title = {{Growth analysis and energy accumulation in aboveground biomass of the
   population Galium odoratum (L.) S c op. (Rubiaceae) in submountain beech
   forest}},
Journal = {{EKOLOGIA-BRATISLAVA}},
Year = {{2002}},
Volume = {{21}},
Number = {{1}},
Pages = {{15-26}},
Abstract = {{Production as well as energy equivalent of aboveground biomass of Galium
   odoratum (L.) population were studied during the years 1994-1995. The
   energy values of the monitored species moved from 17,191 J.g(-1) up to
   15,319 J.g(-1) (sterile ramets). In the case of the fertile ramets the
   values fluctuated between 17,659 J.g(-1) and 15,295 J.g(-1). A higher
   annual increment in dry matter (3.62 g.m(-2).yr(-1)) and energy
   (56.25kJ.m(-2).yr(-1)) were found in 1995 (42\% difference). Vegetative
   development lasted 226-231 days. Full flowering was observed in the
   beginning of the 3(rd) decade of May. Generative cycle lasted 135-147
   days. Growth-production process as well as biomass energy of the
   shoots-ramets development reflected the different influence of climate
   within the years under observation.}},
