@inproceedings{ ISI:000179507800005,
Author = {Kock, R and Kebkiba, B and Heinonen, R and Bedane, B},
Editor = {{Gibbs, EPJ and Bokma, BH}},
Title = {{Wildlife and pastoral society - Shifting paradigms in disease control}},
Booktitle = {{DOMESTIC ANIMAL/WILDLIFE INTERFACE: ISSUE FOR DISEASE CONTROL,
   CONSERVATION, SUSTAINABLE FOOD PRODUCTION, AND EMERGING DISEASES}},
