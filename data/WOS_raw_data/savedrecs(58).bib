@article{ ISI:000071704800010,
Author = {Bougrier, S and Collet, B and Geairon, P and Geffard, O and Heral, M and
   Deslous-Paoli, JM},
Title = {{Respiratory time activity of the Japanese oyster Crassostrea gigas
   (Thunberg)}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
Year = {{1998}},
Volume = {{219}},
Number = {{1-2}},
Pages = {{205-216}},
Month = {{JAN 9}},
Abstract = {{A continuous monitoring system has been developed to measure the
   respiratory time activity (RTA) of the oyster Crassostrea gigas
   (Thunberg). This activity was recorded over 24 h using this technique,
   at constant temperatures of 15 and 21 degrees C in the laboratory. At 21
   degrees C, respiratory activity was measured before and after a period
   of starvation, during which spawning occurred. In the field, RTA was
   measured during two successive tidal cycles of a day. The respiratory
   time activity was defined as the percentage of time spent active by the
   oyster relative to the total experimental time. Important individual
   variability was noticed, percentage of time spent active ranged from 0
   (RTA = 0\%) to 100\% (RTA = 100\%). RTA measured on the whole
   experimental population, taking into account active and non-active
   oysters, varied from 52 to 86\% under laboratory conditions and,
   according to the season, from 44 to 82\% in the field. Considering only
   the active oysters (RTA > 0\%) which represented, according to the
   different experiments, from 75 to 100\% of the studied population, RTA
   varied from 55 to 86\% in the laboratory and from 59 to 94\% in the
   field experiments. The influence of RTA on growth predictions is
   discussed. (C) 1998 Elsevier Science B.V.}},
