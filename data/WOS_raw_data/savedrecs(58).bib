@article{ ISI:000072723300019,
Author = {Wall, GR and Phillips, PJ and Riva-Murray, K},
Title = {{Seasonal and spatial patterns of nitrate and silica concentrations in
   Canajoharie Creek, New York}},
Journal = {{JOURNAL OF ENVIRONMENTAL QUALITY}},
