@article{ ISI:000072447100002,
Author = {Simmons, AM},
Title = {{Survey of the parasitoids of Bemisia argentifolii (Homoptera :
   Aleyrodidae) in coastal South Carolina using yellow sticky traps}},
Journal = {{JOURNAL OF ENTOMOLOGICAL SCIENCE}},
Year = {{1998}},
Volume = {{33}},
Number = {{1}},
Pages = {{7-14}},
Month = {{JAN}},
Abstract = {{A survey was conducted to identify which native parasitoids of Bemisia
   argentifolii Bellows and Perring (sweetpotato whitefly strain-B =
   silverleaf whitefly) are present in coastal South Carolina and to
   monitor their seasonal abundance. South Carolina is the northern
   geographic limit of year-round field populations of B. argentifolii in
   the eastern U.S. Yellow sticky cards were used to monitor the
   parasitoids. Parasitoid abundance varied over time and among five
   coastal South Carolina sweetpotato field locations where pesticide was
   not used. Five species of parasitoids were found (Encarsia pergandiella,
   E. nigricephala, E. strenua, E. quaintancei, and Eretmocerus sp.). The
   most abundant species were E. nigricephala and E. pergandiella (89\% in
   1993; 70\% in 1994). No parasitoid fauna of B. argentifolii had
   heretofore been reported from fields in South Carolina. This background
   information will aid in developing a biological control component in a
   management strategy for B. argentifolii.}},
