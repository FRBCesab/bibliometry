@article{ ISI:000074549400016,
Author = {Sprecher, SL and Getsinger, KD and Stewart, AB},
Title = {{Selective effects of aquatic herbicides on sago pondweed}},
Journal = {{JOURNAL OF AQUATIC PLANT MANAGEMENT}},
Year = {{1998}},
Volume = {{36}},
Pages = {{64-68}},
Month = {{JAN}},
Abstract = {{Three aquatic herbicides effective on the exotic weed Eurasian
   watermilfoil (Myriophyllum spicatum L.)-2,4-D ((2,4-dichlorophenoxy)
   acetic acid), endothall (7-oxabicycol {[}2.2.1] heptane-2,3-dicarboxylic
   acid) and triclopr ({[}(3,5,6-trichloro-2-pyridinyl) oxy] acetic
   acid)-were evaluated in the laboratory for selective effect or efficacy
   on the native submersed species, sage pondweed (Potamogeton pectinatus
   L.). For each herbicide, three concentrations in ranges associated with
   Eurasian watermilfoil or sage pondweed control were applied ill static
   exposures of 24 hr, and plants were monitored for 35 d. Endothall at
   0.5, 1 and 2 mg L-1 significantly reduced final biomass, by greater than
   or equal to 72\%, confirming that this herbicide will not maintain
   populations of sage pondweed where it is used to manage Eurasian
   watermilfoil. Application of the growth regulator-type systemic
   herbicides at 1, 1.5 and 2 mg L-1 resulted in no significant reduction
   in biomass from 2,4-D, but up to 24\% reduction with triclopr. The more
   selective activity of these compounds towards sage pondweed supports
   their use for controlling Eurasian watermilfoil in plant communities
   where it is desirable to maintain the native species. However, in areas
   where sage pondweed is itself a nuisance plant, endothall gives
   effective chemical control.}},
