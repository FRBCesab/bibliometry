@article{ ISI:000075806100051,
Author = {Da Ros, L and Marin, MG and Nesto, N and Ford, SE},
Title = {{Preliminary results of a field study on some stress-related parameters
   in Tapes philippinarum naturally infected by the protozoan Perkinsus sp.}},
