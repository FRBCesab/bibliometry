@article{ ISI:000076661200002,
Author = {Schneider, F and Koch, G and Jung, C and Verreet, JA},
Title = {{The application of molecular markers for genetic characterization of
   Septoria tritici populations}},
Journal = {{ZEITSCHRIFT FUR PFLANZENKRANKHEITEN UND PFLANZENSCHUTZ-JOURNAL OF PLANT
   DISEASES AND PROTECTION}},
Year = {{1998}},
Volume = {{105}},
Number = {{5}},
Pages = {{452-461}},
Month = {{SEP}},
Abstract = {{Molecular markers can be used for determining BNA-polymorphisms among
   populations of phytopathogenic Fungi. We have characterized 22 single
   spore isolates of Septoria tritici from different locations (30 km apart
   from each other) in North Germany by different marker systems. On the
   average, restriction fragment length polymorphism (RFLP) markers cloned
   from single copy or repetitive DNA yielded 2.4 and 5.8 polymorphic
   bands, respectively. For each primer combination, random amplified
   polymorphic DNA (RAPD) markers gave 6.3 and amplified fragment length
   polymorphism (AFLP) markers 36.3 polymorphic bands, respectively.
   Between the groups of isolates, similarities ranged from 0.38 to 0.23
   for RFLPs, 0.43 to 0.25 for AFLPs, and 0.35 to 0.15 for RAPDs,
   respectively. However, clustering was apparent For isolates sampled from
   the same location. For monitoring S. tritici infection in the field, the
   AFLP marker technique proved to be most powerful due to its high
   polymorphism rate.}},
