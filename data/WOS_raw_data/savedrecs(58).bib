@article{ ISI:000075871000013,
Author = {Smallwood, KS and Wilcox, B and Leidy, R and Yarris, K},
Title = {{Indicators assessment for habitat conservation plan of Yolo County,
   California, USA}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
