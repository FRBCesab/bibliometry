@article{ ISI:000076682900008,
Author = {Puchkovskii, SV},
Title = {{The role of man in forming the communication system of the brown bear
   (Ursus arctos L.), and the problem of monitoring}},
Journal = {{RUSSIAN JOURNAL OF ECOLOGY}},
Year = {{1998}},
Volume = {{29}},
Number = {{5}},
Pages = {{344-348}},
Month = {{SEP-OCT}},
Abstract = {{The results of long-term observations of ``bear{''} trees in Udmurtiya
   are considered. Distant communication between brown bears in population
   is the most plausible function of tree marking. Nowadays, the situation
   with communication systems of brown bear and some other mammals is
   mainly determined by the results of human activity (road network,
   glades, paths, patchiness of the territory). The intensity of tree
   marking may reflect the degree of anthropogenic influence on the brown
   bear population and its living conditions. Differences in the number of
   bear trees (per 10 km of census route) reflect differences in the
   density of the brown bear population. In monitoring, bear trees may be
   used as a source of important information about the state of the brown
   bear population and corresponding ecosystems.}},
