@article{ ISI:000077622800010,
Author = {Gunn, JM and Mills, KH},
Title = {{The potential for restoration of acid-damaged lake trout lakes}},
Journal = {{RESTORATION ECOLOGY}},
Year = {{1998}},
Volume = {{6}},
Number = {{4}},
Pages = {{390-397}},
Month = {{DEC}},
Abstract = {{This review of long-term monitoring and experimental manipulation
   studies from Ontario, Canada, offers encouraging evidence that lake
   trout (Salvelinus namaycush) populations can be restored to acidified
   lakes once the problem of acid deposition is solved in North America.
   Predicting the rate of chemical and biological recovery remains
   difficult, because there are several limnological attributes (dissolved
   organic carbon, flushing rate) and biological attributes (acid
   sensitivity, species longevity, fish community composition) of lake
   trout ecosystems that can either enhance or delay the recovery process.
   Some options for managing recovering lakes are also reviewed, including
   increased pollution controls, liming, hatchery stocking, and harvest
   controls.}},
