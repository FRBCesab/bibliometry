@article{ ISI:000075910600004,
Author = {Pereira, AP and Graca, MAS and Molles, M},
Title = {{Leaf litter decomposition in relation to litter physico-chemical
   properties, fungal biomass, arthropod colonization, and geographical
   origin of plant species}},
Journal = {{PEDOBIOLOGIA}},
