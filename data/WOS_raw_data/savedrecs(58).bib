@article{ ISI:000075130900015,
Author = {Baier-Anderson, C and Anderson, RS},
Title = {{Evaluation of the immunotoxicity of chlorothalonil to striped bass
   phagocytes following in vitro exposure}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY}},
Year = {{1998}},
Volume = {{17}},
Number = {{8}},
Pages = {{1546-1551}},
Month = {{AUG}},
Abstract = {{The fungicide chlorothalonil (TCIN), with both agricultural and
   horticultural applications, is a common aquatic pollutant. The
   immunotoxic potential of TCIN was investigated following a 20-h in vitro
   exposure of striped bass (Morone saxatilus) macrophages to a range of
   sublethal concentrations. Cells from the anterior kidney were separated
   by density-gradient centrifugation to obtain a macrophage-enriched cell
   population. Following TCIN exposure, reactive oxygen species (ROS)
   production was evaluated using luminol-augmented chemiluminescence;
   phagocytic capacity was measured by the ingestion of fluorescein
   isothiocyanate-conjugated yeast; and NADPH production was estimated by
   monitoring the reduction of a water-soluble tetrazolium,
   3-(4,5-dimethylthiazol
   2-yl)-5-(3-carboxymethoxyphenyl)-2-(4-sulfophenyl)-2H-tetrazolium, inner
   salt. Results indicate that TCIN, at concentrations greater than or
   equal to 250 mu g/L, decreased ROS production in both zymosan- and
   phorbol myristate acetate (PMA)-stimulated cells in a dose-dependent
   manner. The PMA-stimulated NADPH production coincident with the
   respiratory burst was also decreased at concentration greater than or
   equal to 250 mu g/L. The TCIN did not appear to alter phagocytic
   ability. Taken together, these results indicate that the mechanism of
   toxicity affects the activation or function of the NADPH oxidase but not
   the uptake of particles by phagocytosis, and that these effects can
   probably be localized to the signal transduction pathway or the
   assemblage of NADPH oxidase.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
