@article{ ISI:000075544100004,
Author = {Wang, SH and Gergerich, RC},
Title = {{Immunofluorescent localization of tobacco ringspot nepovirus in the
   vector nematode Xiphinema americanum}},
Journal = {{PHYTOPATHOLOGY}},
