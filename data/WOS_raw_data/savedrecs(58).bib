@article{ ISI:000075806100086,
Author = {Morrall, CE and Trapido-Rosenthal, HG and Knap, AH and Depledge, MH},
Title = {{Development of nitric oxide and nitric oxide synthase as
   ecotoxicological biomarkers in the tropical marine environment}},
Journal = {{MARINE ENVIRONMENTAL RESEARCH}},
Year = {{1998}},
Volume = {{46}},
Number = {{1-5}},
Pages = {{429-432}},
Month = {{JUL-DEC}},
Note = {{9th International Symposium on Responses of Marine Organisms to
   Pollutants (PRIMO 9), BERGEN, NORWAY, APR 27-30, 1997}},
Abstract = {{The enzyme nitric oxide synthase (NOS) catalyses reactions that result
   in the generation of nitric oxide (NO), a multipotent signal molecule.
   We have begun to characterise the activity of NOS found in the host
   tissue of the symbiotic tropical/sub-tropical anemone Aiptasia pallida,
   and have monitored changes in the activity of this enzyme under
   different environmental conditions. Our characterisation studies
   indicate that the NOS activity from Aiptasia is similar to activities
   from other organisms in terms of cellular localisation, cofactor
   requirements, and inhibition profiles. Our environmental experiments
   indicate that: (I) the incubation of whole animals at temperatures
   representative of the highest and lowest extremes to which the Bermuda
   population would be exposed results in an NOS activity reduction
   relative to animals maintained at the control temperature of 25 degrees
   C; and (2) treatment with copper produces variable changes in NOS
   activity relative to untreated controls. (C) 1998 Elsevier Science Ltd.
   All Fights reserved.}},
