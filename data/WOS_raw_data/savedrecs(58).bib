@article{ ISI:000076661200001,
Author = {Cirvilleri, G and Caldarera, G},
Title = {{Use of lux-marker genes to monitor survival of antagonistic Pseudomonas
   fluorescens on the phylloplane}},
Journal = {{ZEITSCHRIFT FUR PFLANZENKRANKHEITEN UND PFLANZENSCHUTZ-JOURNAL OF PLANT
   DISEASES AND PROTECTION}},
Year = {{1998}},
Volume = {{105}},
Number = {{5}},
Pages = {{441-451}},
Month = {{SEP}},
Abstract = {{Lux-marked strains of a Pseudomonas fluorescens strain effective as
   biological control agent were analyzed for survival ability on the
   phylloplane of bean plants under growth chamber conditions. The
   persistence of the genetically engineered bacteria was monitored by
   bioluminescence detection syst ems as well as by conventional serial
   dilution and plating methods. The lux-mutants were able to establish
   large populations on wet bean leaves, comparable with the wild-type
   strain. None of them had lost the bioluminescence and the ability to
   grow on selective media during phylloplane colonization. One of the
   lux-mutants tested exhibit ed significantly higher light production
   relative to the other lux-mutants during 12 days on bean plants.
   Bioluminescence proved to be an innocuous mutation and a sensitive
   detection method for further studies on population dynamics and
   behaviour under various conditions during bacterial colonization of
   plants.}},
