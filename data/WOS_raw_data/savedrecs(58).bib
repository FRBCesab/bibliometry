@article{ ISI:000075518400023,
Author = {Jackson, DM and Klun, JA and Khrimian, AP and Simmons, AM and Sorensen,
   KA},
Title = {{Monitoring pickleworm (Lepidoptera : Pyralidae) moths with
   pheromone-baited traps}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{1998}},
Volume = {{91}},
Number = {{4}},
Pages = {{950-956}},
Month = {{AUG}},
Abstract = {{Populations of male moths of pickleworm, Diaphania nitidalis (Stoll),
   were monitored using 5 types of traps baited with virgin female moths
   and several formulations of sex pheromone lures. Modified box traps and
   wire-cone traps were highly effective when baited with either synthetic
   lures or virgin females. Unitraps baited with virgin females were less
   effective than box or wire-cone traps, and unitraps were ineffective
   when baited with synthetic pheromone lures. Nylon-cone traps were less
   effective than wire-cone or box traps. Pherocon 1C sticky traps did not
   work with any lure.}},
