@article{ ISI:000073447900003,
Author = {Nusser, SM and Breidt, EJ and Fuller, WA},
Title = {{Design and estimation for investigating the dynamics of natural
   resources}},
Journal = {{ECOLOGICAL APPLICATIONS}},
Year = {{1998}},
