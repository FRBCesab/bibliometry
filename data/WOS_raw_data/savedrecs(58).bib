@article{ ISI:000076457100002,
Author = {Gibb, JA and Fitzgerald, BM},
Title = {{Dynamics of sparse rabbits (Oryctolagus cuniculus), orongorongo valley,
   new Zealand}},
Journal = {{NEW ZEALAND JOURNAL OF ZOOLOGY}},
