@article{ ISI:000075549200022,
Author = {Stebbings, RE and Killeen, IJ},
Title = {{Translocation of habitat for the snail Vertigo moulinsiana in England}},
Journal = {{JOURNAL OF CONCHOLOGY}},
Year = {{1998}},
Number = {{2}},
Pages = {{191-204}},
Month = {{JUN}},
Note = {{Conference on Molluscan Conservation - a Strategy for the 21st Century,
   CARDIFF, WALES, NOV, 1996}},
Organization = {{Natl Museums \& Galleries Wales}},
Abstract = {{Discovery of populations of the marsh snail Vertigo moulinsiana near and
   within the route of a new road in southern England whilst site clearance
   was in progress, caused the urgent development of a strategy to protect
   the animals. The snail's inclusion on Annex II of the European Habitats
   \& Species Directive made it vital for appropriate action to be
   implemented to protect the snail along the proposed road. As it was not
   feasible to change the route it was decided to translocate the wetland
   habitat supporting the Vertigo moulinsiana populations to places
   off-route. As a further safeguard, new marsh habitat was created to
   provide the opportunity for V. moulinsiana colonisation. The methods of
   translocation and the subsequent monitoring programme are described.
   Early results indicate that the translocation of snail rich vegetation
   has been successful and that the snails are breeding.}},
