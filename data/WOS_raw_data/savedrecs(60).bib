@article{ ISI:000072802800037,
Author = {van der Meeren, GI},
Title = {{Preliminary acoustic tracking of native and transplanted European
   lobsters (Homarus gammarus) in an open sea lagoon}},
Journal = {{MARINE AND FRESHWATER RESEARCH}},
