@article{ ISI:A1996UP68900017,
Author = {vanWijnen, JH and Slob, R and JongmansLiedekerken, G and vandeWeerdt,
   RHJ and Woudenberg, F},
Title = {{Exposure to polycyclic aromatic hydrocarbons among Dutch children}},
Journal = {{ENVIRONMENTAL HEALTH PERSPECTIVES}},
Year = {{1996}},
Volume = {{104}},
Number = {{5}},
Pages = {{530-534}},
Month = {{MAY}},
Abstract = {{We determined the urinary 1-hydroxypyrene (1-HP) concentration and the
   creatinine-adjusted 1-HP concentration in 644 randomly selected Dutch
   children aged 1-6 years and living in five areas with roughly different
   levels of polycyclic atomatic hydrocarbons (PAHs) in soil and ambient
   air. The presence of other factors that might influence the exposure to
   PAHs was studied using a questionnaire. To evaluate the reliability of a
   single urinary 1-HP determination, measurements were repeated after 3
   weeks for approximately 200 children. The mean urinary 1-HP content of
   the total study population was 2.06 nmol/l. This varied from 1.58 nmol/l
   in the reference area (Flevoland) to 2.71 nmol/l in the valley of the
