@article{ ISI:000075795700011,
Author = {Buley, KR and Prior, KA and Gibson, RC},
Title = {{In search of the St. Lucia racer Liophis ornatus, with further
   observations on the reptile fauna of the Maria Islands, St. Lucia, West
   Indies}},
