@inproceedings{ ISI:A1997BH89V00097,
Author = {KriegerBrockett, B and Deming, JW and Herwig, RP},
Book-Group-Author = {{BATTELLE MEM INST}},
Title = {{An assessment of organic contaminant biodegradation rates in marine
   environments}},
Booktitle = {{IN SITU AND ON-SITE BIOREMEDIATION, VOL 4}},
Series = {{BIOREMEDIATION SERIES}},
Year = {{1997}},
Volume = {{4(4)}},
Number = {{4}},
