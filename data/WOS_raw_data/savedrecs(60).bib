Author = {Hernandez, MA and Gonzalez, N},
Editor = {{Chilton, J}},
Title = {{Impact of rising piezometric levels on Greater Buenos Aires due to
   partial changing of water services infrastructure}},
Booktitle = {{GROUNDWATER IN THE URBAN ENVIRONMENT - VOL I: PROBLEMS, PROCESSES AND
   MANAGEMENT}},
Year = {{1997}},
Pages = {{237-242}},
