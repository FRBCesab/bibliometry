@inproceedings{ ISI:000074636300013,
Author = {Lee, HL and Sunwoo, Y},
Editor = {{Incecik, S and Ekinci, E and Yardim, F and Bayram, A}},
Title = {{The application of various ozone air quality indicators to Seoul, Korea}},
