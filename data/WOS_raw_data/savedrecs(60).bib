@article{ ISI:A1996VF37100013,
Author = {Hurd, MK and Perry, SA and Perry, WB},
Title = {{Nontarget effects of a test application of diflubenzuron to the forest
   canopy on stream macroinvertebrates}},
Journal = {{ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY}},
