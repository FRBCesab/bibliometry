@inproceedings{ ISI:000085901000053,
Author = {Lindeman, ST and Engstrom, RT and Palmer, WE and Millsap, B},
Book-Group-Author = {{SAF
   SAF}},
Title = {{Landscape restoration research at tall timbers research station using
   fire and forest management}},
Booktitle = {{MEETING IN THE MIDDLE, PROCEEDINGS}},
Year = {{1997}},
Pages = {{349-350}},
Note = {{1997 National Convention of the Society-of-American-Foresters, MEMPHIS,
   TN, OCT 04-08, 1997}},
Organization = {{Soc Amer Foresters}},
Abstract = {{A key land management goal for the Tall Timbers Research Station
   property (1585 ha) is to maintain upland pine-dominated habitats with
   frequent (one- to three-year) low-intensity prescribed fires to provide
   habitat for characteristic native animals, particularly the Northern
   Bobwhite (Colinus virginianus). Currently, loblolly and shortleaf pine
   (Pinus taeda and P. echinata), are common in the uplands, but the
   presence of scattered longleaf pine (P. palustris) suggest that this
   species was once more common. Dominance of fire-intolerant pines on
   upland sites has led to a land management dilemma: short fire return
   intervals (1-3 years) are needed to control hardwoods in the understory,
   but it kills seedling and sapling loblolly and shortleaf pines. Low
   levels of pine regeneration has created an age structure dominated by
   older age classes, and a steadily increasing hardwood component. For
   ecological and economic reasons, we have initiated a project to reduce
   hardwood dominance on upland habitats on 480 hectares of Tall Timbers.
   Management actions include a timber harvest to remove merchantable
   hardwoods and combinations of drum chopping, mowing and herbicide
   treatment. Longleaf pine is being planted as part of experiments testing
   different methods of restoration. Response of songbird, Northern
   Bobwhite, Cooper's Hawk (Accipiter cooperi) populations and ground cover
   vegetation will be monitored before and after hardwood removal.}},
