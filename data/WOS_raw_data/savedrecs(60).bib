@article{ ISI:000075795700010,
Author = {Gibson, RC and Freeman, M},
Title = {{Conservation at home: Recovery programme for the agile frog Rana
   dalmatina in Jersey}},
Journal = {{DODO-JOURNAL OF THE WILDLIFE PRESERVATION TRUSTS}},
Year = {{1997}},
Volume = {{33}},
Pages = {{91-104}},
Abstract = {{The agile frog Rana dalmatina enjoys a wide distribution throughout much
   of southern and central Europe, but is found in only a few northern
   locations including Jersey. The frog is not found anywhere else in the
   British Isles and may soon be lost from Jersey as populations have been
   declining rapidly for a number of years. Recent investigations of their
   historical strongholds suggest that the species is reduced to a single
   fragile population at L'Ouaisne Common. A collaborative project
   involving the Jersey Wildlife Preservation Trust Herpetology Department,
   States of Jersey Planning and Environment Committee Environmental
   Services Unit and the Societe Jersiaise Zoology Section has led to the
   establishment and maintenance of captive breeding colonies,
   investigation of water quality and other environmental factors
   island-wide, and the release of captive-bred and captive-reared tadpoles
   and frogs into regularly monitored locations.}},
