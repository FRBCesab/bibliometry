@article{ ISI:A1997WE39600003,
Author = {Vrain, TC and Forge, TA and DeYoung, R},
Title = {{Population dynamics of Pratylenchus penetrans parasitizing raspberry}},
Journal = {{FUNDAMENTAL AND APPLIED NEMATOLOGY}},
Year = {{1997}},
Volume = {{20}},
Number = {{1}},
Pages = {{29-36}},
Abstract = {{The root-lesion nematode Pratylenchus penetrans is a severe pathogen of
   raspberry (Rubus idaeus L.) and other small fruit crops. Temporal
   changes in the size and distribution between root and soil habitats of a
   population of dhe nematode under raspberry were monitored. Soil and root
   samples were collected at four depths from five plots in an 8-year-old
   raspberry planting every 2 weeks for 2 years. Trends of nematode
   densities were not identical in the 2 years. There were no sustained
   changes in the fall and winter of the first year, a reduction in
   nematode population in the first spring followed by increases in early
   summer and early fail followed by a decrease in late fall. There were no
   sustained changes in the second year except fur a single reduction in
   late spring and summer. The proportion of the nematode population in
   roots ranged from a maximum of 0.69 to a minimum of 0.07, but that
   proportion did not follow a seasonal pattern. Nematode densities were
   not correlated with root abundance. Because of the lack of regular
   seasonal population trends, it is suggested thai parameters other than
   temperature and root availability may have controlled reproduction of
   the nematodes, and distribution between roots and rhizosphere soil of
   perennial crops.}},
