@article{ ISI:A1996VM53700012,
Author = {Dunn, EH and Larivee, J and Cyr, A},
Title = {{Can checklist programs be used to monitor populations of birds recorded
   during the migration season?}},
Journal = {{WILSON BULLETIN}},
Year = {{1996}},
Volume = {{108}},
Number = {{3}},
Pages = {{540-549}},
Month = {{SEP}},
Abstract = {{Quebec's EPOQ program compiles birders' `'checklists,'' each of which
   reports numbers of birds seen on one day at one site. We analyzed EPOQ
   data from the migration season alone (1971-92), to see if these
   unstandardized counts might monitor trends in populations that nest
   farther north. Two sets of trends were computed for each of 58 species,
   from annual indices based either on abundance or on frequency of
   detection. Both spring EPOQ trends were significantly correlated with
   Breeding Bird Survey trends for Quebec, while only those based on
   abundance performed well in fall. There was a positive bias in magnitude
   of EPOQ trends, but negative EPOQ trends were reliable indicators of
   negative BBS trends. Analysis of sub-sets of the data showed that sample
   size had little qualitative effect. Checklist data should not be relied
   on for quantitative population monitoring, but they do contain useful
   information for detection or corroboration of negative trends.}},
