@article{ ISI:A1996VY85100003,
Author = {Piotrowski, JK and Orlowski, C and Bem, EM and Brys, M and Baran, E},
Title = {{The monitoring of cadmium, zinc and copper in the kidneys and liver of
   humans deceased in the region of Cracow (Poland)}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
Year = {{1996}},
Volume = {{43}},
Number = {{3}},
Pages = {{227-236}},
Month = {{DEC}},
Abstract = {{Cadmium, zinc and copper levels were determined in the renal cortex and
   liver of 60 inhabitants of Cracow, Poland. Cadmium levels in the renal
   cortex were contained in broad limits of 5-176 mu g/g, mean 50.6 mu g/g
   (wet weight). Maximum levels were found in the age group of about 50-60
   years. The levels were slightly higher in men (53 mu g/g) than in women
   (45 mu g/g), with no effect of location within the region. The levels in
   smokers (62 mu g/g) were much higher than in non-smokers (32 mu g/g).
   The above relations were less pronounced for cadmium levels in the
   liver. Whole body retention of cadmium followed the pattern of cadmium
   in renal cortex. The level of zinc in renal cortex reflected those of
   cadmium. A significant proportion of the population (54\% in smokers,
   9\% in non-smokers) showed cadmium levels in renal cortex exceeding the
   reference level of 50 mu g/g recently accepted for general population.
   In the view of the authors the exposure to cadmium of the population of
   Cracow is excessive and calls for attention.}},
