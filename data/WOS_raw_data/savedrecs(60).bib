@article{ ISI:A1996VZ49200022,
Author = {Blachly, JS and Forschler, BT},
Title = {{Suppression of late-season Argentine ant (Hymenoptera: Formicidae) field
   populations using a perimeter treatment with containerized baits}},
Journal = {{JOURNAL OF ECONOMIC ENTOMOLOGY}},
Year = {{1996}},
Volume = {{89}},
Number = {{6}},
Pages = {{1497-1500}},
Month = {{DEC}},
Abstract = {{The efficacy of containerized ant baits used in a perimeter treatment in
   suppressing late-season Argentine ant, Linepithema humile (Mayr),
   foraging populations was evaluated. Three different containerized ant
   baits containing either 0.9\% hydramethylnon or 0.5\% sulfluramid in 2
   different bait matrix formulations were placed around buildings of the
   complex. Semiweekly ant counts at honeywater monitors, biweekly bait
   consumption, and monthly tenant complaint log requests served as
   evaluation criteria. Bait consumption was slightly less for one of 2
   sulfuramid baits tested. However ant foraging activity, as measured at
   honeywater monitors, was reduced by 84-88\% within 2 wk of exposure to
   baits regardless of bait type. Tenant complaints remained unchanged at
   untreated buildings but were 4 times less than those from treatment
   buildings by the end of the 8-wk trial.}},
