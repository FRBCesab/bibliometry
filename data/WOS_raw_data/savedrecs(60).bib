@inproceedings{ ISI:A1997BH89W00108,
Author = {Bohan, DG and Schlett, WS},
Book-Group-Author = {{BATTELE MEM INST}},
Title = {{Enhanced natural bioremediation using a time release oxygen compound}},
Booktitle = {{IN SITU AND ON-SITE BIOREMEDIATION, VOL 5}},
Series = {{BIOREMEDIATION SERIES}},
Year = {{1997}},
Volume = {{4(5)}},
Number = {{5}},
Pages = {{475-480}},
Note = {{4th International In Situ and On-Site Bioremediation Symposium, NEW
   ORLEANS, LA, APR 28-MAY 01, 1997}},
Organization = {{Battelle Mem Inst; Ajou Univ, Coll Engn; Amer Petr Inst; Asian Inst
   Technol; Biotreatment News; Fluor Daniel GTI; Gas Res Inst; Inst
   Francais Petr; Japan Res Inst Ltd; Mitsubishi Corp; Natl Ctr Integrated
   Bioremediat Res \& Dev; OHM Remediat Serv Corp; Parsons Engn Sci Inc;
   Umweltbundesamt, Germany; USAF Armstrong Lab Environ Directorate; USAF
   Ctr Environm Excellence; US EPA; USN Facil Engn Ser Ctr; Waste
   Management Inc; Water Technol Int Corp; W Reg Hazardous Subst Res Ctr}},
Abstract = {{A time release oxygen compound was placed into an aquifer to facilitate
   bioremediation of a BTEX plume at a leaking underground storage tank
   site. The objective was to accelerate naturally occurring biodegradation
   by using an oxygen release compound(R) (ORC), which slowly delivers
   oxygen over an extended period of time. The treatment area was estimated
   to be approximately 36 m by 21 m, with a 3 m thick contaminated
   saturated zone containing approximately 13.4 kg of BTEX (benzene,
   toluene, ethylbenzene, and xylenes). ORC was applied by drilling 50
   borings with 10.8 cm inner diameter (ID) hollow stem augers and
   installing a precast 10.2 cm diameter cylinder of the compound into the
   upper three meters of the aquifer. BTEX concentrations and microbial
   populations were measured from a series of five monitoring wells in the
   impacted area. The mass of the BTEX compounds totaled approximately 13.4
   kg before ORC installation. In the 316 days following the installation
   of ORC, the contaminant mass has been reduced to 6.2 kg. This decrease
