@inproceedings{ ISI:000072400000047,
Author = {Pelliccioni, A and Ventrone, I and Poli, U and Lepore, L and Angeloni, M},
Editor = {{Power, H and Tirabassi, T and Brebbia, CA}},
Title = {{Use of the Weibull distribution parameters to characterize the
   atmospheric pollution monitoring}},
Booktitle = {{AIR POLLUTION V}},
Year = {{1997}},
Pages = {{507-516}},
Note = {{5th International Conference on Air Pollution - Modelling, Monitoring
   and Management (AIR POLLUTION V), BOLOGNA, ITALY, SEP 16-18, 1997}},
Organization = {{Wessex Inst Technol, Southampton; CNR, Ist Studio Fenomeni Fis \& Chim
   Bassa Alta Atmosfera, Bologna}},
Abstract = {{We perform a statistical analysis, based on the determination of the
   probability density function of an experimental data set, related to air
   pollutants, collected in the urban area of pome. Data have been measured
   during 1993 at fixed site stations of the PMP (Presidio Multizonale di
   Prevenzione) network and at a DOAS (Differential Optical Absorption
   Spectrometer) station.
   The Weibull distribution is taken into consideration to reproduce the
   experimental data distributions.
   We deal with two aspects: with regard to the first we discuss about the
   opportunity to introduce a new ``indicator{''} for concentration level
   evaluated from the Weibull distribution and linked to air quality
   threshold. This parameter is related to the ``difference{''} between
   mean and mode of the Weibull distribution. It appears that the knowledge
   of the mean concentration could not be enough to give information about
   the exposure levels for the population.
   With the regard to the second aspect the study shows that the
   adimensional parameter of the Weibull distribution could be related to
   typologies of emissions and to distributions of atmospheric turbolence.}},
