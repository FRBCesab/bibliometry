@article{ ISI:A1996VP15600001,
Author = {Weber, JM and Meia, JS},
Title = {{Habitat use by the red fox Vulpes vulpes in a mountainous area}},
Journal = {{ETHOLOGY ECOLOGY \& EVOLUTION}},
Year = {{1996}},
Volume = {{8}},
Number = {{3}},
Pages = {{223-232}},
Month = {{SEP}},
Abstract = {{We monitored 10 red foxes (Vulpes vulpes) by radio-tracking from
   September 1989 to February 1993 in a mountainous area in western
   Switzerland. We examined habitat use and assessed the influence of
   rainfall and snow-cover on it. Wooded habitats were consistently
   preferred by foxes, whereas open areas were under-utilized. There were
   seasonal variations in habitat use. During summer and autumn, foxes were
   more active in grassland, wooded areas were used more in winter.
   Activity near human settlements was rare, and without any seasonal
   trend. Rainfall, unlike snow-cover, had no influence on habitat use.}},
