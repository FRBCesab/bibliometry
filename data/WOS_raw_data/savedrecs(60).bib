@article{ ISI:A1996UG15200022,
Author = {Havelka, MA and Millar, JS},
Title = {{Multiple nest boxes per station in the study of Peromyscus leucopus}},
Journal = {{AMERICAN MIDLAND NATURALIST}},
Year = {{1996}},
Volume = {{135}},
Number = {{2}},
Pages = {{376-380}},
Month = {{APR}},
Abstract = {{Single artificial nest boxes have frequently been used to monitor
   populations of Peromyscus leucopus. We monitored four boxes at each of
   20 stations to determine patterns of use of multiple boxes at a station
   for a population of white-footed mice (Peromyscus leucopus
   noveboracensis). Some litters were divided between adjacent boxes at a
   station (8\% of observations in the breeding and postbreeding seasons)
   and darns sometimes nested in a box adjacent to their litter (14\% of
   observations in the breeding season; 11\% of observations in the
   postbreeding season). Our results suggest that multiple nest boxes per
   station may aid in the determination of litter maternity and
   composition, and may permit more accurate sampling of the study
   population than single boxes.}},
