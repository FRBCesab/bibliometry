@inproceedings{ ISI:000078270700008,
Author = {Levine, L},
Editor = {{Kaye, TN and Liston, A and Love, RM and Luoma, DL and Meinke, RJ and Wilson, MV}},
Title = {{The influence of light and other factors on germination and growth in
   Bensoniella oregana (Saxifragaceae)}},
