@article{ ISI:A1996UG15200018,
Author = {VanderKloet, SP and Cabilio, P},
Title = {{Ten year study of the annual variation in berry and seed production in a
   population of Vaccinium corymbosum L}},
Journal = {{AMERICAN MIDLAND NATURALIST}},
Year = {{1996}},
Volume = {{135}},
Number = {{2}},
Pages = {{349-356}},
Month = {{APR}},
Abstract = {{Fifty Nova Scotia highbush blueberry plants (Vaccinium corymbosum) were
   monitored for ten years for berry production and seed set. The plants
   exhibit biennial fluctuation in berry production but individual plants
   retained the same rank among other plants in the number of viable seeds
   produced per berry from year to year. No significant correlation was
   found between berry production and precipitation or temperature. Two
   plants produced about 27\% of the total seed output of this population
   sample while the ten poorest producers contributed <1\% of the total
   seed output. No recruitment was observed, nevertheless recruitment from
   either direct germination or from a small but persistent seed bank is
   possible.}},
