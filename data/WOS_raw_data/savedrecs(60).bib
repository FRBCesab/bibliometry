@article{ ISI:A1996VG10400002,
Author = {Taylor, MR and Moss, ST and Ladle, M},
Title = {{Temporal changes in the level of infestation of Simulium ornatum Meigen
   (Complex) (Simuliidae: Diptera) larvae by the endosymbiotic fungus
   Harpella melusinae Lichtwardt (Harpellales: Trichomycetes)}},
Journal = {{HYDROBIOLOGIA}},
Year = {{1996}},
Volume = {{328}},
Number = {{2}},
Pages = {{117-125}},
Month = {{AUG 9}},
Abstract = {{The fungus Harpella melusinae (Harpellales: Trichomycetes) is obligately
   associated with the midguts of larval Simuliidae (Diptera). The level of
   infestation of a population of Simulium ornatum by H. melusinae was
   monitored at a stream in Hampshire, England. Significant temporal
   changes in the level of infestation were recorded during monthly and
   weekly collections; a twenty-fold increase being recorded over a
   nine-day period. Possible mechanisms by which these changes occur are
   discussed.}},
