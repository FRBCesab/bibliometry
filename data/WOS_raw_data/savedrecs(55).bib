Language = {{English}},
Affiliation = {{Bahmani, I (Reprint Author), Dairying Res COrp, Hamilton, New Zealand.
   Dairying Res COrp, Hamilton, New Zealand.}},
ISBN = {{85-7133-010-7}},
Keywords = {{ryegrass; fillering; reproductive fillers; ecotypes; cultivar selection;
   persistence; nitrogen; cow grazing}},
Keywords-Plus = {{STOCKING RATES; LOLIUM-PERENNE; NEW-ZEALAND; GROWTH}},
