Title = {{The 1998 Baltimore particulate matter Epidemiology-Exposure Study: Part
   2. Personal exposure assessment associated with an elderly study
   population}},
Journal = {{JOURNAL OF EXPOSURE ANALYSIS AND ENVIRONMENTAL EPIDEMIOLOGY}},
Year = {{2000}},
