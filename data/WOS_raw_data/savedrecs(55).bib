@inproceedings{ ISI:000184053000044,
Author = {Dobony, CA and Edwards, JW and Ford, WM and Allen, TJ},
Editor = {{Eversole, AG}},
Title = {{Nesting success of ruffed grouse in West Virginia}},
Booktitle = {{PROCEEDINGS OF THE FIFTY-FIFTH ANNUAL CONFERENCE OF THE SOUTHEASTERN
