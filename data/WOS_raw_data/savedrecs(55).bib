@article{ ISI:000088954000011,
Author = {Smaal, AC and Lucas, L},
Title = {{Regulation and monitoring of marine aquaculture in The Netherlands}},
Journal = {{JOURNAL OF APPLIED ICHTHYOLOGY-ZEITSCHRIFT FUR ANGEWANDTE ICHTHYOLOGIE}},
