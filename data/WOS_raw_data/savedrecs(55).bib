@article{ ISI:000167230800006,
Author = {Kupfer, A and Kneitz, S},
Title = {{Population ecology of the great crested newt (Triturus cristatus) in an
   agricultural landscape: Dynamics, pond fidelity and dispersal}},
Journal = {{HERPETOLOGICAL JOURNAL}},
Year = {{2000}},
Volume = {{10}},
