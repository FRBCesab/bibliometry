@article{ ISI:000172381900003,
Author = {Mishin, VL and Zyryanov, SV and Goryaev, YI},
Title = {{Preliminary insight into the harbour seal population of the east Murman
   coast of the Barents Sea}},
Journal = {{MAMMALIA}},
Year = {{2001}},
Volume = {{65}},
Number = {{3}},
Pages = {{295-300}},
Note = {{13th Annual Meeting of the European-Cetacean-Society (ECS), VALENCIA,
   SPAIN, APR 05, 1999}},
Organization = {{European Cetacean Soc}},
Abstract = {{Five species of pinnipeds occur in the Russian Arctic, among which only
   two species, the grey seal, Halichoerus grypus, and the harbour seal,
   Phoca vitulina, breed in Murman waters, i.e. along the northern coast of
   the Kola peninsula, Russia (Fig. 1). At the beginning of the century,
   very few harbour seals were observed in the Murman region (Smirnov
   1903). More recently, Kondakov (1992) and Zyryanov (1995, 1997)
   described a harbour seal colony in Ivanovskaya Bay (East Murman) and in
   several other bays and estuaries, where haul-out sites are used by
   smaller groups of harbour seals (Fig. 1). This Murman population appears
   to be the only significant concentration of harbour seals in the
   European part of Russia. Nonetheless, to date, very little is known
   about the Ivanovskaya Bay colony and thus further investigation is
   needed on the ecology, population size and reproductive status of this
   species along the Murman coast. The species is commonly monitored in
   adjacent areas of Scandinavia and Spitsbergen (Prestrud and Gjertz 1990;
   Bjorge 1991; Henriksen and Haug 1994). In this paper, we present
   preliminary results of investigations on the annual pup production of
   harbour seals at the main breeding site, the distribution of seals
   observed outside the breeding season and the daily activity cycle of
   seals during the summer.}},
Publisher = {{WALTER DE GRUYTER GMBH}},
Address = {{GENTHINER STRASSE 13, D-10785 BERLIN, GERMANY}},
