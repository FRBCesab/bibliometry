@article{ ISI:000172381900004,
Author = {Pires, R and Neves, HC},
Title = {{Mediterranean monk seal Monachus monachus conservation: a case study in
   the Desertas Islands}},
Journal = {{MAMMALIA}},
Year = {{2001}},
Volume = {{65}},
Number = {{3}},
Pages = {{301-308}},
Note = {{13th Annual Meeting of the European-Cetacean-Society (ECS), VALENCIA,
   SPAIN, APR   05, 1999}},
Organization = {{European Cetacean Soc}},
Abstract = {{The population of monk seals Monachus monachus on the Desertas Islands
   started to decline dramatically after the 1970s, hence the introduction
   of a project for the conservation of the species in 1988. Since then
   measures have been taken to achieve effective protection of those
   islands, and environmental education, monitoring and research programmes
   have been carried out. Methodologies used have been as non-intrusive as
   possible. Ten years later, the results have proved to be very positive.
   The monk seal, as well as its habitat, are adequately protected and the
   population, estimated at 20 individuals, is now on the increase with the
   growing number of births. With regard to the finding of biological and
   ecological data on the species, although somehow limited, it has been
   sufficient to help establish the most suitable strategies for the
   protection and conservation of the monk seal on the Desertas Islands.}},
