@article{ ISI:000088214400023,
Author = {Fieberg, J and Ellner, SP},
Title = {{When is it meaningful to estimate an extinction probability?}},
Journal = {{ECOLOGY}},
Year = {{2000}},
Volume = {{81}},
Number = {{7}},
Pages = {{2040-2047}},
Month = {{JUL}},
Abstract = {{Recently Don Ludwig has shown that calculations of extinction
   probabilities based on currently available data are often meaningless
   due to the large uncertainty accompanying the estimates. Here we address
   two questions posed by his findings. Can one ever calculate extinction
   probabilities accurately? If so, how much data would be necessary? Our
   analysis indicates that reliable predictions of long-term extinction
   probabilities are likely to require unattainable amounts of data.
   Analytic calculations based on diffusion approximations indicate that
   reliable predictions of extinction probabilities can be made only for
   short-term time horizons (10\% to 20\% as long as the period over which
   the population has been monitored). Simulation results for unstructured
   and structured populations (three stage classes) agree with these
   calculations.}},
Publisher = {{ECOLOGICAL SOC AMER}},
Address = {{1707 H ST NW, STE 400, WASHINGTON, DC 20006-3915 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Fieberg, J (Reprint Author), N Carolina State Univ, Dept Stat, Biomath Grad Program, Raleigh, NC 27695 USA.
   N Carolina State Univ, Dept Stat, Biomath Grad Program, Raleigh, NC 27695 USA.}},
ISSN = {{0012-9658}},
Keywords = {{conservation; data requirements; diffusion approximation; extinction;
