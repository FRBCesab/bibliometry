@article{ ISI:000174027000012,
Author = {Anglesio, L and Benedetto, A and Bonino, A and Colla, D and Martire, F
   and Fusette, SS and d'Amore, G},
Title = {{Population exposure to electromagnetic fields generated by radio base
   stations: Evaluation of the urban background by using provisional model
   and instrumental measurements}},
