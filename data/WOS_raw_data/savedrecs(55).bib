@article{ ISI:000089031600015,
Author = {Gillis, EA and Krebs, CJ},
Title = {{Survival of dispersing versus philopatric juvenile snowshoe hares: do
   dispersers die?}},
Journal = {{OIKOS}},
Year = {{2000}},
Volume = {{90}},
Number = {{2}},
Pages = {{343-346}},
Month = {{AUG}},
Abstract = {{We used radio-telemetry to monitor the survival of dispersing and
   philopatric juvenile snowshoe hares (Lepus americanus) in southwestern
   Yukon Territory, Canada, during a cyclic population increase. Neither
   28-d survival nor the proportion of hares surviving to breed differed
   significantly between juvenile hares that dispersed and those that did
   not, nor was there a significant relationship between dispersal distance
   and fate (dead or alive). Our results indicate that the overall survival
   cost associated with natal dispersal is low for snowshoe hares during
   the early increase of the hare cycle.}},
