@inproceedings{ ISI:000167541400019,
Author = {Gallais, A and Charcosset, A and Goldringer, I and Hospital, F and
   Moreau, L},
Editor = {{Gallais, A and Dillmann, C and Goldringer, I}},
Title = {{Progress and prospects for marker-assisted selection}},
