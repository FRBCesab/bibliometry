@article{ ISI:000165549400009,
Author = {Kremp, A},
Title = {{Distribution, dynamics and in situ seeding potential of Scrippsiella
   hangoei (Dinophyceae) cyst populations from the Baltic Sea}},
Journal = {{JOURNAL OF PLANKTON RESEARCH}},
